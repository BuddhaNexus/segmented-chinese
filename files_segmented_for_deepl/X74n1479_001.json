[
    {
        "source_sentence": "孔雀尊經科儀",
        "from": "X74n1479_001:0539a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(念大悲呪畢洒淨)。",
        "from": "X74n1479_001:0539a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦如來。",
        "from": "X74n1479_001:0539a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來說經教主。",
        "from": "X74n1479_001:0539a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀如來。",
        "from": "X74n1479_001:0539a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來降臨法會。",
        "from": "X74n1479_001:0539a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難陀尊者。",
        "from": "X74n1479_001:0539a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者同垂加護。",
        "from": "X74n1479_001:0539a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切以。",
        "from": "X74n1479_001:0539a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旃檀林內六銖價重於娑婆。",
        "from": "X74n1479_001:0539a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "給孤園中一炷通津於靈鷲。",
        "from": "X74n1479_001:0539a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古鼎盤旋清瑞氣。",
        "from": "X74n1479_001:0539a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉爐繚遶碧祥雲。",
        "from": "X74n1479_001:0539a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今熱處獻如來。",
        "from": "X74n1479_001:0539a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願慈悲哀納受。",
        "from": "X74n1479_001:0539a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰啟。",
        "from": "X74n1479_001:0539a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖中聖主天中天王。",
        "from": "X74n1479_001:0539a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間名稱普聞。",
        "from": "X74n1479_001:0539a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拔循環救苦之慈父。",
        "from": "X74n1479_001:0539a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "濟業海利樂之導師。",
        "from": "X74n1479_001:0539a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應願似空谷傳聲。",
        "from": "X74n1479_001:0539a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赴感若春回大聲。",
        "from": "X74n1479_001:0539a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五天調御。",
        "from": "X74n1479_001:0539a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界世雄。",
        "from": "X74n1479_001:0539a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降臨道場。",
        "from": "X74n1479_001:0539a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作乎明證(通情念)。",
        "from": "X74n1479_001:0539a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　本師釋迦牟尼佛　　孔雀海會佛菩薩　天龍八部降吉祥　　摩訶般若波羅蜜蓋聞孔雀明王經者。",
        "from": "X74n1479_001:0539a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經名孔雀。",
        "from": "X74n1479_001:0539a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說佛姥之因緣。",
        "from": "X74n1479_001:0539a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪說毒蛇。",
        "from": "X74n1479_001:0539a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救眾生之患難。",
        "from": "X74n1479_001:0539a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求所請。",
        "from": "X74n1479_001:0539a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨願隨心。",
        "from": "X74n1479_001:0539a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食鋪排。",
        "from": "X74n1479_001:0539a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各香花供養。",
        "from": "X74n1479_001:0539a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各生歡喜。",
        "from": "X74n1479_001:0539a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸听加持。",
        "from": "X74n1479_001:0539a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言演處。",
        "from": "X74n1479_001:0539a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四百四病盡消除。",
        "from": "X74n1479_001:0539a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密語宣時。",
        "from": "X74n1479_001:0539a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬禍萬殃俱殄滅。",
        "from": "X74n1479_001:0539a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天神地神水神藥叉神。",
        "from": "X74n1479_001:0539a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首歸依。",
        "from": "X74n1479_001:0539a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山王龍王河王大仙。",
        "from": "X74n1479_001:0539a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傾心敬仰。",
        "from": "X74n1479_001:0539a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅又女不能為害。",
        "from": "X74n1479_001:0539a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥叉將總願垂祥。",
        "from": "X74n1479_001:0539a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灾星變作福星。",
        "from": "X74n1479_001:0539a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡曜化為吉曜。",
        "from": "X74n1479_001:0539b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公私口舌似氷消。",
        "from": "X74n1479_001:0539b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭魅蠱毒而霧散。",
        "from": "X74n1479_001:0539b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在官者清平吉利。",
        "from": "X74n1479_001:0539b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居家者福壽康寧。",
        "from": "X74n1479_001:0539b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若老若少保安康。",
        "from": "X74n1479_001:0539b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是男是女皆慶。",
        "from": "X74n1479_001:0539b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明王威力。",
        "from": "X74n1479_001:0539b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "週遍河沙。",
        "from": "X74n1479_001:0539b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘆莫能窮。",
        "from": "X74n1479_001:0539b03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚揚難盡。",
        "from": "X74n1479_001:0539b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲開大部靈文。",
        "from": "X74n1479_001:0539b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先稱七佛嘉號。",
        "from": "X74n1479_001:0539b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊眾毋勞。",
        "from": "X74n1479_001:0539b04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨言後和。",
        "from": "X74n1479_001:0539b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孔雀明王佛大孔雀明王佛",
        "from": "X74n1479_001:0539b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金耀孔雀明王佛佛姥大孔雀明王佛",
        "from": "X74n1479_001:0539b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明王佛明王佛",
        "from": "X74n1479_001:0539b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雪山南面金耀佛姥大孔雀明王佛",
        "from": "X74n1479_001:0539b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出靈鷲山。",
        "from": "X74n1479_001:0539b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人天敬仰。",
        "from": "X74n1479_001:0539b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊鹿野苑。",
        "from": "X74n1479_001:0539b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉大法輪。",
        "from": "X74n1479_001:0539b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰讚洪慈。",
        "from": "X74n1479_001:0539b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俯垂明證。",
        "from": "X74n1479_001:0539b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茲辰誦經開啟。",
        "from": "X74n1479_001:0539b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有意敷陳。",
        "from": "X74n1479_001:0539b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大圓鏡中。",
        "from": "X74n1479_001:0539b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諒垂昭鑒。",
        "from": "X74n1479_001:0539b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茲據(入意)。",
        "from": "X74n1479_001:0539b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即日沐手然香。",
        "from": "X74n1479_001:0539b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拜于雪山教主孔雀能仁。",
        "from": "X74n1479_001:0539b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願展金眸。",
        "from": "X74n1479_001:0539b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑒茲微懇。",
        "from": "X74n1479_001:0539b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉於　年　月　日。",
        "from": "X74n1479_001:0539b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請命僧眾于家供佛。",
        "from": "X74n1479_001:0539b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作證演念。",
        "from": "X74n1479_001:0539b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛姆大孔雀明王尊經。",
        "from": "X74n1479_001:0539b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持經文神呪。",
        "from": "X74n1479_001:0539b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱揚萬德洪名。",
        "from": "X74n1479_001:0539b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所集功德。",
        "from": "X74n1479_001:0539b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先伸回向廣大真如實際。",
        "from": "X74n1479_001:0539b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴無工佛果菩提。",
        "from": "X74n1479_001:0539b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "工酧佛祖之深恩。",
        "from": "X74n1479_001:0539b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次答龍天之厚德。",
        "from": "X74n1479_001:0539b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祝扶當生本命星官。",
        "from": "X74n1479_001:0539b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照臨吉威乾像。",
        "from": "X74n1479_001:0539b20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願等一真慈。",
        "from": "X74n1479_001:0539b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同納功德。",
        "from": "X74n1479_001:0539b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共降禎祥。",
        "from": "X74n1479_001:0539b21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所冀　向去。",
        "from": "X74n1479_001:0539b21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身安壽求。",
        "from": "X74n1479_001:0539b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福集灾消。",
        "from": "X74n1479_001:0539b21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在佛光中吉祥如意。",
        "from": "X74n1479_001:0539b21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來開啟情悃如斯。",
        "from": "X74n1479_001:0539b22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仰勞尊眾。",
        "from": "X74n1479_001:0539b22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不倦慈悲振鈴。",
        "from": "X74n1479_001:0539b22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦佛姆大孔雀明王經。",
        "from": "X74n1479_001:0539b23_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前啟請法(入經畢)。",
        "from": "X74n1479_001:0539b23_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉如意輪王菩薩摩訶薩。",
        "from": "X74n1479_001:0539c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至滿怛囉莎縛賀(舉一切天神咸集會)因緣自性所出生。",
        "from": "X74n1479_001:0539c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有種種微妙花香燈塗果供養。",
        "from": "X74n1479_001:0539c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奉獻上師三寶尊。",
        "from": "X74n1479_001:0539c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願慈悲哀納受。",
        "from": "X74n1479_001:0539c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戶唵薩哩縛怛佗誐哆唵布瑟畢度畢阿盧計頞底儞尾底捨不達布喏[口*彌]迦三摩怛囉薩普囉曩三摩耶吽奉香華自在明王佛。",
        "from": "X74n1479_001:0539c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結界　持國天王降吉祥",
        "from": "X74n1479_001:0539c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　東方世界主　　持國大天王",
        "from": "X74n1479_001:0539c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　統領彥達嚩　　結成瑜伽界",
        "from": "X74n1479_001:0539c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　南方世界主　　增長大天王",
        "from": "X74n1479_001:0539c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　統領矩畔拏　　結成灌頂界",
        "from": "X74n1479_001:0539c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　西方世界主　　廣目大天王",
        "from": "X74n1479_001:0539c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　統領大龍主　　結成蓮華界",
        "from": "X74n1479_001:0539c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　北方世界主　　多聞大天王",
        "from": "X74n1479_001:0539c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　統領藥叉主　　結成羯磨界",
        "from": "X74n1479_001:0539c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　中方世界主　　大梵大天王",
        "from": "X74n1479_001:0539c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　統領人天眾　　結成瑜伽界",
        "from": "X74n1479_001:0539c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　東方世界主　　佛號阿閦尊　　流演妙伽陀　結成金剛界　南方世界主　　佛號寶生尊　　流演妙伽陀　結成蓮華界　北方世界主　　佛號成就尊　　流演妙伽陀　結成羯磨界　中方世界主　　佛號毗盧尊　　流演妙伽陀　結成瑜伽界　奉請東方阿閦佛　　其身青色放光明　手印執持降魔杵　　眾等志心歸命禮　今將密語遶香壇　　憑此結成金剛界　奉請南方寶生佛　　其身赤色放光明　手印執持摩尼寶　　眾等志心歸命禮　今將密語遶香壇　　憑此結成灌頂界　奉請西方彌陀佛　　其身白色放光明　手印執持妙蓮花　　眾等志心歸命禮　今將密語遶香壇　　憑此結成蓮華界　奉請北方成就佛　　其身黑色放光明　手印執持妙降魔　　眾等志心歸命禮　今將密語遶香壇　　憑此結成羯磨界　奉請中方毗盧佛　　其身黃色放光明　手印執持千輻輪　　眾等志心歸命禮　今將密語遶香壇　　憑此結成瑜伽界集福迎祥幢幡為寶蓋。",
        "from": "X74n1479_001:0539c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "振動金鈴。",
        "from": "X74n1479_001:0540a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殄滅諸阿閦持國寶生增長妖恠。",
        "from": "X74n1479_001:0540a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌陀佛如來慈悲甚廣大。",
        "from": "X74n1479_001:0540a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣目天王來。",
        "from": "X74n1479_001:0540a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就多聞",
        "from": "X74n1479_001:0540a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毗盧大梵",
        "from": "X74n1479_001:0540a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結蓮華界　金剛　灌頂　羯磨　瑜伽　天阿蘇來藥叉等(摩訶摩瑜利金耀界云云)　來听法者應志心(南無大摩訶摩瑜利)孔雀明王佛(云云)上卷圓滿保平安南無大孔雀明王佛　歸命禮真如沒馱耶中卷藥叉奉藥叉聖眾降吉祥掛金索戒定真香。",
        "from": "X74n1479_001:0540b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚氣衝天上。",
        "from": "X74n1479_001:0540b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施主虔誠。",
        "from": "X74n1479_001:0540b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爇在金爐放。",
        "from": "X74n1479_001:0540b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頃刻氛氤。",
        "from": "X74n1479_001:0540b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即遍滿十方。",
        "from": "X74n1479_001:0540b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔日耶輸。",
        "from": "X74n1479_001:0540b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免難除灾障。",
        "from": "X74n1479_001:0540b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寄生草",
        "from": "X74n1479_001:0540b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香纔爇爐。",
        "from": "X74n1479_001:0540b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "焚寶鼎中。",
        "from": "X74n1479_001:0540b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旃檀況乳真堪供。",
        "from": "X74n1479_001:0540b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祥雲繚遶蓮華動。",
        "from": "X74n1479_001:0540b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方諸佛下天宮。",
        "from": "X74n1479_001:0540b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清涼山羅漢。",
        "from": "X74n1479_001:0540b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來受人間供明王佛。",
        "from": "X74n1479_001:0540b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揭句寸那神藥叉將。",
        "from": "X74n1479_001:0540b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波吒黎手處藥叉神。",
        "from": "X74n1479_001:0540b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跋阿囉[噙-离+(恭-共)]哆藥叉王。",
        "from": "X74n1479_001:0540b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住窣吐奴邑。",
        "from": "X74n1479_001:0540b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賢善大藥叉。",
        "from": "X74n1479_001:0540b17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住於世囉城。",
        "from": "X74n1479_001:0540b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清江引",
        "from": "X74n1479_001:0540b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛首旃檀香。",
        "from": "X74n1479_001:0540b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養裊裊祥雲起。",
        "from": "X74n1479_001:0540b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "透上九霄中。",
        "from": "X74n1479_001:0540b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結成香雲蓋。",
        "from": "X74n1479_001:0540b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願慈悲哀納受。",
        "from": "X74n1479_001:0540b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浪淘沙",
        "from": "X74n1479_001:0540b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沉乳共旃檀。",
        "from": "X74n1479_001:0540b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馥郁馨香。",
        "from": "X74n1479_001:0540b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金爐纔熱遍十方。",
        "from": "X74n1479_001:0540b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔日耶輸曾免難。",
        "from": "X74n1479_001:0540b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火內除殃。",
        "from": "X74n1479_001:0540b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七懺悔　求哀懺悔明王佛",
        "from": "X74n1479_001:0540c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三千界內慈悲主　　百億洲中大法王　咸開慧目鑒凡情　　眾生有求皆感應(讚嘆三寶。",
        "from": "X74n1479_001:0540c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恭請證明。",
        "from": "X74n1479_001:0540c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為今辰。",
        "from": "X74n1479_001:0540c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演經信□。",
        "from": "X74n1479_001:0540c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "泊家眷等。",
        "from": "X74n1479_001:0540c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目茲演經。",
        "from": "X74n1479_001:0540c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "〔問〕去各人身中所有病苦。",
        "from": "X74n1479_001:0540c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云云願早安痊)能離病苦明王佛。",
        "from": "X74n1479_001:0540c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　佛住真如法界內　　無相無形離諸垢　眾生觀見種種身　　一切恐怖皆遠離能除恐怖　恐怖恐怖悉皆遠離。",
        "from": "X74n1479_001:0540c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　解結解結解冤結　　解了多生冤和業　冤家債生兩相逢　　相逢相遇相歡悅冤家冤家各相歡喜　解冤釋結。",
        "from": "X74n1479_001:0540c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　明王經力不思議　　救度眾生無盡期　施主志心歸命禮　　不饒益事皆饒益不饒益事廣大饒益　廣大饒益。",
        "from": "X74n1479_001:0540c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　四八端嚴微妙相　　僧祇三大刧修來　面如滿月目如蓮　　天上人間咸恭敬毒藥毒藥化為甘露　化為甘露。",
        "from": "X74n1479_001:0540c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　天上天下無如佛　　十方世界亦無比　世間所有我盡見　　一切無有如佛者佗人厭禱悉皆消滅　能離厭禱。",
        "from": "X74n1479_001:0540c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一障二語三毒罪　　四重五逆六根愆　七遮八難罪消容　　九結十纏皆懺悔懺除罪業　五言偈　頂中紅肉髻　　如日映青山　　眉間白玉毫　似月臨滄海　稽首天中天　　降誕毗藍園　　分手指天地　七步躡紅蓮　海印澄三昧　　慈光照十方　　弘施十二願　故號法中王　菩薩清涼月　　猶如碧鏡空　　眾生心水淨　菩薩影現中　百刧千生罪　　一念蕩消除　　似大焚枯草　滅盡無有餘四言　佛真法身　　猶若虗空　　應物現形　如水月中　大圓滿覺　　應跡西乾　　心包太虗　量週沙界　佛法僧寶　　眾生福田　　凡在禱祈　必彰感應　常住三寶　　體徧十方　　隨眾生心　感而應現　佛功德山　　惟高惟聳　　法智慧海　最廣最深羅剎女　羅剎女眾降吉祥羅剎女眾降吉祥羅剎女眾降大吉祥羅剎女眾降吉祥消灾障羅利女眾降大吉祥四平聲　龍王聖眾降吉祥佛世尊龍王龍樹王梵天龍王大龍王帝釋龍王消障灾燄摩龍王保安康蜜峯採華佛世尊龍王梵天龍王龍樹王菩薩燄摩龍王大海龍王南無龍樹王菩薩清江引天阿蘇來藥叉等來听法者應志心孔雀明王經請佛作證明　大慈大悲　　救苦救難　　千手千眼　水月光中　　普陀岩畔　　一葉扁舟　白衣妙相　　紫竹林中　　風波浪內　隨心滿願金字經天阿蘇囉藥叉等來听法者應志心孔雀明王經大慈大悲觀世音消灾障唵嚤呢叭[口*迷]吽。",
        "from": "X74n1479_001:0540c21_0",
        "context_with_next_sentence": true
    },
