[
    {
        "source_sentence": "解捲論一卷",
        "from": "T31n1620_001:0883c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳那菩薩造",
        "from": "T31n1620_001:0883c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳西印度三藏真諦譯",
        "from": "T31n1620_001:0883c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三界者。",
        "from": "T31n1620_001:0883c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯以言名為體。",
        "from": "T31n1620_001:0883c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由強分別。",
        "from": "T31n1620_001:0883c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非實有法故不得真。",
        "from": "T31n1620_001:0883c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由簡擇門諸法自性。",
        "from": "T31n1620_001:0883c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為生不顛倒智故立此論。",
        "from": "T31n1620_001:0883c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於藤起蛇知　　見藤則無境",
        "from": "T31n1620_001:0883c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昏昧時中。",
        "from": "T31n1620_001:0883c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在非遠處。",
        "from": "T31n1620_001:0883c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於藤色形。",
        "from": "T31n1620_001:0883c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見似蛇相。",
        "from": "T31n1620_001:0883c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為境所誑。",
        "from": "T31n1620_001:0883c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未見差別。",
        "from": "T31n1620_001:0883c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼是蛇。",
        "from": "T31n1620_001:0883c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生決定解。",
        "from": "T31n1620_001:0883c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見藤異相。",
        "from": "T31n1620_001:0883c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如分別故。",
        "from": "T31n1620_001:0883c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛妄生故。",
        "from": "T31n1620_001:0883c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔解但是亂知則無有境。",
        "from": "T31n1620_001:0883c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若見藤分已　　藤知如蛇知",
        "from": "T31n1620_001:0883c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若分分思量。",
        "from": "T31n1620_001:0883c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "析此藤境。",
        "from": "T31n1620_001:0883c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藤體不可得。",
        "from": "T31n1620_001:0883c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無體。",
        "from": "T31n1620_001:0883c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此藤知如蛇知。",
        "from": "T31n1620_001:0883c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是亂知。",
        "from": "T31n1620_001:0883c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於藤諸分中。",
        "from": "T31n1620_001:0883c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如是思量分析。",
        "from": "T31n1620_001:0883c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體相不可得故。",
        "from": "T31n1620_001:0883c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此知緣藤及藤分悉是亂知。",
        "from": "T31n1620_001:0883c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切假名類　　簡擇自性時",
        "from": "T31n1620_001:0883c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　假名從他起　　乃至俗知境",
        "from": "T31n1620_001:0883c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依分分析觀察藤等。",
        "from": "T31n1620_001:0883c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見自體故。",
        "from": "T31n1620_001:0883c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蛇知此藤等知。",
        "from": "T31n1620_001:0883c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是亂知。",
        "from": "T31n1620_001:0883c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實無有境一切假名。",
        "from": "T31n1620_001:0883c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法瓶衣人等。",
        "from": "T31n1620_001:0883c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若觀瓦等諸分。",
        "from": "T31n1620_001:0883c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至俗智境在。",
        "from": "T31n1620_001:0883c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及最後分。",
        "from": "T31n1620_001:0883c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中瓶等假名從他而起。",
        "from": "T31n1620_001:0883c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最後無分析難顯離皆無一切假名類最後分無分析。",
        "from": "T31n1620_001:0883c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯一隣虛。",
        "from": "T31n1620_001:0883c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離一大餘大及一大並不可顯現。",
        "from": "T31n1620_001:0883c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有體故。",
        "from": "T31n1620_001:0883c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如兔角等。",
        "from": "T31n1620_001:0883c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其異云何。",
        "from": "T31n1620_001:0883c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隣虛者不可立為一物。",
        "from": "T31n1620_001:0883c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有物必有方異。",
        "from": "T31n1620_001:0883c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如瓶等。",
        "from": "T31n1620_001:0883c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓶等諸物是世間有。",
        "from": "T31n1620_001:0884a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有六方異。",
        "from": "T31n1620_001:0884a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故有分不成一物。",
        "from": "T31n1620_001:0884a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若隣虛是有。",
        "from": "T31n1620_001:0884a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應有六方。",
        "from": "T31n1620_001:0884a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則是有分不成一物若不成一物。",
        "from": "T31n1620_001:0884a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為多物所成。",
        "from": "T31n1620_001:0884a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與瓶不異。",
        "from": "T31n1620_001:0884a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無實體。",
        "from": "T31n1620_001:0884a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智人於俗境　　勿起真實意",
        "from": "T31n1620_001:0884a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此三界唯有散亂。",
        "from": "T31n1620_001:0884a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智人欲求解脫。",
        "from": "T31n1620_001:0884a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不應起真實計。",
        "from": "T31n1620_001:0884a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T31n1620_001:0884a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有亂識。",
        "from": "T31n1620_001:0884a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答若汝言我信瓶等。",
        "from": "T31n1620_001:0884a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外物自性不可得故。",
        "from": "T31n1620_001:0884a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有分別亂識。",
        "from": "T31n1620_001:0884a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣無境起。",
        "from": "T31n1620_001:0884a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T31n1620_001:0884a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻化人乾闥婆城等實非有。",
        "from": "T31n1620_001:0884a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亂識似幻等起而非無。",
        "from": "T31n1620_001:0884a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是義不然。",
        "from": "T31n1620_001:0884a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不成就故。",
        "from": "T31n1620_001:0884a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何不成就。",
        "from": "T31n1620_001:0884a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如所見不如是有故。",
        "from": "T31n1620_001:0884a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亂識似無物由物無體。",
        "from": "T31n1620_001:0884a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何識得有。",
        "from": "T31n1620_001:0884a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如所緣塵自性能緣自性亦如是。",
        "from": "T31n1620_001:0884a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所緣塵既無。",
        "from": "T31n1620_001:0884a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亂識不能自起。",
        "from": "T31n1620_001:0884a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由他功力。",
        "from": "T31n1620_001:0884a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他既不成。",
        "from": "T31n1620_001:0884a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起義何在。",
        "from": "T31n1620_001:0884a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是義故。",
        "from": "T31n1620_001:0884a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亂識有義。",
        "from": "T31n1620_001:0884a15_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何得立。",
        "from": "T31n1620_001:0884a15_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於世間無如此法種子等生因。",
        "from": "T31n1620_001:0884a15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無所生芽等。",
        "from": "T31n1620_001:0884a16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果是有則無是處。",
        "from": "T31n1620_001:0884a16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說幻化等譬亦不可立。",
        "from": "T31n1620_001:0884a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切假名物　　若細心思量",
        "from": "T31n1620_001:0884a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智人欲等惑　　能除如蛇怖",
        "from": "T31n1620_001:0884a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如是說已。",
        "from": "T31n1620_001:0884a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "識三界但假名。",
        "from": "T31n1620_001:0884a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除瓶等麁識。",
        "from": "T31n1620_001:0884a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習微細心。",
        "from": "T31n1620_001:0884a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世間所立瓶衣等物。",
        "from": "T31n1620_001:0884a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由假名有。",
        "from": "T31n1620_001:0884a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "約世俗心不違此事。",
        "from": "T31n1620_001:0884a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後為遣此俗心。",
        "from": "T31n1620_001:0884a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方起簡擇心。",
        "from": "T31n1620_001:0884a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但見唯有亂識。",
        "from": "T31n1620_001:0884a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有外塵。",
        "from": "T31n1620_001:0884a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亂識因不成就故似無物故。",
        "from": "T31n1620_001:0884a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體則不成就。",
        "from": "T31n1620_001:0884a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內外既無所有。",
        "from": "T31n1620_001:0884a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得會法空一切分別所作。",
        "from": "T31n1620_001:0884a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲等諸惑智人易除。",
        "from": "T31n1620_001:0884a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如於藤妄起蛇想。",
        "from": "T31n1620_001:0884a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而生怖畏。",
        "from": "T31n1620_001:0884a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見差別。",
        "from": "T31n1620_001:0884a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定知是藤。",
        "from": "T31n1620_001:0884a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除蛇怖。",
        "from": "T31n1620_001:0884a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由思量能起。",
        "from": "T31n1620_001:0884a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲等諸塵自性速易能滅。",
        "from": "T31n1620_001:0884a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲等惑妄亦復如是。",
        "from": "T31n1620_001:0884a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智人不違世　　隨說世間法",
        "from": "T31n1620_001:0884a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若欲滅惑障　　依真應觀察",
        "from": "T31n1620_001:0884b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世間瓶衣等物。",
        "from": "T31n1620_001:0884b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信有不違。",
        "from": "T31n1620_001:0884b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或說示他如此智人先隨此事。",
        "from": "T31n1620_001:0884b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後若求解脫應修真理。",
        "from": "T31n1620_001:0884b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "簡擇世法自性。",
        "from": "T31n1620_001:0884b03_4",
        "context_with_next_sentence": true
