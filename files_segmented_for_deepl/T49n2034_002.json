[
    {
        "source_sentence": "歷代三寶紀卷第二(帝年次前漢新王後漢)開皇十七年翻經學士臣費長房上帝年次者。",
        "from": "T49n2034_002:0029b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉邦字季。",
        "from": "T49n2034_002:0029b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沛郡豐人。",
        "from": "T49n2034_002:0029b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母媼息于大澤夢與神交。",
        "from": "T49n2034_002:0029b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "感而生季。",
        "from": "T49n2034_002:0029b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首龍顏額隆準。",
        "from": "T49n2034_002:0029b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寬仁大度美鬚髯。",
        "from": "T49n2034_002:0029b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左股髀間有七十二黑子。",
        "from": "T49n2034_002:0029b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因託暴秦鼓其潛翼。",
        "from": "T49n2034_002:0029b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既吞強楚遂奮雲氣。",
        "from": "T49n2034_002:0029b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後凡經七十二戰。",
        "from": "T49n2034_002:0029b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天下乃定。",
        "from": "T49n2034_002:0029b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑子之應其瑞在茲。",
        "from": "T49n2034_002:0029b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既遠承周因爾稱漢。",
        "from": "T49n2034_002:0029b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改咸陽曰長安。",
        "from": "T49n2034_002:0029b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而都之。",
        "from": "T49n2034_002:0029b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自從開闢三皇五帝夏殷周秦迄文景前並以帝王標年。",
        "from": "T49n2034_002:0029b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無別立號。",
        "from": "T49n2034_002:0029b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至武帝世始製年名稱。",
        "from": "T49n2034_002:0029b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建元云元狩。",
        "from": "T49n2034_002:0029b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二年霍去病討匈奴。",
        "from": "T49n2034_002:0029b13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過居延山擒休屠王。",
        "from": "T49n2034_002:0029b13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲其金人以為大神。",
        "from": "T49n2034_002:0029b14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "列置於宮。",
        "from": "T49n2034_002:0029b14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香禮之。",
        "from": "T49n2034_002:0029b15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即像初至。",
        "from": "T49n2034_002:0029b15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三年鑿昆明池。",
        "from": "T49n2034_002:0029b15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍於下際悉是灰墨。",
        "from": "T49n2034_002:0029b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝以其事問東方朔。",
        "from": "T49n2034_002:0029b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朔奉答云。",
        "from": "T49n2034_002:0029b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非臣所知。",
        "from": "T49n2034_002:0029b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陛下可訪西域胡人。",
        "from": "T49n2034_002:0029b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其後張騫使大夏還。",
        "from": "T49n2034_002:0029c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漢始知有身毒國。",
        "from": "T49n2034_002:0029c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又平帝世。",
        "from": "T49n2034_002:0029c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大夫劉向自稱。",
        "from": "T49n2034_002:0029c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "余覽典籍往往見有佛經。",
        "from": "T49n2034_002:0029c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及刪列仙傳云。",
        "from": "T49n2034_002:0029c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得藏書緬尋太史創撰列仙圖。",
        "from": "T49n2034_002:0029c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自六代迄到于今七百餘人。",
        "from": "T49n2034_002:0029c04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向檢虛實定得一百四十六人。",
        "from": "T49n2034_002:0029c04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其七十四人已見佛經。",
        "from": "T49n2034_002:0029c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "推此豈非漢時經目及古舊二錄道安所載十二賢者等經例是。",
        "from": "T49n2034_002:0029c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所餘止有七十二人為列仙傳。",
        "from": "T49n2034_002:0029c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此蓋博識覩其大倫。",
        "from": "T49n2034_002:0029c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將知周時久流釋典。",
        "from": "T49n2034_002:0029c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情識未洽歸信者稀。",
        "from": "T49n2034_002:0029c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秦雖爇除漢復興出。",
        "from": "T49n2034_002:0029c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故令劉向搜藏書見更列仙傳云。",
        "from": "T49n2034_002:0029c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又哀帝世。",
        "from": "T49n2034_002:0029c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元壽年中景憲使於大月支國受得浮圖經。",
        "from": "T49n2034_002:0029c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驗向所尋便實錄矣。",
        "from": "T49n2034_002:0029c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王莽字巨君。",
        "from": "T49n2034_002:0029c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元城人。",
        "from": "T49n2034_002:0029c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為漢大司馬翼輔平帝。",
        "from": "T49n2034_002:0029c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元始五年乙丑帝崩。",
        "from": "T49n2034_002:0029c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莽立宣帝玄孫嬰號稱孺子。",
        "from": "T49n2034_002:0029c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莽為丞相。",
        "from": "T49n2034_002:0029c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居攝三年。",
        "from": "T49n2034_002:0029c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即真十五年通數十八年。",
        "from": "T49n2034_002:0029c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實止十七載。",
        "from": "T49n2034_002:0029c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莽為景帝第七世孫劉玄字聖公所滅。",
        "from": "T49n2034_002:0029c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱更始元。",
        "from": "T49n2034_002:0029c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄立二年復為赤眉所殺。",
        "from": "T49n2034_002:0029d01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉秀字文叔。",
        "from": "T49n2034_002:0029d01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高祖九世孫。",
        "from": "T49n2034_002:0029d02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長沙定王後。",
        "from": "T49n2034_002:0029d02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居乎南陽之白水鄉。",
        "from": "T49n2034_002:0029d02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父為濟陽令。",
        "from": "T49n2034_002:0029d03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生秀於縣中。",
        "from": "T49n2034_002:0029d03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有赤光之瑞。",
        "from": "T49n2034_002:0029d03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是歲嘉穀一莖九穗。",
        "from": "T49n2034_002:0029d03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因名為秀。",
        "from": "T49n2034_002:0029d04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隆準日角口大眉長面美髭髯。",
        "from": "T49n2034_002:0029d04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性善仁愛。",
        "from": "T49n2034_002:0029d05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起於南陽。",
        "from": "T49n2034_002:0029d05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除蕩赤眉。",
        "from": "T49n2034_002:0029d05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂定天下。",
        "from": "T49n2034_002:0029d06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為後漢光武帝焉。",
        "from": "T49n2034_002:0029d06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徙都洛陽。",
        "from": "T49n2034_002:0029d06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號年建武。",
        "from": "T49n2034_002:0029d07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞昔世語光武起南陽。",
        "from": "T49n2034_002:0029d07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南陽無賤士。",
        "from": "T49n2034_002:0029d07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常謂此虛誕。",
        "from": "T49n2034_002:0029d08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見今經說。",
        "from": "T49n2034_002:0029d08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋迦出天竺。",
        "from": "T49n2034_002:0029d08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天竺多聖人。",
        "from": "T49n2034_002:0029d08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方驗斯真實。",
        "from": "T49n2034_002:0029d09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至明帝世永平七年。",
        "from": "T49n2034_002:0029d09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽感神夢。",
        "from": "T49n2034_002:0029d09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覺已歡悅。",
        "from": "T49n2034_002:0029d10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命使西行。",
        "from": "T49n2034_002:0029d10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因爾宅心遊志福境。",
        "from": "T49n2034_002:0029d10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗從容暇問弟東平王處。",
        "from": "T49n2034_002:0029d11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "家居何者最樂。",
        "from": "T49n2034_002:0029d11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東平璝偉腰帶十圍。",
        "from": "T49n2034_002:0029d12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即奏對云。",
        "from": "T49n2034_002:0029d12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為善最樂。",
        "from": "T49n2034_002:0029d12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝稱之曰。",
        "from": "T49n2034_002:0029d13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此言甚大。",
        "from": "T49n2034_002:0029d13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "足可以副王之腰腹。",
        "from": "T49n2034_002:0029d13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後使迴還。",
        "from": "T49n2034_002:0029d13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍詔宣譯。",
        "from": "T49n2034_002:0029d14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "源發自此流注達今。",
        "from": "T49n2034_002:0029d14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法海無窮玄河靡竭。",
        "from": "T49n2034_002:0029d14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後兩漢通及新王。",
        "from": "T49n2034_002:0029d15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合二十六君。",
        "from": "T49n2034_002:0029d15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四百二十五載。",
        "from": "T49n2034_002:0029d16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紀錄帝世東西京云。",
        "from": "T49n2034_002:0029d16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(前漢)高帝(十二年都長安)　惠帝(七年)　呂后攝(八年)　文帝(二十三年)　景帝(十六年)　武帝(五十四年)　昭帝(十三年)　宣帝(二十五年)　元帝(十六年)　成帝(二十六年)　哀帝(六年)　平帝(五年)　新王莾(十七年治長安)　更始帝(二年亦長安)(後漢)光武帝(二十三年都洛陽)　明帝(十八年十年譯經)　章帝(十三年)　和帝(十七年)　殤帝(一年)　安帝(十九年)　順帝(十九年)　沖帝(一年)　質帝(一年)　桓帝(二十一年)　靈帝(二十二年)　獻帝(三十年)(前漢乙未)高帝元(劉氏名邦字阿季。",
        "from": "T49n2034_002:0029e01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堯後。",
        "from": "T49n2034_002:0030a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "治十二年都長安。",
        "from": "T49n2034_002:0030a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已四百五年)　(丙申)二　(丁酉)三　(戊戌)四　(己亥)五　(庚子)六　(辛丑)七(改咸陽為長安)　(壬寅)八　(癸卯)九　(甲辰)十　(乙巳)十一　(丙午)十二(丁未)惠帝盈元(高帝子治七年。",
        "from": "T49n2034_002:0030a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛入涅槃來至此已四百一十五年)　(戊申)二　(己酉)三(舂發長安六百里男女十四萬六千人。",
        "from": "T49n2034_002:0030a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "築長安城舊城西面。",
        "from": "T49n2034_002:0030a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日了。",
        "from": "T49n2034_002:0030a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六月又發諸侯率一十二萬人築南面)　(庚戌)四　(辛亥)五(正月又發六百里男女十四萬五千人築城北面。",
        "from": "T49n2034_002:0030a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日了。",
        "from": "T49n2034_002:0030a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夏又發一十二萬五千人築城東面。",
        "from": "T49n2034_002:0030a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十日了。",
        "from": "T49n2034_002:0030a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秋九月長安城四面一切悉訖)　(壬子)六　(癸丑)七(甲寅)呂后元(高帝后攝政八年佛入涅槃來至此已四百二十二年)　(乙卯)二　(丙辰)三　(丁巳)四　(戊午)五　(己未)六　(庚申)七　(辛酉)八(壬戌)文帝恒元(高帝庶子治二十三年佛涅槃來至此已四百三十二年)　(癸亥)二　(甲子)三　(乙丑)四　(丙寅)五　(丁卯)六　(戊辰)七　(己巳)八　(庚午)九　(辛未)十　(壬申)十一　(癸酉)十二　(甲戌)十三　(乙亥)十四　(丙子)十五　(丁丑)十六　(戊寅)後元元年　(己卯)二　(庚辰)三　(辛巳)四(文殊師利般泥洹經云。",
        "from": "T49n2034_002:0030a06_11",
        "context_with_next_sentence": true
    },
