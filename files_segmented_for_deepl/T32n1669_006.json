[
    {
        "source_sentence": "大宗地玄文本論卷第六",
        "from": "T32n1669_006:0678b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_006:0678b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_006:0678b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎無盡無窮塵塵數量道路大決擇分",
        "from": "T32n1669_006:0678b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十一",
        "from": "T32n1669_006:0678b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說大龍王重重廣海無盡大藏大決擇分。",
        "from": "T32n1669_006:0678b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說無盡無窮塵塵數量道路大決擇分。",
        "from": "T32n1669_006:0678b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_006:0678b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_006:0678b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　塵塵道路中　　如前所說量",
        "from": "T32n1669_006:0678b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　亦有五十一　　決定金剛位",
        "from": "T32n1669_006:0678b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依此位立相　　則有十種重",
        "from": "T32n1669_006:0678b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以為道路量　　二因一果等",
        "from": "T32n1669_006:0678b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_006:0678b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就此門中有幾數位。",
        "from": "T32n1669_006:0678b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一種真金剛位。",
        "from": "T32n1669_006:0678b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足圓滿無闕失故。",
        "from": "T32n1669_006:0678b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈塵塵道路中如前所說量亦有五十一決定金剛位故。",
        "from": "T32n1669_006:0678b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就此位中。",
        "from": "T32n1669_006:0678b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有十種變對法門。",
        "from": "T32n1669_006:0678b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能攝門量。",
        "from": "T32n1669_006:0678b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為十。",
        "from": "T32n1669_006:0678b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者二因一果門。",
        "from": "T32n1669_006:0678b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者一因一果門。",
        "from": "T32n1669_006:0678b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者少因多果門。",
        "from": "T32n1669_006:0678b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者因果一味門。",
        "from": "T32n1669_006:0678c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者無因無果門。",
        "from": "T32n1669_006:0678c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者自然安住門。",
        "from": "T32n1669_006:0678c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者因果門。",
        "from": "T32n1669_006:0678c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者果因門。",
        "from": "T32n1669_006:0678c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者言說門。",
        "from": "T32n1669_006:0678c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者言人門。",
        "from": "T32n1669_006:0678c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_006:0678c03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是十門以為門量。",
        "from": "T32n1669_006:0678c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈依此位立相則有十種重以為道路量二因一果等故。",
        "from": "T32n1669_006:0678c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼第一門形相如何。",
        "from": "T32n1669_006:0678c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_006:0678c06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以信心為初　　如次率自類",
        "from": "T32n1669_006:0678c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　取所餘位初　　至於定心位",
        "from": "T32n1669_006:0678c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　則取如來地　　亦如其次第",
        "from": "T32n1669_006:0678c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不退心為初　　率同品自類",
        "from": "T32n1669_006:0678c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　取餘位第二　　至於願心位",
        "from": "T32n1669_006:0678c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　亦取如來地　　修行位為初",
        "from": "T32n1669_006:0678c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如次率自類　　取餘位第三",
        "from": "T32n1669_006:0678c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　至於正心位　　亦取如來地",
        "from": "T32n1669_006:0678c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不退位為初　　如次率自類",
        "from": "T32n1669_006:0678c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　取餘位第四　　至灌頂住位",
        "from": "T32n1669_006:0678c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　亦取如來地　　離癡行為初",
        "from": "T32n1669_006:0678c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如次率自類　　取餘位第五",
        "from": "T32n1669_006:0678c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　至於無著行　　亦取如來地",
        "from": "T32n1669_006:0678c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　尊重行為初　　如次率自類",
        "from": "T32n1669_006:0678c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　取餘位第六　　至於真實行",
        "from": "T32n1669_006:0678c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　亦取如來地　　隨順觀眾生",
        "from": "T32n1669_006:0678c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　迴向以為初　　如次率自類",
        "from": "T32n1669_006:0678c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　取所餘四位　　各數量契當",
        "from": "T32n1669_006:0678c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　餘有如來地　　以同地為伴",
        "from": "T32n1669_006:0678c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　莊嚴一覺海　　各因及果稱",
        "from": "T32n1669_006:0678c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　配釋廣觀察　　其理當分明",
        "from": "T32n1669_006:0678c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_006:0678c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此門中為明何義。",
        "from": "T32n1669_006:0678c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示五十一位皆為同量。",
        "from": "T32n1669_006:0678c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二種因感得一果。",
        "from": "T32n1669_006:0678c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣三寶海無窮盡故。",
        "from": "T32n1669_006:0678c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此義云何。",
        "from": "T32n1669_006:0679a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂信心及發心地之二種因。",
        "from": "T32n1669_006:0679a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同一行相不相捨離。",
        "from": "T32n1669_006:0679a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱行合轉住一所作。",
        "from": "T32n1669_006:0679a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起無量具生無邊德。",
        "from": "T32n1669_006:0679a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足莊嚴一大覺海。",
        "from": "T32n1669_006:0679a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能生長因。",
        "from": "T32n1669_006:0679a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為最上第一出生增長決定真實本藏原母遠離繫縛莊嚴無勝地種子海會所莊嚴。",
        "from": "T32n1669_006:0679a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果名為具足真金剛。",
        "from": "T32n1669_006:0679a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿大慈悲法身虛空等無差別最初地地無上極海一盡大覺無二山王。",
        "from": "T32n1669_006:0679a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次念心地歡喜行地之二種法。",
        "from": "T32n1669_006:0679a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能長養因。",
        "from": "T32n1669_006:0679a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足莊嚴一大覺海。",
        "from": "T32n1669_006:0679a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因名為安樂常明決定增長無苦無妄自然照達一切法性無所障礙種子海會。",
        "from": "T32n1669_006:0679a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果名為常樂總明大虛空界甚深恒了無二山王。",
        "from": "T32n1669_006:0679a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次精進心地救護一切眾生迴向地之二種法。",
        "from": "T32n1669_006:0679a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能長養因。",
        "from": "T32n1669_006:0679a13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足莊嚴一大覺海。",
        "from": "T32n1669_006:0679a13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因名為發起殊勝大悲光水遠離懈怠常度常行本地自性具足通達種子海會。",
        "from": "T32n1669_006:0679a14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果名為慈悲光明常恒達慧無二山王。",
        "from": "T32n1669_006:0679a16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次慧心地逆流歡喜地之二種法。",
        "from": "T32n1669_006:0679a17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能長養因。",
        "from": "T32n1669_006:0679a17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足莊嚴一大覺海。",
        "from": "T32n1669_006:0679a18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因名為大真金剛日月光明自性離苦除斷闇品種子海會。",
        "from": "T32n1669_006:0679a18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果名為極極重地無上一體自然窮了無二山王。",
        "from": "T32n1669_006:0679a19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次定心地大極地地之二種法。",
        "from": "T32n1669_006:0679a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能長養因。",
        "from": "T32n1669_006:0679a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足莊嚴一大覺海。",
        "from": "T32n1669_006:0679a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因名為決定安寂遠離散亂照曜無窮水水火火種子海會。",
        "from": "T32n1669_006:0679a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果名為寂圓滿地明圓滿地具足德藏無二山王。",
        "from": "T32n1669_006:0679a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸佛皆悉各各作三大事。",
        "from": "T32n1669_006:0679a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_006:0679a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者興化。",
        "from": "T32n1669_006:0679a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者說法。",
        "from": "T32n1669_006:0679a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者勝進。",
        "from": "T32n1669_006:0679a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言興化者。",
        "from": "T32n1669_006:0679a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出興一十方世界之塵量變化身故。",
        "from": "T32n1669_006:0679a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言說法者。",
        "from": "T32n1669_006:0679a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說一十方世界之塵量發心信地法門海故。",
        "from": "T32n1669_006:0679a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言勝進者。",
        "from": "T32n1669_006:0679a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向上上位如次入故。",
        "from": "T32n1669_006:0679a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_006:0679a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就變化中亦皆各各有此三事。",
        "from": "T32n1669_006:0679a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應廣通達。",
        "from": "T32n1669_006:0679b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是如是隨隨如如。",
        "from": "T32n1669_006:0679b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後後諸位例前應知。",
        "from": "T32n1669_006:0679b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈以信心為初如次率自類取所餘位初至於定心位則取如來地故。",
        "from": "T32n1669_006:0679b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說二因一果門。",
        "from": "T32n1669_006:0679b04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說一因一果門。",
        "from": "T32n1669_006:0679b04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_006:0679b05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_006:0679b05_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一種位　　各不待他力",
        "from": "T32n1669_006:0679b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　獨住自家中　　感得一果故",
        "from": "T32n1669_006:0679b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　因及果名字　　如其次第加",
        "from": "T32n1669_006:0679b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　種子大覺故　　配釋應了知",
        "from": "T32n1669_006:0679b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　次第同前說　　增減不同耳",
        "from": "T32n1669_006:0679b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_006:0679b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_006:0679b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示因果二法數量契當莊嚴覺道。",
        "from": "T32n1669_006:0679b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三寶之海轉廣大故。",
        "from": "T32n1669_006:0679b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一種位各不待他力獨住自家中感得一果故。",
        "from": "T32n1669_006:0679b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故因及果稱於本名字。",
        "from": "T32n1669_006:0679b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加種子大覺之稱故。",
        "from": "T32n1669_006:0679b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈因及果名字如其次第加種子大覺故配釋應了知故。",
        "from": "T32n1669_006:0679b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第轉相與前所說等無差別。",
        "from": "T32n1669_006:0679b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯增數量減數量別各不同耳。",
        "from": "T32n1669_006:0679b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈次第同前說增減不同耳。",
        "from": "T32n1669_006:0679b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故如是諸佛皆悉各各作三大事。",
        "from": "T32n1669_006:0679b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名同前說義有不同。",
        "from": "T32n1669_006:0679b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言興化者。",
        "from": "T32n1669_006:0679b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出興十十方世界之塵量變化身故。",
        "from": "T32n1669_006:0679b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言說法者。",
        "from": "T32n1669_006:0679b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣說十十方世界之塵量各各因位法門海故。",
        "from": "T32n1669_006:0679b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言勝進者。",
        "from": "T32n1669_006:0679b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向上上位如次入故。",
        "from": "T32n1669_006:0679b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就變化身中。",
        "from": "T32n1669_006:0679b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有此三事。",
        "from": "T32n1669_006:0679b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應廣通達。",
        "from": "T32n1669_006:0679b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說一因一果門。",
        "from": "T32n1669_006:0679b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說少因多果門。",
        "from": "T32n1669_006:0679b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_006:0679b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_006:0679b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一位中　　一一皆各各",
        "from": "T32n1669_006:0679b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　感五十果位　　名少因多果",
        "from": "T32n1669_006:0679b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_006:0679b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此門中為明何義。",
        "from": "T32n1669_006:0679b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示唯一種因感五十果。",
        "from": "T32n1669_006:0679b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無礙自在無所闕失。",
        "from": "T32n1669_006:0679b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三寶之海轉廣大故。",
        "from": "T32n1669_006:0679b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一位中一一皆各各感五十果位名少因多果故。",
        "from": "T32n1669_006:0679c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸佛皆悉各各作三大事。",
        "from": "T32n1669_006:0679c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名如前說義有不同。",
        "from": "T32n1669_006:0679c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言興化者。",
        "from": "T32n1669_006:0679c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出興百十方世界之塵量變化身故。",
        "from": "T32n1669_006:0679c03_9",
        "context_with_next_sentence": true
    },
    {
