    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_006:0679c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一位中　　一一皆各各",
        "from": "T32n1669_006:0679c09_0",
        "context_with_next_sentence": true
    },
    {
