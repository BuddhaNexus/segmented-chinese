[
    {
        "source_sentence": "心經貫義",
        "from": "X26n0554_001:0876a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經。",
        "from": "X26n0554_001:0876a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本觀自在菩薩所修之妙行。",
        "from": "X26n0554_001:0876a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所證之究竟涅槃。",
        "from": "X26n0554_001:0876a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙行即行深般若。",
        "from": "X26n0554_001:0876a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅槃。",
        "from": "X26n0554_001:0876a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體具三德。",
        "from": "X26n0554_001:0876a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂法身般若解脫。",
        "from": "X26n0554_001:0876a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "般若。",
        "from": "X26n0554_001:0876a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即照見。",
        "from": "X26n0554_001:0876a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身。",
        "from": "X26n0554_001:0876a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即蘊空。",
        "from": "X26n0554_001:0876a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫。",
        "from": "X26n0554_001:0876a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即度苦。",
        "from": "X26n0554_001:0876a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰照見五蘊皆空度一切苦厄。",
        "from": "X26n0554_001:0876a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而佛欲人之得。",
        "from": "X26n0554_001:0876a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同菩薩之得。",
        "from": "X26n0554_001:0876a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰色不異空空不異色。",
        "from": "X26n0554_001:0876a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復恐空色名生。",
        "from": "X26n0554_001:0876a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知當體即是。",
        "from": "X26n0554_001:0876a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰色即是空空即是色。",
        "from": "X26n0554_001:0876a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其有欲得此者。",
        "from": "X26n0554_001:0876a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直下一念不生。",
        "from": "X26n0554_001:0876a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "體露真常。",
        "from": "X26n0554_001:0876a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即名觀自在。",
        "from": "X26n0554_001:0876a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其迷此者。",
        "from": "X26n0554_001:0876a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身心熾然。",
        "from": "X26n0554_001:0876a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為六道四生。",
        "from": "X26n0554_001:0876a12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如或於此未能一念相應。",
        "from": "X26n0554_001:0876a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即當秉起大丈夫志。",
        "from": "X26n0554_001:0876a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "截斷心意識路。",
        "from": "X26n0554_001:0876a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不落卜度知見。",
        "from": "X26n0554_001:0876a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無分別智。",
        "from": "X26n0554_001:0876a13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凝然在前。",
        "from": "X26n0554_001:0876a14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照此如幻身心。",
        "from": "X26n0554_001:0876a14_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當處寂滅。",
        "from": "X26n0554_001:0876a14_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是真空。",
        "from": "X26n0554_001:0876a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真空獨露。",
        "from": "X26n0554_001:0876a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是曰法身。",
        "from": "X26n0554_001:0876a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身無著。",
        "from": "X26n0554_001:0876a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為解脫。",
        "from": "X26n0554_001:0876a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身體離生滅虗妄等相。",
        "from": "X26n0554_001:0876a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無世之根塵識。",
        "from": "X26n0554_001:0876a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身前後際斷。",
        "from": "X26n0554_001:0876a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無三世十二因緣。",
        "from": "X26n0554_001:0876a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身不屬世出世間因果。",
        "from": "X26n0554_001:0876a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無苦集滅道。",
        "from": "X26n0554_001:0876a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身本自圓成。",
        "from": "X26n0554_001:0876a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初離能所修證。",
        "from": "X26n0554_001:0876a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無智亦無得。",
        "from": "X26n0554_001:0876a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法身離一切相。",
        "from": "X26n0554_001:0876a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而非斷滅。",
        "from": "X26n0554_001:0876a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故以無修而修。",
        "from": "X26n0554_001:0876a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是依般若波羅蜜多。",
        "from": "X26n0554_001:0876a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無證而證。",
        "from": "X26n0554_001:0876a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是證究竟涅槃。",
        "from": "X26n0554_001:0876a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無得而得。",
        "from": "X26n0554_001:0876a20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是得阿耨菩提。",
        "from": "X26n0554_001:0876a21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出入六根門頭。",
        "from": "X26n0554_001:0876a21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍法界。",
        "from": "X26n0554_001:0876a21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈而莫測。",
        "from": "X26n0554_001:0876a21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰是大神呪。",
        "from": "X26n0554_001:0876a22_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈光獨耀。",
        "from": "X26n0554_001:0876a22_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰是大明呪。",
        "from": "X26n0554_001:0876a22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出於數量之表。",
        "from": "X26n0554_001:0876a22_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰是無上呪。",
        "from": "X26n0554_001:0876a23_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非三賢十聖可比。",
        "from": "X26n0554_001:0876a23_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰是無等等呪。",
        "from": "X26n0554_001:0876a23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離名絕義。",
        "from": "X26n0554_001:0876a24_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故曰揭諦揭諦。",
        "from": "X26n0554_001:0876a24_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波羅揭諦。",
        "from": "X26n0554_001:0876a24_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波羅僧揭諦。",
        "from": "X26n0554_001:0876a24_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩提薩婆訶。",
        "from": "X26n0554_001:0876b01_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心經貫義(終)",
        "from": "X26n0554_001:0876b02_0",
        "context_with_next_sentence": false
    }
]