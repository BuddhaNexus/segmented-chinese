[
    {
        "source_sentence": "俱舍論頌疏",
        "from": "X53n0842_029:0521a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　破我品",
        "from": "X53n0842_029:0521a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫雲披月現。",
        "from": "X53n0842_029:0521a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執喪空明。",
        "from": "X53n0842_029:0521a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不摧邪。",
        "from": "X53n0842_029:0521a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將何顯正。",
        "from": "X53n0842_029:0521a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此品來意。",
        "from": "X53n0842_029:0521a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良有以焉。",
        "from": "X53n0842_029:0521a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有滯相之賓迷方之子。",
        "from": "X53n0842_029:0521a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法無主宰。",
        "from": "X53n0842_029:0521a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫生我執。",
        "from": "X53n0842_029:0521a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身見一起。",
        "from": "X53n0842_029:0521a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惑隨生。",
        "from": "X53n0842_029:0521a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溺三有河。",
        "from": "X53n0842_029:0521a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背八正路。",
        "from": "X53n0842_029:0521a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其為過也。",
        "from": "X53n0842_029:0521a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不亦悲哉。",
        "from": "X53n0842_029:0521a10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論主將登一子。",
        "from": "X53n0842_029:0521a10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情慇四心。",
        "from": "X53n0842_029:0521a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傷而愍之。",
        "from": "X53n0842_029:0521a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故制斯論。",
        "from": "X53n0842_029:0521a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執我不同。",
        "from": "X53n0842_029:0521a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總有三種。",
        "from": "X53n0842_029:0521a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一犢子部。",
        "from": "X53n0842_029:0521a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二數論宗。",
        "from": "X53n0842_029:0521a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三勝論宗。",
        "from": "X53n0842_029:0521a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破此三計。",
        "from": "X53n0842_029:0521a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣如長論。",
        "from": "X53n0842_029:0521a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今唯釋頌。",
        "from": "X53n0842_029:0521a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可具陳。",
        "from": "X53n0842_029:0521a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略舉總宗。",
        "from": "X53n0842_029:0521a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為發起。",
        "from": "X53n0842_029:0521a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犢子部者。",
        "from": "X53n0842_029:0521a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八部中之一部也。",
        "from": "X53n0842_029:0521a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在世時。",
        "from": "X53n0842_029:0521a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一外道。",
        "from": "X53n0842_029:0521a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厥名犢子。",
        "from": "X53n0842_029:0521a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此部執。",
        "from": "X53n0842_029:0521a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同斯外道。",
        "from": "X53n0842_029:0521a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故以標名。",
        "from": "X53n0842_029:0521a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此品破中。",
        "from": "X53n0842_029:0521a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先破犢子。",
        "from": "X53n0842_029:0521a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次破數論。",
        "from": "X53n0842_029:0521a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後破勝論。",
        "from": "X53n0842_029:0521a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良為此部。",
        "from": "X53n0842_029:0521a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是佛弟子。",
        "from": "X53n0842_029:0521a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見同外道。",
        "from": "X53n0842_029:0521a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊所不應。",
        "from": "X53n0842_029:0521a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故先破之。",
        "from": "X53n0842_029:0521a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今言破彼。",
        "from": "X53n0842_029:0521a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先料其計。",
        "from": "X53n0842_029:0521a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論云。",
        "from": "X53n0842_029:0521a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然犢子部執。",
        "from": "X53n0842_029:0521a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有補特伽羅。",
        "from": "X53n0842_029:0521a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其體與五蘊。",
        "from": "X53n0842_029:0521a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不一不異。",
        "from": "X53n0842_029:0521a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解云。",
        "from": "X53n0842_029:0521a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼計真我。",
        "from": "X53n0842_029:0521a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名補特伽羅。",
        "from": "X53n0842_029:0521a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此云數取趣。",
        "from": "X53n0842_029:0521a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊剎那滅。",
        "from": "X53n0842_029:0521a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於轉無能。",
        "from": "X53n0842_029:0521a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但由於我。",
        "from": "X53n0842_029:0521a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能捨此蘊。",
        "from": "X53n0842_029:0521a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能續餘蘊。",
        "from": "X53n0842_029:0521a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數取諸趣。",
        "from": "X53n0842_029:0521a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故執實我無。",
        "from": "X53n0842_029:0521a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名補特伽羅。",
        "from": "X53n0842_029:0521a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "計我與蘊。",
        "from": "X53n0842_029:0521a20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不一不異。",
        "from": "X53n0842_029:0521a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非斷非常。",
        "from": "X53n0842_029:0521a21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言蘊一。",
        "from": "X53n0842_029:0521a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊滅我滅。",
        "from": "X53n0842_029:0521a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便成斷見。",
        "from": "X53n0842_029:0521a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言蘊異。",
        "from": "X53n0842_029:0521a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊滅我在。",
        "from": "X53n0842_029:0521a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則常見計。",
        "from": "X53n0842_029:0521a22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆墮其邊。",
        "from": "X53n0842_029:0521a22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故知此我。",
        "from": "X53n0842_029:0521a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與五蘊法。",
        "from": "X53n0842_029:0521a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不一不異。",
        "from": "X53n0842_029:0521a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非斷非常。",
        "from": "X53n0842_029:0521a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論主破云。",
        "from": "X53n0842_029:0521a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝所執我。",
        "from": "X53n0842_029:0521a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為實為假。",
        "from": "X53n0842_029:0521a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若言是實。",
        "from": "X53n0842_029:0521a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應與蘊異。",
        "from": "X53n0842_029:0521a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有別性故。",
        "from": "X53n0842_029:0521a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如色等實。",
        "from": "X53n0842_029:0521b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與受等異。",
        "from": "X53n0842_029:0521b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有實體。",
        "from": "X53n0842_029:0521b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必應有因。",
        "from": "X53n0842_029:0521b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如五蘊實。",
        "from": "X53n0842_029:0521b01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從因生故。",
        "from": "X53n0842_029:0521b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若計因生。",
        "from": "X53n0842_029:0521b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則無常。",
        "from": "X53n0842_029:0521b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便違汝宗。",
        "from": "X53n0842_029:0521b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我非無常。",
        "from": "X53n0842_029:0521b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有實體。",
        "from": "X53n0842_029:0521b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不從因生。",
        "from": "X53n0842_029:0521b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應是無為。",
        "from": "X53n0842_029:0521b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同外道見。",
        "from": "X53n0842_029:0521b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又汝計我非無為也。",
        "from": "X53n0842_029:0521b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若非因生。",
        "from": "X53n0842_029:0521b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又非無為。",
        "from": "X53n0842_029:0521b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝所執我。",
        "from": "X53n0842_029:0521b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應成無用。",
        "from": "X53n0842_029:0521b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若我是假。",
        "from": "X53n0842_029:0521b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便因我說。",
        "from": "X53n0842_029:0521b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無實我故。",
        "from": "X53n0842_029:0521b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又彼部法。",
        "from": "X53n0842_029:0521b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五法藏。",
        "from": "X53n0842_029:0521b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世為三。",
        "from": "X53n0842_029:0521b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為為四。",
        "from": "X53n0842_029:0521b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我為第五不可說藏。",
        "from": "X53n0842_029:0521b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我在生死。",
        "from": "X53n0842_029:0521b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可說與三世。",
        "from": "X53n0842_029:0521b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一為異。",
        "from": "X53n0842_029:0521b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若入涅槃。",
        "from": "X53n0842_029:0521b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可說與無為亦為一為異。",
        "from": "X53n0842_029:0521b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故立此我。",
        "from": "X53n0842_029:0521b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為不可說藏。",
        "from": "X53n0842_029:0521b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破云。",
        "from": "X53n0842_029:0521b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我與前四法藏。",
        "from": "X53n0842_029:0521b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不可說為異故。",
        "from": "X53n0842_029:0521b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即不應說為第五法藏。",
        "from": "X53n0842_029:0521b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我與前四。",
        "from": "X53n0842_029:0521b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不可說為一故。",
        "from": "X53n0842_029:0521b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即不可說為非第五。",
        "from": "X53n0842_029:0521b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非第五者。",
        "from": "X53n0842_029:0521b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是前四法藏。",
        "from": "X53n0842_029:0521b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既第五非第五。",
        "from": "X53n0842_029:0521b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱不可說。",
        "from": "X53n0842_029:0521b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但應建立前四法藏。",
        "from": "X53n0842_029:0521b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來理破。",
        "from": "X53n0842_029:0521b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已下教證。",
        "from": "X53n0842_029:0521b12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論云。",
        "from": "X53n0842_029:0521b12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薄伽梵告梵志言。",
        "from": "X53n0842_029:0521b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我說一切有。",
        "from": "X53n0842_029:0521b13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是十二處攝。",
        "from": "X53n0842_029:0521b13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若數取趣。",
        "from": "X53n0842_029:0521b13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非是處攝。",
        "from": "X53n0842_029:0521b13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無體理成。",
        "from": "X53n0842_029:0521b14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是處攝。",
        "from": "X53n0842_029:0521b14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則不應言是不可說。",
        "from": "X53n0842_029:0521b14_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又汝部誦契經亦言。",
        "from": "X53n0842_029:0521b14_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸所有眼。",
        "from": "X53n0842_029:0521b15_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸所有色。",
        "from": "X53n0842_029:0521b15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說乃至苾蒭當知。",
        "from": "X53n0842_029:0521b15_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來齊此。",
        "from": "X53n0842_029:0521b16_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施設一切。",
        "from": "X53n0842_029:0521b16_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立一切。",
        "from": "X53n0842_029:0521b16_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有實體法。",
        "from": "X53n0842_029:0521b16_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中無有補特伽羅。",
        "from": "X53n0842_029:0521b16_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何可說彼有實體。",
        "from": "X53n0842_029:0521b17_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頻婆娑羅經亦說。",
        "from": "X53n0842_029:0521b17_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中無有我我所性。",
        "from": "X53n0842_029:0521b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有一切眾苦法體。",
        "from": "X53n0842_029:0521b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將正已生。",
        "from": "X53n0842_029:0521b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說。",
        "from": "X53n0842_029:0521b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據上理教。",
        "from": "X53n0842_029:0521b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都無我性。",
        "from": "X53n0842_029:0521b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何犢子橫計執我耶。",
        "from": "X53n0842_029:0521b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此論主引頌證破。",
        "from": "X53n0842_029:0521b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中有二。",
        "from": "X53n0842_029:0521b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一正破。",
        "from": "X53n0842_029:0521b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二流通。",
        "from": "X53n0842_029:0521b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初有十七行頌是正破。",
        "from": "X53n0842_029:0521b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後有三行是流通。",
        "from": "X53n0842_029:0521b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初正破中。",
        "from": "X53n0842_029:0521b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "束成六分。",
        "from": "X53n0842_029:0521b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一正顯真空分。",
        "from": "X53n0842_029:0521b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二明佛遍知分。",
        "from": "X53n0842_029:0521b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三置答益物分。",
        "from": "X53n0842_029:0521b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四因相不測分。",
        "from": "X53n0842_029:0521b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五果生先後分。",
        "from": "X53n0842_029:0521b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六業報難知分。",
        "from": "X53n0842_029:0521b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就正顯真空分中。",
        "from": "X53n0842_029:0521c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初是羅漢尼說。",
        "from": "X53n0842_029:0521c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次是世尊說。",
        "from": "X53n0842_029:0521c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今則是初。",
        "from": "X53n0842_029:0521c02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論云。",
        "from": "X53n0842_029:0521c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有阿羅漢尼。",
        "from": "X53n0842_029:0521c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名世羅(此云小山)為魔王說。",
        "from": "X53n0842_029:0521c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　汝墮惡見趣　　於空行聚中　　妄執有有情　智者達非有　　如即攬眾分　　假想立為車　世俗立有情　　應知攬諸蘊釋曰。",
        "from": "X53n0842_029:0521c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝墮惡見趣者。",
        "from": "X53n0842_029:0521c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "標魔過也。",
        "from": "X53n0842_029:0521c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝此魔王。",
        "from": "X53n0842_029:0521c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "墮惡見趣。",
        "from": "X53n0842_029:0521c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身見名惡。",
        "from": "X53n0842_029:0521c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一障聖道。",
        "from": "X53n0842_029:0521c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二為癡覆。",
        "from": "X53n0842_029:0521c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三為惑因。",
        "from": "X53n0842_029:0521c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱之為惡。",
        "from": "X53n0842_029:0521c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趣向生死。",
        "from": "X53n0842_029:0521c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為流轉因。",
        "from": "X53n0842_029:0521c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因之為趣。",
        "from": "X53n0842_029:0521c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡見即趣。",
        "from": "X53n0842_029:0521c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名惡見趣。",
        "from": "X53n0842_029:0521c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於空行聚中。",
        "from": "X53n0842_029:0521c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄執有有情者。",
        "from": "X53n0842_029:0521c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正釋惡見也。",
        "from": "X53n0842_029:0521c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五蘊和合。",
        "from": "X53n0842_029:0521c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為行聚。",
        "from": "X53n0842_029:0521c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都無作者。",
        "from": "X53n0842_029:0521c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱之為空。",
        "from": "X53n0842_029:0521c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執實有情。",
        "from": "X53n0842_029:0521c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為惡見也。",
        "from": "X53n0842_029:0521c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智者達非有者。",
        "from": "X53n0842_029:0521c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯真空也。",
        "from": "X53n0842_029:0521c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有情是假。",
        "from": "X53n0842_029:0521c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖道達非有。",
        "from": "X53n0842_029:0521c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既除我執。",
        "from": "X53n0842_029:0521c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故顯真空。",
        "from": "X53n0842_029:0521c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如即攬眾分至攬諸蘊者。",
        "from": "X53n0842_029:0521c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉喻釋成。",
        "from": "X53n0842_029:0521c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初則喻顯。",
        "from": "X53n0842_029:0521c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後則法合。",
        "from": "X53n0842_029:0521c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攬輪輞等以成其車。",
        "from": "X53n0842_029:0521c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "車即是假。",
        "from": "X53n0842_029:0521c14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攬五蘊法。",
        "from": "X53n0842_029:0521c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以成有情。",
        "from": "X53n0842_029:0521c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情何得實。",
        "from": "X53n0842_029:0521c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其次世尊。",
        "from": "X53n0842_029:0521c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於雜阿笈摩中為婆羅門婆抱梨說(婆抱梨此云棗。",
        "from": "X53n0842_029:0521c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母怜子。",
        "from": "X53n0842_029:0521c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目此棗名也)。",
        "from": "X53n0842_029:0521c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　婆抱梨諦聽　　能解諸結法　　謂依心故染　亦依心故淨　　我實無我性　　顛倒故執有　無有情無我　　唯有有因法　　謂十二有支　所攝蘊處界　　審思此一切　　無補特伽羅　既觀內是空　　觀外空亦爾　　能修知觀者　亦都不可得釋曰。",
        "from": "X53n0842_029:0521c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆抱梨諦聽。",
        "from": "X53n0842_029:0521c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能解諸結法者。",
        "from": "X53n0842_029:0521c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸聽法也。",
        "from": "X53n0842_029:0521c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結謂結縛。",
        "from": "X53n0842_029:0521c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解結妙法。",
        "from": "X53n0842_029:0522a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝善聽之。",
        "from": "X53n0842_029:0522a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂依心故染。",
        "from": "X53n0842_029:0522a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦依心故淨者。",
        "from": "X53n0842_029:0522a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "標所說法也。",
        "from": "X53n0842_029:0522a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心染即染。",
        "from": "X53n0842_029:0522a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心淨便淨。",
        "from": "X53n0842_029:0522a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝依淨心結能解矣。",
        "from": "X53n0842_029:0522a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我實無我性。",
        "from": "X53n0842_029:0522a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顛倒故執有者。",
        "from": "X53n0842_029:0522a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋前依心染也。",
        "from": "X53n0842_029:0522a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "都無實我。",
        "from": "X53n0842_029:0522a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "橫計作者。",
        "from": "X53n0842_029:0522a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "倒想起其心成染焉。",
        "from": "X53n0842_029:0522a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有情無我。",
        "from": "X53n0842_029:0522a04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有有因法者。",
        "from": "X53n0842_029:0522a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據依心淨也。",
        "from": "X53n0842_029:0522a05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果由因起。",
        "from": "X53n0842_029:0522a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名有因法。",
        "from": "X53n0842_029:0522a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但由因力。",
        "from": "X53n0842_029:0522a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果遂得生。",
        "from": "X53n0842_029:0522a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果由因生。",
        "from": "X53n0842_029:0522a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實無作者。",
        "from": "X53n0842_029:0522a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故無我見也。",
        "from": "X53n0842_029:0522a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因果和合。",
        "from": "X53n0842_029:0522a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假名為人。",
        "from": "X53n0842_029:0522a07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故元有情也。",
        "from": "X53n0842_029:0522a07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "達有因法。",
        "from": "X53n0842_029:0522a07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情我見滅。",
        "from": "X53n0842_029:0522a07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心便淨矣。",
        "from": "X53n0842_029:0522a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十二有支。",
        "from": "X53n0842_029:0522a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至亦都不可得者。",
        "from": "X53n0842_029:0522a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋前有因法也。",
        "from": "X53n0842_029:0522a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二有支。",
        "from": "X53n0842_029:0522a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薀處界三。",
        "from": "X53n0842_029:0522a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一切法。",
        "from": "X53n0842_029:0522a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有因果。",
        "from": "X53n0842_029:0522a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無數取趣。",
        "from": "X53n0842_029:0522a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但有情身。",
        "from": "X53n0842_029:0522a10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無數取趣。",
        "from": "X53n0842_029:0522a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是內空。",
        "from": "X53n0842_029:0522a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外非情法。",
        "from": "X53n0842_029:0522a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復法空。",
        "from": "X53n0842_029:0522a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修此空觀。",
        "from": "X53n0842_029:0522a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不可得。",
        "from": "X53n0842_029:0522a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詳此經頌。",
        "from": "X53n0842_029:0522a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是佛世尊。",
        "from": "X53n0842_029:0522a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於小乘經。",
        "from": "X53n0842_029:0522a11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密談大教。",
        "from": "X53n0842_029:0522a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真空之理。",
        "from": "X53n0842_029:0522a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於斯盡矣。",
        "from": "X53n0842_029:0522a12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犢子部難。",
        "from": "X53n0842_029:0522a12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無實我。",
        "from": "X53n0842_029:0522a12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則世尊非一切智。",
        "from": "X53n0842_029:0522a13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要因真我。",
        "from": "X53n0842_029:0522a13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非剎那滅。",
        "from": "X53n0842_029:0522a13_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方頓遍知。",
        "from": "X53n0842_029:0522a13_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以心心所剎那剎那異生滅故。",
        "from": "X53n0842_029:0522a14_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故心心所。",
        "from": "X53n0842_029:0522a14_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非頓遍知。",
        "from": "X53n0842_029:0522a14_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論主答云。",
        "from": "X53n0842_029:0522a15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但約相續。",
        "from": "X53n0842_029:0522a15_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有堪能故。",
        "from": "X53n0842_029:0522a15_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知一切法名一切智。",
        "from": "X53n0842_029:0522a15_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非由一念能頓遍知。",
        "from": "X53n0842_029:0522a16_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此已下舉頌以答。",
        "from": "X53n0842_029:0522a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是第二明佛通知分。",
        "from": "X53n0842_029:0522a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中有二。",
        "from": "X53n0842_029:0522a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一舉頌正明。",
        "from": "X53n0842_029:0522a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二引頌證成。",
        "from": "X53n0842_029:0522a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今則是初。",
        "from": "X53n0842_029:0522a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "X53n0842_029:0522a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　由相續有能　　如火食一切　　如是一切智　非由頓遍知釋曰。",
        "from": "X53n0842_029:0522a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火食一切。",
        "from": "X53n0842_029:0522a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈一念火。",
        "from": "X53n0842_029:0522a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能燒一切。",
        "from": "X53n0842_029:0522a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但約相續。",
        "from": "X53n0842_029:0522a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說燒一切。",
        "from": "X53n0842_029:0522a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛智亦爾。",
        "from": "X53n0842_029:0522a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但據相續。",
        "from": "X53n0842_029:0522a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名一切智。",
        "from": "X53n0842_029:0522a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此第二引證。",
        "from": "X53n0842_029:0522a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犢子部問。",
        "from": "X53n0842_029:0522a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何得知。",
        "from": "X53n0842_029:0522a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "約相續說。",
        "from": "X53n0842_029:0522a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知一切法非我遍知。",
        "from": "X53n0842_029:0522a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論主答云。",
        "from": "X53n0842_029:0522a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說佛世尊有三世故。",
        "from": "X53n0842_029:0522a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犢子部問。",
        "from": "X53n0842_029:0522a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於何處說。",
        "from": "X53n0842_029:0522b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此論主引頌成證。",
        "from": "X53n0842_029:0522b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如有頌曰。",
        "from": "X53n0842_029:0522b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若過去諸佛　　若未來諸佛　　及現在諸佛　皆滅眾生憂釋曰。",
        "from": "X53n0842_029:0522b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此頌既說有三世佛。",
        "from": "X53n0842_029:0522b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故約佛智。",
        "from": "X53n0842_029:0522b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於相續位。",
        "from": "X53n0842_029:0522b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知一切法。",
        "from": "X53n0842_029:0522b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不由於我。",
        "from": "X53n0842_029:0522b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能頓遍知。",
        "from": "X53n0842_029:0522b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以佛世尊三世攝故。",
        "from": "X53n0842_029:0522b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼此計我體是佛世尊。",
        "from": "X53n0842_029:0522b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復說我體非三世攝。",
        "from": "X53n0842_029:0522b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此頌既說佛在三世。",
        "from": "X53n0842_029:0522b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故汝計我非是世尊。",
        "from": "X53n0842_029:0522b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我既非佛。",
        "from": "X53n0842_029:0522b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明知遍知不由於我。",
        "from": "X53n0842_029:0522b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名頓遍知。",
        "from": "X53n0842_029:0522b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "理必應爾。",
        "from": "X53n0842_029:0522b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱舍頌疏　論本第二十九",
        "from": "X53n0842_029:0522b11_0",
        "context_with_next_sentence": false
    }
]