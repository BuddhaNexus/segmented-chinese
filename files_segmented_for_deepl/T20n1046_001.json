[
    {
        "source_sentence": "六字大陀羅尼呪經",
        "from": "T20n1046_001:0043c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附梁錄",
        "from": "T20n1046_001:0043c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1046_001:0043c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時婆伽婆。",
        "from": "T20n1046_001:0043c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住王舍城耆闍崛山中。",
        "from": "T20n1046_001:0043c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大比丘眾五百人俱。",
        "from": "T20n1046_001:0043c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時長老阿難。",
        "from": "T20n1046_001:0043c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為旃陀梨女呪術所收爾時長老阿難。",
        "from": "T20n1046_001:0043c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1046_001:0043c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今強為他收去。",
        "from": "T20n1046_001:0043c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆伽婆我今強為他收去修伽陀。",
        "from": "T20n1046_001:0043c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時婆伽婆告長老阿難言。",
        "from": "T20n1046_001:0043c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝來阿難汝莫驚怖。",
        "from": "T20n1046_001:0043c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難汝當受持六字大陀羅尼呪。",
        "from": "T20n1046_001:0043c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為令四眾利益安隱安樂吉祥行故。",
        "from": "T20n1046_001:0043c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "T20n1046_001:0043c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯(須何反)地(除隷反)梯(吐稽反)曇(徒紺反)安(於軻反)茶(徒嫁反)隸般茶(徒嫁反)隸葛羅馳(除寄反)稽由隷　薩帝　婆帝　耶[賒-示+米]婆帝　底闍婆帝　頻頭摩帝阿難是呪。",
        "from": "T20n1046_001:0043c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令宿食不消尋得消化。",
        "from": "T20n1046_001:0043c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除吐下等病。",
        "from": "T20n1046_001:0043c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除風病。",
        "from": "T20n1046_001:0043c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熱病冷病雜病。",
        "from": "T20n1046_001:0043c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅一切諸邪呪術。",
        "from": "T20n1046_001:0043c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅起屍。",
        "from": "T20n1046_001:0043c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滅一切形像厭蠱。",
        "from": "T20n1046_001:0043c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難若有人知此神呪姓名者。",
        "from": "T20n1046_001:0043c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼則不怖畏王難。",
        "from": "T20n1046_001:0043c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不怖畏怨敵難不怖畏賊難。",
        "from": "T20n1046_001:0043c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不怖畏火難不怖畏水難。",
        "from": "T20n1046_001:0043c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於城邑聚落及在曠野悉無所畏。",
        "from": "T20n1046_001:0043c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為他人伺求其過無過可說。",
        "from": "T20n1046_001:0043c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若食毒藥毒不能害轉為利益。",
        "from": "T20n1046_001:0043c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此六字大陀羅尼呪乃是七三藐三佛陀所說。",
        "from": "T20n1046_001:0043c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦是梵王娑婆主說。",
        "from": "T20n1046_001:0043c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋提桓因四大天王所說。",
        "from": "T20n1046_001:0043c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦皆隨喜破諸呪術消伏起屍。",
        "from": "T20n1046_001:0044a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切形像厭蠱皆悉破壞斷滅。",
        "from": "T20n1046_001:0044a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長老阿難聞佛所說歡喜奉行。",
        "from": "T20n1046_001:0044a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六字大陀羅尼呪經",
        "from": "T20n1046_001:0044a04_0",
        "context_with_next_sentence": false
    }
]