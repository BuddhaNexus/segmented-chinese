[
    {
        "source_sentence": "何耶揭唎婆像法",
        "from": "T20n1073_001:0170b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次取淨褺不得截割。",
        "from": "T20n1073_001:0170b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請一畫師最功能。",
        "from": "T20n1073_001:0170b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿還其價。",
        "from": "T20n1073_001:0170b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香湯洗浴著新淨衣與受八戒。",
        "from": "T20n1073_001:0170b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日如是。",
        "from": "T20n1073_001:0170b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於清淨處作一水壇。",
        "from": "T20n1073_001:0170b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱廣四肘(水壇之法如餘部說)呪師護身結界畢已。",
        "from": "T20n1073_001:0170b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其壇中燒種種香。",
        "from": "T20n1073_001:0170b17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散種種華供養已訖。",
        "from": "T20n1073_001:0170b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此壇內畫作菩薩。",
        "from": "T20n1073_001:0170b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身長佛一磔(長短正當以人一肘)總有四面。",
        "from": "T20n1073_001:0170b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中菩薩面極令端正。",
        "from": "T20n1073_001:0170b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作慈悲顏顏色赤白頭髮純青。",
        "from": "T20n1073_001:0170b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左邊一面作大瞋怒黑色之面。",
        "from": "T20n1073_001:0170b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狗牙上出頭髮微竪如火焰色。",
        "from": "T20n1073_001:0170b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊一面作大笑顏。",
        "from": "T20n1073_001:0170b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤白端正似菩薩面。",
        "from": "T20n1073_001:0170b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭髮純青三面頭上。",
        "from": "T20n1073_001:0170b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各戴天冠及著耳璫。",
        "from": "T20n1073_001:0170b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其天冠上有一化佛結跏趺坐。",
        "from": "T20n1073_001:0170b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中面頂上作碧馬頭仍令合口。",
        "from": "T20n1073_001:0170b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩頸下著寶瓔珞。",
        "from": "T20n1073_001:0170b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "項背圓光數重色作。",
        "from": "T20n1073_001:0170b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手屈臂手當乳前把紅蓮花。",
        "from": "T20n1073_001:0170b26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其蓮花與菩薩頭齊正臨左膊。",
        "from": "T20n1073_001:0170b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其華臺上作一化佛。",
        "from": "T20n1073_001:0170b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著緋袈裟結跏趺坐。",
        "from": "T20n1073_001:0170b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "項背有光。",
        "from": "T20n1073_001:0170b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手仰掌五指申。",
        "from": "T20n1073_001:0170b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臂肘平屈。",
        "from": "T20n1073_001:0170b29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其手掌擎真陀摩尼(唐云如意寶)其珠團圓如作白色。",
        "from": "T20n1073_001:0170b29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赤色光炎圍繞其珠。",
        "from": "T20n1073_001:0170c01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其右手正當珠下雨種種寶。",
        "from": "T20n1073_001:0170c02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其左膊上。",
        "from": "T20n1073_001:0170c03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著弊耶(二合)迦羅者摩(二合唐云虎皮)如著祇支。",
        "from": "T20n1073_001:0170c03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當右腋下掩皮結帶。",
        "from": "T20n1073_001:0170c04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更用虎皮縵其跨上。",
        "from": "T20n1073_001:0170c04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以外臂釧天衣裙等。",
        "from": "T20n1073_001:0170c05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆如餘處畫菩薩法。",
        "from": "T20n1073_001:0170c05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如令菩薩端身正立紅蓮華上。",
        "from": "T20n1073_001:0170c06_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空懸寶傘蓋菩薩頭上。",
        "from": "T20n1073_001:0170c06_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其上空中畫作種種天音樂具。",
        "from": "T20n1073_001:0170c07_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩邊空中須陀會天舞蹈供養。",
        "from": "T20n1073_001:0170c08_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫此像時用香汁畫。",
        "from": "T20n1073_001:0170c08_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皮膠不當用。",
        "from": "T20n1073_001:0170c09_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叉以蘇木佉子(唐云芸臺)擣以為末。",
        "from": "T20n1073_001:0170c09_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極令微細用酪(作酪殘汁也)汁。",
        "from": "T20n1073_001:0170c10_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和如和麨法。",
        "from": "T20n1073_001:0170c10_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用何耶揭唎婆心呪。",
        "from": "T20n1073_001:0170c11_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪三七遍已。",
        "from": "T20n1073_001:0170c11_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其病人空腹腹之。",
        "from": "T20n1073_001:0170c11_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腹內所有一切毒虫悉皆吐出。",
        "from": "T20n1073_001:0170c12_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又畫作像法。",
        "from": "T20n1073_001:0170c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取新瓦雜瓶未曾用。",
        "from": "T20n1073_001:0170c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瓶染作青色。",
        "from": "T20n1073_001:0170c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其瓦瓶上畫作阿耶揭唎婆觀世音像。",
        "from": "T20n1073_001:0170c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像身高如來一磔(等人一肘)畫作四箇歡喜之面。",
        "from": "T20n1073_001:0170c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左邊一面畫作黑色。",
        "from": "T20n1073_001:0170c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼精綠色狗牙上出。",
        "from": "T20n1073_001:0170c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊一面畫作赤色。",
        "from": "T20n1073_001:0170c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名喫呪面。",
        "from": "T20n1073_001:0170c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當中前面作菩薩面。",
        "from": "T20n1073_001:0170c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極令端正。",
        "from": "T20n1073_001:0170c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫作白色。",
        "from": "T20n1073_001:0170c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離像頂上懸於空中。",
        "from": "T20n1073_001:0170c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畫作青面。",
        "from": "T20n1073_001:0170c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出口吐寶珠。",
        "from": "T20n1073_001:0170c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其四頭上各戴寶冠。",
        "from": "T20n1073_001:0170c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其寶冠上皆化佛坐。",
        "from": "T20n1073_001:0170c20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩左手把於蓮華。",
        "from": "T20n1073_001:0170c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈肘向上捧在膊前。",
        "from": "T20n1073_001:0170c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右臂垂下五指皆申。",
        "from": "T20n1073_001:0170c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施無畏手。",
        "from": "T20n1073_001:0170c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手腕上皆作寶釧。",
        "from": "T20n1073_001:0170c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像項下著寶瓔珞。",
        "from": "T20n1073_001:0170c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身分莊嚴如餘處說觀世音身莊嚴之法。",
        "from": "T20n1073_001:0170c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其像立在寶蓮花上。",
        "from": "T20n1073_001:0170c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作此像已。",
        "from": "T20n1073_001:0170c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從白月十日起首食大麥乳糜。",
        "from": "T20n1073_001:0170c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後十三日十四日十五日。",
        "from": "T20n1073_001:0170c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日三夜斷食。",
        "from": "T20n1073_001:0170c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取娑迦比遮香乾陀那句利華呪此二物。",
        "from": "T20n1073_001:0170c26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一投當前面上至八千遍。",
        "from": "T20n1073_001:0170c27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其面口中放出大光明。",
        "from": "T20n1073_001:0170c28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圍繞呪師身。",
        "from": "T20n1073_001:0170c28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已還入像口。",
        "from": "T20n1073_001:0170c29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於其口中出如意珠。",
        "from": "T20n1073_001:0170c29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即收取之珠已。",
        "from": "T20n1073_001:0170c29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得十四千歲壽命。",
        "from": "T20n1073_001:0171a01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得七寶轉輪聖王。",
        "from": "T20n1073_001:0171a01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命終以後生安樂國。",
        "from": "T20n1073_001:0171a02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從觀世音足下而坐。",
        "from": "T20n1073_001:0171a02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又有一法當乞食喫喫大麥乳糜誦七萬遍。",
        "from": "T20n1073_001:0171a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一萬遍時毘那耶迦即得被縛。",
        "from": "T20n1073_001:0171a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二萬遍呪法成就。",
        "from": "T20n1073_001:0171a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三萬遍即得安善那摩那熾羅藥法成就。",
        "from": "T20n1073_001:0171a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日三夜不食。",
        "from": "T20n1073_001:0171a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作法手把其藥。",
        "from": "T20n1073_001:0171a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正呪藥時。",
        "from": "T20n1073_001:0171a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥中若現三種相者。",
        "from": "T20n1073_001:0171a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知藥成。",
        "from": "T20n1073_001:0171a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三相者謂煖煙焰。",
        "from": "T20n1073_001:0171a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得成就安陀羅陀那。",
        "from": "T20n1073_001:0171a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昇空而去得成就持呪仙人。",
        "from": "T20n1073_001:0171a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在得入阿脩羅宮。",
        "from": "T20n1073_001:0171a09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四萬遍離地四指而行自在。",
        "from": "T20n1073_001:0171a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五萬遍則得隨意昇空行。",
        "from": "T20n1073_001:0171a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六萬遍即得千歲活。",
        "from": "T20n1073_001:0171a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師成就呪仙。",
        "from": "T20n1073_001:0171a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人隨其所作任運皆成。",
        "from": "T20n1073_001:0171a12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切所呼喚者皆來。",
        "from": "T20n1073_001:0171a12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七萬遍呪師即得頭髮變作螺髮成就。",
        "from": "T20n1073_001:0171a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其所須應念即至。",
        "from": "T20n1073_001:0171a14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親近承事於觀世音。",
        "from": "T20n1073_001:0171a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知三世一切眾生死此生彼。",
        "from": "T20n1073_001:0171a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得成就。",
        "from": "T20n1073_001:0171a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若又法若欲打西(去音)若(若耶反)者。",
        "from": "T20n1073_001:0171a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何耶揭唎婆像前三日三夜斷食香摩壇上。",
        "from": "T20n1073_001:0171a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種香花。",
        "from": "T20n1073_001:0171a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨飲食乳粥菓等種種供養。",
        "from": "T20n1073_001:0171a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擣娑闍羅娑香。",
        "from": "T20n1073_001:0171a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以白蜜和為八千丸。",
        "from": "T20n1073_001:0171a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一丸香一遍已。",
        "from": "T20n1073_001:0171a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投火中燒。",
        "from": "T20n1073_001:0171a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是香盡。",
        "from": "T20n1073_001:0171a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼西惹眾尋即初見變為馬頭。",
        "from": "T20n1073_001:0171a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更經少時。",
        "from": "T20n1073_001:0171a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉為何耶揭哩婆頭。",
        "from": "T20n1073_001:0171a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師奇上向彼眾時。",
        "from": "T20n1073_001:0171a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切西惹眾悉皆散滅。",
        "from": "T20n1073_001:0171a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法先以大麥與牛喫已。",
        "from": "T20n1073_001:0171a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取糞中麥洗乾誠擣煮為乳糜。",
        "from": "T20n1073_001:0171a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從白月一日食此乳糜。",
        "from": "T20n1073_001:0171a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至十三日即斷不食。",
        "from": "T20n1073_001:0171a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何耶揭哩婆像前。",
        "from": "T20n1073_001:0171a25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以塗香塗地散種種花。",
        "from": "T20n1073_001:0171a25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒諸名香。",
        "from": "T20n1073_001:0171a26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種飲食供養畢已。",
        "from": "T20n1073_001:0171a26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取迦比闍香擣為細末。",
        "from": "T20n1073_001:0171a26_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水和為丸。",
        "from": "T20n1073_001:0171a27_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數滿八千。",
        "from": "T20n1073_001:0171a27_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取迦地羅木為柴燃火。",
        "from": "T20n1073_001:0171a27_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一香丸一呪一投。",
        "from": "T20n1073_001:0171a28_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於火中燒。",
        "from": "T20n1073_001:0171a28_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至八千丸。",
        "from": "T20n1073_001:0171a29_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡其炭變為蘇跋那金黃。",
        "from": "T20n1073_001:0171a29_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重百千兩。",
        "from": "T20n1073_001:0171b01_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若欲喫他呪者。",
        "from": "T20n1073_001:0171b01_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取己身中小許血已。",
        "from": "T20n1073_001:0171b01_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和白芥子為八千丸。",
        "from": "T20n1073_001:0171b02_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑月十四日。",
        "from": "T20n1073_001:0171b02_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日不食。",
        "from": "T20n1073_001:0171b03_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取佉陀羅木鑪中燃火。",
        "from": "T20n1073_001:0171b03_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取前白芥子丸。",
        "from": "T20n1073_001:0171b03_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一丸投火中燒。",
        "from": "T20n1073_001:0171b04_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至芥子丸盡。",
        "from": "T20n1073_001:0171b04_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸呪悉皆喫盡(喫云外國語也。",
        "from": "T20n1073_001:0171b05_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即同大唐破滅語訓。",
        "from": "T20n1073_001:0171b05_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他者是外道也)又法但誦呪。",
        "from": "T20n1073_001:0171b05_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行於曠野處及諸沙磧。",
        "from": "T20n1073_001:0171b06_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所須飲食自然而出。",
        "from": "T20n1073_001:0171b06_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法日月蝕時。",
        "from": "T20n1073_001:0171b07_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取淨牛蘇一兩新瓦鉢盛。",
        "from": "T20n1073_001:0171b07_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即把此鉢仰看日月。",
        "from": "T20n1073_001:0171b08_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪鉢中蘇乃至蘇煖。",
        "from": "T20n1073_001:0171b08_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或煙火出。",
        "from": "T20n1073_001:0171b09_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當爾之時即飲此蘇服此蘇已。",
        "from": "T20n1073_001:0171b09_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨欲詣處舉意即至。",
        "from": "T20n1073_001:0171b10_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若常誦此呪。",
        "from": "T20n1073_001:0171b10_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怖畏得無所畏。",
        "from": "T20n1073_001:0171b10_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有障難悉皆解脫。",
        "from": "T20n1073_001:0171b11_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若患路陀瘡(患此瘡者悉皆死也)及諸毒蛇之所齩者(此蛇人見即死)或患健毘吒雞瘡。",
        "from": "T20n1073_001:0171b11_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此等諸瘡呪黃土埿。",
        "from": "T20n1073_001:0171b13_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一千遍。",
        "from": "T20n1073_001:0171b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗其瘡上即得除差。",
        "from": "T20n1073_001:0171b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切怖畏心念即得除。",
        "from": "T20n1073_001:0171b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又法若人忽逢一切水難。",
        "from": "T20n1073_001:0171b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心念即得不被沈溺。",
        "from": "T20n1073_001:0171b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等法不可廣說。",
        "from": "T20n1073_001:0171b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨意即成。",
        "from": "T20n1073_001:0171b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若造大曼荼羅者。",
        "from": "T20n1073_001:0171b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得行欲。",
        "from": "T20n1073_001:0171b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賀耶揭哩婆療病印(二手內縛竪二中指頭。",
        "from": "T20n1073_001:0171b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相合並屈二大指。",
        "from": "T20n1073_001:0171b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以二頭指屈握二大指甲根。",
        "from": "T20n1073_001:0171b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指甲皆背相著)唵鶻嚕嚧鳩盧馱(去音)那(三)嗚[合*牛]泮(四)娑嚩賀賀耶揭唎婆乞食印(仰左手各竪五指。",
        "from": "T20n1073_001:0171b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將右手下向以中指無名指頭。",
        "from": "T20n1073_001:0171b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各相拄著)",
        "from": "T20n1073_001:0171b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵是那耶(二句誦二七遍)",
        "from": "T20n1073_001:0171b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上二印可在解禁刀印之次。",
        "from": "T20n1073_001:0171b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作此法先當縛禁毘那夜迦。",
        "from": "T20n1073_001:0171b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當對賀耶揭哩婆像前。",
        "from": "T20n1073_001:0171b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取後木長八指截。",
        "from": "T20n1073_001:0171b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒火焰出。",
        "from": "T20n1073_001:0171b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取安悉香作八百丸。",
        "from": "T20n1073_001:0171b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一丸一呪一投火中。",
        "from": "T20n1073_001:0171b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至香盡。",
        "from": "T20n1073_001:0171b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘那夜迦。",
        "from": "T20n1073_001:0171b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自被縛呪曰。",
        "from": "T20n1073_001:0171b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命跢姪他(一)主嚕主嚕(二)毘主嚕主嚕(三)伽跢耶莎(四)摩訶婆(去音)羅(五)薩婆毘知那毘那夜乾(上音六)皤吒婆謨枯知(去音二合七)步跢(去音)耶泮(八)若人夜失不淨。",
        "from": "T20n1073_001:0171c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取於白線呪八百遍。",
        "from": "T20n1073_001:0171c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後作呪索。",
        "from": "T20n1073_001:0171c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一呪一結為三七結。",
        "from": "T20n1073_001:0171c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即用繫腰。",
        "from": "T20n1073_001:0171c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更不漏失。",
        "from": "T20n1073_001:0171c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1073_001:0171c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命多姪他(一)勃地勃地(二)素勃地(三)因達[口*梨](二合)勃地(四)摩迷輸(上音)迦[口*藍](上音二合五)婆羅謀者都(六)莎(去)訶又法呪袈裟誦一七遍。",
        "from": "T20n1073_001:0171c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已結作一結。",
        "from": "T20n1073_001:0171c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得護身成就呪曰(歸命)。",
        "from": "T20n1073_001:0171c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哆姪他(一)訶(去音)哩訶(去音)哩(二)無訶(去)[口*梨](三)句訶(去)[口*梨](四)薩婆突瑟擔(二合五)瞻婆夜弭(上六)夜跋竭[口*闌](二合)地(七)那文社弭(八)莎(去)訶(已上了寫隨誦成驗之次)賀耶揭哩婆儀軌一",
        "from": "T20n1073_001:0171c13_0",
        "context_with_next_sentence": false
    }
]