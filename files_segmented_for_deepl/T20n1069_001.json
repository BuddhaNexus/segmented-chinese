[
    {
        "source_sentence": "十一面觀自在菩薩心密言念",
        "from": "T20n1069_001:0139c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦儀軌經卷上",
        "from": "T20n1069_001:0139c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府議同三司特進試鴻臚卿肅國",
        "from": "T20n1069_001:0139c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空謚大鑒",
        "from": "T20n1069_001:0139c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T20n1069_001:0139c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T20n1069_001:0139c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1069_001:0139c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵住補陀落山大聖觀自在宮殿中。",
        "from": "T20n1069_001:0139c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其山無量娑羅。",
        "from": "T20n1069_001:0139c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多麼羅。",
        "from": "T20n1069_001:0139c25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻蔔。",
        "from": "T20n1069_001:0139c25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無憂阿底目。",
        "from": "T20n1069_001:0140a01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多迦種種花樹莊嚴。",
        "from": "T20n1069_001:0140a01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大苾芻眾八千人俱。",
        "from": "T20n1069_001:0140a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有九十九俱胝那庾多百千菩薩俱。",
        "from": "T20n1069_001:0140a02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量百千淨居天眾。",
        "from": "T20n1069_001:0140a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在大自在梵王天子而為上首。",
        "from": "T20n1069_001:0140a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍遶而為說法。",
        "from": "T20n1069_001:0140a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩與無量持明仙圍遶。",
        "from": "T20n1069_001:0140a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往詣世尊所至佛所已。",
        "from": "T20n1069_001:0140a05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面禮足右遶世尊三匝。",
        "from": "T20n1069_001:0140a06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "退坐一面白佛言。",
        "from": "T20n1069_001:0140a06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我有心密語名十一面。",
        "from": "T20n1069_001:0140a07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一俱胝如來同共宣說。",
        "from": "T20n1069_001:0140a07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說之。",
        "from": "T20n1069_001:0140a08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益安樂一切有情。",
        "from": "T20n1069_001:0140a08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切疾病。",
        "from": "T20n1069_001:0140a09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "止諸不吉祥惡夢。",
        "from": "T20n1069_001:0140a09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及制非命。",
        "from": "T20n1069_001:0140a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不淨信者令淨信。",
        "from": "T20n1069_001:0140a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切障尾曩夜迦。",
        "from": "T20n1069_001:0140a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心所希望皆稱遂故。",
        "from": "T20n1069_001:0140a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不見於天世魔世梵世及沙門婆羅門眾。",
        "from": "T20n1069_001:0140a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此心密語加護救濟攝受。",
        "from": "T20n1069_001:0140a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "息災吉祥免治罰。",
        "from": "T20n1069_001:0140a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離刀杖毒藥故。",
        "from": "T20n1069_001:0140a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有能違越者無有是處。",
        "from": "T20n1069_001:0140a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除宿業不決定心受持此密語一切如來稱讚護念。",
        "from": "T20n1069_001:0140a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來隨喜世尊我念過去。",
        "from": "T20n1069_001:0140a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殑伽沙等數劫過後。",
        "from": "T20n1069_001:0140a16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如來名百蓮花眼髻無障礙無染力光王如來。",
        "from": "T20n1069_001:0140a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於爾時為大仙人。",
        "from": "T20n1069_001:0140a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼如來受此心密語。",
        "from": "T20n1069_001:0140a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔受得已十方一切如來現前。",
        "from": "T20n1069_001:0140a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得見一切如來故便獲得無生法忍。",
        "from": "T20n1069_001:0140a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此密語有如是大功德藏。",
        "from": "T20n1069_001:0140a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1069_001:0140a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以淨信心殷重心。",
        "from": "T20n1069_001:0140a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憶念作意。",
        "from": "T20n1069_001:0140a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現世得十種勝利。",
        "from": "T20n1069_001:0140a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為十。",
        "from": "T20n1069_001:0140a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者離諸疾病。",
        "from": "T20n1069_001:0140a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者一切如來攝受。",
        "from": "T20n1069_001:0140a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者任運獲得金銀財寶諸穀麥等。",
        "from": "T20n1069_001:0140a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者一切怨敵不能沮壞。",
        "from": "T20n1069_001:0140a25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者國王王子在於王宮先言慰問。",
        "from": "T20n1069_001:0140a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者不被毒藥蠱毒。",
        "from": "T20n1069_001:0140a26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寒熱等病皆不著身。",
        "from": "T20n1069_001:0140a27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者一切刀杖所不能害。",
        "from": "T20n1069_001:0140a27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者水不能溺。",
        "from": "T20n1069_001:0140a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者火不能燒。",
        "from": "T20n1069_001:0140a28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者不非命中夭。",
        "from": "T20n1069_001:0140a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又獲四種功德。",
        "from": "T20n1069_001:0140a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者臨命終時得見如來。",
        "from": "T20n1069_001:0140a29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者不生於惡趣。",
        "from": "T20n1069_001:0140b01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者不非命終。",
        "from": "T20n1069_001:0140b01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者從此世界得生極樂國土。",
        "from": "T20n1069_001:0140b02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我念過去。",
        "from": "T20n1069_001:0140b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十殑伽沙劫過後。",
        "from": "T20n1069_001:0140b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有佛名曼陀羅香如來。",
        "from": "T20n1069_001:0140b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我於是時為長者。",
        "from": "T20n1069_001:0140b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從彼如來受此心密言。",
        "from": "T20n1069_001:0140b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超四十萬劫生死棄捨。",
        "from": "T20n1069_001:0140b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我由此密言晝夜念誦作意。",
        "from": "T20n1069_001:0140b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得一切佛住大悲智藏菩薩解脫法門。",
        "from": "T20n1069_001:0140b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有繫縛臨當刑戮。",
        "from": "T20n1069_001:0140b07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水火風賊蠱毒厭禱。",
        "from": "T20n1069_001:0140b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人非人等種種苦難。",
        "from": "T20n1069_001:0140b08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此我於一切有情。",
        "from": "T20n1069_001:0140b08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能作歸依救護安慰。",
        "from": "T20n1069_001:0140b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洲渚室宅勝趣。",
        "from": "T20n1069_001:0140b09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此密言力。",
        "from": "T20n1069_001:0140b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收攝一切暴惡藥叉羅剎等先令發起慈心哀愍。",
        "from": "T20n1069_001:0140b10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後安置立於阿耨多羅三藐三菩提。",
        "from": "T20n1069_001:0140b11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我此密言。",
        "from": "T20n1069_001:0140b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有如是大威德。",
        "from": "T20n1069_001:0140b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由誦一遍滅除四重皆得清淨。",
        "from": "T20n1069_001:0140b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及犯五無間罪蠲除悉皆無餘。",
        "from": "T20n1069_001:0140b13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況諸罪而不除滅。",
        "from": "T20n1069_001:0140b14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼人獲得一切無量俱胝那庾多佛積集善根。",
        "from": "T20n1069_001:0140b15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若聞此心密言若誦若持。",
        "from": "T20n1069_001:0140b16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切意願皆得滿足。",
        "from": "T20n1069_001:0140b16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若族姓男族姓女苾芻苾芻尼近仕男近仕女婆羅門剎利毘舍首陀及餘類。",
        "from": "T20n1069_001:0140b17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於白月十四日或十五日。",
        "from": "T20n1069_001:0140b19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我不食一日一夜。",
        "from": "T20n1069_001:0140b19_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清齋念誦。",
        "from": "T20n1069_001:0140b20_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超四萬劫生死。",
        "from": "T20n1069_001:0140b20_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情纔稱念我名。",
        "from": "T20n1069_001:0140b20_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超稱百千俱胝那庾多如來名號。",
        "from": "T20n1069_001:0140b21_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得不退轉。",
        "from": "T20n1069_001:0140b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離一切病患。",
        "from": "T20n1069_001:0140b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免一切夭死災橫。",
        "from": "T20n1069_001:0140b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離身口意不善行。",
        "from": "T20n1069_001:0140b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能依教相應。",
        "from": "T20n1069_001:0140b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作意觀行。",
        "from": "T20n1069_001:0140b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛菩提如在掌中。",
        "from": "T20n1069_001:0140b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時婆伽梵讚觀自在菩薩言。",
        "from": "T20n1069_001:0140b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉佛子。",
        "from": "T20n1069_001:0140b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝於一切有情起大悲愍。",
        "from": "T20n1069_001:0140b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能以此方便。",
        "from": "T20n1069_001:0140b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安立一切有情於無上正等菩提。",
        "from": "T20n1069_001:0140b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以授記深生隨喜。",
        "from": "T20n1069_001:0140b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當說之。",
        "from": "T20n1069_001:0140b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時觀自在菩薩摩訶薩。",
        "from": "T20n1069_001:0140b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起整理衣服。",
        "from": "T20n1069_001:0140b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏袒右肩頂禮佛足。",
        "from": "T20n1069_001:0140b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說自根本密言。",
        "from": "T20n1069_001:0140b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩(二合下同)怛囉(二合下同)夜(引)也曩莫阿(引下同)哩夜(二合下同)枳穰(二合引)曩娑(引)誐囉吠[口*路]者曩尾喻(二合)訶囉惹(引)也怛他(引下同)蘖多(引)夜囉訶(二合下同)帝三藐三沒馱(引)也曩莫薩嚩怛他蘖帝毘庾(二合)囉訶(二合)毘藥(二合下同)三藐三沒馱(引)毘藥(二合)曩莫阿(引)哩夜(二合)嚩路枳帝濕嚩(二合下同)囉也冐地薩怛嚩(二合引下同)也摩訶薩怛嚩(二合)也摩訶迦(引下同)嚕抳迦(引下同)也怛儞也(二合下同)他(引下同)囉娜囉娜囉地哩地哩度嚕度嚕壹知(陟爰反下同)嚩知者隷者隷鉢囉(二合)者隷矩蘇銘矩蘇摩嚩[口*(隸-木+上)]壹里弭里止里止致惹(引)羅麼跛曩也跛羅麼秫馱薩怛嚩(二合)摩訶迦(引)嚕尼迦(引)娑嚩(二合引下同)訶(引下同)次說澡浴灌灑淨衣密言。",
        "from": "T20n1069_001:0140c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨囉怛曩(二合)怛囉(二合)夜(引)也曩莫阿(引)哩夜(二合)嚩路枳帝濕嚩(二合)囉也冐地薩怛嚩(二合)也摩訶薩怛嚩(二合)也摩訶迦嚕抳迦(引)也怛儞也(二合)他訶訶訶訶壹里弭里止里尾里企隷徙隷娑嚩(二合)訶以此密言加持水。",
        "from": "T20n1069_001:0140c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沐浴灌灑潔身淨服等。",
        "from": "T20n1069_001:0141a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應誦七遍。",
        "from": "T20n1069_001:0141a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩復說獻焚香密言曰。",
        "from": "T20n1069_001:0141a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他拄(陟古反下同)嚕拄嚕訶訶訶訶娑嚩(二合引)訶(引)以此密言加持香燒奉獻。",
        "from": "T20n1069_001:0141a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在大菩薩復說獻花密言曰。",
        "from": "T20n1069_001:0141a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合引)他悉哩悉哩地里地里悉哩地哩娑嚩(二合引)訶(引)以此密言加持花及燈奉獻當誦七遍。",
        "from": "T20n1069_001:0141a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩復說奉獻飲食密言曰。",
        "from": "T20n1069_001:0141a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他娑(引下同)黎娑黎悉哩悉哩素嚕素嚕娑嚩(二合引)訶(引)以此密言加持飲食等奉獻當誦二十一遍。",
        "from": "T20n1069_001:0141a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次觀自在菩薩說護摩密言曰。",
        "from": "T20n1069_001:0141a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他訶徙麼達徙者隷虎嚕虎嚕祖嚕祖嚕蘇(上)嚕蘇(上)嚕母嚕母嚕娑嚩(二合引)訶(引)以此密言加持惹底木燃火。",
        "from": "T20n1069_001:0141a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以惹底木搵酥蜜酪。",
        "from": "T20n1069_001:0141a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搵兩頭擲火中燒。",
        "from": "T20n1069_001:0141a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜不食。",
        "from": "T20n1069_001:0141a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十一遍投火供養。",
        "from": "T20n1069_001:0141a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後求成就事。",
        "from": "T20n1069_001:0141a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩復說結方隅界密言曰。",
        "from": "T20n1069_001:0141a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他伊里弭里止里弭里底里呬里娑嚩(二合引)訶(引)以此密言加持水白芥子或灰應用結界當誦七遍。",
        "from": "T20n1069_001:0141a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時觀自在菩薩復說奉送聖眾還宮密言曰。",
        "from": "T20n1069_001:0141a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也(二合)他弭致覩致止致孽縒孽縒婆誐挽(引)曩哩夜(二合)嚩路枳帝濕嚩(二合)囉娑嚩(二合)婆(引)嚩南娑嚩(二合引)訶(引)誦此密言想奉送聖者歸本宮殿。",
        "from": "T20n1069_001:0141a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今說念誦福利。",
        "from": "T20n1069_001:0141b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先不修持能成一切事業。",
        "from": "T20n1069_001:0141b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人患寒熱病。",
        "from": "T20n1069_001:0141b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一日一發或二日三日四日一發。",
        "from": "T20n1069_001:0141b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若著鬼神彌怛拏毘舍遮。",
        "from": "T20n1069_001:0141b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "癲癎瘰癧白癩。",
        "from": "T20n1069_001:0141b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及蠱毒毒蟲等。",
        "from": "T20n1069_001:0141b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持土芥子和白檀香一七遍塗即愈。",
        "from": "T20n1069_001:0141b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切業障皆得清淨。",
        "from": "T20n1069_001:0141b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患邪風加持油。",
        "from": "T20n1069_001:0141b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗即愈。",
        "from": "T20n1069_001:0141b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患耳痛以青木香油。",
        "from": "T20n1069_001:0141b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和樺皮煎渧耳中。",
        "from": "T20n1069_001:0141b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其痛即止。",
        "from": "T20n1069_001:0141b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能治半頭痛。",
        "from": "T20n1069_001:0141b09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切病患處。",
        "from": "T20n1069_001:0141b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔誦加持無不除愈。",
        "from": "T20n1069_001:0141b10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔誦即成就。",
        "from": "T20n1069_001:0141b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲成就者。",
        "from": "T20n1069_001:0141b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以堅好無隙白檀香。",
        "from": "T20n1069_001:0141b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彫觀自在菩薩身。",
        "from": "T20n1069_001:0141b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長一尺三寸。",
        "from": "T20n1069_001:0141b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作十一頭．四臂。",
        "from": "T20n1069_001:0141b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右邊第一手把念珠。",
        "from": "T20n1069_001:0141b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手施無畏。",
        "from": "T20n1069_001:0141b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左第一手持蓮花。",
        "from": "T20n1069_001:0141b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二手執君持。",
        "from": "T20n1069_001:0141b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其十一面當前三面作寂靜相。",
        "from": "T20n1069_001:0141b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左三面威怒相。",
        "from": "T20n1069_001:0141b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右三面利牙出現相。",
        "from": "T20n1069_001:0141b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後一面作笑怒容。",
        "from": "T20n1069_001:0141b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最上一面作如來相。",
        "from": "T20n1069_001:0141b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠中各有化佛。",
        "from": "T20n1069_001:0141b18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩身種種瓔珞莊嚴。",
        "from": "T20n1069_001:0141b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "像成已於有佛舍利處安置。",
        "from": "T20n1069_001:0141b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦者身著淨衣。",
        "from": "T20n1069_001:0141b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若在家者持八戒。",
        "from": "T20n1069_001:0141b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三時供養無限數念誦。",
        "from": "T20n1069_001:0141b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從白月一日乃至八日後於淨處置此觀自在菩薩形像。",
        "from": "T20n1069_001:0141b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面西喫乳或穬麥。",
        "from": "T20n1069_001:0141b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒沈香蘇合檀香。",
        "from": "T20n1069_001:0141b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力。",
        "from": "T20n1069_001:0141b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至十三日其日食三白食。",
        "from": "T20n1069_001:0141b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大供養。",
        "from": "T20n1069_001:0141b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取菩提樹木燃火。",
        "from": "T20n1069_001:0141b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更別取菩提樹木。",
        "from": "T20n1069_001:0141b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長十指截。",
        "from": "T20n1069_001:0141b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蘇合香油搵兩頭。",
        "from": "T20n1069_001:0141b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一千八遍投護摩爐中。",
        "from": "T20n1069_001:0141b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "地即震動其像亦動。",
        "from": "T20n1069_001:0141b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞從像最上面口中出聲讚修行者言。",
        "from": "T20n1069_001:0141b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉佛子汝能勤苦求願。",
        "from": "T20n1069_001:0141b28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆令汝意願滿足。",
        "from": "T20n1069_001:0141b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賜汝成就騰空隱形。",
        "from": "T20n1069_001:0141b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持明仙轉輪法王。",
        "from": "T20n1069_001:0141b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至與我無異。",
        "from": "T20n1069_001:0141c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝必現獲如是等成就。",
        "from": "T20n1069_001:0141c01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次第二儀則。",
        "from": "T20n1069_001:0141c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從白月十五日。",
        "from": "T20n1069_001:0141c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於舍利塔中安像。",
        "from": "T20n1069_001:0141c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜不食。",
        "from": "T20n1069_001:0141c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一百八枚惹底花(蘇末那花廣府有)誦密言一遍一擲擊像。",
        "from": "T20n1069_001:0141c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於像當前面。",
        "from": "T20n1069_001:0141c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出大吼聲。",
        "from": "T20n1069_001:0141c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者不應怖畏。",
        "from": "T20n1069_001:0141c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則大地震動。",
        "from": "T20n1069_001:0141c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦不應間斷。",
        "from": "T20n1069_001:0141c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即求請願言。",
        "from": "T20n1069_001:0141c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我能與一切無主無依眾生。",
        "from": "T20n1069_001:0141c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作大依怙。",
        "from": "T20n1069_001:0141c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能滿一切有情一切意願。",
        "from": "T20n1069_001:0141c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令滿足得無障礙。",
        "from": "T20n1069_001:0141c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次密言者。",
        "from": "T20n1069_001:0141c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於月蝕時取酥一兩置於銀器。",
        "from": "T20n1069_001:0141c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像前誦密言七遍。",
        "from": "T20n1069_001:0141c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自喫及與他。",
        "from": "T20n1069_001:0141c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切疾病皆得除愈。",
        "from": "T20n1069_001:0141c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況能從初蝕乃至月復盈滿。",
        "from": "T20n1069_001:0141c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦不獲悉地。",
        "from": "T20n1069_001:0141c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次密言者。",
        "from": "T20n1069_001:0141c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先澡浴清淨潔服。",
        "from": "T20n1069_001:0141c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取雄黃或牛黃。",
        "from": "T20n1069_001:0141c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對像前誦心密言一千八遍。",
        "from": "T20n1069_001:0141c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三種相現。",
        "from": "T20n1069_001:0141c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後點額得三種成就。",
        "from": "T20n1069_001:0141c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其功効上中下等。",
        "from": "T20n1069_001:0141c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以和水灌沐其身。",
        "from": "T20n1069_001:0141c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除一切障難。",
        "from": "T20n1069_001:0141c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離一切惡夢不祥。",
        "from": "T20n1069_001:0141c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得吉祥榮盛一切。",
        "from": "T20n1069_001:0141c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾疫皆得除愈。",
        "from": "T20n1069_001:0141c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次法以香花奉獻聖觀自在。",
        "from": "T20n1069_001:0142a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取烟脂加持一百八遍。",
        "from": "T20n1069_001:0142a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗左微忿怒面額。",
        "from": "T20n1069_001:0142a03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降伏一切他敵軍陣。",
        "from": "T20n1069_001:0142a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次法若人疫牛疫及餘畜疫。",
        "from": "T20n1069_001:0142a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對觀自在菩薩。",
        "from": "T20n1069_001:0142a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取苦練木搵芥子油。",
        "from": "T20n1069_001:0142a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作護摩。",
        "from": "T20n1069_001:0142a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以緋縷右槎作綫。",
        "from": "T20n1069_001:0142a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "麁如銅箸。",
        "from": "T20n1069_001:0142a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為兩條。",
        "from": "T20n1069_001:0142a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一遍作一結乃至七結。",
        "from": "T20n1069_001:0142a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫於患者頸下或頭髻。",
        "from": "T20n1069_001:0142a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則疫病除息解脫。",
        "from": "T20n1069_001:0142a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次法被拏枳爾等諸魅所持者。",
        "from": "T20n1069_001:0142a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取白線如前加持。",
        "from": "T20n1069_001:0142a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫於寂靜面經宿。",
        "from": "T20n1069_001:0142a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後取結二十一結。",
        "from": "T20n1069_001:0142a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦密言一徧一結。",
        "from": "T20n1069_001:0142a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫病者頸下即得除愈。",
        "from": "T20n1069_001:0142a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次法若有他敵及疾疫災禍。",
        "from": "T20n1069_001:0142a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令入國界欲結方隅界者。",
        "from": "T20n1069_001:0142a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒薰陸香作護摩。",
        "from": "T20n1069_001:0142a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取五色線加持。",
        "from": "T20n1069_001:0142a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫於寂靜面。",
        "from": "T20n1069_001:0142a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成堅固大威德方隅界。",
        "from": "T20n1069_001:0142a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次密言者欲共怨敵異論。",
        "from": "T20n1069_001:0142a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得勝者。",
        "from": "T20n1069_001:0142a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應供養觀自在菩薩。",
        "from": "T20n1069_001:0142a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以嚩捨迦木燒芸臺子。",
        "from": "T20n1069_001:0142a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持白線一百八遍。",
        "from": "T20n1069_001:0142a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫於忿怒面。",
        "from": "T20n1069_001:0142a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬪諍言訟皆悉得勝除息。",
        "from": "T20n1069_001:0142a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若繫於寂靜面。",
        "from": "T20n1069_001:0142a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切障難。",
        "from": "T20n1069_001:0142a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次以眾香和水。",
        "from": "T20n1069_001:0142a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "浴觀自在菩薩。",
        "from": "T20n1069_001:0142a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又加持浴觀自在水。",
        "from": "T20n1069_001:0142a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百八遍浴毘那夜迦像。",
        "from": "T20n1069_001:0142a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切障難皆悉殄滅。",
        "from": "T20n1069_001:0142a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一面觀自在菩薩心密言念誦儀軌經卷上",
        "from": "T20n1069_001:0142a25_0",
        "context_with_next_sentence": false
    }
]