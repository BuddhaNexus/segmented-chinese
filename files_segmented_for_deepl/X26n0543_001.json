[
    {
        "source_sentence": "心經提綱",
        "from": "X26n0543_001:0830b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明　李卓吾　撰",
        "from": "X26n0543_001:0830b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心經者。",
        "from": "X26n0543_001:0830b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說心之徑要也。",
        "from": "X26n0543_001:0830b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心本無有。",
        "from": "X26n0543_001:0830b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而世人妄以為有。",
        "from": "X26n0543_001:0830b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無無。",
        "from": "X26n0543_001:0830b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而學者執以為無。",
        "from": "X26n0543_001:0830b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無分而能所立。",
        "from": "X26n0543_001:0830b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是自罣礙也。",
        "from": "X26n0543_001:0830b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自恐怖也。",
        "from": "X26n0543_001:0830b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自顛倒也。",
        "from": "X26n0543_001:0830b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安得自在。",
        "from": "X26n0543_001:0830b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨不觀於自在菩薩乎。",
        "from": "X26n0543_001:0830b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼其智慧行深。",
        "from": "X26n0543_001:0830b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既到自在彼岸矣。",
        "from": "X26n0543_001:0830b23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斯時也。",
        "from": "X26n0543_001:0830b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然照見色受想行識五蘊皆空。",
        "from": "X26n0543_001:0830b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本無生死可得。",
        "from": "X26n0543_001:0830b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故能出離生死苦海。",
        "from": "X26n0543_001:0830b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而度脫一切苦厄焉。",
        "from": "X26n0543_001:0830c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此一經之總要也。",
        "from": "X26n0543_001:0830c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下文重重說破皆以明此。",
        "from": "X26n0543_001:0830c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故遂呼而告之曰。",
        "from": "X26n0543_001:0830c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利子。",
        "from": "X26n0543_001:0830c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿謂吾說空便即著空也。",
        "from": "X26n0543_001:0830c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我說色不異於空也。",
        "from": "X26n0543_001:0830c03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如我說空不異於色也。",
        "from": "X26n0543_001:0830c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然但言不異。",
        "from": "X26n0543_001:0830c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶是二物有對。",
        "from": "X26n0543_001:0830c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖復合而為一。",
        "from": "X26n0543_001:0830c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶存一也。",
        "from": "X26n0543_001:0830c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其實我所說色。",
        "from": "X26n0543_001:0830c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是說空。",
        "from": "X26n0543_001:0830c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色之外無空矣。",
        "from": "X26n0543_001:0830c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我所說空。",
        "from": "X26n0543_001:0830c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是說色。",
        "from": "X26n0543_001:0830c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空之外無色矣。",
        "from": "X26n0543_001:0830c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非但無色。",
        "from": "X26n0543_001:0830c07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而亦無空。",
        "from": "X26n0543_001:0830c07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此真空也。",
        "from": "X26n0543_001:0830c07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故又呼而告之曰。",
        "from": "X26n0543_001:0830c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舍利子。",
        "from": "X26n0543_001:0830c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸法空相。",
        "from": "X26n0543_001:0830c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無空可名。",
        "from": "X26n0543_001:0830c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況更有生滅垢淨增減名相。",
        "from": "X26n0543_001:0830c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故。",
        "from": "X26n0543_001:0830c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色本不生。",
        "from": "X26n0543_001:0830c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空本不滅。",
        "from": "X26n0543_001:0830c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說色非垢。",
        "from": "X26n0543_001:0830c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說空非淨。",
        "from": "X26n0543_001:0830c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在色不增。",
        "from": "X26n0543_001:0830c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在空不減。",
        "from": "X26n0543_001:0830c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非億之也。",
        "from": "X26n0543_001:0830c10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空中原無是耳。",
        "from": "X26n0543_001:0830c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故五蘊皆空。",
        "from": "X26n0543_001:0830c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色受想行識也。",
        "from": "X26n0543_001:0830c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六根皆空。",
        "from": "X26n0543_001:0830c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眼耳鼻舌身意也。",
        "from": "X26n0543_001:0830c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六塵皆空。",
        "from": "X26n0543_001:0830c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無色聲香味觸法也。",
        "from": "X26n0543_001:0830c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八界皆空。",
        "from": "X26n0543_001:0830c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無眼界乃至無意識界也。",
        "from": "X26n0543_001:0831a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至生老病死。",
        "from": "X26n0543_001:0831a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明與無明。",
        "from": "X26n0543_001:0831a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四諦智證等皆無所得。",
        "from": "X26n0543_001:0831a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此自在菩薩智慧觀照到無所得之彼岸也如此。",
        "from": "X26n0543_001:0831a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所得既無。",
        "from": "X26n0543_001:0831a03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然無罣礙恐怖與夫顛倒夢想矣。",
        "from": "X26n0543_001:0831a03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現視生死。",
        "from": "X26n0543_001:0831a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而究竟涅槃矣。",
        "from": "X26n0543_001:0831a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈惟菩薩。",
        "from": "X26n0543_001:0831a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖過去現在未來三世諸佛。",
        "from": "X26n0543_001:0831a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦以此智慧。",
        "from": "X26n0543_001:0831a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得到彼岸。",
        "from": "X26n0543_001:0831a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共成無上正等正覺焉耳。",
        "from": "X26n0543_001:0831a06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則信乎盡大地眾生。",
        "from": "X26n0543_001:0831a06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有不是佛者。",
        "from": "X26n0543_001:0831a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃知此真空妙智。",
        "from": "X26n0543_001:0831a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大神呪。",
        "from": "X26n0543_001:0831a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大明呪。",
        "from": "X26n0543_001:0831a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無上呪。",
        "from": "X26n0543_001:0831a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無等等呪。",
        "from": "X26n0543_001:0831a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能出離生死苦海。",
        "from": "X26n0543_001:0831a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度脫一切苦厄。",
        "from": "X26n0543_001:0831a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真實不虗也。",
        "from": "X26n0543_001:0831a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則空之難言也久矣。",
        "from": "X26n0543_001:0831a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執色者泥色。",
        "from": "X26n0543_001:0831a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說空者滯空。",
        "from": "X26n0543_001:0831a10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及至兩無所依。",
        "from": "X26n0543_001:0831a10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則又一切撥無因果。",
        "from": "X26n0543_001:0831a10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信經中分明讚嘆。",
        "from": "X26n0543_001:0831a11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空即是色。",
        "from": "X26n0543_001:0831a11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更有何空。",
        "from": "X26n0543_001:0831a11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色即是空。",
        "from": "X26n0543_001:0831a11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更有何色。",
        "from": "X26n0543_001:0831a12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無空無色。",
        "from": "X26n0543_001:0831a12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚何有有有無於我罣礙。",
        "from": "X26n0543_001:0831a12_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不得自在耶。",
        "from": "X26n0543_001:0831a12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則觀者。",
        "from": "X26n0543_001:0831a13_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但以自家智慧。",
        "from": "X26n0543_001:0831a13_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時常觀照。",
        "from": "X26n0543_001:0831a13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則彼岸當自得之矣。",
        "from": "X26n0543_001:0831a13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩豈異人哉。",
        "from": "X26n0543_001:0831a14_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但能一觀照之焉耳。",
        "from": "X26n0543_001:0831a14_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人人皆菩薩。",
        "from": "X26n0543_001:0831a15_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不自見也。",
        "from": "X26n0543_001:0831a15_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故言菩薩。",
        "from": "X26n0543_001:0831a15_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則人人一矣。",
        "from": "X26n0543_001:0831a15_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無聖愚也。",
        "from": "X26n0543_001:0831a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言三世諸佛。",
        "from": "X26n0543_001:0831a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則古今一矣。",
        "from": "X26n0543_001:0831a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無先後也。",
        "from": "X26n0543_001:0831a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奈之何可使由而不可使知者眾也。",
        "from": "X26n0543_001:0831a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可使知。",
        "from": "X26n0543_001:0831a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為菩薩。",
        "from": "X26n0543_001:0831a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可使知。",
        "from": "X26n0543_001:0831a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為凡民。",
        "from": "X26n0543_001:0831a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為禽獸。",
        "from": "X26n0543_001:0831a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為木石。",
        "from": "X26n0543_001:0831a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卒歸於泯泯爾矣。",
        "from": "X26n0543_001:0831a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心經提綱(終)",
        "from": "X26n0543_001:0831a20_0",
        "context_with_next_sentence": false
    }
]