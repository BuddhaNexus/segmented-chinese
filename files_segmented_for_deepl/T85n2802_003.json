[
    {
        "source_sentence": "瑜伽論第三十三卷手記",
        "from": "T85n2802_003:0942c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四得果瑜伽處初",
        "from": "T85n2802_003:0942c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "談述　福慧隨聽",
        "from": "T85n2802_003:0942c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言廣辨二定者。",
        "from": "T85n2802_003:0942c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無相滅盡二定也　言五神通者。",
        "from": "T85n2802_003:0942c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初靜慮等八定之果即八定根本地也　言諸相者。",
        "from": "T85n2802_003:0942c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂離欲色二界欲也。",
        "from": "T85n2802_003:0942c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此上七門依世道立果也　言觀察諸諦者。",
        "from": "T85n2802_003:0942c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂四決擇分修行人也　言如實而通者。",
        "from": "T85n2802_003:0942c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則是其見道位也　言廣分別於修者。",
        "from": "T85n2802_003:0942c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則是其修習位也　言究竟為其後者。",
        "from": "T85n2802_003:0942c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則是其無學位也　言少分樂斷者。",
        "from": "T85n2802_003:0942c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初發心修七作意樂見修二惑人也　言彼初修　言一者等者。",
        "from": "T85n2802_003:0942c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸異生未斷見修二惑故　言二不具縛等者。",
        "from": "T85n2802_003:0942c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸有學已斷見惑分斷修惑故也　言如是乃至等者。",
        "from": "T85n2802_003:0942c28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從第二靜慮已上至無所有處故成六也　言又於靜慮等者。",
        "from": "T85n2802_003:0942c29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂靜慮等八定為因而能引發無心滅盡二定及五神通果。",
        "from": "T85n2802_003:0943a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故謂天眼等也　言若生者。",
        "from": "T85n2802_003:0943a03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂前第六生差別門也　言若相者。",
        "from": "T85n2802_003:0943a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂前第七離欲相門也。",
        "from": "T85n2802_003:0943a04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中不列第八觀察諸諦等餘四門者。",
        "from": "T85n2802_003:0943a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出世道因果攝故　言骨鎖者。",
        "from": "T85n2802_003:0943a06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯五欲猶如枯骨。",
        "from": "T85n2802_003:0943a06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖食無味顯無厭足也　言凝血者。",
        "from": "T85n2802_003:0943a07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯多所共有義如市纏害眾比有分　言草炬火者。",
        "from": "T85n2802_003:0943a08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯惡行因義也。",
        "from": "T85n2802_003:0943a09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂執草炬不棄者必燒其手　言一分炭火者。",
        "from": "T85n2802_003:0943a09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯增長愛欲也。",
        "from": "T85n2802_003:0943a10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若近火坑不能止渴　言毒蛇者。",
        "from": "T85n2802_003:0943a11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯智人所遠離義也　言夢所見者。",
        "from": "T85n2802_003:0943a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯速趣消滅義也　言假借莊嚴具者。",
        "from": "T85n2802_003:0943a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯依託眾緣義也　言如樹端者。",
        "from": "T85n2802_003:0943a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯放逸危六地義也。",
        "from": "T85n2802_003:0943a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂受欲者必墮惡趣故　言如是等者。",
        "from": "T85n2802_003:0943a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此八喻義如思所成地密意伽他中已廣說故　言如是諸欲等。",
        "from": "T85n2802_003:0943a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一十六句中。",
        "from": "T85n2802_003:0943a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前八句與前八喻一一相合。",
        "from": "T85n2802_003:0943a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後八依無常等顯其欲過。",
        "from": "T85n2802_003:0943a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如論應知　言此作意等者。",
        "from": "T85n2802_003:0943a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由了相作意為因故生後勝解作意　言間雜者。",
        "from": "T85n2802_003:0943a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂間雜修聞思二法成了相作意也　言唯用修行者。",
        "from": "T85n2802_003:0943a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯依修所成慧行相緣初靜慮所緣意也　言即此勝解等者。",
        "from": "T85n2802_003:0943a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由初中後三勝解為因故成後遠離作意。",
        "from": "T85n2802_003:0943a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言最初生起等也　言俱行作意者。",
        "from": "T85n2802_003:0943a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂斷欲界麁相煩惱之時。",
        "from": "T85n2802_003:0943a25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與作意俱也。",
        "from": "T85n2802_003:0943a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下五門皆取前前為因成後後法　言諸煩惱者。",
        "from": "T85n2802_003:0943a26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初靜慮中障礙現行也　言麁重者。",
        "from": "T85n2802_003:0943a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無輕安性故也　言從是已後等者。",
        "from": "T85n2802_003:0943a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由遠離作意故修七法為樂生因方成遠離。",
        "from": "T85n2802_003:0943a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一愛樂於斷。",
        "from": "T85n2802_003:0943b01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二愛樂遠離。",
        "from": "T85n2802_003:0943b01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三於諸斷中見勝功德。",
        "from": "T85n2802_003:0943b01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四觸證少分。",
        "from": "T85n2802_003:0943b02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初證得離生樂故。",
        "from": "T85n2802_003:0943b02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五於時時間欣樂作意。",
        "from": "T85n2802_003:0943b03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六於時時間厭離作意。",
        "from": "T85n2802_003:0943b03_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七為欲斷除初靜慮五蓋障。",
        "from": "T85n2802_003:0943b04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言為欲除遣等也　言習近等者。",
        "from": "T85n2802_003:0943b04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初中後三心也。",
        "from": "T85n2802_003:0943b05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於初靜慮障中未能斷除諸種習故。",
        "from": "T85n2802_003:0943b06_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見淨妙色等便起妄。",
        "from": "T85n2802_003:0943b07_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言不能捨等也　言諸行者。",
        "from": "T85n2802_003:0943b07_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂色等諸行也　言此由暫時等者。",
        "from": "T85n2802_003:0943b08_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有難。",
        "from": "T85n2802_003:0943b09_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是初靜慮定斷欲界麁相者。",
        "from": "T85n2802_003:0943b09_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為能頓斷久遠種子。",
        "from": "T85n2802_003:0943b10_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為當但斷現行耶。",
        "from": "T85n2802_003:0943b10_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云此由暫時等也　言又於遠離等者。",
        "from": "T85n2802_003:0943b11_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_003:0943b12_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七作意中何故但明遠離等四說有受樂之相。",
        "from": "T85n2802_003:0943b12_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不說餘三耶。",
        "from": "T85n2802_003:0943b13_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答。",
        "from": "T85n2802_003:0943b13_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為了相勝解觀察此三作意於散位說無定地故不能生樂　言無有間隙者。",
        "from": "T85n2802_003:0943b13_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂受終已來離生喜樂無間斷故　言五支者。",
        "from": "T85n2802_003:0943b15_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一尋。",
        "from": "T85n2802_003:0943b16_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二伺。",
        "from": "T85n2802_003:0943b16_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三喜。",
        "from": "T85n2802_003:0943b16_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四樂。",
        "from": "T85n2802_003:0943b16_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五心一境性也　言住欲界等者。",
        "from": "T85n2802_003:0943b17_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂能對治欲界麁相及能修證初靜慮果故也。",
        "from": "T85n2802_003:0943b17_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_003:0943b18_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七作意中何故但明　言了相作意等者。",
        "from": "T85n2802_003:0943b19_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五欲應斷初靜慮應得　言能於所得等者。",
        "from": "T85n2802_003:0943b20_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不觀察者於未得中便起得相名增上慢也　言又若了相等者。",
        "from": "T85n2802_003:0943b21_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此二法總能隨順厭離五欲及修對治故　言如是其等者。",
        "from": "T85n2802_003:0943b22_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依蕃本云。",
        "from": "T85n2802_003:0943b24_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是六種作意當知攝入四作意中。",
        "from": "T85n2802_003:0943b24_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中意說。",
        "from": "T85n2802_003:0943b25_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除加行究竟果作意。",
        "from": "T85n2802_003:0943b25_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘六皆是因位作意故攝入隨順等四法中也　言隨所在地等者。",
        "from": "T85n2802_003:0943b25_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂初靜慮也。",
        "from": "T85n2802_003:0943b27_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初靜慮麁相。",
        "from": "T85n2802_003:0943b27_63",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二靜慮淨相明觀待義。",
        "from": "T85n2802_003:0943b27_64",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘三道理准前應知　言如前應知者。",
        "from": "T85n2802_003:0943b28_65",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如了相作意遍於初靜慮。",
        "from": "T85n2802_003:0943b29_66",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝解等六皆亦遍。",
        "from": "T85n2802_003:0943c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故如前所說　言初靜慮者　言復次此中等者。",
        "from": "T85n2802_003:0943c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依蕃本。",
        "from": "T85n2802_003:0943c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先標經句。",
        "from": "T85n2802_003:0943c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後方解釋。",
        "from": "T85n2802_003:0943c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中間其樣經句。",
        "from": "T85n2802_003:0943c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說者應知　言樣句者。",
        "from": "T85n2802_003:0943c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云。",
        "from": "T85n2802_003:0943c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初靜慮者謂離欲惡不善法。",
        "from": "T85n2802_003:0943c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有尋有伺離生喜樂。",
        "from": "T85n2802_003:0943c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於初靜慮具足安住　言煩惱欲者。",
        "from": "T85n2802_003:0943c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂男女互相愛染也　言事欲者。",
        "from": "T85n2802_003:0943c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂貪財寶等也　言相應者離者。",
        "from": "T85n2802_003:0943c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂離五欲相應義也　言境界離者。",
        "from": "T85n2802_003:0943c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五欲境也　言煩惱者。",
        "from": "T85n2802_003:0943c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂煩惱欲也　言欲者。",
        "from": "T85n2802_003:0943c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂事欲也　言由此為因者。",
        "from": "T85n2802_003:0943c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由加行究竟作意為因故也　言由所修習者。",
        "from": "T85n2802_003:0943c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於加行究竟果成辨故也　言得隨所樂者。",
        "from": "T85n2802_003:0943c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂出入等得自在故也　言艱難者。",
        "from": "T85n2802_003:0943c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂少治便能與定相應也　言梗澁者。",
        "from": "T85n2802_003:0943c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梗為略也。",
        "from": "T85n2802_003:0943c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不假功用故言其心盡夜能正者與隨順等三心一一相合　言隨所樂欲者。",
        "from": "T85n2802_003:0943c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於欲界中定成就者能於七日方出其定　言復次於有尋等者。",
        "from": "T85n2802_003:0943c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若依蕃本。",
        "from": "T85n2802_003:0943c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四靜慮一一皆先標其經句。",
        "from": "T85n2802_003:0943c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後方解釋。",
        "from": "T85n2802_003:0943c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂大小乘經皆說言。",
        "from": "T85n2802_003:0943c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二靜慮者謂尋伺寂靜故。",
        "from": "T85n2802_003:0943c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內等靜故。",
        "from": "T85n2802_003:0943c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心一趣故。",
        "from": "T85n2802_003:0943c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定生喜樂。",
        "from": "T85n2802_003:0943c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於第二靜慮具足安住　言成此彼於等者。",
        "from": "T85n2802_003:0943c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云。",
        "from": "T85n2802_003:0943c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言第三靜慮者。",
        "from": "T85n2802_003:0943c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依此離欲安住於捨有正念正知有身樂受。",
        "from": "T85n2802_003:0943c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸聖宣說具足捨念及與正知。",
        "from": "T85n2802_003:0943c23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住身受樂依其無喜。",
        "from": "T85n2802_003:0943c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三靜慮具足安住。",
        "from": "T85n2802_003:0943c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋義如論　言喜者。",
        "from": "T85n2802_003:0943c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是第二亂心也　言喜相違心者。",
        "from": "T85n2802_003:0943c25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂捨受也　言色身者。",
        "from": "T85n2802_003:0943c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前五色根也　言意者。",
        "from": "T85n2802_003:0943c26_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意根也　言第三靜慮已上等者。",
        "from": "T85n2802_003:0943c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二門顯說諸因也　言下地者。",
        "from": "T85n2802_003:0943c27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二靜慮已下也　言上地者。",
        "from": "T85n2802_003:0943c28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是第四靜慮已上也　言諸聖者。",
        "from": "T85n2802_003:0943c29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂佛及聖弟子等。",
        "from": "T85n2802_003:0944a01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於蕃本此中合有離喜及具足安住第三靜慮。",
        "from": "T85n2802_003:0944a01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋義同前故譯者不出。",
        "from": "T85n2802_003:0944a02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說者應知　言復次等者。",
        "from": "T85n2802_003:0944a03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有難。",
        "from": "T85n2802_003:0944a03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故第四靜慮之中契經但說樂斷不說對治耶。",
        "from": "T85n2802_003:0944a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云此中對治等也。",
        "from": "T85n2802_003:0944a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂斷樂對治與前斷尋伺及喜對治相似故。",
        "from": "T85n2802_003:0944a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以不說也。",
        "from": "T85n2802_003:0944a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中亦合先標經句。",
        "from": "T85n2802_003:0944a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故經云。",
        "from": "T85n2802_003:0944a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言第四靜慮者。",
        "from": "T85n2802_003:0944a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂斷故。",
        "from": "T85n2802_003:0944a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦斷故。",
        "from": "T85n2802_003:0944a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先憂喜沒。",
        "from": "T85n2802_003:0944a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼於爾時不苦不樂捨念清淨。",
        "from": "T85n2802_003:0944a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於第四靜慮具足安住。",
        "from": "T85n2802_003:0944a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋義如下　言先所斷者。",
        "from": "T85n2802_003:0944a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂喜受憂受苦受也　言今所斷者。",
        "from": "T85n2802_003:0944a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂第四禪所斷樂受也　言今依此中等者。",
        "from": "T85n2802_003:0944a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_003:0944a12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若第四靜慮斷樂受者。",
        "from": "T85n2802_003:0944a12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故經說第四靜慮亦斷苦喜憂三耶。",
        "from": "T85n2802_003:0944a13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云今於此中等也。",
        "from": "T85n2802_003:0944a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖斷苦樂名為捨受。",
        "from": "T85n2802_003:0944a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由先修於初二三靜慮時喜憂斷故。",
        "from": "T85n2802_003:0944a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中總說為第四斷也　言青黃赤白等者。",
        "from": "T85n2802_003:0944a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂欲色二界色也　言離欲者。",
        "from": "T85n2802_003:0944a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂厭色界欲也。",
        "from": "T85n2802_003:0944a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於蕃本亦應先標經句。",
        "from": "T85n2802_003:0944a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云。",
        "from": "T85n2802_003:0944a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言空無邊處者。",
        "from": "T85n2802_003:0944a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色相出過故。",
        "from": "T85n2802_003:0944a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有對相滅沒故。",
        "from": "T85n2802_003:0944a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種相不作意故。",
        "from": "T85n2802_003:0944a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入空無邊。",
        "from": "T85n2802_003:0944a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於空無邊處具足安住　言眾多品類者。",
        "from": "T85n2802_003:0944a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂欲色界中青黃等皆因形顯和合故。",
        "from": "T85n2802_003:0944a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能於無色作其障礙也　言由遠離等者。",
        "from": "T85n2802_003:0944a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於色界中雖無飲食等。",
        "from": "T85n2802_003:0944a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有宮殿衣服莊嚴具故。",
        "from": "T85n2802_003:0944a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故云等相也　言復次若由等者。",
        "from": "T85n2802_003:0944a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云。",
        "from": "T85n2802_003:0944a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言識無邊處者。",
        "from": "T85n2802_003:0944a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超過一切空無邊處入識無邊。",
        "from": "T85n2802_003:0944a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於識無識邊處具足安住　言復次從識無邊處等者。",
        "from": "T85n2802_003:0944a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云。",
        "from": "T85n2802_003:0944a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無所有。",
        "from": "T85n2802_003:0944a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超過一切識無邊處無所有。",
        "from": "T85n2802_003:0944a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無所有處具足安住也。",
        "from": "T85n2802_003:0944a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由厭於識無邊處為因故。",
        "from": "T85n2802_003:0944a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無所有處發起勝解　言或色者。",
        "from": "T85n2802_003:0944a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色蘊也　言非色者。",
        "from": "T85n2802_003:0944b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘四蘊也　言一切有者。",
        "from": "T85n2802_003:0944b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂前諸靜慮中有相無相非無相等所有相不可得故。",
        "from": "T85n2802_003:0944b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言非有相等也　言謂非如無等者。",
        "from": "T85n2802_003:0944b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此二定一切心中無相定滅前六識。",
        "from": "T85n2802_003:0944b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若滅盡定有其二種。",
        "from": "T85n2802_003:0944b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不還人入者滅第七識已下。",
        "from": "T85n2802_003:0944b05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若阿羅漢入者滅第八識　言蜜中者。",
        "from": "T85n2802_003:0944b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如蜂等處於蜜中而難得。",
        "from": "T85n2802_003:0944b07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出世定亦爾。",
        "from": "T85n2802_003:0944b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故蕃本云。",
        "from": "T85n2802_003:0944b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身相狀而沈於地也　言等者。",
        "from": "T85n2802_003:0944b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無相定依四靜慮能入。",
        "from": "T85n2802_003:0944b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若滅盡定依四無色定能入故也　言暫時止息等者。",
        "from": "T85n2802_003:0944b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂見想受二法惱亂身心故暫時止息也　言所緣者。",
        "from": "T85n2802_003:0944b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂想受二法也　言不念作意者。",
        "from": "T85n2802_003:0944b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂不念其想也　言有心位者。",
        "from": "T85n2802_003:0944b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂修了想相等六作意時。",
        "from": "T85n2802_003:0944b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆有第六意識故　言心不復轉者。",
        "from": "T85n2802_003:0944b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂第七加行究竟果位時也　言心心法滅者。",
        "from": "T85n2802_003:0944b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂第六意識已下心及所俱不行故　言根本者。",
        "from": "T85n2802_003:0944b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂已得四靜慮加行究竟果作意也　言了知依義者。",
        "from": "T85n2802_003:0944b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五神通義理也　言了知依法者。",
        "from": "T85n2802_003:0944b18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂了知五神通因果法也　言有時者。",
        "from": "T85n2802_003:0944b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂鈍根人於此生中修輕舉等十二相故。",
        "from": "T85n2802_003:0944b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後生中方能發起五種神通　言有分者。",
        "from": "T85n2802_003:0944b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂利根人此生中修便能發起五通相　言此想者。",
        "from": "T85n2802_003:0944b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由輕舉於身發起輕舉勝解想故也。",
        "from": "T85n2802_003:0944b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下九門義並同此也　言熟練者。",
        "from": "T85n2802_003:0944b24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂帛練也　言廣說等者。",
        "from": "T85n2802_003:0944b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂如地水相作餘風火相作變化勝解當知亦爾。",
        "from": "T85n2802_003:0944b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論文言廣說等也　言或以其身等者。",
        "from": "T85n2802_003:0944b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由空界相成就相　言或於其地等者。",
        "from": "T85n2802_003:0944b27_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此柔軟輕舉相成就故　言或於廣大等者。",
        "from": "T85n2802_003:0944b28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此由身心符順等成就故　言隨其所應者。",
        "from": "T85n2802_003:0944b29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已上四相一一相合。",
        "from": "T85n2802_003:0944c01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由攝受空界相勝解故。",
        "from": "T85n2802_003:0944c02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸墻壁等直過無礙。",
        "from": "T85n2802_003:0944c02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘皆准此　言一分者。",
        "from": "T85n2802_003:0944c03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從欲界至初靜慮中間唯有鬼神等造色不能障礙故　言謂由此等者。",
        "from": "T85n2802_003:0944c03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂由隨念相成就故。",
        "from": "T85n2802_003:0944c05_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能知自身從生至老所作事業麁相門中不失次第。",
        "from": "T85n2802_003:0944c05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於微細相不能憶念。",
        "from": "T85n2802_003:0944c07_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂是世道麁細皆能隨念。",
        "from": "T85n2802_003:0944c07_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此顯宿住智通也　言又於種種等者。",
        "from": "T85n2802_003:0944c08_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於五趣有情所有善不善等業能感善惡果作用之中。",
        "from": "T85n2802_003:0944c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能明了知者。",
        "from": "T85n2802_003:0944c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名光明相也　言常變色者。",
        "from": "T85n2802_003:0944c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若嗔時觸誤他人面色有變故　言廣說乃至等者。",
        "from": "T85n2802_003:0944c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂諸有情隨其所有貪等諸行變異相者。",
        "from": "T85n2802_003:0944c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名他心智通也　言變事通者。",
        "from": "T85n2802_003:0944c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂變地為水等也　言化事通者。",
        "from": "T85n2802_003:0944c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於無中現有故也　言勝解者。",
        "from": "T85n2802_003:0944c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂變少為多等也　言無諍者。",
        "from": "T85n2802_003:0944c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂無諍三昧也。",
        "from": "T85n2802_003:0944c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引發有情故　言願智者。",
        "from": "T85n2802_003:0944c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂廣大行願也。",
        "from": "T85n2802_003:0944c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘文可見　言文聖等。",
        "from": "T85n2802_003:0944c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中雖說神境通。",
        "from": "T85n2802_003:0944c18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五神皆有差別　言皆能成辦者。",
        "from": "T85n2802_003:0944c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂前五相能成辦神境通。",
        "from": "T85n2802_003:0944c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘五皆有成辦。",
        "from": "T85n2802_003:0944c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前所說　言間雜修。",
        "from": "T85n2802_003:0944c20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂世出世道俱修故也　言若處者。",
        "from": "T85n2802_003:0944c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於三品中空處識處合為下品。",
        "from": "T85n2802_003:0944c21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘二中上　言由彼諸天等者。",
        "from": "T85n2802_003:0944c22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有問。",
        "from": "T85n2802_003:0944c23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無四定為有處所為無處所。",
        "from": "T85n2802_003:0944c23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故論答云無有色形等也。",
        "from": "T85n2802_003:0944c23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖無處所。",
        "from": "T85n2802_003:0944c24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但依作用空識等故在差別名也　言即於此中下品等者。",
        "from": "T85n2802_003:0944c24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂此五品為因故證五淨居天也。",
        "from": "T85n2802_003:0944c26_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑜伽論第三十三卷手記竟",
        "from": "T85n2802_003:0944c27_0",
        "context_with_next_sentence": false
    }
]