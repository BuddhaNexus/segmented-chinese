[
    {
        "source_sentence": "佛說大金色孔雀王呪經一卷",
        "from": "T19n0987_001:0479a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附秦錄",
        "from": "T19n0987_001:0479a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛南無法南無比丘僧　南無七佛等正覺。",
        "from": "T19n0987_001:0479a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無辟支佛南無諸羅漢。",
        "from": "T19n0987_001:0479a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無彌勒等一切菩薩。",
        "from": "T19n0987_001:0479a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無諸阿那含南無斯陀含南無須陀洹。",
        "from": "T19n0987_001:0479a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無世間正信向者。",
        "from": "T19n0987_001:0479a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我禮彼眾已。",
        "from": "T19n0987_001:0479a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲說大孔雀王呪經。",
        "from": "T19n0987_001:0479a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願如意成就。",
        "from": "T19n0987_001:0479a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉諸鬼神等明聽。",
        "from": "T19n0987_001:0479a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若空虛在地及水居者。",
        "from": "T19n0987_001:0479a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍阿修羅摩樓多迦樓羅乾闥婆緊那羅摩睺羅伽。",
        "from": "T19n0987_001:0479a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夜叉羅剎餓鬼毘舍遮浮陀鳩槃茶富單那羯吒富單那乾陀溫摩陀車耶阿鉢摩羅欝多羅伽等。",
        "from": "T19n0987_001:0479a13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽我所說。",
        "from": "T19n0987_001:0479a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神所食吸氣鬼。",
        "from": "T19n0987_001:0479a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食石蜜鬼食血鬼。",
        "from": "T19n0987_001:0479a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食髓鬼食肉鬼。",
        "from": "T19n0987_001:0479a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食脂鬼食胎鬼。",
        "from": "T19n0987_001:0479a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食命鬼食力鬼。",
        "from": "T19n0987_001:0479a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食髮鬼食華鬼。",
        "from": "T19n0987_001:0479a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食聲鬼食氣鬼。",
        "from": "T19n0987_001:0479a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食惡心鬼食陰謀心鬼。",
        "from": "T19n0987_001:0479a18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "害心鬼好奪他命諸鬼神等。",
        "from": "T19n0987_001:0479a18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽我欲說此大孔雀王呪經。",
        "from": "T19n0987_001:0479a19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今與香華飲食汝等受之。",
        "from": "T19n0987_001:0479a20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "害汝惡心陰謀心害。",
        "from": "T19n0987_001:0479a20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來食香下氣慈心善心。",
        "from": "T19n0987_001:0479a21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信佛信法信比丘僧心者。",
        "from": "T19n0987_001:0479a21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽我所說。",
        "from": "T19n0987_001:0479a22_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十四羅剎女。",
        "from": "T19n0987_001:0479a22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名黑闇二名作黑闇。",
        "from": "T19n0987_001:0479a22_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三名鳩槃茶四名白具。",
        "from": "T19n0987_001:0479a23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五名華眼六名取子。",
        "from": "T19n0987_001:0479a23_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七名取髮八名作黃。",
        "from": "T19n0987_001:0479a24_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九名垂下十名極垂下。",
        "from": "T19n0987_001:0479a24_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一名伺便十二名閻羅使。",
        "from": "T19n0987_001:0479a25_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三名閻羅羅剎十四名噉鬼。",
        "from": "T19n0987_001:0479a26_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等受我華香飲食。",
        "from": "T19n0987_001:0479a26_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護某甲及諸眷屬。",
        "from": "T19n0987_001:0479a27_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使我呪句如意成就吉。",
        "from": "T19n0987_001:0479a27_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時我聞。",
        "from": "T19n0987_001:0479a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛住舍衛國祇樹給孤獨園。",
        "from": "T19n0987_001:0479a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時園中有一年少比丘名曰吉祥。",
        "from": "T19n0987_001:0479b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家未久為僧洗浴破薪。",
        "from": "T19n0987_001:0479b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有異木下有一黑蛇來。",
        "from": "T19n0987_001:0479b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "螫比丘足指。",
        "from": "T19n0987_001:0479b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悶絕躄地目翻吐沫。",
        "from": "T19n0987_001:0479b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時長老阿難。",
        "from": "T19n0987_001:0479b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見吉祥比丘所患極重。",
        "from": "T19n0987_001:0479b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見已惶怖往詣佛所。",
        "from": "T19n0987_001:0479b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面著地為佛作禮。",
        "from": "T19n0987_001:0479b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前白佛言世尊。",
        "from": "T19n0987_001:0479b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此舍衛國祇樹給孤獨園中。",
        "from": "T19n0987_001:0479b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一新學比丘名曰吉祥。",
        "from": "T19n0987_001:0479b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為僧洗浴破薪。",
        "from": "T19n0987_001:0479b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異木下有一黑蛇出。",
        "from": "T19n0987_001:0479b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "螫比丘右足指。",
        "from": "T19n0987_001:0479b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悶絕躄地目翻吐沫云何治之。",
        "from": "T19n0987_001:0479b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告阿難。",
        "from": "T19n0987_001:0479b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝持如來大孔雀王呪經。",
        "from": "T19n0987_001:0479b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護吉祥比丘。",
        "from": "T19n0987_001:0479b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與結界結呪。",
        "from": "T19n0987_001:0479b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使毒不能害。",
        "from": "T19n0987_001:0479b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刀杖不加眾患悉除。",
        "from": "T19n0987_001:0479b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天所為若龍所為。",
        "from": "T19n0987_001:0479b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩樓多所為迦樓羅所為。",
        "from": "T19n0987_001:0479b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾闥婆所為緊那羅所為。",
        "from": "T19n0987_001:0479b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩睺羅伽所為夜叉所為。",
        "from": "T19n0987_001:0479b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎所為餓鬼所為。",
        "from": "T19n0987_001:0479b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘舍遮所為浮陀所為。",
        "from": "T19n0987_001:0479b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鳩槃茶所為富單那所為。",
        "from": "T19n0987_001:0479b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黑富單那所為乾陀所為。",
        "from": "T19n0987_001:0479b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熅摩陀所為車耶所為。",
        "from": "T19n0987_001:0479b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿鉢摩羅所為欝多伽所為。",
        "from": "T19n0987_001:0479b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉遮所為羯磨那所為。",
        "from": "T19n0987_001:0479b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉軀陀所為翅蘭那所為。",
        "from": "T19n0987_001:0479b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘多茶所為脂遮所為。",
        "from": "T19n0987_001:0479b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卑沙迦所為。",
        "from": "T19n0987_001:0479b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若非法食非法吐。",
        "from": "T19n0987_001:0479b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非法視非法舉。",
        "from": "T19n0987_001:0479b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非法越非法觸。",
        "from": "T19n0987_001:0479b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有熱病一日二日。",
        "from": "T19n0987_001:0479b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三日四日五日六日。",
        "from": "T19n0987_001:0479b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日半月一月。",
        "from": "T19n0987_001:0479b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一日一時發間發。",
        "from": "T19n0987_001:0479b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鬼神熱病風火水作。",
        "from": "T19n0987_001:0479b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霍亂熱。",
        "from": "T19n0987_001:0479b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若半身痛頭痛眼痛。",
        "from": "T19n0987_001:0479b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面痛頦痛咽喉痛項痛。",
        "from": "T19n0987_001:0479b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "耳痛齒痛。",
        "from": "T19n0987_001:0479b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口痛心痛背痛。",
        "from": "T19n0987_001:0479b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腹痛。",
        "from": "T19n0987_001:0479b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腰痛陰痛髀痛。",
        "from": "T19n0987_001:0479b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髆手痛脚痛。",
        "from": "T19n0987_001:0479b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支節痛一切。",
        "from": "T19n0987_001:0479b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸痛皆令除愈擁護某甲說偈呪曰。",
        "from": "T19n0987_001:0479b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝安夜安　中間常安　晝夜安隱　諸佛所護即說呪曰。",
        "from": "T19n0987_001:0479b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊稚比稚　吉稚迄稚　婆稚尼稚　阿稚伽稚　豆加稚　阿稚槃求稚　婆修卑　舍脂　膩阿婁呵膩　婆婁訶膩　烏婁呵膩伊隸彌隸帝　隸帝隸　彌隸帝　隸頭隸頭頭隸　伊緻彌緻　毘獵地毘摩利休婁休婁　阿舍婆契伽稚摩訶伽稚婆枳那枳只句婁句婁多破漏鋸婁鋸婁度沙曇婆度曇婆豆摩曇婆瞿那耶比羅耶　醯輸醯輸　醯利醯利　彌利彌利　底利底利　周漏周漏　茂吼茂吼　茂吼茂吼　茂漏茂漏　茂漏茂漏茂漏　吼吼吼吼吼吼吼吼　吼婆吼婆婆婆婆婆婆婆婆婆闍婆闍羅闍闍羅　闍闍羅　摩陀摩陀禰　哆婆哆婆禰　座婆座婆羅羅禰錢頭鼻伽闍禰拔　沙禰怖吒禰　婆遮禰　劍婆禰　摩陀禰　慢遲　緻迦摩迦利　舍利迦加利　舍迦　座利婆羅禰　頭摩頭摩利　婆爾彌　具羅夜婆　利毘羅夜　拔沙頭提婆　沙滿頭提婆　沙滿提那　伊只利　死悉波呵此大孔雀王呪經。",
        "from": "T19n0987_001:0479c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朝說自護晝則安隱。",
        "from": "T19n0987_001:0479c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "暮說自護夜則安隱。",
        "from": "T19n0987_001:0479c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0987_001:0479c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吼吼吼吼吼吼　那伽利離　曇婆　梨梨　離毘摩梨梨離　暉夜暉夜　毘闍毘闍　兜備求備　伊羅彌羅　伊伽羅彌　伊利彌羅　伊利彌帝利　知利彌利伊知利醯利　帝利彌帝利　修利彌帝利　妬修妬衢羅支羅　遮婆羅　毘羅遮　伊知利毘知利知利　南無阿羅呵陀　阿羅陀羅　拔沙頭提波三滿提那阿難我今為汝說大孔雀王心呪。",
        "from": "T19n0987_001:0479c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0987_001:0480a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊緻彌緻帝利卑利彌緻彌利。",
        "from": "T19n0987_001:0480a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝彌藪曇婆兜婆　修婆至利　吉毘夜毘那彌稚　南無佛陀南　至利吉死　婆多牟梨伊緻訶羅　盧醯多牟羅兜婆菴婆　鳩致鳩鳩那致鳩那致　知羅鳩闍那致　阿茶拔多夜那婆魔　沙陀舍魔賜帝　伊利彌利　吉利彌利　翅羅彌羅　翅斗牟隸　頭頭摩薩頭彌利陀利彌　薩浮婆帝　浮薩帝　浮薩帝伊羅婆悉哆　婆翅那迦羅　那羅摩吉利　彌羅佶羅　摩結隸　伊帝薩闍隸　兜備兜備　阿帝婆羅那帝　阿那那那帝　拔沙頭提婆耶善陀　翅那那　羅迄稚　婆羅迄稚　訶利多利　鳩多利　伊利彌利　吉帝利彌帝　伊利彌　悉譚　豆陀羅　毘陀沙波訶呪句成就吉。",
        "from": "T19n0987_001:0480a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此名大孔雀王呪心。",
        "from": "T19n0987_001:0480a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難此大孔雀王呪。",
        "from": "T19n0987_001:0480a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彌勒菩薩隨順所說。",
        "from": "T19n0987_001:0480a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0987_001:0480a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尸利梨　尸利尸利　婆地地樹樹　知婆帝隸訶隸訶隸隸　阿利膩　[仁二+覃]帝　賒　婆隸　尸備扠羅扠羅　波膩波膩　菩提菩提　菩提菩提　薩斗婆菩提婆利遮羅尸裔　悉婆訶阿難此大孔雀王呪。",
        "from": "T19n0987_001:0480a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娑婆世界主梵天王隨順所說。",
        "from": "T19n0987_001:0480a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0987_001:0480a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醯隷醯隷　呵隷隷彌　摩利膩　婆居利吉利　吉利　吉利吉利　吉利底　梵魔裔鳩蘭茶　翅毘圖呵　哹婁哹婁哹婁哹婁　悉波訶阿難是大孔雀王呪。",
        "from": "T19n0987_001:0480a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天帝釋隨順所說即。",
        "from": "T19n0987_001:0480b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說呪曰。",
        "from": "T19n0987_001:0480b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陀羅禪兜隷　摩羅禪兜隷　遮[惃日+(向口+人)]致禪　兜隷摩呵膩　伽婆膩　伽多膩　醯利尸利稠帝尸利多柳多柳　那肆醯呵呵呵呵呵　私醯脂底　毘略帝　鳩柳鳩柳　婆羅誓兜吒兜吒肆　婆吒婆吒肆私利私利劫卑　隷劫卑　羅牟隷　訶醯吼令一切凶惡皆悉降伏攝其手足支節莫令得動及至三十三天　憂致者利　修羅婆帝　　婆視羅　婆視羅　婆視羅　婆視羅　波帝　悉波訶阿難汝受是先出神仙名號。",
        "from": "T19n0987_001:0480b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終吉成就志念成就呪術成就。",
        "from": "T19n0987_001:0480b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常修苦行止住山林。",
        "from": "T19n0987_001:0480b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勢力自在威變速疾。",
        "from": "T19n0987_001:0480b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五通如意遊步虛空。",
        "from": "T19n0987_001:0480b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今說其名號。",
        "from": "T19n0987_001:0480b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂名八脫大仙名左大仙。",
        "from": "T19n0987_001:0480b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左天大仙波利大仙。",
        "from": "T19n0987_001:0480b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白施大仙善及大仙。",
        "from": "T19n0987_001:0480b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆住大仙迦葉大仙。",
        "from": "T19n0987_001:0480b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老迦葉大仙皺眉大仙。",
        "from": "T19n0987_001:0480b18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "支山大仙爾樂大仙。",
        "from": "T19n0987_001:0480b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名是大仙名前大仙。",
        "from": "T19n0987_001:0480b19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鹿頭大仙闍羅大仙。",
        "from": "T19n0987_001:0480b19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二熟大仙黑二熟大仙。",
        "from": "T19n0987_001:0480b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "採清大仙忍辱大仙。",
        "from": "T19n0987_001:0480b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名稱大仙名舡大仙。",
        "from": "T19n0987_001:0480b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名鴿大仙名馬大仙。",
        "from": "T19n0987_001:0480b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名雪大仙赤目大仙。",
        "from": "T19n0987_001:0480b22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難事大仙毘若沙耶大仙。",
        "from": "T19n0987_001:0480b22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名鎧大仙名秋大仙。",
        "from": "T19n0987_001:0480b23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太白大仙名歲大仙。",
        "from": "T19n0987_001:0480b23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無諍大仙夜步大仙。",
        "from": "T19n0987_001:0480b24_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名覺大仙縛指大仙。",
        "from": "T19n0987_001:0480b24_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持香大仙一角大仙。",
        "from": "T19n0987_001:0480b25_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仙角大仙揭瞿大仙。",
        "from": "T19n0987_001:0480b25_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "單茶耶那大仙名頭大仙。",
        "from": "T19n0987_001:0480b26_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分界大仙劫畢羅大仙。",
        "from": "T19n0987_001:0480b26_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞿曇大仙赤馬大仙。",
        "from": "T19n0987_001:0480b27_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毛聚大仙無樂大仙。",
        "from": "T19n0987_001:0480b27_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉膩盧大仙。",
        "from": "T19n0987_001:0480b28_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難斯等是先出大仙。",
        "from": "T19n0987_001:0480b28_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造四圍陀所為如意。",
        "from": "T19n0987_001:0480b29_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善行嚴迅有大威勢。",
        "from": "T19n0987_001:0480b29_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所作悉辦斯等亦以此大孔雀王呪經。",
        "from": "T19n0987_001:0480c01_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護某甲令壽百歲得見百秋。",
        "from": "T19n0987_001:0480c01_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0987_001:0480c02_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "訶利訶利訶利　絺利彌利　修利修利醯利醯利　彌利彌利　陀覆陀茶覆　伽波膩　摩陀膩　陀訶膩　伽多膩　波遮膩　波多膩　訶那膩　羅遮羅　遮羅膩　波吒膩　摩訶膩　擔(都甘反)波膩　闍婆膩　悉波訶四天王及諸大鬼神王。",
        "from": "T19n0987_001:0480c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦以孔雀王呪。",
        "from": "T19n0987_001:0480c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擁護某甲令壽百歲得見百秋。",
        "from": "T19n0987_001:0480c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T19n0987_001:0480c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿迦帝　阿迦帝　聞羅膩　聞羅膩　阿羅尼　吼翅吼翅　負翅負翅　訶茶　訶茶　訶茶　訶茶　訶茶　訶茶　訶茶　訶茶　訶茶　訶茶　訶茶　除其怨家陀訶　陀訶　陀訶　陀訶　陀訶　陀訶　陀訶　陀訶　陀訶　陀訶(己道反)　其有向某生惡念者皆悉治之鉢柘　鉢柘　鉢柘　鉢柘　鉢柘　鉢柘　鉢柘　鉢柘　鉢柘　鉢柘　其有向求便者皆悉治之豆豆豆豆豆豆豆豆豆豆　滅其怨惡訶訶訶訶訶訶訶訶訶訶　闍帝　闍帝　闍帝　闍帝　闍帝　闍帝　闍帝　闍帝　闍帝　闍帝　滅其怨家周婁　周婁　周婁　周婁　周婁　周婁　周婁　周婁　周婁　周婁　醯利　醯利　醯利　醯利　醯利　醯利　醯利　醯利　醯利　醯利　彌利　彌利　彌利　彌利　彌利　彌利　彌利　彌利　彌利　彌利　睺漏　睺漏　睺漏　睺漏　睺漏　睺漏　睺漏　睺漏　睺漏　睺漏　脂致　脂致　脂致　脂致　脂致　脂致　脂致　脂致　脂致　致脂　醯翅　彌翅　支翅　魔翅得賢　吉利善賢成一切事無垢淨妙月賢日愛曇備　社曇備　毘蠅　加隷　擁護某甲令壽百歲得見百秋即說呪曰。",
        "from": "T19n0987_001:0480c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解說經下結呪語。",
        "from": "T19n0987_001:0481a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿迦螺　阿魯難律頭　阿尼難檀婁如此沙　譚迦沙　尼遲婁螺　阿尼呵　阿尼呵舟流所屬伊題無肆遲東波頭東波阿題嘻和遲比比那　無無遲　南無薩檀三耶佛陀　南嘻遲者遲絺金摩多牟羅優頭摩　沙摩頭摩　阿利鳩梨題多鳩旗那波梨不受戒嬈人者十天起大水使汝不得止居某甲　呵梨迦　南無薩單三耶三佛陀那嘻履梨比履梨俱陀嘻　南無薩單三耶三佛浮陀　南無浮陀斯　南無檀摩斯　南無僧伽斯　南無阿梨耶婆盧吉知。",
        "from": "T19n0987_001:0481a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尸喜利利夷　菩提薩埵　摩訶薩埵　多陀提[口*哥]利伽羅利伽比利　賓伽羅叉薩復竇吒利立黎闍梨耶梨槃　南無質提悉波訶　吉知尼吉知　繡婁那者電[少/兔]　悉波訶　南無浮陀斯　南無檀摩斯　南無僧伽斯　因地祇薩　復闍比丘　悉波訶佛說呪賊經南無佛　南無法　南無比丘僧　南無諸過去七佛　南無諸佛弟子。",
        "from": "T19n0987_001:0481a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無利閉利鬼神王禮是已說此呪經即從如願。",
        "from": "T19n0987_001:0481a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方有山名揵陀摩訶衍。",
        "from": "T19n0987_001:0481a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有鬼神名利閉利。",
        "from": "T19n0987_001:0481a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居山彼有是四師弟子　安陀尼　閉摩尼　攢呵尼　令賊忘閉尼　令賊急坐　抄多摩尼　令賊急生　立攢呵尼　令賊愚癡　如是烏不羅利陀提摩　閉迦利當搭賊口齒。",
        "from": "T19n0987_001:0481b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至解縷乃解我某甲擁護。",
        "from": "T19n0987_001:0481b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟除凶惡害賊者。",
        "from": "T19n0987_001:0481b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆銷滅令其隱。",
        "from": "T19n0987_001:0481b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初說是呪賊經時。",
        "from": "T19n0987_001:0481b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以二十九日為呵利提耶。",
        "from": "T19n0987_001:0481b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然燈燒膠香為伊怖然。",
        "from": "T19n0987_001:0481b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七燈脫散華。",
        "from": "T19n0987_001:0481b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為鬼子母然七燈。",
        "from": "T19n0987_001:0481b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼子母前讀是七遍。",
        "from": "T19n0987_001:0481b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃吉。",
        "from": "T19n0987_001:0481b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合號利離鬼子王得福德。",
        "from": "T19n0987_001:0481b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為然七燈乃燒香散華。",
        "from": "T19n0987_001:0481b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法華神呪經",
        "from": "T19n0987_001:0481b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安爾曼爾　摩禰摩禰　旨隷旨隸　遮梨弟賒咩賒履多韋擅帝　目帝　目多目多　履沙履阿韋娑履　喪履娑履　叉裔阿阿闍膩擅帝　阿叉裔　賒履陀羅尼　阿盧伽婆簸遮毘叉膩禰毘剃　阿便吼羅禰利剃阿盧哆婆　隷輸陀漚究隷　阿羅隷　婆羅梨　首迦差　阿三摩三履　佛陀吉毘利求帝　達摩波利差帝僧伽涅瞿沙禰　婆舍婆舍輸陀　曼多羅叉曼多羅叉掖多　那婁多那婁多憍舍略　惡叉邏　惡叉冶多冶　阿婆盧　阿摩那　多掖世尊是陀羅尼呪。",
        "from": "T19n0987_001:0481b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六十二恒河沙諸佛所說。",
        "from": "T19n0987_001:0481b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有侵毀是法師者。",
        "from": "T19n0987_001:0481b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為侵毀是諸佛已。",
        "from": "T19n0987_001:0481b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇施所說呪",
        "from": "T19n0987_001:0481b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "座隷　摩呵座隷郁枳目只阿羅隷阿羅婆第涅隷第　涅隷多羅第　伊致抳韋致抳　涅隷搱　涅隷頞婆底世尊是陀羅尼呪。",
        "from": "T19n0987_001:0481b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒河沙諸佛所說。",
        "from": "T19n0987_001:0481c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王所說呪",
        "from": "T19n0987_001:0481c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿利那利　[少/兔]那利　阿那盧那履　拘那履持國天王所說呪阿迦禰伽禰　瞿利乾陀利　旃陀利　摩瞪耆　常求利浮婁沙抳頞底世尊是呪四十二億諸佛所說。",
        "from": "T19n0987_001:0481c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅剎女所說呪",
        "from": "T19n0987_001:0481c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊提履　伊提泯　伊提履　阿提履　伊提履　泯履　泯履　泯履　泯履　泯履　婁醯　婁醯　婁醯　婁醯　多醯　多醯　兜醯　[少/兔]醯　寧上我頭上莫惱亂此法師者。",
        "from": "T19n0987_001:0481c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破七分如阿梨樹枝。",
        "from": "T19n0987_001:0481c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大涅槃經呪。",
        "from": "T19n0987_001:0481c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六天魔王波旬所說呪。",
        "from": "T19n0987_001:0481c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "卓枳　吒吒羅卓枳　盧呵隷摩訶　盧呵利阿羅　遮羅多羅沙呵爾時世尊受其神呪不受供養。",
        "from": "T19n0987_001:0481c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說大金色孔雀王呪經",
        "from": "T19n0987_001:0481c18_0",
        "context_with_next_sentence": false
    }
