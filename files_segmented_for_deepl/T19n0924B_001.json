[
    {
        "source_sentence": "藥師如來念誦儀軌一卷",
        "from": "T19n0924B_001:0030b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺三藏沙門大廣智",
        "from": "T19n0924B_001:0030b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T19n0924B_001:0030b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛一時在維那離音樂樹下。",
        "from": "T19n0924B_001:0030b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與三萬六千比丘人俱。",
        "from": "T19n0924B_001:0030b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及十二神王。",
        "from": "T19n0924B_001:0030b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并諸眷屬天龍八部大神王。",
        "from": "T19n0924B_001:0030b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在如是等大眾會中。",
        "from": "T19n0924B_001:0030b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是法已。",
        "from": "T19n0924B_001:0030b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾皆聞無不歡喜。",
        "from": "T19n0924B_001:0030b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是即名為結願神呪。",
        "from": "T19n0924B_001:0030b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即薄伽梵說呪曰。",
        "from": "T19n0924B_001:0030b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南謨薄伽筏帝鞞殺社寠嚕蔽瑠璃鉢唎婆喝囉闍耶怛他揭多怛姪他唵鞞殺逝鞞殺社三沒揭帝婆婆呵若有受持此真言。",
        "from": "T19n0924B_001:0030b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能拔身中過去生死一切重罪。",
        "from": "T19n0924B_001:0030b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復經歷三塗。",
        "from": "T19n0924B_001:0030b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免離九橫超越眾苦。",
        "from": "T19n0924B_001:0030b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本印真言。",
        "from": "T19n0924B_001:0030b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左右手頭指以下八指。",
        "from": "T19n0924B_001:0030b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反叉入於掌。",
        "from": "T19n0924B_001:0030b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二大指來去呪曰。",
        "from": "T19n0924B_001:0030b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵戰馱祇哩娑婆呵",
        "from": "T19n0924B_001:0030b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結護讚。",
        "from": "T19n0924B_001:0030b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩縛勃馱曩薩縛爾曩(入)迦盧拏怛麼(二合平)迦訶羅(引)覩迷(存念於我)俱嚕吠女哩也(二合)入縛羅(引)怛他誐多麼訶捺耶(成就義)曩謨悉羯羅(歸命頂禮義)藥師如來念誦儀軌一卷",
        "from": "T19n0924B_001:0030b25_0",
        "context_with_next_sentence": false
    }
]