    {
        "source_sentence": "根本說一切有部百一羯磨。",
        "from": "X74n1499_004:0985b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本說一切有部戒經。",
        "from": "X74n1499_004:0985b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本說一切有部苾蒭尼戒經。",
        "from": "X74n1499_004:0985b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本說一切有部毗柰耶尼陀那目得迦攝頌襍事攝頌。",
        "from": "X74n1499_004:0985b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本薩婆多部律攝。",
        "from": "X74n1499_004:0985b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本說一切有部毗柰耶頌(拜觀同上)唐大薦福寺沙門釋義淨譯。",
        "from": "X74n1499_004:0985b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十尊者勝友造。",
        "from": "X74n1499_004:0985b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一尊者毗舍佉造。",
        "from": "X74n1499_004:0985b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請根本說一切有部出家授近圓羯磨儀軌。",
        "from": "X74n1499_004:0985b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及苾蒭習略法(拜觀同上)",
        "from": "X74n1499_004:0985b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二本元帝師苾蒭拔合思巴集。",
        "from": "X74n1499_004:0985b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請戒因緣經(拜觀同上)",
        "from": "X74n1499_004:0985b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姚秦涼州沙門竺佛念譯。",
        "from": "X74n1499_004:0985b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請解脫戒本經(拜觀同上)",
        "from": "X74n1499_004:0985b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出迦葉毗部。",
        "from": "X74n1499_004:0985b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "元魏中天竺婆羅門瞿曇般若流支譯。",
        "from": "X74n1499_004:0985b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請善見毗婆沙律(拜觀同上)蕭齊西域沙門僧伽跋陀羅譯。",
        "from": "X74n1499_004:0985b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請毗尼母經(拜觀同上)",
        "from": "X74n1499_004:0985b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附秦錄。",
        "from": "X74n1499_004:0985b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛阿毗曇經(拜觀同上)",
        "from": "X74n1499_004:0985b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳優禪尼國沙門真諦譯。",
        "from": "X74n1499_004:0985b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請舍利弗問經(拜觀同上)",
        "from": "X74n1499_004:0985c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東晉錄。",
        "from": "X74n1499_004:0985c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名。",
        "from": "X74n1499_004:0985c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請優波離問經(拜觀同上)",
        "from": "X74n1499_004:0985c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉宋中天竺沙門求那跋摩譯。",
        "from": "X74n1499_004:0985c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說目連所問經。",
        "from": "X74n1499_004:0985c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及犯戒眾輕重經(拜觀同上)宋中印土沙門法天譯。",
        "from": "X74n1499_004:0985c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二安世高譯。",
        "from": "X74n1499_004:0985c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二經同。",
        "from": "X74n1499_004:0985c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請迦葉禁戒經(拜觀同上)",
        "from": "X74n1499_004:0985c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劉宋居士沮渠京聲譯。",
        "from": "X74n1499_004:0985c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請律二十二明了論(拜觀同上)正量部佛陀多羅多法師造。",
        "from": "X74n1499_004:0985c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陳優禪尼國沙門真諦譯。",
        "from": "X74n1499_004:0985c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請大比丘三千威儀(拜觀同上)後漢安息國沙門安世高譯。",
        "from": "X74n1499_004:0985c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請沙彌十戒法并威儀。",
        "from": "X74n1499_004:0985c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及沙彌威儀。",
        "from": "X74n1499_004:0985c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說沙彌十戒儀則經(拜觀同上)附東晉錄。",
        "from": "X74n1499_004:0985c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二求那跋摩譯。",
        "from": "X74n1499_004:0985c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三施護譯。",
        "from": "X74n1499_004:0985c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請沙彌尼戒經。",
        "from": "X74n1499_004:0985c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及沙彌尼離戒文(拜觀同上)今在漢錄。",
        "from": "X74n1499_004:0985c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二失譯人名。",
        "from": "X74n1499_004:0985c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說優婆塞五戒相經(拜觀同上)劉宋中天竺沙門求那跋摩譯。",
        "from": "X74n1499_004:0985c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在迦維羅衛國。",
        "from": "X74n1499_004:0985c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨飯王請為優婆塞分別五戒可悔不可悔者。",
        "from": "X74n1499_004:0985c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "X74n1499_004:0985c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不犯成佛道。",
        "from": "X74n1499_004:0985c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "犯者三塗。",
        "from": "X74n1499_004:0985c22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說戒消灾經(拜觀同上)吳月支國優婆塞支謙譯。",
        "from": "X74n1499_004:0985c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持歸戒人。",
        "from": "X74n1499_004:0985c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼神畏避。",
        "from": "X74n1499_004:0985c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二人受五戒。",
        "from": "X74n1499_004:0986a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同四百九十八人見佛。",
        "from": "X74n1499_004:0986a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆證果。",
        "from": "X74n1499_004:0986a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說大愛道比丘尼經(拜觀同上)北涼錄。",
        "from": "X74n1499_004:0986a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名。",
        "from": "X74n1499_004:0986a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說苾蒭五法經(拜觀同上)宋中印土沙門法天譯。",
        "from": "X74n1499_004:0986a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說五法得離依止等。",
        "from": "X74n1499_004:0986a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及說戒七種差別。",
        "from": "X74n1499_004:0986a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說比丘迦尸迦十法經(拜觀同上)法天譯。",
        "from": "X74n1499_004:0986a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足慚愧多聞等十法。",
        "from": "X74n1499_004:0986a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得為大師。",
        "from": "X74n1499_004:0986a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說五恐怖世經(拜觀同上)劉宋居士沮渠京聲譯。",
        "from": "X74n1499_004:0986a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說末世不遵戒律說恐怖事。",
        "from": "X74n1499_004:0986a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說齋經(拜觀同上)",
        "from": "X74n1499_004:0986a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳月支國優婆塞支謙譯。",
        "from": "X74n1499_004:0986a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說六齋日受八支五念法。",
        "from": "X74n1499_004:0986a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心奉請佛說目連問戒律中五百輕重事經(拜觀同上)西晉錄。",
        "from": "X74n1499_004:0986a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名。",
        "from": "X74n1499_004:0986a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首品與犯戒罪輕重及目連所問經同。",
        "from": "X74n1499_004:0986a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘與諸律論不盡合。",
        "from": "X74n1499_004:0986a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以上小乘律。",
        "from": "X74n1499_004:0986a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水陸道場法輪寶懺卷第四",
        "from": "X74n1499_004:0986a20_0",
        "context_with_next_sentence": false
    }
]