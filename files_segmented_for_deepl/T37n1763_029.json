[
    {
        "source_sentence": "大般涅槃經集解卷第二十九",
        "from": "T37n1763_029:0480c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　出功德天黑闇女譬　釋死苦",
        "from": "T37n1763_029:0480c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖行品之第三",
        "from": "T37n1763_029:0480c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉生之根本凡有如是(至)觀於所生已見過患。",
        "from": "T37n1763_029:0480c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0480c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0480c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以凡夫顛倒貪生。",
        "from": "T37n1763_029:0480c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而惡老死。",
        "from": "T37n1763_029:0480c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明菩薩不爾也。",
        "from": "T37n1763_029:0480c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_029:0480c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三寄觀行門。",
        "from": "T37n1763_029:0480c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明八苦相也。",
        "from": "T37n1763_029:0480c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中有六譬。",
        "from": "T37n1763_029:0480c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆明凡夫貪生。",
        "from": "T37n1763_029:0480c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而菩薩見過。",
        "from": "T37n1763_029:0480c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "棄而不樂也。",
        "from": "T37n1763_029:0480c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_029:0480c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下第二觀過也。",
        "from": "T37n1763_029:0480c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有六譬也。",
        "from": "T37n1763_029:0480c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉如有女人入於他舍(至)散華供養恭敬禮拜。",
        "from": "T37n1763_029:0480c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0480c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0480c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為菩薩作譬也。",
        "from": "T37n1763_029:0480c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩愛生情薄。",
        "from": "T37n1763_029:0480c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見生死過。",
        "from": "T37n1763_029:0480c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則俱厭也。",
        "from": "T37n1763_029:0480c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生是貪悅之本。",
        "from": "T37n1763_029:0480c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬女。",
        "from": "T37n1763_029:0480c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五陰是生所住處。",
        "from": "T37n1763_029:0480c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬舍。",
        "from": "T37n1763_029:0480c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘因得果。",
        "from": "T37n1763_029:0480c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬之他也。",
        "from": "T37n1763_029:0480c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生理可樂。",
        "from": "T37n1763_029:0480c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬之端正。",
        "from": "T37n1763_029:0480c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智以御身。",
        "from": "T37n1763_029:0480c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬之舍主。",
        "from": "T37n1763_029:0480c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名以表實。",
        "from": "T37n1763_029:0480c17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬問名也。",
        "from": "T37n1763_029:0480c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生是善果。",
        "from": "T37n1763_029:0480c17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其因淨故。",
        "from": "T37n1763_029:0480c17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬功德也。",
        "from": "T37n1763_029:0480c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果非不淨譬也。",
        "from": "T37n1763_029:0480c18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "考名責實。",
        "from": "T37n1763_029:0480c18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問所作也。",
        "from": "T37n1763_029:0480c18_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有生有實。",
        "from": "T37n1763_029:0480c19_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬能與也。",
        "from": "T37n1763_029:0480c19_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於門外更見一女(至)汝若不去當斷汝命。",
        "from": "T37n1763_029:0480c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0480c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0480c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨生名死。",
        "from": "T37n1763_029:0480c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬門外也。",
        "from": "T37n1763_029:0480c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "反生易顯。",
        "from": "T37n1763_029:0480c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不須辭費也。",
        "from": "T37n1763_029:0480c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即持利刀者。",
        "from": "T37n1763_029:0480c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醫藥救生。",
        "from": "T37n1763_029:0480c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬持利刀。",
        "from": "T37n1763_029:0480c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以防死也。",
        "from": "T37n1763_029:0480c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_029:0480c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死是人之所惡。",
        "from": "T37n1763_029:0480c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悲情內故。",
        "from": "T37n1763_029:0480c23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬之門外也。",
        "from": "T37n1763_029:0480c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女人答言汝甚愚癡(至)我皆不用各隨意去。",
        "from": "T37n1763_029:0480c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0480c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智秀曰。",
        "from": "T37n1763_029:0480c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生在報前。",
        "from": "T37n1763_029:0480c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬姊。",
        "from": "T37n1763_029:0480c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死居報後。",
        "from": "T37n1763_029:0480c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬之妹也。",
        "from": "T37n1763_029:0480c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時二女便共相將(至)是故俱請令住我家。",
        "from": "T37n1763_029:0480c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0480c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_029:0480c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫也。",
        "from": "T37n1763_029:0480c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無法財。",
        "from": "T37n1763_029:0480c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故喻貧也。",
        "from": "T37n1763_029:0480c29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉菩薩摩訶薩亦復如是(至)是故貪愛生死二法。",
        "from": "T37n1763_029:0481a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智秀曰。",
        "from": "T37n1763_029:0481a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雙合譬也。",
        "from": "T37n1763_029:0481a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_029:0481a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第一以二女譬凡夫。",
        "from": "T37n1763_029:0481a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與菩薩相對也。",
        "from": "T37n1763_029:0481a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉如婆羅門(至)如彼童子取菓還棄。",
        "from": "T37n1763_029:0481a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0481a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幼稚童子。",
        "from": "T37n1763_029:0481a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬始業菩薩見苦不深。",
        "from": "T37n1763_029:0481a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未全忘生。",
        "from": "T37n1763_029:0481a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如取菓已捨。",
        "from": "T37n1763_029:0481a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若智行俱深。",
        "from": "T37n1763_029:0481a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則不取不捨也。",
        "from": "T37n1763_029:0481a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_029:0481a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩是高行之族。",
        "from": "T37n1763_029:0481a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以譬婆羅門子。",
        "from": "T37n1763_029:0481a07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曠劫為地獄所苦。",
        "from": "T37n1763_029:0481a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絕人天食也。",
        "from": "T37n1763_029:0481a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "糞譬陰身。",
        "from": "T37n1763_029:0481a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菓譬生也。",
        "from": "T37n1763_029:0481a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欣生之情。",
        "from": "T37n1763_029:0481a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬欲取也。",
        "from": "T37n1763_029:0481a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生之為過。",
        "from": "T37n1763_029:0481a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖之所說。",
        "from": "T37n1763_029:0481a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬智訶也。",
        "from": "T37n1763_029:0481a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知貪生之患。",
        "from": "T37n1763_029:0481a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬赧然有愧也。",
        "from": "T37n1763_029:0481a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為生天中有五欲樂。",
        "from": "T37n1763_029:0481a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬淨洗也。",
        "from": "T37n1763_029:0481a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不愛者。",
        "from": "T37n1763_029:0481a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬還棄也。",
        "from": "T37n1763_029:0481a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_029:0481a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第二以童子譬小菩薩。",
        "from": "T37n1763_029:0481a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與大菩薩對也。",
        "from": "T37n1763_029:0481a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉譬如有人(至)以其不見老病死故。",
        "from": "T37n1763_029:0481a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0481a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四衢道。",
        "from": "T37n1763_029:0481a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬佛四無量無齊限也。",
        "from": "T37n1763_029:0481a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器譬經藏。",
        "from": "T37n1763_029:0481a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食譬施戒定。",
        "from": "T37n1763_029:0481a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛為在家之人。",
        "from": "T37n1763_029:0481a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初讚施戒。",
        "from": "T37n1763_029:0481a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生死之樂。",
        "from": "T37n1763_029:0481a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後說無常。",
        "from": "T37n1763_029:0481a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚涅槃樂。",
        "from": "T37n1763_029:0481a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法始終。",
        "from": "T37n1763_029:0481a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆佛說。",
        "from": "T37n1763_029:0481a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫貪生。",
        "from": "T37n1763_029:0481a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見後過。",
        "from": "T37n1763_029:0481a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人見過。",
        "from": "T37n1763_029:0481a18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生貪也。",
        "from": "T37n1763_029:0481a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又四衢譬四辨。",
        "from": "T37n1763_029:0481a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "器譬名味句身。",
        "from": "T37n1763_029:0481a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食譬施戒定也。",
        "from": "T37n1763_029:0481a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "賣之譬勸行也。",
        "from": "T37n1763_029:0481a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠來飢虛。",
        "from": "T37n1763_029:0481a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬經三途久也。",
        "from": "T37n1763_029:0481a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是何物者。",
        "from": "T37n1763_029:0481a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱其所求。",
        "from": "T37n1763_029:0481a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問以定之。",
        "from": "T37n1763_029:0481a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食主答言者。",
        "from": "T37n1763_029:0481a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒定能得天人之樂。",
        "from": "T37n1763_029:0481a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後必無常也。",
        "from": "T37n1763_029:0481a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人聞已者。",
        "from": "T37n1763_029:0481a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為菩薩說也。",
        "from": "T37n1763_029:0481a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多與我價者。",
        "from": "T37n1763_029:0481a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生天不受四趣之苦也。",
        "from": "T37n1763_029:0481a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_029:0481a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說法聚人之處。",
        "from": "T37n1763_029:0481a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬四衢也。",
        "from": "T37n1763_029:0481a25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化主說人天因果。",
        "from": "T37n1763_029:0481a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取人利養如賣也。",
        "from": "T37n1763_029:0481a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩雖知天中報妙。",
        "from": "T37n1763_029:0481a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見有死患。",
        "from": "T37n1763_029:0481a26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不欲生故。",
        "from": "T37n1763_029:0481a26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻如彼人雖欲買食。",
        "from": "T37n1763_029:0481a27_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一惡故。",
        "from": "T37n1763_029:0481a27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以遂捨也。",
        "from": "T37n1763_029:0481a27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_029:0481a28_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第三以賣食譬釋教也。",
        "from": "T37n1763_029:0481a28_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若生死之果如此者。",
        "from": "T37n1763_029:0481a28_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖人何意。",
        "from": "T37n1763_029:0481a29_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此五戒十善。",
        "from": "T37n1763_029:0481a29_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受人天報耶。",
        "from": "T37n1763_029:0481b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是善不受義之誠證也。",
        "from": "T37n1763_029:0481b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此教本接下愚。",
        "from": "T37n1763_029:0481b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若不聞人天之樂。",
        "from": "T37n1763_029:0481b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫豈背行道耶。",
        "from": "T37n1763_029:0481b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉譬如毒樹(至)下至十歲俱亦受苦。",
        "from": "T37n1763_029:0481b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0481b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二譬同顯有生有死。",
        "from": "T37n1763_029:0481b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脩短無異耳。",
        "from": "T37n1763_029:0481b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_029:0481b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四第五二譬。",
        "from": "T37n1763_029:0481b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偏據生死果報。",
        "from": "T37n1763_029:0481b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯苦而無樂。",
        "from": "T37n1763_029:0481b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉如譬嶮岸上有草(至)大涅槃經觀於生苦。",
        "from": "T37n1763_029:0481b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0481b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬菩薩尚不貪天上報。",
        "from": "T37n1763_029:0481b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況復欣此人中果耶。",
        "from": "T37n1763_029:0481b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_029:0481b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "險岸譬五陰身也。",
        "from": "T37n1763_029:0481b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "草覆譬無明也。",
        "from": "T37n1763_029:0481b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "岸邊甘露。",
        "from": "T37n1763_029:0481b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬妙藥報也。",
        "from": "T37n1763_029:0481b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大坑譬死苦也。",
        "from": "T37n1763_029:0481b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便取者。",
        "from": "T37n1763_029:0481b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪樂果故。",
        "from": "T37n1763_029:0481b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脩善因也。",
        "from": "T37n1763_029:0481b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脚跌者。",
        "from": "T37n1763_029:0481b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永淪生死也。",
        "from": "T37n1763_029:0481b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_029:0481b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為外道作譬也。",
        "from": "T37n1763_029:0481b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "險岸譬非想也。",
        "from": "T37n1763_029:0481b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "草覆譬彼生死果報。",
        "from": "T37n1763_029:0481b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而外道不知計為涅槃也。",
        "from": "T37n1763_029:0481b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甘露者。",
        "from": "T37n1763_029:0481b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬禪定樂也。",
        "from": "T37n1763_029:0481b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽千年者。",
        "from": "T37n1763_029:0481b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八萬劫也。",
        "from": "T37n1763_029:0481b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貪其味者。",
        "from": "T37n1763_029:0481b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著禪定也。",
        "from": "T37n1763_029:0481b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深坑者。",
        "from": "T37n1763_029:0481b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生死苦也。",
        "from": "T37n1763_029:0481b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脚跌者。",
        "from": "T37n1763_029:0481b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外道以非想為涅槃。",
        "from": "T37n1763_029:0481b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失理故墮三塗也。",
        "from": "T37n1763_029:0481b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉云何菩薩摩訶薩(至)大涅槃經觀於老苦。",
        "from": "T37n1763_029:0481b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0481b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆與衰壞作譬也。",
        "from": "T37n1763_029:0481b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王譬壯色。",
        "from": "T37n1763_029:0481b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敵國譬老。",
        "from": "T37n1763_029:0481b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他土人民以譬憶念少盛之事也。",
        "from": "T37n1763_029:0481b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉云何菩薩摩訶薩(至)大涅槃經觀於病苦。",
        "from": "T37n1763_029:0481b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0481b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端正譬健色。",
        "from": "T37n1763_029:0481b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫人譬愛結。",
        "from": "T37n1763_029:0481b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王譬病也。",
        "from": "T37n1763_029:0481b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "挑一目等。",
        "from": "T37n1763_029:0481b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬壞盛色。",
        "from": "T37n1763_029:0481b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉云何菩薩摩訶薩(至)住於大乘大涅槃呪。",
        "from": "T37n1763_029:0481b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0481b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除二禪者。",
        "from": "T37n1763_029:0481b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡死苦一至。",
        "from": "T37n1763_029:0481b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無不惱亂。",
        "from": "T37n1763_029:0481b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故以災為譬。",
        "from": "T37n1763_029:0481b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩死至。",
        "from": "T37n1763_029:0481b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不亂也。",
        "from": "T37n1763_029:0481b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩羅毒蛇其色黑。",
        "from": "T37n1763_029:0481b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若觸人衣。",
        "from": "T37n1763_029:0481b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒人衣觸其行處。",
        "from": "T37n1763_029:0481b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆死。",
        "from": "T37n1763_029:0481b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故名觸毒。",
        "from": "T37n1763_029:0481b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿竭多星。",
        "from": "T37n1763_029:0481b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八月出也。",
        "from": "T37n1763_029:0481b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因星結呪。",
        "from": "T37n1763_029:0481b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消四毒也。",
        "from": "T37n1763_029:0481c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_029:0481c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "病之為苦。",
        "from": "T37n1763_029:0481c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破壞浮囊。",
        "from": "T37n1763_029:0481c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實為最苦也。",
        "from": "T37n1763_029:0481c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次迦葉譬如有人(至)大涅槃經觀於死苦。",
        "from": "T37n1763_029:0481c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_029:0481c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_029:0481c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來說死。",
        "from": "T37n1763_029:0481c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而未說苦。",
        "from": "T37n1763_029:0481c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今言於險難處。",
        "from": "T37n1763_029:0481c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有資糧者。",
        "from": "T37n1763_029:0481c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸受根滅。",
        "from": "T37n1763_029:0481c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受根起。",
        "from": "T37n1763_029:0481c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰死也。",
        "from": "T37n1763_029:0481c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受根起。",
        "from": "T37n1763_029:0481c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則生塗斷絕。",
        "from": "T37n1763_029:0481c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬險難也。",
        "from": "T37n1763_029:0481c06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受根不起。",
        "from": "T37n1763_029:0481c06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起則無法。",
        "from": "T37n1763_029:0481c07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無法則不可資用。",
        "from": "T37n1763_029:0481c07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬無資糧也。",
        "from": "T37n1763_029:0481c07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來塗茫茫。",
        "from": "T37n1763_029:0481c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫知所之。",
        "from": "T37n1763_029:0481c08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬懸遠也。",
        "from": "T37n1763_029:0481c08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五根皆滅。",
        "from": "T37n1763_029:0481c08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意識獨行。",
        "from": "T37n1763_029:0481c09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬無伴侶。",
        "from": "T37n1763_029:0481c09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業風所飄。",
        "from": "T37n1763_029:0481c09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求生不住。",
        "from": "T37n1763_029:0481c09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬常行也。",
        "from": "T37n1763_029:0481c10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五道未分。",
        "from": "T37n1763_029:0481c10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬不知邊際也。",
        "from": "T37n1763_029:0481c10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇得生緣。",
        "from": "T37n1763_029:0481c10_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘癡受生。",
        "from": "T37n1763_029:0481c11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬無燈明也。",
        "from": "T37n1763_029:0481c11_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上來諸譬。",
        "from": "T37n1763_029:0481c11_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說欲受生之苦。",
        "from": "T37n1763_029:0481c11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自此下譬死之苦也。",
        "from": "T37n1763_029:0481c12_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說一切苦。",
        "from": "T37n1763_029:0481c12_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆由受生。",
        "from": "T37n1763_029:0481c12_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心滅名死。",
        "from": "T37n1763_029:0481c13_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅不由戶。",
        "from": "T37n1763_029:0481c13_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬無門也。",
        "from": "T37n1763_029:0481c13_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不受根起。",
        "from": "T37n1763_029:0481c13_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬有處所也。",
        "from": "T37n1763_029:0481c14_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不覺苦樂。",
        "from": "T37n1763_029:0481c14_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬無痛處。",
        "from": "T37n1763_029:0481c14_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醫藥不加。",
        "from": "T37n1763_029:0481c14_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬不可治也。",
        "from": "T37n1763_029:0481c15_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸根漸滅。",
        "from": "T37n1763_029:0481c15_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "強力所不制。",
        "from": "T37n1763_029:0481c15_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬無遮止也。",
        "from": "T37n1763_029:0481c15_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受根已滅。",
        "from": "T37n1763_029:0481c16_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可更生。",
        "from": "T37n1763_029:0481c16_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬不得脫也。",
        "from": "T37n1763_029:0481c16_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生滅根異。",
        "from": "T37n1763_029:0481c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外相不改。",
        "from": "T37n1763_029:0481c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬無破壞也。",
        "from": "T37n1763_029:0481c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人不喜見。",
        "from": "T37n1763_029:0481c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬愁毒也。",
        "from": "T37n1763_029:0481c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不能害人。",
        "from": "T37n1763_029:0481c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬非惡色也。",
        "from": "T37n1763_029:0481c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人懼其禍。",
        "from": "T37n1763_029:0481c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬怖畏也。",
        "from": "T37n1763_029:0481c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是根皆滅。",
        "from": "T37n1763_029:0481c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬在身也。",
        "from": "T37n1763_029:0481c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅不可見。",
        "from": "T37n1763_029:0481c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬不可覺知也。",
        "from": "T37n1763_029:0481c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大般涅槃經集解卷第二十九",
        "from": "T37n1763_029:0481c21_0",
        "context_with_next_sentence": false
    }
]