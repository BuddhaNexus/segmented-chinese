[
    {
        "source_sentence": "佛說如來成道經",
        "from": "T85n2890_001:1405a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脚踏千福輪行身披□□□□□□□□之瓶。",
        "from": "T85n2890_001:1405a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "胸前萬字了了分明。",
        "from": "T85n2890_001:1405a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背上圓光晃照暉盈。",
        "from": "T85n2890_001:1405a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子王頰毛蟸旋生。",
        "from": "T85n2890_001:1405a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出廣長舌。",
        "from": "T85n2890_001:1405a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍覆幽冥。",
        "from": "T85n2890_001:1405a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照曜無極。",
        "from": "T85n2890_001:1405a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝日之明。",
        "from": "T85n2890_001:1405a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見佛光者。",
        "from": "T85n2890_001:1405a28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永不死生如來。",
        "from": "T85n2890_001:1405a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "槃而不死。",
        "from": "T85n2890_001:1405a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "涅而不生。",
        "from": "T85n2890_001:1405a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攪之不濁。",
        "from": "T85n2890_001:1405a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "澄之不清。",
        "from": "T85n2890_001:1405a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擔復不重。",
        "from": "T85n2890_001:1405a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "點復不輕。",
        "from": "T85n2890_001:1405b01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幽復不闇。",
        "from": "T85n2890_001:1405b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯復不明。",
        "from": "T85n2890_001:1405b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名即不惜。",
        "from": "T85n2890_001:1405b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利即不爭。",
        "from": "T85n2890_001:1405b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辱之不忿。",
        "from": "T85n2890_001:1405b02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寵之不榮。",
        "from": "T85n2890_001:1405b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散復不壞。",
        "from": "T85n2890_001:1405b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聚復不并。",
        "from": "T85n2890_001:1405b03_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高而不危。",
        "from": "T85n2890_001:1405b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下而平。",
        "from": "T85n2890_001:1405b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夾而無伴。",
        "from": "T85n2890_001:1405b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣而異成。",
        "from": "T85n2890_001:1405b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殺即不死。",
        "from": "T85n2890_001:1405b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "活即不生。",
        "from": "T85n2890_001:1405b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白髮非老。",
        "from": "T85n2890_001:1405b04_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少復非嬰。",
        "from": "T85n2890_001:1405b05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "視之不見其體。",
        "from": "T85n2890_001:1405b05_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽之不聞其聲。",
        "from": "T85n2890_001:1405b05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大身彌輪八極畟塞空庭。",
        "from": "T85n2890_001:1405b06_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小則針穴裏走馬。",
        "from": "T85n2890_001:1405b06_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵裏藏形噀。",
        "from": "T85n2890_001:1405b07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海變成蘇酪。",
        "from": "T85n2890_001:1405b07_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "指地瑠璃水精。",
        "from": "T85n2890_001:1405b07_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捻山即知斤兩豁。",
        "from": "T85n2890_001:1405b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海總作空坑。",
        "from": "T85n2890_001:1405b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微塵算得其數。",
        "from": "T85n2890_001:1405b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心知一切眾生。",
        "from": "T85n2890_001:1405b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天宮樓閣指即化城。",
        "from": "T85n2890_001:1405b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所須皆得不用功呈。",
        "from": "T85n2890_001:1405b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行即蕩蕩而無礙。",
        "from": "T85n2890_001:1405b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住則湛湛而不傾。",
        "from": "T85n2890_001:1405b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掛肉眼觀三世之事。",
        "from": "T85n2890_001:1405b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用五眼常同如盲居。",
        "from": "T85n2890_001:1405b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無非非意處無是是之情。",
        "from": "T85n2890_001:1405b12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超三乘之境。",
        "from": "T85n2890_001:1405b12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "外越六度之眾生。",
        "from": "T85n2890_001:1405b13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "振叫威烈。",
        "from": "T85n2890_001:1405b13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不使怕驚。",
        "from": "T85n2890_001:1405b13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攬即不著。",
        "from": "T85n2890_001:1405b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看即有形。",
        "from": "T85n2890_001:1405b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無端出沒。",
        "from": "T85n2890_001:1405b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運轉自寧。",
        "from": "T85n2890_001:1405b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞼眼即萬里。",
        "from": "T85n2890_001:1405b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開眼即停。",
        "from": "T85n2890_001:1405b15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即非相可相。",
        "from": "T85n2890_001:1405b15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非名可名。",
        "from": "T85n2890_001:1405b15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒而不灰。",
        "from": "T85n2890_001:1405b16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溺而不泥。",
        "from": "T85n2890_001:1405b16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臺即不起。",
        "from": "T85n2890_001:1405b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押亦不摧。",
        "from": "T85n2890_001:1405b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拾得物不歡喜。",
        "from": "T85n2890_001:1405b17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失物却不悲啼。",
        "from": "T85n2890_001:1405b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大眾不樂樂。",
        "from": "T85n2890_001:1405b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨自不拪拪。",
        "from": "T85n2890_001:1405b18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將刀斫不恨恨。",
        "from": "T85n2890_001:1405b18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "附藥療不治治。",
        "from": "T85n2890_001:1405b18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二心俱一種平等。",
        "from": "T85n2890_001:1405b19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閦然齊分。",
        "from": "T85n2890_001:1405b19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身百億處處赴齊。",
        "from": "T85n2890_001:1405b19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名迷達。",
        "from": "T85n2890_001:1405b20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二名如來。",
        "from": "T85n2890_001:1405b20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當讚之時地動天雷。",
        "from": "T85n2890_001:1405b20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切徒眾各各所歸。",
        "from": "T85n2890_001:1405b21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有信此天必覆之。",
        "from": "T85n2890_001:1405b21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若當不信寒凍無衣。",
        "from": "T85n2890_001:1405b22_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若寫一卷家富豐財。",
        "from": "T85n2890_001:1405b22_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若寫兩卷聰明智開。",
        "from": "T85n2890_001:1405b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教人讀誦橫病不來。",
        "from": "T85n2890_001:1405b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空中讚曰。",
        "from": "T85n2890_001:1405b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思議。",
        "from": "T85n2890_001:1405b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說如來成道經",
        "from": "T85n2890_001:1405b25_0",
        "context_with_next_sentence": false
    }
]