[
    {
        "source_sentence": "佛說辟除賊害呪經",
        "from": "T21n1406_001:0922a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛南無法南無比丘僧。",
        "from": "T21n1406_001:0922a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無過去七佛。",
        "from": "T21n1406_001:0922a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無諸佛南無諸佛弟子。",
        "from": "T21n1406_001:0922a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無諸師南無諸師弟子。",
        "from": "T21n1406_001:0922a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無默利薛利鬼神王。",
        "from": "T21n1406_001:0922a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮是已便說是呪。",
        "from": "T21n1406_001:0922a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我所呪皆從如願。",
        "from": "T21n1406_001:0922a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "北方有山名健陀摩訶術。",
        "from": "T21n1406_001:0922a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有鬼神王名默利陰利居止彼。",
        "from": "T21n1406_001:0922a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四姊弟。",
        "from": "T21n1406_001:0922a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為四。",
        "from": "T21n1406_001:0922a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安檀尼　閻摩尼　瘞摩尼　無呵尼　安檀尼　令賊目盲　無訶尼　令賊住　瘞摩尼　令賊坐　無呵尼　令賊愚癡　癡如是　漚羅利　無羅利　壇坻遮　波頭摩　遮迦利　當使賊　口齒噤　至解縷　乃得脫說如是呪已。",
        "from": "T21n1406_001:0922a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便言我為某甲若干人等。",
        "from": "T21n1406_001:0922a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作擁護辟邪害皆令得安隱。",
        "from": "T21n1406_001:0922a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始諷誦是經時。",
        "from": "T21n1406_001:0922a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用月二十九日。",
        "from": "T21n1406_001:0922a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛前然七燈燒。",
        "from": "T21n1406_001:0922a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "膠香散花說。",
        "from": "T21n1406_001:0922a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是呪七過并。",
        "from": "T21n1406_001:0922a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪願默利羅鬼神王使得福德亦。",
        "from": "T21n1406_001:0922a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為然燈燒香散花復為鬼子母然七燈燒香散花亦。",
        "from": "T21n1406_001:0922a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當說是經七過後。",
        "from": "T21n1406_001:0922a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪乃告當如是語即。",
        "from": "T21n1406_001:0922a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從如願。",
        "from": "T21n1406_001:0922a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說辟除賊害呪經",
        "from": "T21n1406_001:0922a28_0",
        "context_with_next_sentence": false
    }
]