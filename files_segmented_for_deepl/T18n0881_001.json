    {
        "source_sentence": "　十六為上首　　彌勒持軍持",
        "from": "T18n0881_001:0339a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不空蓮華眼　　除憂梵夾相",
        "from": "T18n0881_001:0339a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　除惡三股杵　　香象執蓮華",
        "from": "T18n0881_001:0339a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大精進鏘戟　　虛空藏寶光",
        "from": "T18n0881_001:0339a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智幢幢幖幟　　無量光蓮華",
        "from": "T18n0881_001:0339a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　賢護持寶瓶　　綱明綱傘蓋",
        "from": "T18n0881_001:0339a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　月光半月幢　　無量意梵夾",
        "from": "T18n0881_001:0339b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　辨積持華雲　　金剛藏獨股",
        "from": "T18n0881_001:0339b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　普賢五智印　　種子初阿字",
        "from": "T18n0881_001:0339b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　吽字左按地",
        "from": "T18n0881_001:0339b04_0",
        "context_with_next_sentence": true
    },
    {
