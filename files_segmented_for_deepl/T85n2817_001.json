[
    {
        "source_sentence": "大乘經纂要義一卷",
        "from": "T85n2817_001:1183a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　敬禮無上一切佛　　證於三身天人尊　教法甚深難思議　　無為解脫僧和合　是真歸依於三寶　　諸佛出世如憂曇　如悔盲龜遇浮木　　人身難得甚希有　從茲一失復難期　　於此身中終漸進　永常棄捨於八難　　依教修行加精進　善惡之業亦不亡　　如因值種果亦然　所以遠離於十惡　　精勤修習於十善　須善分別其二諦　　常捨輪迴求解脫　勤行正道增智慧　　願修聖跡證無為如是一切眾生先須歸依三寶敬事遵仰。",
        "from": "T85n2817_001:1183a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三寶者何。",
        "from": "T85n2817_001:1183a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂佛法僧寶。",
        "from": "T85n2817_001:1183a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言佛寶者。",
        "from": "T85n2817_001:1183a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂法體報化之身具足三種名為佛寶。",
        "from": "T85n2817_001:1183a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言法身者。",
        "from": "T85n2817_001:1183a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具不思議無分別智。",
        "from": "T85n2817_001:1183a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切法自在觀察。",
        "from": "T85n2817_001:1183a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言報身者。",
        "from": "T85n2817_001:1183a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是不思議功德成就。",
        "from": "T85n2817_001:1183a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超過人天快樂。",
        "from": "T85n2817_001:1183a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足相好殊勝離於生死。",
        "from": "T85n2817_001:1183a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證得究竟盧舍那身。",
        "from": "T85n2817_001:1183a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言化身者。",
        "from": "T85n2817_001:1183a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就慈悲誓願究竟。",
        "from": "T85n2817_001:1183a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡於生死度脫眾生。",
        "from": "T85n2817_001:1183a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨機引道永離眾苦。",
        "from": "T85n2817_001:1183a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入於母胎誕身。",
        "from": "T85n2817_001:1183a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家降伏魔怨。",
        "from": "T85n2817_001:1183a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉正法輪。",
        "from": "T85n2817_001:1183a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "示涅槃相。",
        "from": "T85n2817_001:1183a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足德者。",
        "from": "T85n2817_001:1183a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超過人天時乃現耳。",
        "from": "T85n2817_001:1183a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故難遇。",
        "from": "T85n2817_001:1183a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世無喻者。",
        "from": "T85n2817_001:1183a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以假世間寶顯其法義。",
        "from": "T85n2817_001:1183a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生獲無上果。",
        "from": "T85n2817_001:1183a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離生老輪迴之苦。",
        "from": "T85n2817_001:1183a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少分歸依即蒙加護。",
        "from": "T85n2817_001:1183a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡鬼神不能侵擾。",
        "from": "T85n2817_001:1183a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具斯五義。",
        "from": "T85n2817_001:1183a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲如意寶於世出世為人天師。",
        "from": "T85n2817_001:1183a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩足中尊真實依故。",
        "from": "T85n2817_001:1183a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言法寶者。",
        "from": "T85n2817_001:1183a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有教法於真俗諦說十二分。",
        "from": "T85n2817_001:1183a29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修八聖道悟真體性。",
        "from": "T85n2817_001:1183b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本來寂靜無住涅槃。",
        "from": "T85n2817_001:1183b01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "號名三寶。",
        "from": "T85n2817_001:1183b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以佛未出世不能顯其祕要之法。",
        "from": "T85n2817_001:1183b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來出世甚為希有。",
        "from": "T85n2817_001:1183b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所說教法亦復如是。",
        "from": "T85n2817_001:1183b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非餘外道能顯其義。",
        "from": "T85n2817_001:1183b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法所不能及。",
        "from": "T85n2817_001:1183b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以斯法寶利樂之道與佛無異。",
        "from": "T85n2817_001:1183b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名法寶。",
        "from": "T85n2817_001:1183b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言僧者。",
        "from": "T85n2817_001:1183b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大菩薩眾不著生死。",
        "from": "T85n2817_001:1183b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不住涅槃。",
        "from": "T85n2817_001:1183b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自利利他具足成就。",
        "from": "T85n2817_001:1183b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸羅漢八上人等住解脫地。",
        "from": "T85n2817_001:1183b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫比丘各各信受樂於聖位出家學道。",
        "from": "T85n2817_001:1183b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別別解脫。",
        "from": "T85n2817_001:1183b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依教修行。",
        "from": "T85n2817_001:1183b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖未證果而能修行軌範。",
        "from": "T85n2817_001:1183b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聖法佛不出世亦無僧寶。",
        "from": "T85n2817_001:1183b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等同如來生希有相。",
        "from": "T85n2817_001:1183b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切無上智德具足。",
        "from": "T85n2817_001:1183b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依聞思修善漸微妙。",
        "from": "T85n2817_001:1183b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間諸仙具聖惠者。",
        "from": "T85n2817_001:1183b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修安樂地。",
        "from": "T85n2817_001:1183b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是造作僧寶為勝。",
        "from": "T85n2817_001:1183b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上三寶諸功德力有大慈悲。",
        "from": "T85n2817_001:1183b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最尊最勝不思議故。",
        "from": "T85n2817_001:1183b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能拔眾生現在苦難。",
        "from": "T85n2817_001:1183b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離生死長劫輪迴。",
        "from": "T85n2817_001:1183b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能加護得解脫樂。",
        "from": "T85n2817_001:1183b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當供養無上三寶。",
        "from": "T85n2817_001:1183b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須恭敬作禮歸依能生一切福德之芽。",
        "from": "T85n2817_001:1183b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離一切苦惱之事。",
        "from": "T85n2817_001:1183b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛如來具是功德。",
        "from": "T85n2817_001:1183b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出興於世甚為希有。",
        "from": "T85n2817_001:1183b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸群生同種善根。",
        "from": "T85n2817_001:1183b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現於世如憂曇花。",
        "from": "T85n2817_001:1183b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛出世傳甚深法亦復如是。",
        "from": "T85n2817_001:1183b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛出於世雖有深法遇人身者要須具足善業成就。",
        "from": "T85n2817_001:1183b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其難亦爾。",
        "from": "T85n2817_001:1183b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如大海於海底中有一盲龜。",
        "from": "T85n2817_001:1183b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽億萬劫百年。",
        "from": "T85n2817_001:1183b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一度昇於水上。",
        "from": "T85n2817_001:1183b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或時有人棄一竅木置於海中。",
        "from": "T85n2817_001:1183b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其龜出水以頭遇斯浮木之孔甚為希有。",
        "from": "T85n2817_001:1183b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或得人身難亦如是。",
        "from": "T85n2817_001:1183b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖得人身六根具者得聞正法身心自在。",
        "from": "T85n2817_001:1183b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依教修行最甚難有。",
        "from": "T85n2817_001:1183b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生之界不可思議。",
        "from": "T85n2817_001:1183b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其中獲得人身六根具足自在受生極最難故。",
        "from": "T85n2817_001:1183b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以如來及法現於世間亦復難遇。",
        "from": "T85n2817_001:1183b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人身難得。",
        "from": "T85n2817_001:1183b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今乃獲得大報分故。",
        "from": "T85n2817_001:1183b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時乃一現於前數劫承善業故方得人身。",
        "from": "T85n2817_001:1183b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特須精勤勿令靈棄。",
        "from": "T85n2817_001:1183c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增加智慧及法善根　聖地已來須求勝行。",
        "from": "T85n2817_001:1183c02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誓不復墮地獄餓鬼畜生之趣。",
        "from": "T85n2817_001:1183c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不聞三寶邊地下賤雖得人身盲聾音噁長壽天難或生人中信邪到見。",
        "from": "T85n2817_001:1183c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖佛出世佛前佛後八難之中。",
        "from": "T85n2817_001:1183c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種苦惱障道之處。",
        "from": "T85n2817_001:1183c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不復墮落永離八難。",
        "from": "T85n2817_001:1183c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來世尊證大果報。",
        "from": "T85n2817_001:1183c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸修行者勤加精進如救頭燃蠲除八難大善成就。",
        "from": "T85n2817_001:1183c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然善惡業纖毫不亡。",
        "from": "T85n2817_001:1183c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如昔仙人嬌答摩等以草刺於鹿蠅遺道。",
        "from": "T85n2817_001:1183c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於五百歲下部之中受木籤報。",
        "from": "T85n2817_001:1183c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後乃致死。",
        "from": "T85n2817_001:1183c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昔有童子以恭敬心施僧白石一掬之許。",
        "from": "T85n2817_001:1183c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承斯因緣於無量劫生豪富家。",
        "from": "T85n2817_001:1183c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雨其七寶具足成就快樂自在。",
        "from": "T85n2817_001:1183c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以善惡業隨類受身苦樂之果。",
        "from": "T85n2817_001:1183c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨業異熟所以惡業十不善等勿令親近理須遠離。",
        "from": "T85n2817_001:1183c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何假言哉。",
        "from": "T85n2817_001:1183c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十不善者。",
        "from": "T85n2817_001:1183c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身有三種行殺盜婬。",
        "from": "T85n2817_001:1183c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口有四種。",
        "from": "T85n2817_001:1183c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄言綺語惡口兩舌。",
        "from": "T85n2817_001:1183c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意有三種。",
        "from": "T85n2817_001:1183c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂貪瞋癡。",
        "from": "T85n2817_001:1183c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如茲總結名為十惡。",
        "from": "T85n2817_001:1183c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承此果報生於三塗。",
        "from": "T85n2817_001:1183c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能迴十惡名為十善。",
        "from": "T85n2817_001:1183c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若離殺生十惡緣等得生天上并及人間生於勝處。",
        "from": "T85n2817_001:1183c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此身上聖位已來若能修行皆得成就。",
        "from": "T85n2817_001:1183c20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知。",
        "from": "T85n2817_001:1183c21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十善應勤修學人中修善知諸法相名為丈夫。",
        "from": "T85n2817_001:1183c21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善能挍量真俗二諦得悟自他二性之理。",
        "from": "T85n2817_001:1183c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中世諦法相因緣所生。",
        "from": "T85n2817_001:1183c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻化如夢如炎如溫。",
        "from": "T85n2817_001:1183c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "界入善與不善有福無福。",
        "from": "T85n2817_001:1183c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間法相種種相貌分別。",
        "from": "T85n2817_001:1183c25_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但假言詮顯彰表示一切諸相。",
        "from": "T85n2817_001:1183c25_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有漏之法皆自然性無常苦空無我為相。",
        "from": "T85n2817_001:1183c26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言無漏法者。",
        "from": "T85n2817_001:1183c27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空無我故如是二諦知一切法輪迴。",
        "from": "T85n2817_001:1183c27_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明行識名色六入觸受愛取有生老死十二因緣如輪流轉於六道生受苦溫身。",
        "from": "T85n2817_001:1183c28_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各隨稟根器利鈍隨類得成。",
        "from": "T85n2817_001:1184a01_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言解脫聖所依地。",
        "from": "T85n2817_001:1184a02_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鈍根性受聲聞乘。",
        "from": "T85n2817_001:1184a02_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中根之者辟支佛果。",
        "from": "T85n2817_001:1184a03_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨根所證各各差別。",
        "from": "T85n2817_001:1184a03_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無上大乘等為三種。",
        "from": "T85n2817_001:1184a04_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其鈍根者證於小乘。",
        "from": "T85n2817_001:1184a04_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但能自利無益他義。",
        "from": "T85n2817_001:1184a05_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厭生死苦修寂滅樂。",
        "from": "T85n2817_001:1184a05_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨諸不善禁根境識。",
        "from": "T85n2817_001:1184a06_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初觀四諦修證預流八輩等地無餘涅槃。",
        "from": "T85n2817_001:1184a06_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辟支佛者觀四諦法因緣所生遂滅生死之原。",
        "from": "T85n2817_001:1184a07_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證於聖道無餘涅槃。",
        "from": "T85n2817_001:1184a08_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具上人智取向大乘生死涅槃不分別。",
        "from": "T85n2817_001:1184a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二自利利他無上菩提。",
        "from": "T85n2817_001:1184a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩大慈修助道法。",
        "from": "T85n2817_001:1184a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從從極喜地乃至法雲。",
        "from": "T85n2817_001:1184a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第修行證菩薩。",
        "from": "T85n2817_001:1184a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已仍修六度布施愛語利行同事四攝之法福智具足成不思議無上上智。",
        "from": "T85n2817_001:1184a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲十力四無所畏具足成就。",
        "from": "T85n2817_001:1184a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證於佛地利樂眾生。",
        "from": "T85n2817_001:1184a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡生死際不般涅槃。",
        "from": "T85n2817_001:1184a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘纂要義一卷",
        "from": "T85n2817_001:1184a16_0",
        "context_with_next_sentence": false
    }
]