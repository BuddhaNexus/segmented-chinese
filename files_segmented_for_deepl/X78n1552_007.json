[
    {
        "source_sentence": "修西聞見錄卷七",
        "from": "X78n1552_007:0419a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東土　雞圓偶　集",
        "from": "X78n1552_007:0419a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鼠事長　慶",
        "from": "X78n1552_007:0419a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咫觀老人。",
        "from": "X78n1552_007:0419a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初不信佛。",
        "from": "X78n1552_007:0419a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉尺密祈彌陀加被。",
        "from": "X78n1552_007:0419a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已數年矣。",
        "from": "X78n1552_007:0419a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "赴浙闈。",
        "from": "X78n1552_007:0419a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨老人至上天竺。",
        "from": "X78n1552_007:0419a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀音冥力轉其機。",
        "from": "X78n1552_007:0419a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是夕老人夢一庭花開。",
        "from": "X78n1552_007:0419a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持鰣魚半尾附玉尺。",
        "from": "X78n1552_007:0419a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將帶烹之。",
        "from": "X78n1552_007:0419a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一百日中。",
        "from": "X78n1552_007:0419a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發心念佛。",
        "from": "X78n1552_007:0419a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂漸次斷菸酒葷血。",
        "from": "X78n1552_007:0419a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勤閱大乘典籍。",
        "from": "X78n1552_007:0419a07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "館麻村將十年。",
        "from": "X78n1552_007:0419a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "居停家有鼠。",
        "from": "X78n1552_007:0419a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈然默受熏染。",
        "from": "X78n1552_007:0419a08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐化不朽。",
        "from": "X78n1552_007:0419a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老人取歸。",
        "from": "X78n1552_007:0419a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "裝金。",
        "from": "X78n1552_007:0419a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日供香一枝。",
        "from": "X78n1552_007:0419a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曰。",
        "from": "X78n1552_007:0419a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此亦善知識也。",
        "from": "X78n1552_007:0419a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至今鼠像在海天精舍。",
        "from": "X78n1552_007:0419a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修淨業人每見之。",
        "from": "X78n1552_007:0419a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多欷歔感愧者(海天精舍一名放生菴)。",
        "from": "X78n1552_007:0419a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蜈蚣說玉清生",
        "from": "X78n1552_007:0419a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "物之獲益於人也。",
        "from": "X78n1552_007:0419a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖一時之靈蠢因殊。",
        "from": "X78n1552_007:0419a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而累劫之真光平等。",
        "from": "X78n1552_007:0419a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略栽一粒。",
        "from": "X78n1552_007:0419a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便獲參天。",
        "from": "X78n1552_007:0419a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無足怪也。",
        "from": "X78n1552_007:0419a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蓮池。",
        "from": "X78n1552_007:0419a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨土中人也。",
        "from": "X78n1552_007:0419a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出門見童子以竹篾弓蜈蚣之身。",
        "from": "X78n1552_007:0419a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀而贖其命。",
        "from": "X78n1552_007:0419a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他日蓮池對客說法。",
        "from": "X78n1552_007:0419a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蜈蚣能來聽之。",
        "from": "X78n1552_007:0419a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事閱多年。",
        "from": "X78n1552_007:0419a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而因非偶爾。",
        "from": "X78n1552_007:0419a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其化為人天妙相以趨淨土也久矣。",
        "from": "X78n1552_007:0419a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化鳥神物能超淨穢說李　生",
        "from": "X78n1552_007:0419a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有般若心而不能勝出。",
        "from": "X78n1552_007:0419a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口才利。",
        "from": "X78n1552_007:0419a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善根顯。",
        "from": "X78n1552_007:0419a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻帶惡業。",
        "from": "X78n1552_007:0419a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誤失人身。",
        "from": "X78n1552_007:0419a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉世或作天鳥而說法。",
        "from": "X78n1552_007:0419a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若帝釋之象。",
        "from": "X78n1552_007:0419a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神通全與天同。",
        "from": "X78n1552_007:0419a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非其餘神象之可及。",
        "from": "X78n1552_007:0419a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "龍王上首。",
        "from": "X78n1552_007:0419a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至超地位而有餘。",
        "from": "X78n1552_007:0419a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神神交參。",
        "from": "X78n1552_007:0419a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真假莫測。",
        "from": "X78n1552_007:0419a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非但西方鳥為彌陀化作而已。",
        "from": "X78n1552_007:0419a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若阿彌陀佛。",
        "from": "X78n1552_007:0419a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入三十七聖曼拏囉中。",
        "from": "X78n1552_007:0419a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉名觀自在王。",
        "from": "X78n1552_007:0419a23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即乘孔雀以表自在。",
        "from": "X78n1552_007:0419a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不坐蓮花。",
        "from": "X78n1552_007:0419a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此鳥此佛。",
        "from": "X78n1552_007:0419a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一耶二耶。",
        "from": "X78n1552_007:0419b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大吉祥天。",
        "from": "X78n1552_007:0419b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切清淨。",
        "from": "X78n1552_007:0419b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然而有六牙白象。",
        "from": "X78n1552_007:0419b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以鼻絞馬腦瓶。",
        "from": "X78n1552_007:0419b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傾諸珍寶。",
        "from": "X78n1552_007:0419b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌於頂上。",
        "from": "X78n1552_007:0419b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又何自來乎。",
        "from": "X78n1552_007:0419b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘗言輪王為一切凡中自在。",
        "from": "X78n1552_007:0419b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其眷屬靈純者為兵藏女寶。",
        "from": "X78n1552_007:0419b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其襍作者化為象馬之類。",
        "from": "X78n1552_007:0419b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五類有情。",
        "from": "X78n1552_007:0419b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往昔與輪王。",
        "from": "X78n1552_007:0419b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖曰同願。",
        "from": "X78n1552_007:0419b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而行之出入大矣(又如一字頂輪王。",
        "from": "X78n1552_007:0419b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應念而現七寶。",
        "from": "X78n1552_007:0419b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其男女蠢靈。",
        "from": "X78n1552_007:0419b05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非實非虛。",
        "from": "X78n1552_007:0419b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "純乎聖凡。",
        "from": "X78n1552_007:0419b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然化中亦一一有因也)神物之乘願輪而超淨穢者。",
        "from": "X78n1552_007:0419b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仗他力成就。",
        "from": "X78n1552_007:0419b07_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自不能知。",
        "from": "X78n1552_007:0419b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世之愛鳥愛貓愛鼠愛犬。",
        "from": "X78n1552_007:0419b08_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惜未能轉此愛心。",
        "from": "X78n1552_007:0419b08_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為所愛眾生作 字種紅玻瓈色觀。",
        "from": "X78n1552_007:0419b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以引作蓮花種族。",
        "from": "X78n1552_007:0419b09_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自身入於物類之中。",
        "from": "X78n1552_007:0419b09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為上首。",
        "from": "X78n1552_007:0419b10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲勝報焉。",
        "from": "X78n1552_007:0419b10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真下劣之可哀矣。",
        "from": "X78n1552_007:0419b10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 讀若紇哩(二合)。",
        "from": "X78n1552_007:0419b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿彌陀佛蓮花部字種。",
        "from": "X78n1552_007:0419b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀之。",
        "from": "X78n1552_007:0419b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大有神力紗用。",
        "from": "X78n1552_007:0419b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修西聞見錄卷七(終)",
        "from": "X78n1552_007:0419b15_0",
        "context_with_next_sentence": false
    }
]