[
    {
        "source_sentence": "曼殊室利菩薩呪藏中一字呪",
        "from": "T20n1182_001:0781a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王經",
        "from": "T20n1182_001:0781a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏法師義淨奉　制譯",
        "from": "T20n1182_001:0781a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼佛。",
        "from": "T20n1182_001:0781a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在淨居天上。",
        "from": "T20n1182_001:0781a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於天眾中跏趺正念。",
        "from": "T20n1182_001:0781a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普觀大眾而告之曰。",
        "from": "T20n1182_001:0781a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝諸天子。",
        "from": "T20n1182_001:0781a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當至心聽曼殊室利童子微妙之行。",
        "from": "T20n1182_001:0781a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂壇場神呪作法。",
        "from": "T20n1182_001:0781a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所須療病護身。",
        "from": "T20n1182_001:0781a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一祕密最上呪心。",
        "from": "T20n1182_001:0781a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來所共宣說咸皆稱歎。",
        "from": "T20n1182_001:0781a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大呪王。",
        "from": "T20n1182_001:0781a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "T20n1182_001:0781a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能誦此呪者。",
        "from": "T20n1182_001:0781a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則為受持一切神呪。",
        "from": "T20n1182_001:0781a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝諸天眾。",
        "from": "T20n1182_001:0781a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大呪王無能違者。",
        "from": "T20n1182_001:0781a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使曼殊室利童子菩薩摩訶薩。",
        "from": "T20n1182_001:0781a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪亦能隨心自在引攝將來。",
        "from": "T20n1182_001:0781b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令使現前為作眾事。",
        "from": "T20n1182_001:0781b02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況復諸餘菩薩。",
        "from": "T20n1182_001:0781b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世出世間所有神呪。",
        "from": "T20n1182_001:0781b03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切障礙悉皆除遣。",
        "from": "T20n1182_001:0781b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有大勇猛有大威力。",
        "from": "T20n1182_001:0781b04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神驗第一。",
        "from": "T20n1182_001:0781b04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸呪中上一字中尊。",
        "from": "T20n1182_001:0781b04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此之一字悉能成就一切事業。",
        "from": "T20n1182_001:0781b05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能圓滿所有善法。",
        "from": "T20n1182_001:0781b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常在一切世出世間。",
        "from": "T20n1182_001:0781b06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪術之上不可毀壞。",
        "from": "T20n1182_001:0781b07_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是一切佛心。",
        "from": "T20n1182_001:0781b07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉能滿足所有求願。",
        "from": "T20n1182_001:0781b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝諸天眾。",
        "from": "T20n1182_001:0781b08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸皆一心聽此不可思議具大威力最上神呪。",
        "from": "T20n1182_001:0781b08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1182_001:0781b09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵叱洛呬焰(此有四字總成一字是故梵漢二體俱存)汝等天眾。",
        "from": "T20n1182_001:0781b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是一切祕密中勝一字呪王。",
        "from": "T20n1182_001:0781b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情無敢違者。",
        "from": "T20n1182_001:0781b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切鬼神不敢親近。",
        "from": "T20n1182_001:0781b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛為吉祥事。",
        "from": "T20n1182_001:0781b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切呪中最能成立隨意所為。",
        "from": "T20n1182_001:0781b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間作大尊勝。",
        "from": "T20n1182_001:0781b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於求財者有大自在能令富盛。",
        "from": "T20n1182_001:0781b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸瞋恚中此令慈善。",
        "from": "T20n1182_001:0781b15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸含識悲愍為性。",
        "from": "T20n1182_001:0781b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有違逆者皆悉順從。",
        "from": "T20n1182_001:0781b16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要而言之。",
        "from": "T20n1182_001:0781b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡有所為悉皆成就。",
        "from": "T20n1182_001:0781b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縱不作法入壇場等直爾誦持。",
        "from": "T20n1182_001:0781b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手觸彼時即便隨意。",
        "from": "T20n1182_001:0781b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪衣著時。",
        "from": "T20n1182_001:0781b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人皆愛樂見者歡喜若呪齒木用揩齒時。",
        "from": "T20n1182_001:0781b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "齒疼即差。",
        "from": "T20n1182_001:0781b19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患眼時。",
        "from": "T20n1182_001:0781b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取先陀婆鹽研之為末。",
        "from": "T20n1182_001:0781b20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪七遍已少置眼中。",
        "from": "T20n1182_001:0781b20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其痛便止。",
        "from": "T20n1182_001:0781b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患耳者。",
        "from": "T20n1182_001:0781b21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取象馬糞聚上地菌。",
        "from": "T20n1182_001:0781b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并苣蕂油先陀婆鹽。",
        "from": "T20n1182_001:0781b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各取少許呪之七遍。",
        "from": "T20n1182_001:0781b22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一處研使碎絞取汁煖之。",
        "from": "T20n1182_001:0781b23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渧耳孔中其痛便止。",
        "from": "T20n1182_001:0781b23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有女人。",
        "from": "T20n1182_001:0781b24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將產之時。",
        "from": "T20n1182_001:0781b24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被胎所惱腹中結痛不能疾出。",
        "from": "T20n1182_001:0781b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取阿吒留灑根或牛膝根。",
        "from": "T20n1182_001:0781b25_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取無蟲水磨擣令碎。",
        "from": "T20n1182_001:0781b26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪之七遍塗在臍下。",
        "from": "T20n1182_001:0781b26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能易出。",
        "from": "T20n1182_001:0781b27_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人被射。",
        "from": "T20n1182_001:0781b27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "箭鏃入身不能出者。",
        "from": "T20n1182_001:0781b27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可取陳酥呪一百八遍。",
        "from": "T20n1182_001:0781b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令彼飲之其鏃便出。",
        "from": "T20n1182_001:0781b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患宿食不消。",
        "from": "T20n1182_001:0781b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "腹中結痛上變下瀉癨亂畏死者。",
        "from": "T20n1182_001:0781b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可取烏鹽或先陀婆鹽或諸雜鹽類。",
        "from": "T20n1182_001:0781c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪之七遍。",
        "from": "T20n1182_001:0781c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "研碎煖水令服便差。",
        "from": "T20n1182_001:0781c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復苦痢不能斷者。",
        "from": "T20n1182_001:0781c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取橘柚根及榠樝根。",
        "from": "T20n1182_001:0781c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "磨擣呪之七遍。",
        "from": "T20n1182_001:0781c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和水服之即差。",
        "from": "T20n1182_001:0781c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是石女無產生法。",
        "from": "T20n1182_001:0781c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求男女者。",
        "from": "T20n1182_001:0781c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取阿說健陀根。",
        "from": "T20n1182_001:0781c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以酥熟煎擣之令碎。",
        "from": "T20n1182_001:0781c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和黃牛乳呪二十五遍。",
        "from": "T20n1182_001:0781c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "待彼女人身淨之時令飲其藥。",
        "from": "T20n1182_001:0781c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妻莫犯他男。",
        "from": "T20n1182_001:0781c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫莫犯他女。",
        "from": "T20n1182_001:0781c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未久之間即便有娠。",
        "from": "T20n1182_001:0781c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或復女人斷緒無子。",
        "from": "T20n1182_001:0781c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經三五年或復多年。",
        "from": "T20n1182_001:0781c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或被他禁呪或由厭禱。",
        "from": "T20n1182_001:0781c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或因諸病或他所惱或遭毒藥。",
        "from": "T20n1182_001:0781c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇此惡緣遂無子息者。",
        "from": "T20n1182_001:0781c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應取少許孔雀尾。",
        "from": "T20n1182_001:0781c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安陳酥中箭之數沸。",
        "from": "T20n1182_001:0781c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "研令相得。",
        "from": "T20n1182_001:0781c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投少石蜜量如棗許。",
        "from": "T20n1182_001:0781c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪二十七遍服之令盡。",
        "from": "T20n1182_001:0781c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於後七日中日日常以石蜜和乳。",
        "from": "T20n1182_001:0781c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每呪七遍飲之。",
        "from": "T20n1182_001:0781c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女身清淨諸病皆差即便有娠。",
        "from": "T20n1182_001:0781c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有女人月水不息。",
        "from": "T20n1182_001:0781c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以阿藍部根或以藍根一握擣之。",
        "from": "T20n1182_001:0781c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和乳熟煎呪一百八遍服之即差。",
        "from": "T20n1182_001:0781c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若患頭痛者。",
        "from": "T20n1182_001:0781c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以烏羽呪之七遍。",
        "from": "T20n1182_001:0781c18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掃拂痛處即便永差。",
        "from": "T20n1182_001:0781c18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人患瘧。",
        "from": "T20n1182_001:0781c19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或一日二日三日四日發者。",
        "from": "T20n1182_001:0781c19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或常熱病或暫時熱病。",
        "from": "T20n1182_001:0781c20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應以乳粥和酥呪一百八遍。",
        "from": "T20n1182_001:0781c20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "食之即差。",
        "from": "T20n1182_001:0781c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人被他厭魅蠱毒所中。",
        "from": "T20n1182_001:0781c21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應作反緣心。",
        "from": "T20n1182_001:0781c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪自己面一百八遍。",
        "from": "T20n1182_001:0781c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀彼病人所患便差。",
        "from": "T20n1182_001:0781c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人被邪鬼羯吒布單那等諸鬼所著。",
        "from": "T20n1182_001:0781c23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或小兒諸病。",
        "from": "T20n1182_001:0781c24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但是一切非人所為共相惱者。",
        "from": "T20n1182_001:0781c24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應呪自手一百八遍。",
        "from": "T20n1182_001:0782a01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩病人頭即眾病皆差。",
        "from": "T20n1182_001:0782a01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人被蛇蠍所蜇或狂狗所傷。",
        "from": "T20n1182_001:0782a02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以氣急吹瘡上。",
        "from": "T20n1182_001:0782a02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪之七七遍即差。",
        "from": "T20n1182_001:0782a03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人患癩病瘦病者。",
        "from": "T20n1182_001:0782a03_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應洗浴潔淨於閑靜處。",
        "from": "T20n1182_001:0782a04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常誦此呪悉皆除愈。",
        "from": "T20n1182_001:0782a04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡誦呪之人。",
        "from": "T20n1182_001:0782a05_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常須遠離惡人不淨臭穢之處不近酒肉五辛。",
        "from": "T20n1182_001:0782a05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心受持無不驗者。",
        "from": "T20n1182_001:0782a06_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有一日常誦一遍。",
        "from": "T20n1182_001:0782a07_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能護自身。",
        "from": "T20n1182_001:0782a07_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦二遍能護同伴。",
        "from": "T20n1182_001:0782a07_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦三遍能護一家。",
        "from": "T20n1182_001:0782a08_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦四遍能護一村。",
        "from": "T20n1182_001:0782a08_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦五遍能護一城。",
        "from": "T20n1182_001:0782a09_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦百遍能護一國。",
        "from": "T20n1182_001:0782a09_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦千遍能護四天下。",
        "from": "T20n1182_001:0782a10_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以要言之略述如是。",
        "from": "T20n1182_001:0782a10_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "療病護身隨時利益。",
        "from": "T20n1182_001:0782a11_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若更有餘所為之事。",
        "from": "T20n1182_001:0782a11_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆隨意作無不成就。",
        "from": "T20n1182_001:0782a12_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸天大眾聞佛說此一字呪王經已。",
        "from": "T20n1182_001:0782a12_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜奉行(若復有人志求解脫希大利益現望果證者當可至心依呪藏中作大呪法廣說如經)。",
        "from": "T20n1182_001:0782a13_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說一字呪王經",
        "from": "T20n1182_001:0782a15_0",
        "context_with_next_sentence": false
    }
]