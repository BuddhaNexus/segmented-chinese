[
    {
        "source_sentence": "千轉陀羅尼觀世音菩薩呪",
        "from": "T20n1035_001:0017b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐總持寺沙門智通譯",
        "from": "T20n1035_001:0017b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨曷囉(上)跢娜怛囉耶(余何反)夜(弋可反一)娜麼(莫我)阿唎耶跋盧枳羝鑠(上)筏囉夜(二)菩提(徒儞反)薩跢婆夜(三)莫訶薩跢婆夜(四)莫訶迦嚕嬭(奴綺反)迦夜(五)跢姪他(六)闍(上)曳闍曳(七)闍(上)夜婆醯(上)儞(八)闍(上)榆跢唎(上九)迦(上)羅迦(上)羅(十)麼羅麼羅(十一)者羅者羅(上十二)綺拏(上)綺拏(十三)薩囉(上)皤(符可反)羯囉(上)摩跋囉(上)拏儞迷(十四)薄伽跋底(都儞反)娑(上)訶薩囉(平)薄羯羝(十五)薩囉(上)皤菩(上)陀跋盧枳羝(十六)者芻(上眼十七)輸(上)嚕怛囉(上耳十八)揭囉(上)拏(上鼻十九)是訶(上)皤(舌二十)迦夜(身二十一)末弩(心二十二)毘(上)輸達儞(二十三)素(上)囉(上)素(上)囉(上二十四)鉢囉(上)素囉鉢囉(上)素囉(上二十五)薩囉(上)皤菩(上)陀阿(上)提(徒儞反)瑟絺(上)羝娑婆訶(二十六)達囉(上)麼陀覩揭囉(上)鞞娑(上)婆訶(二十七)阿(上)婆皤(二十八)娑(上)皤婆皤(二十九)達囉(上)摩皤(三十)蒱馱(上)尼(奴移反)曳(三十一)娑婆訶(三十二)千轉印與觀世音心印同(唯足不同)頭指以去四指反叉。",
        "from": "T20n1035_001:0017b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向內相捺。",
        "from": "T20n1035_001:0017c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左大指屈入頭指中。",
        "from": "T20n1035_001:0017c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右大指舒直。",
        "from": "T20n1035_001:0017c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向內勿曲。",
        "from": "T20n1035_001:0017c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩腕相合。",
        "from": "T20n1035_001:0017c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩脚作丁字形。",
        "from": "T20n1035_001:0017c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃右脚直立左膝曲在外。",
        "from": "T20n1035_001:0017c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弩跨身屈向左邊。",
        "from": "T20n1035_001:0017c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以心印當右乳前勿著乳。",
        "from": "T20n1035_001:0017c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面作笑顏頭向右(手印與觀世音心印同唯身脚法用別)每月十五日洗浴於淨室。",
        "from": "T20n1035_001:0017c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手作心印誦呪。",
        "from": "T20n1035_001:0017c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅四重五逆(此印是阿地多崛多師譯出)。",
        "from": "T20n1035_001:0017c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千轉云。",
        "from": "T20n1035_001:0017c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦此呪已惡業消滅。",
        "from": "T20n1035_001:0017c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至七遍五逆罪滅。",
        "from": "T20n1035_001:0017c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若滿十遍已罪無不滅。",
        "from": "T20n1035_001:0017c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十萬遍面見觀音種種莊嚴者。",
        "from": "T20n1035_001:0017c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七日之中初作法時。",
        "from": "T20n1035_001:0017c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟得食乳糜蘇酪白餅粳米飯。",
        "from": "T20n1035_001:0017c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得食鹽醬菜。",
        "from": "T20n1035_001:0017c24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最後一日勿食。",
        "from": "T20n1035_001:0017c24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五日夜空腹。",
        "from": "T20n1035_001:0018a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛前馺馺誦呪無定數。",
        "from": "T20n1035_001:0018a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見像動搖。",
        "from": "T20n1035_001:0018a02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出聲唱言善哉。",
        "from": "T20n1035_001:0018a02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放光明曜。",
        "from": "T20n1035_001:0018a02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以真珠寶物安呪師頂上。",
        "from": "T20n1035_001:0018a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即知成驗。",
        "from": "T20n1035_001:0018a03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面見觀音已。",
        "from": "T20n1035_001:0018a03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得滿一切善願。",
        "from": "T20n1035_001:0018a04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於睡眠中夢見觀音種種莊嚴者。",
        "from": "T20n1035_001:0018a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善事成就。",
        "from": "T20n1035_001:0018a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡業消滅。",
        "from": "T20n1035_001:0018a05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直轉讀者亦得滅罪。",
        "from": "T20n1035_001:0018a05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲東西行時。",
        "from": "T20n1035_001:0018a06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先呪手七遍以摩拭面。",
        "from": "T20n1035_001:0018a06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所至之處無諸災橫若能清淨如法常誦不廢。",
        "from": "T20n1035_001:0018a07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得第一地。",
        "from": "T20n1035_001:0018a08_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有女人能誦持者後成男子。",
        "from": "T20n1035_001:0018a08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更不重受如是女形。",
        "from": "T20n1035_001:0018a09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先作壇安置供養備訖。",
        "from": "T20n1035_001:0018a09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然始誦呪其壇四面各長十六肘。",
        "from": "T20n1035_001:0018a10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四重作規。",
        "from": "T20n1035_001:0018a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "院相皆外白色內四色。",
        "from": "T20n1035_001:0018a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各一重如似壁勢。",
        "from": "T20n1035_001:0018a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是八重也。",
        "from": "T20n1035_001:0018a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合五方色四面開門。",
        "from": "T20n1035_001:0018a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東西南北相當。",
        "from": "T20n1035_001:0018a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正中一重不須開門。",
        "from": "T20n1035_001:0018a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大瓦罐八枚瓮子四枚。",
        "from": "T20n1035_001:0018a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿盛水插柏及樹枝。",
        "from": "T20n1035_001:0018a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安水罐瓮中。",
        "from": "T20n1035_001:0018a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作白餅粳米飯乳酪蘇蜜香果子等供養。",
        "from": "T20n1035_001:0018a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然十六枝燈。",
        "from": "T20n1035_001:0018a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四門外各插十六隻未經用箭。",
        "from": "T20n1035_001:0018a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掛五綵線於箭上。",
        "from": "T20n1035_001:0018a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取佉陀羅木(紫檀木是)四枝。",
        "from": "T20n1035_001:0018a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二長五指二長六指。",
        "from": "T20n1035_001:0018a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釘壇四角。",
        "from": "T20n1035_001:0018a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "散種種時非時花於壇上。",
        "from": "T20n1035_001:0018a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又取頗伽木(拘無子少者是)一千八百枚。",
        "from": "T20n1035_001:0018a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各長一尺。",
        "from": "T20n1035_001:0018a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若一瓦鉢若銅鉢。",
        "from": "T20n1035_001:0018a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "和蘇乳塗此木枝。",
        "from": "T20n1035_001:0018a20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪一枝一遍即擲火中。",
        "from": "T20n1035_001:0018a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無佉陀羅木時(赤棗木代亦得)須畫一白色觀音聖者像。",
        "from": "T20n1035_001:0018a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨力大小作。",
        "from": "T20n1035_001:0018a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又先翻法云。",
        "from": "T20n1035_001:0018a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千劫聚集業障。",
        "from": "T20n1035_001:0018a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時誦念悉皆滅盡。",
        "from": "T20n1035_001:0018a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得千佛聚集善根。",
        "from": "T20n1035_001:0018a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得背千劫流轉生老病死邊際。",
        "from": "T20n1035_001:0018a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨此身已即見千轉輪聖王。",
        "from": "T20n1035_001:0018a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒持十善。",
        "from": "T20n1035_001:0018a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲生諸佛淨土者。",
        "from": "T20n1035_001:0018a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜各三時誦二十一遍。",
        "from": "T20n1035_001:0018a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿三七日如其所欲。",
        "from": "T20n1035_001:0018a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即於夢中或見佛金色形像及菩薩形像(此是先相即知當生淨土)。",
        "from": "T20n1035_001:0018a27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上件呪及功能。",
        "from": "T20n1035_001:0018a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並是通師總持寺翻梵本出。",
        "from": "T20n1035_001:0018a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其印法者崛多師譯出。",
        "from": "T20n1035_001:0018b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別譯本云。",
        "from": "T20n1035_001:0018b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持法。",
        "from": "T20n1035_001:0018b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取突婆香并苗五斤。",
        "from": "T20n1035_001:0018b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "楮木一千箇長一尺。",
        "from": "T20n1035_001:0018b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并蘇總呪一千遍。",
        "from": "T20n1035_001:0018b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨欲燒之。",
        "from": "T20n1035_001:0018b03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以蘇塗木莖及香上。",
        "from": "T20n1035_001:0018b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各呪一遍燒之。",
        "from": "T20n1035_001:0018b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豫五月六月取牛糞陰乾。",
        "from": "T20n1035_001:0018b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "突婆香水和作泥作場身。",
        "from": "T20n1035_001:0018b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二肘圓作場。",
        "from": "T20n1035_001:0018b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "場上安種種花香。",
        "from": "T20n1035_001:0018b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "冬月無花刻花安之。",
        "from": "T20n1035_001:0018b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然薰陸香。",
        "from": "T20n1035_001:0018b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪師面向東。",
        "from": "T20n1035_001:0018b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐誦滿一千遍法成。",
        "from": "T20n1035_001:0018b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未呪之時用此呪水二十一遍。",
        "from": "T20n1035_001:0018b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或七遍服之。",
        "from": "T20n1035_001:0018b09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪水解污淨室道場佛堂洗身呪柳枝打病呪水治病並得。",
        "from": "T20n1035_001:0018b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各二十一遍。",
        "from": "T20n1035_001:0018b11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能一生日別三時時別誦二十一遍。",
        "from": "T20n1035_001:0018b11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅罪不可思議通一切用。",
        "from": "T20n1035_001:0018b12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千轉陀羅尼觀世音菩薩呪",
        "from": "T20n1035_001:0018b13_0",
        "context_with_next_sentence": false
    }
]