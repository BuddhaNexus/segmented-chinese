[
    {
        "source_sentence": "佛說鬼子母經",
        "from": "T21n1262_001:0290c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人今附西晉錄",
        "from": "T21n1262_001:0290c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛遊大兜國。",
        "from": "T21n1262_001:0290c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時國中有一母人。",
        "from": "T21n1262_001:0290c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "多子性極惡。",
        "from": "T21n1262_001:0290c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常喜行盜人子殺噉之。",
        "from": "T21n1262_001:0290c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亡子家亦不知何誰取者。",
        "from": "T21n1262_001:0290c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行街里啼哭。",
        "from": "T21n1262_001:0290c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人已還共議。",
        "from": "T21n1262_001:0290c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是非一日。",
        "from": "T21n1262_001:0290c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難及眾沙門。",
        "from": "T21n1262_001:0290c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出行輒見啼哭人。",
        "from": "T21n1262_001:0290c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已還共議。",
        "from": "T21n1262_001:0290c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "傷哀亡子家。",
        "from": "T21n1262_001:0290c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即知眾沙門議。",
        "from": "T21n1262_001:0290c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛到眾沙門所。",
        "from": "T21n1262_001:0290c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛問眾沙門。",
        "from": "T21n1262_001:0290c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向者何等議眾沙門。",
        "from": "T21n1262_001:0290c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難白佛言。",
        "from": "T21n1262_001:0290c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向者出行分衛。",
        "from": "T21n1262_001:0290c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見街里有啼哭人眾多。",
        "from": "T21n1262_001:0290c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即問啼哭人。",
        "from": "T21n1262_001:0290c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝何為啼哭。",
        "from": "T21n1262_001:0290c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報言。",
        "from": "T21n1262_001:0290c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生亡我子。",
        "from": "T21n1262_001:0290c13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知屍死處所。",
        "from": "T21n1262_001:0290c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是啼哭者非一家皆亡子。",
        "from": "T21n1262_001:0290c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便為阿難眾沙門說。",
        "from": "T21n1262_001:0290c15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是國中盜人子者。",
        "from": "T21n1262_001:0290c15_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非凡人故。",
        "from": "T21n1262_001:0290c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現鬼子母。",
        "from": "T21n1262_001:0290c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今生作人。",
        "from": "T21n1262_001:0290c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喜行盜人子。",
        "from": "T21n1262_001:0290c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是母有千子。",
        "from": "T21n1262_001:0290c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百子在天上。",
        "from": "T21n1262_001:0290c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百子在世間。",
        "from": "T21n1262_001:0290c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千子皆為鬼王。",
        "from": "T21n1262_001:0290c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一王者從數萬鬼。",
        "from": "T21n1262_001:0290c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五百鬼王。",
        "from": "T21n1262_001:0290c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在天上嬈諸天。",
        "from": "T21n1262_001:0290c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百鬼王。",
        "from": "T21n1262_001:0290c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在世間嬈帝王人民如是。",
        "from": "T21n1262_001:0290c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百鬼王。",
        "from": "T21n1262_001:0290c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天亦無奈何。",
        "from": "T21n1262_001:0290c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難白佛言。",
        "from": "T21n1262_001:0290c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼子母來在是國中。",
        "from": "T21n1262_001:0290c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今寧可勅令不盜人子耶。",
        "from": "T21n1262_001:0290c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言大善。",
        "from": "T21n1262_001:0290c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可令不復盜人子。",
        "from": "T21n1262_001:0290c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難問佛言。",
        "from": "T21n1262_001:0290c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用何等方便。",
        "from": "T21n1262_001:0290c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使不復盜人子耶。",
        "from": "T21n1262_001:0290c23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便語阿難。",
        "from": "T21n1262_001:0290c23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到是母所居。",
        "from": "T21n1262_001:0290c24_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾沙門共伺是母出已。",
        "from": "T21n1262_001:0290c24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後悉歛取子。",
        "from": "T21n1262_001:0290c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來著精舍中逃之。",
        "from": "T21n1262_001:0290c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾沙門即往。",
        "from": "T21n1262_001:0290c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伺是母出行。",
        "from": "T21n1262_001:0290c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨後歛取子。",
        "from": "T21n1262_001:0290c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得千數子。",
        "from": "T21n1262_001:0290c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逃著精舍中。",
        "from": "T21n1262_001:0290c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是母便復行盜人子。",
        "from": "T21n1262_001:0290c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來入舍中。",
        "from": "T21n1262_001:0290c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見其子。",
        "from": "T21n1262_001:0290c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便捨他人子。",
        "from": "T21n1262_001:0290c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不敢復殺。",
        "from": "T21n1262_001:0290c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便行索其子。",
        "from": "T21n1262_001:0290c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍舍中不知其子處。",
        "from": "T21n1262_001:0290c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便出行至街里遍城中不得。",
        "from": "T21n1262_001:0290c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復出城外索不得。",
        "from": "T21n1262_001:0291a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便入城行道啼哭。",
        "from": "T21n1262_001:0291a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是十日。",
        "from": "T21n1262_001:0291a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母便狂。",
        "from": "T21n1262_001:0291a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被髮入市。",
        "from": "T21n1262_001:0291a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啼哭自擗撲仰天。",
        "from": "T21n1262_001:0291a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大呼為狂梁語。",
        "from": "T21n1262_001:0291a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不能復飲食。",
        "from": "T21n1262_001:0291a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛遣沙門。",
        "from": "T21n1262_001:0291a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往視之。",
        "from": "T21n1262_001:0291a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見母問。",
        "from": "T21n1262_001:0291a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何為市中被髮啼哭。",
        "from": "T21n1262_001:0291a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母即報沙門言。",
        "from": "T21n1262_001:0291a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亡我子眾多故哭耳。",
        "from": "T21n1262_001:0291a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門言。",
        "from": "T21n1262_001:0291a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝欲得汝子不。",
        "from": "T21n1262_001:0291a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母報言。",
        "from": "T21n1262_001:0291a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我欲得之。",
        "from": "T21n1262_001:0291a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門言。",
        "from": "T21n1262_001:0291a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝審欲得者。",
        "from": "T21n1262_001:0291a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是間有佛。",
        "from": "T21n1262_001:0291a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可往問佛。",
        "from": "T21n1262_001:0291a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛者知當來已去之事。",
        "from": "T21n1262_001:0291a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝往則可得汝子。",
        "from": "T21n1262_001:0291a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母聞是語則歡喜意解便隨沙門。",
        "from": "T21n1262_001:0291a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去到佛所。",
        "from": "T21n1262_001:0291a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜前為佛作禮。",
        "from": "T21n1262_001:0291a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即問母。",
        "from": "T21n1262_001:0291a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何為市中啼哭。",
        "from": "T21n1262_001:0291a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母報佛言。",
        "from": "T21n1262_001:0291a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亡我子故。",
        "from": "T21n1262_001:0291a10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛問母。",
        "from": "T21n1262_001:0291a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝捨汝子至何所而亡汝子。",
        "from": "T21n1262_001:0291a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母即默然不言。",
        "from": "T21n1262_001:0291a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復問母。",
        "from": "T21n1262_001:0291a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝捨子至何所而反。",
        "from": "T21n1262_001:0291a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "默然無語。",
        "from": "T21n1262_001:0291a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母知盜人子為惡。",
        "from": "T21n1262_001:0291a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母即起為佛作禮。",
        "from": "T21n1262_001:0291a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面著地。",
        "from": "T21n1262_001:0291a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我愚癡故。",
        "from": "T21n1262_001:0291a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復問。",
        "from": "T21n1262_001:0291a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝有子愛之不。",
        "from": "T21n1262_001:0291a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母言。",
        "from": "T21n1262_001:0291a14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有子坐起常欲著我傍。",
        "from": "T21n1262_001:0291a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復問。",
        "from": "T21n1262_001:0291a15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝有子知愛之。",
        "from": "T21n1262_001:0291a15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何故日行盜他人子。",
        "from": "T21n1262_001:0291a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他人有子。",
        "from": "T21n1262_001:0291a16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如汝愛之。",
        "from": "T21n1262_001:0291a16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亡子家。",
        "from": "T21n1262_001:0291a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦行道啼哭如汝。",
        "from": "T21n1262_001:0291a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝反盜人子殺噉之。",
        "from": "T21n1262_001:0291a17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死後當入太山地獄中。",
        "from": "T21n1262_001:0291a18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母聞是語。",
        "from": "T21n1262_001:0291a18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便恐怖。",
        "from": "T21n1262_001:0291a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復問。",
        "from": "T21n1262_001:0291a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝寧欲得汝子不。",
        "from": "T21n1262_001:0291a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母即起復持頭面著地。",
        "from": "T21n1262_001:0291a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願哀我。",
        "from": "T21n1262_001:0291a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便語母言。",
        "from": "T21n1262_001:0291a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝子若在。",
        "from": "T21n1262_001:0291a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝寧能自悔不。",
        "from": "T21n1262_001:0291a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能自悔。",
        "from": "T21n1262_001:0291a21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當還汝子。",
        "from": "T21n1262_001:0291a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母言。",
        "from": "T21n1262_001:0291a21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我能自悔。",
        "from": "T21n1262_001:0291a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T21n1262_001:0291a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能自悔。",
        "from": "T21n1262_001:0291a22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當作何等自悔。",
        "from": "T21n1262_001:0291a22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母言。",
        "from": "T21n1262_001:0291a23_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我聽佛教誡。",
        "from": "T21n1262_001:0291a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當隨佛語自悔。",
        "from": "T21n1262_001:0291a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛還我子。",
        "from": "T21n1262_001:0291a23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我不敢遠離佛所語。",
        "from": "T21n1262_001:0291a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T21n1262_001:0291a24_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "審如汝語不。",
        "from": "T21n1262_001:0291a24_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "母言。",
        "from": "T21n1262_001:0291a25_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我審如佛語。",
        "from": "T21n1262_001:0291a25_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便授以五戒。",
        "from": "T21n1262_001:0291a25_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一不殺生。",
        "from": "T21n1262_001:0291a25_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二不盜。",
        "from": "T21n1262_001:0291a26_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三不婬。",
        "from": "T21n1262_001:0291a26_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四不兩舌。",
        "from": "T21n1262_001:0291a26_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五不飲酒。",
        "from": "T21n1262_001:0291a26_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "報能悉還其子。",
        "from": "T21n1262_001:0291a27_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛便為說汝有千子。",
        "from": "T21n1262_001:0291a27_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆為說千子名字。",
        "from": "T21n1262_001:0291a28_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百子在天上。",
        "from": "T21n1262_001:0291a28_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是鬼中王。",
        "from": "T21n1262_001:0291a28_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將鬼官屬。",
        "from": "T21n1262_001:0291a29_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嫉害嬈天民。",
        "from": "T21n1262_001:0291a29_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五百子在世間。",
        "from": "T21n1262_001:0291a29_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬈諸人民。",
        "from": "T21n1262_001:0291b01_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝子作鬼王。",
        "from": "T21n1262_001:0291b01_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將數萬鬼。",
        "from": "T21n1262_001:0291b01_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五百子。",
        "from": "T21n1262_001:0291b02_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將鬼官屬。",
        "from": "T21n1262_001:0291b02_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一不可稱數。",
        "from": "T21n1262_001:0291b02_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極嫉害惡。",
        "from": "T21n1262_001:0291b02_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自稱作樹木神者。",
        "from": "T21n1262_001:0291b03_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作地神者。",
        "from": "T21n1262_001:0291b03_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作水神者。",
        "from": "T21n1262_001:0291b03_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或詐為人兄弟妻子。",
        "from": "T21n1262_001:0291b04_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自怨枉家室內外者。",
        "from": "T21n1262_001:0291b04_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作海神者。",
        "from": "T21n1262_001:0291b05_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作船車神者。",
        "from": "T21n1262_001:0291b05_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作舍宅神者。",
        "from": "T21n1262_001:0291b05_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自稱夜在冥中神者。",
        "from": "T21n1262_001:0291b06_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使人夢寤者。",
        "from": "T21n1262_001:0291b06_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或使人恐怖。",
        "from": "T21n1262_001:0291b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為人作怪者。",
        "from": "T21n1262_001:0291b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或自稱星死者。",
        "from": "T21n1262_001:0291b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自稱病死者。",
        "from": "T21n1262_001:0291b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是耗亂人。",
        "from": "T21n1262_001:0291b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "適不在一處。",
        "from": "T21n1262_001:0291b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "極嫉害惡。",
        "from": "T21n1262_001:0291b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是矯稱。",
        "from": "T21n1262_001:0291b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人祠祀烹殺。",
        "from": "T21n1262_001:0291b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人不知多為烹殺。",
        "from": "T21n1262_001:0291b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飲食是鬼。",
        "from": "T21n1262_001:0291b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是鬼亦不肯食。",
        "from": "T21n1262_001:0291b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端嫉害欲使人犯殺。",
        "from": "T21n1262_001:0291b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲使人入地獄中。",
        "from": "T21n1262_001:0291b11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要不食之。",
        "from": "T21n1262_001:0291b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見人祠祀喜。",
        "from": "T21n1262_001:0291b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是鬼亦不能護活人命。",
        "from": "T21n1262_001:0291b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但益罪。",
        "from": "T21n1262_001:0291b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚癡人不知。",
        "from": "T21n1262_001:0291b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "坐鬼貧窮。",
        "from": "T21n1262_001:0291b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鬼子母聞佛說是語。",
        "from": "T21n1262_001:0291b13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即一心自悔。",
        "from": "T21n1262_001:0291b13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得須陀洹道。",
        "from": "T21n1262_001:0291b14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知方來去事。",
        "from": "T21n1262_001:0291b14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長跪白佛言。",
        "from": "T21n1262_001:0291b14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我愚癡不知。",
        "from": "T21n1262_001:0291b15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世世有惡乃爾。",
        "from": "T21n1262_001:0291b15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我得持戒。",
        "from": "T21n1262_001:0291b15_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟中正之道。",
        "from": "T21n1262_001:0291b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我心皆徹視。",
        "from": "T21n1262_001:0291b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還見我千子。",
        "from": "T21n1262_001:0291b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今我乃知。",
        "from": "T21n1262_001:0291b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛所至至語成。",
        "from": "T21n1262_001:0291b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛哀我。",
        "from": "T21n1262_001:0291b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我欲止佛精舍傍。",
        "from": "T21n1262_001:0291b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我欲呼千子王。",
        "from": "T21n1262_001:0291b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我欲使與佛結要。",
        "from": "T21n1262_001:0291b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我欲報復天上天下人恩。",
        "from": "T21n1262_001:0291b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉。",
        "from": "T21n1262_001:0291b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如汝有是意大善。",
        "from": "T21n1262_001:0291b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T21n1262_001:0291b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝從是已去。",
        "from": "T21n1262_001:0291b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當稱是語。",
        "from": "T21n1262_001:0291b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便止佛精舍邊。",
        "from": "T21n1262_001:0291b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其國中人民。",
        "from": "T21n1262_001:0291b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無子者來求子。",
        "from": "T21n1262_001:0291b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當與之子。",
        "from": "T21n1262_001:0291b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在所願。",
        "from": "T21n1262_001:0291b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當勅子姓。",
        "from": "T21n1262_001:0291b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與使隨護人。",
        "from": "T21n1262_001:0291b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得復妄嬈之。",
        "from": "T21n1262_001:0291b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲從鬼子母求願者。",
        "from": "T21n1262_001:0291b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名浮陀摩尼鉢。",
        "from": "T21n1262_001:0291b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "姊名炙匿。",
        "from": "T21n1262_001:0291b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天上天下鬼屬。",
        "from": "T21n1262_001:0291b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是摩尼鉢主四海內。",
        "from": "T21n1262_001:0291b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "船車治生有財產。",
        "from": "T21n1262_001:0291b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆屬摩尼鉢。",
        "from": "T21n1262_001:0291b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼鉢與佛結要。",
        "from": "T21n1262_001:0291b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受戒主護人財物。",
        "from": "T21n1262_001:0291b26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炙匿主人若有產生當救之。",
        "from": "T21n1262_001:0291b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有天王名毘沙門。",
        "from": "T21n1262_001:0291b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主四天地。",
        "from": "T21n1262_001:0291b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護人命。",
        "from": "T21n1262_001:0291b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出入常從毘沙門。",
        "from": "T21n1262_001:0291b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求願有大鬼王。",
        "from": "T21n1262_001:0291b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名阿須倫。",
        "from": "T21n1262_001:0291b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主諸龍王。",
        "from": "T21n1262_001:0291b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸毒氣人。",
        "from": "T21n1262_001:0291b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從求願令毒不干人。",
        "from": "T21n1262_001:0291b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求願當慈心無所用謝。",
        "from": "T21n1262_001:0291c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無所噉食人。",
        "from": "T21n1262_001:0291c01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從求願者在人何求與耳。",
        "from": "T21n1262_001:0291c02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要無所求索。",
        "from": "T21n1262_001:0291c02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不責人。",
        "from": "T21n1262_001:0291c03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人至浮陀摩尼鉢前為作禮。",
        "from": "T21n1262_001:0291c03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說鬼子母經",
        "from": "T21n1262_001:0291c04_0",
        "context_with_next_sentence": false
    }
]