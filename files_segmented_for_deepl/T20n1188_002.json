[
    {
        "source_sentence": "文殊所說最勝名義經卷下",
        "from": "T20n1188_002:0817b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋西天三藏明因妙善普濟法師",
        "from": "T20n1188_002:0817b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金總持等奉　詔譯",
        "from": "T20n1188_002:0817b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩薩妙智身　　出生諸善利",
        "from": "T20n1188_002:0817b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　譬如大地中　　生種種利益",
        "from": "T20n1188_002:0817b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切佛智藏　　流出大法輪",
        "from": "T20n1188_002:0817b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切法自性　　一切自性生",
        "from": "T20n1188_002:0817b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於一剎那中　　出生諸法義",
        "from": "T20n1188_002:0817b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　即得大智慧　　覺了一切法",
        "from": "T20n1188_002:0817b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　深入諸三昧　　安住平等理",
        "from": "T20n1188_002:0817b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸法悉清淨　　成正等正覺",
        "from": "T20n1188_002:0817b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切佛現前　　歡喜義成就",
        "from": "T20n1188_002:0817c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大智焰光明　　滅除諸罪垢",
        "from": "T20n1188_002:0817c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切眾生中　　作最上利益",
        "from": "T20n1188_002:0817c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　苦惱為怨賊　　智慧能戰勝",
        "from": "T20n1188_002:0817c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　精進殊勝相　　微妙大吉祥",
        "from": "T20n1188_002:0817c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　手臂長百肘　　舉足作無蹈",
        "from": "T20n1188_002:0817c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一足按金輪　　下至地絕處",
        "from": "T20n1188_002:0817c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一足覆梵際　　但見指爪甲",
        "from": "T20n1188_002:0817c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　最上勝自在　　法義無差別",
        "from": "T20n1188_002:0817c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　現種種色相　　起於方便智",
        "from": "T20n1188_002:0817c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　周遍十方界　　利益無窮盡",
        "from": "T20n1188_002:0817c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　已離輪迴染　　得三種大樂",
        "from": "T20n1188_002:0817c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　猶如清淨雲　　亦如秋皎月",
        "from": "T20n1188_002:0817c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如日離雲翳　　熾盛大光明",
        "from": "T20n1188_002:0817c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　帝釋大青寶　　莊嚴色最上",
        "from": "T20n1188_002:0817c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　及大如意珠　　亦為妙莊嚴",
        "from": "T20n1188_002:0817c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　所現神通力　　震動百世界",
        "from": "T20n1188_002:0817c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　住念處正勤　　神足及根力",
        "from": "T20n1188_002:0817c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　七覺妙意華　　了知八聖道",
        "from": "T20n1188_002:0817c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　趣正等菩提　　如來功德海",
        "from": "T20n1188_002:0817c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸眾生蘊聚　　猶如於虛空",
        "from": "T20n1188_002:0817c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸眾生心行　　種種相差別",
        "from": "T20n1188_002:0817c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　了知諸根境　　五蘊悉皆空",
        "from": "T20n1188_002:0817c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五蘊義清淨　　諸行無億數",
        "from": "T20n1188_002:0817c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　了知諸行相　　安住實際中",
        "from": "T20n1188_002:0817c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十二緣生法　　體性皆清淨",
        "from": "T20n1188_002:0817c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　四無量真乘　　八智從覺生",
        "from": "T20n1188_002:0817c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　了內根外境　　十二真實義",
        "from": "T20n1188_002:0817c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　祕密十六分　　覺道二十種",
        "from": "T20n1188_002:0817c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是最勝義　　諸佛皆了知",
        "from": "T20n1188_002:0818a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　佛現無數身　　常在於三昧",
        "from": "T20n1188_002:0818a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸眾生心行　　剎那能了知",
        "from": "T20n1188_002:0818a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三乘方便門　　清淨行微妙",
        "from": "T20n1188_002:0818a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　覺法無自性　　住於一乘道",
        "from": "T20n1188_002:0818a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　盡諸煩惱結　　出流轉苦海",
        "from": "T20n1188_002:0818a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　離繫縛稠林　　種種諸苦等",
        "from": "T20n1188_002:0818a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　方便智大悲　　能普遍饒益",
        "from": "T20n1188_002:0818a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　攝受諸有情　　令悟無生忍",
        "from": "T20n1188_002:0818a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切眾生心　　了境界清淨",
        "from": "T20n1188_002:0818a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切眾生心　　平等無差別",
        "from": "T20n1188_002:0818a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切眾生心　　歡喜生愛樂",
        "from": "T20n1188_002:0818a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　令離於散亂　　成就諸功德",
        "from": "T20n1188_002:0818a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　通達三世法　　一切智慧義",
        "from": "T20n1188_002:0818a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五蘊性本空　　三世皆如是",
        "from": "T20n1188_002:0818a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一念生信解　　即見諸佛性",
        "from": "T20n1188_002:0818a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸法自性生　　佛身本無著",
        "from": "T20n1188_002:0818a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　樂見諸佛身　　種種莊嚴相",
        "from": "T20n1188_002:0818a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　樂聞佛菩提　　最上真實義",
        "from": "T20n1188_002:0818a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大呪離文字　　大呪三種性",
        "from": "T20n1188_002:0818a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　泯覩離文字　　出生一切呪",
        "from": "T20n1188_002:0818a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　泯覩百字門　　五字門大空",
        "from": "T20n1188_002:0818a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　祕密十六種　　從泯覩中生",
        "from": "T20n1188_002:0818a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　葛羅非葛羅　　過去有四種",
        "from": "T20n1188_002:0818a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　禪義生億數　　一切作非作",
        "from": "T20n1188_002:0818a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　通達一切禪　　了知根本定",
        "from": "T20n1188_002:0818a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　三昧身最上　　報化身亦然",
        "from": "T20n1188_002:0818a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　普遍十方界　　化度諸眾生",
        "from": "T20n1188_002:0818a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　最後天中天　　吉祥最勝者",
        "from": "T20n1188_002:0818a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　為天人導師　　降伏諸魔眾",
        "from": "T20n1188_002:0818b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如帝釋天王　　能作大布施",
        "from": "T20n1188_002:0818b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　度一切眾生　　出煩惱稠林",
        "from": "T20n1188_002:0818b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十方皆讚歎　　唯一無有二",
        "from": "T20n1188_002:0818b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以大慈悲法　　為精進甲冑",
        "from": "T20n1188_002:0818b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　智慧為弓劍　　滅除煩惱賊",
        "from": "T20n1188_002:0818b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　最勝威神力　　眾魔皆怖畏",
        "from": "T20n1188_002:0818b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　降伏魔怨已　　興廣大供養",
        "from": "T20n1188_002:0818b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　稽首最上師　　諸佛護世者",
        "from": "T20n1188_002:0818b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　頂禮伸讚歎　　親近而奉事",
        "from": "T20n1188_002:0818b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十方虛空界　　供養亦如是",
        "from": "T20n1188_002:0818b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　文殊大吉祥　　菩薩摩訶薩",
        "from": "T20n1188_002:0818b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　具六通三明　　六念皆圓滿",
        "from": "T20n1188_002:0818b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　現大神通力　　智慧到彼岸",
        "from": "T20n1188_002:0818b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　遠離於輪迴　　永得不退轉",
        "from": "T20n1188_002:0818b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　勇猛大精進　　了知一切法",
        "from": "T20n1188_002:0818b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大補特伽羅　　得超最上地",
        "from": "T20n1188_002:0818b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以智慧法雨　　普潤諸眾生",
        "from": "T20n1188_002:0818b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　宣示四法印　　引導於三乘",
        "from": "T20n1188_002:0818b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　清淨最上義　　能作大吉祥",
        "from": "T20n1188_002:0818b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　菩薩所稱讚　　成就金剛尊",
        "from": "T20n1188_002:0818b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　眾生無億數　　瞻仰歸命禮",
        "from": "T20n1188_002:0818b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命大空藏　　讚歎大空藏",
        "from": "T20n1188_002:0818b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命佛覺道　　讚歎佛覺道",
        "from": "T20n1188_002:0818b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命諸佛身　　讚歎諸佛身",
        "from": "T20n1188_002:0818b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命佛欣悅　　讚歎佛欣悅",
        "from": "T20n1188_002:0818b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命佛功德　　讚歎佛功德",
        "from": "T20n1188_002:0818b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命諸佛念　　讚歎諸佛念",
        "from": "T20n1188_002:0818b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命佛喜笑　　讚歎佛喜笑",
        "from": "T20n1188_002:0818b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命諸佛語　　讚歎諸佛語",
        "from": "T20n1188_002:0818c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命佛所愛　　讚歎佛所愛",
        "from": "T20n1188_002:0818c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命諸佛生　　讚歎諸佛生",
        "from": "T20n1188_002:0818c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命佛智生　　讚歎佛智生",
        "from": "T20n1188_002:0818c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命佛戲舞　　讚歎佛戲舞",
        "from": "T20n1188_002:0818c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命一切智　　讚歎一切智",
        "from": "T20n1188_002:0818c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　性空及幻網　　歸讚亦如是",
        "from": "T20n1188_002:0818c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛掌菩薩摩訶薩。",
        "from": "T20n1188_002:0818c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛手菩薩摩訶薩。",
        "from": "T20n1188_002:0818c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1188_002:0818c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來智觀一切智身甚深微妙。",
        "from": "T20n1188_002:0818c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊菩薩摩訶薩。",
        "from": "T20n1188_002:0818c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切眾生。",
        "from": "T20n1188_002:0818c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此清淨最勝名義。",
        "from": "T20n1188_002:0818c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸眾生未淨三業。",
        "from": "T20n1188_002:0818c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令於佛地波羅蜜門福智藏中。",
        "from": "T20n1188_002:0818c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝持三業圓滿清淨。",
        "from": "T20n1188_002:0818c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於最上義未了解者令得了解。",
        "from": "T20n1188_002:0818c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一切諸佛法藏。",
        "from": "T20n1188_002:0818c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆為開發令得悟解。",
        "from": "T20n1188_002:0818c14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生眾善為功德門。",
        "from": "T20n1188_002:0818c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次金剛掌菩薩摩訶薩金剛手菩薩摩訶薩言。",
        "from": "T20n1188_002:0818c15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此最勝名義出生一切淨地波羅蜜法祕密神呪圓滿成就一切智智諸功德海。",
        "from": "T20n1188_002:0818c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨身語意三祕密門。",
        "from": "T20n1188_002:0818c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀想諸佛正等正覺。",
        "from": "T20n1188_002:0818c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就佛智大三摩地。",
        "from": "T20n1188_002:0818c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來清淨法界。",
        "from": "T20n1188_002:0818c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝十力破壞魔怨。",
        "from": "T20n1188_002:0818c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具一切智一切種智。",
        "from": "T20n1188_002:0818c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "根本方便饒益眾生。",
        "from": "T20n1188_002:0818c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德智藏清淨圓滿。",
        "from": "T20n1188_002:0818c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出生菩薩聲聞緣覺。",
        "from": "T20n1188_002:0818c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二乘聖種一切人天。",
        "from": "T20n1188_002:0818c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住大乘諸菩薩行。",
        "from": "T20n1188_002:0818c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入正聖道皆得解脫。",
        "from": "T20n1188_002:0818c23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復能增長菩薩善根。",
        "from": "T20n1188_002:0818c23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝受一切外道異論。",
        "from": "T20n1188_002:0818c24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威德摧伏四種魔怨。",
        "from": "T20n1188_002:0818c24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生同歸聖道。",
        "from": "T20n1188_002:0818c25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫繫縛離諸散亂。",
        "from": "T20n1188_002:0818c25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足一切諸善事業。",
        "from": "T20n1188_002:0818c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除輪迴得真聖道。",
        "from": "T20n1188_002:0818c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以妙香華幢幡傘蓋。",
        "from": "T20n1188_002:0818c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普遍供養一切如來。",
        "from": "T20n1188_002:0818c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速得成就諸呪部門。",
        "from": "T20n1188_002:0818c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸菩薩生愛樂想。",
        "from": "T20n1188_002:0818c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相應般若波羅蜜多。",
        "from": "T20n1188_002:0818c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知菩薩空行無二。",
        "from": "T20n1188_002:0818c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足一切波羅蜜藏。",
        "from": "T20n1188_002:0818c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿一切清淨佛地。",
        "from": "T20n1188_002:0819a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得四真諦聖智現前。",
        "from": "T20n1188_002:0819a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心安住四正念處。",
        "from": "T20n1188_002:0819a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至具足諸佛功德。",
        "from": "T20n1188_002:0819a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次金剛掌菩薩摩訶薩金剛手菩薩摩訶薩言。",
        "from": "T20n1188_002:0819a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此最勝名義經。",
        "from": "T20n1188_002:0819a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切眾生身語意業不善罪垢。",
        "from": "T20n1188_002:0819a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令得遠離一切惡道。",
        "from": "T20n1188_002:0819a05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及得斷除一切業障。",
        "from": "T20n1188_002:0819a05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八難怖畏皆得消除。",
        "from": "T20n1188_002:0819a06_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離惡眠夢獲大吉祥。",
        "from": "T20n1188_002:0819a06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及離一切諸魔怨結。",
        "from": "T20n1188_002:0819a07_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修諸善根福德利益。",
        "from": "T20n1188_002:0819a07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "斷除一切增上我慢。",
        "from": "T20n1188_002:0819a08_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永離一切苦惱輪轉。",
        "from": "T20n1188_002:0819a08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切佛心了知如實菩薩密行了知如實。",
        "from": "T20n1188_002:0819a09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三乘聖智了知如實。",
        "from": "T20n1188_002:0819a10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切呪印了知如實。",
        "from": "T20n1188_002:0819a10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最勝法義生大智慧。",
        "from": "T20n1188_002:0819a11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住安樂行色力自在。",
        "from": "T20n1188_002:0819a11_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得清淨大吉祥事。",
        "from": "T20n1188_002:0819a12_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踴躍以妙句偈。",
        "from": "T20n1188_002:0819a12_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稱揚讚歎此名義經。",
        "from": "T20n1188_002:0819a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能消除一切疾病及大恐怖。",
        "from": "T20n1188_002:0819a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若眾生心有所樂欲。",
        "from": "T20n1188_002:0819a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至心誦持最勝名義悉得如意。",
        "from": "T20n1188_002:0819a14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得清淨即得清淨。",
        "from": "T20n1188_002:0819a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得救護即得救護。",
        "from": "T20n1188_002:0819a15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲得富饒即得富饒。",
        "from": "T20n1188_002:0819a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如漂溺人得獲濟度。",
        "from": "T20n1188_002:0819a16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未得道者令得道果。",
        "from": "T20n1188_002:0819a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乘般若舟到菩提岸。",
        "from": "T20n1188_002:0819a17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如大醫王能除眾病。",
        "from": "T20n1188_002:0819a18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以方便智救護眾生。",
        "from": "T20n1188_002:0819a18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愚癡暗蔽悉得永離。",
        "from": "T20n1188_002:0819a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如意寶隨其所欲。",
        "from": "T20n1188_002:0819a19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益一切皆令圓滿。",
        "from": "T20n1188_002:0819a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊菩薩摩訶薩。",
        "from": "T20n1188_002:0819a20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如實了知一切智智。",
        "from": "T20n1188_002:0819a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足五眼六波羅蜜。",
        "from": "T20n1188_002:0819a21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得四無畏安住十地。",
        "from": "T20n1188_002:0819a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大福智藏三摩地門皆得圓滿。",
        "from": "T20n1188_002:0819a22_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如實了知法性無二。",
        "from": "T20n1188_002:0819a23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如實了知色相差別。",
        "from": "T20n1188_002:0819a23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如實了知種種億數色相清淨。",
        "from": "T20n1188_002:0819a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來自性悉皆空故。",
        "from": "T20n1188_002:0819a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名義經無二法義。",
        "from": "T20n1188_002:0819a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有受持開示顯發。",
        "from": "T20n1188_002:0819a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能利益一切眾生。",
        "from": "T20n1188_002:0819a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令離邪見煩惱稠林。",
        "from": "T20n1188_002:0819a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次金剛掌菩薩摩訶薩金剛手菩薩摩訶薩。",
        "from": "T20n1188_002:0819a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T20n1188_002:0819a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利菩薩摩訶薩。",
        "from": "T20n1188_002:0819a28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切如來智觀無二。",
        "from": "T20n1188_002:0819a28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此名義經最勝尊重。",
        "from": "T20n1188_002:0819a29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛頂中大摩尼珠。",
        "from": "T20n1188_002:0819a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人依此最勝祕密呪門句偈義理。",
        "from": "T20n1188_002:0819b01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日三時受持讀誦解說書寫。",
        "from": "T20n1188_002:0819b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益眾生顯示三乘普令悟入。",
        "from": "T20n1188_002:0819b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時文殊師利菩薩摩訶薩。",
        "from": "T20n1188_002:0819b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說此最勝名義。",
        "from": "T20n1188_002:0819b04_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲令眾生一心信受。",
        "from": "T20n1188_002:0819b04_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得勝解心了達一切最上法門。",
        "from": "T20n1188_002:0819b05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修無住行具足智慧。",
        "from": "T20n1188_002:0819b06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清淨三業發菩提心。",
        "from": "T20n1188_002:0819b06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛諸大菩薩。",
        "from": "T20n1188_002:0819b07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普皆示現平等法門。",
        "from": "T20n1188_002:0819b07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生皆得悟入。",
        "from": "T20n1188_002:0819b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大金剛掌菩薩現忿怒相。",
        "from": "T20n1188_002:0819b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大威力降伏魔怨。",
        "from": "T20n1188_002:0819b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普皆利益一切眾生令得安樂。",
        "from": "T20n1188_002:0819b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顯示祕密三昧道場一切呪印。",
        "from": "T20n1188_002:0819b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引導眾生入正定聚圓滿無餘。",
        "from": "T20n1188_002:0819b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明呪王除諸障難。",
        "from": "T20n1188_002:0819b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "消伏魔怨具大威德。",
        "from": "T20n1188_002:0819b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中常當擁護。",
        "from": "T20n1188_002:0819b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "童真菩薩梵王帝釋。",
        "from": "T20n1188_002:0819b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚕陀羅神那羅延天。",
        "from": "T20n1188_002:0819b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大自在天并子歌哩底計大黑天神。",
        "from": "T20n1188_002:0819b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難底計說囉大神焰魔天王。",
        "from": "T20n1188_002:0819b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水界大神毘沙門天王賀哩帝母。",
        "from": "T20n1188_002:0819b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中常當擁護。",
        "from": "T20n1188_002:0819b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行若住若坐若臥一切時中。",
        "from": "T20n1188_002:0819b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩威神加護。",
        "from": "T20n1188_002:0819b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛及諸菩薩。",
        "from": "T20n1188_002:0819b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "饒益攝受彼諸眾生。",
        "from": "T20n1188_002:0819b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身意語業皆得清淨。",
        "from": "T20n1188_002:0819b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切羅漢聲聞緣覺。",
        "from": "T20n1188_002:0819b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護念攝受彼諸眾生。",
        "from": "T20n1188_002:0819b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切法得無所畏。",
        "from": "T20n1188_002:0819b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是最勝名義聖法。",
        "from": "T20n1188_002:0819b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸經中最為上首。",
        "from": "T20n1188_002:0819b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能於此信解受持。",
        "from": "T20n1188_002:0819b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人則為得菩提道。",
        "from": "T20n1188_002:0819b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或處禪定或居闠鬧。",
        "from": "T20n1188_002:0819b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或入王城聚落之處。",
        "from": "T20n1188_002:0819b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江河園林一切住處。",
        "from": "T20n1188_002:0819b23_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於晝夜中常當擁護令無怖畏。",
        "from": "T20n1188_002:0819b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部人及非人。",
        "from": "T20n1188_002:0819b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至毘舍遮女并諸眷屬。",
        "from": "T20n1188_002:0819b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常當擁護是諸眾生。",
        "from": "T20n1188_002:0819b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令離諸惱得大安隱。",
        "from": "T20n1188_002:0819b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次金剛掌菩薩金剛手菩薩言。",
        "from": "T20n1188_002:0819b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此最勝名義經。",
        "from": "T20n1188_002:0819b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如佛頂珠最上微妙。",
        "from": "T20n1188_002:0819b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德殊勝不可思議。",
        "from": "T20n1188_002:0819b28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每日三時受持讀誦。",
        "from": "T20n1188_002:0819b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正念思惟精進無懈。",
        "from": "T20n1188_002:0819b29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛菩提速能趣入。",
        "from": "T20n1188_002:0819b29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復白佛言世尊。",
        "from": "T20n1188_002:0819c01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利菩薩色相具足觀察思惟。",
        "from": "T20n1188_002:0819c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大願力度脫眾生。",
        "from": "T20n1188_002:0819c02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於空中現一切佛。",
        "from": "T20n1188_002:0819c02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切菩薩種種化身。",
        "from": "T20n1188_002:0819c03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨順眾生所樂示現。",
        "from": "T20n1188_002:0819c03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演說微妙甚深句義。",
        "from": "T20n1188_002:0819c04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引導眾生遠離惡趣。",
        "from": "T20n1188_002:0819c04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生卑賤不墮邊境。",
        "from": "T20n1188_002:0819c05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生不醜陋不墮邪見。",
        "from": "T20n1188_002:0819c05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常生佛剎聽聞正法離無想處。",
        "from": "T20n1188_002:0819c06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生饑饉鬪戰劫中。",
        "from": "T20n1188_002:0819c06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生五濁及賊難處。",
        "from": "T20n1188_002:0819c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生貧窮及困苦處。",
        "from": "T20n1188_002:0819c07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生非法妄說句偈為求名聞。",
        "from": "T20n1188_002:0819c08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生賢善家尊貴人中。",
        "from": "T20n1188_002:0819c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿色相端嚴具足。",
        "from": "T20n1188_002:0819c09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見者歡喜無不愛樂。",
        "from": "T20n1188_002:0819c09_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得宿住通大尊重相具大威德。",
        "from": "T20n1188_002:0819c10_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布施持戒忍辱精進。",
        "from": "T20n1188_002:0819c11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪定智慧方便願力。",
        "from": "T20n1188_002:0819c11_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲喜捨四無量心具足圓滿。",
        "from": "T20n1188_002:0819c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及於一切工巧技藝。",
        "from": "T20n1188_002:0819c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚詠外書皆得了解。",
        "from": "T20n1188_002:0819c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出家求道於一切智心無散失。",
        "from": "T20n1188_002:0819c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了達三乘諸法義利。",
        "from": "T20n1188_002:0819c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經功德出生無量智慧無量善法。",
        "from": "T20n1188_002:0819c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T20n1188_002:0819c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦解其義理為人演說。",
        "from": "T20n1188_002:0819c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人於如來藏中得佛功德。",
        "from": "T20n1188_002:0819c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久當證最上菩提。",
        "from": "T20n1188_002:0819c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具一切智一切種智。",
        "from": "T20n1188_002:0819c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安住世間振大法鼓建大法幢。",
        "from": "T20n1188_002:0819c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作大法王演大法呪。",
        "from": "T20n1188_002:0819c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1188_002:0819c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)薩哩嚩(二合)達哩摩(二合引)阿婆(引)嚩娑嚩(二合)婆(引)嚩尾秫馱嚩惹囉(二合)遏阿暗噁一切法相自性清淨。",
        "from": "T20n1188_002:0819c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂一切如來智身。",
        "from": "T20n1188_002:0819c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊菩薩清淨出生。",
        "from": "T20n1188_002:0819c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T20n1188_002:0819c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噁薩哩嚩(二合)怛陀(引)誐多紇哩(二合)捺野喝囉喝囉唵(引)吽(引)紇哩(二合)文殊菩薩語自在王。",
        "from": "T20n1188_002:0819c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣大宣說一切法性。",
        "from": "T20n1188_002:0819c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如虛空圓滿清淨。",
        "from": "T20n1188_002:0819c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法界智藏呪曰。",
        "from": "T20n1188_002:0819c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿爾時金剛掌菩薩摩訶薩金剛手菩薩摩訶薩。",
        "from": "T20n1188_002:0819c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踴躍恭敬合掌瞻仰如來。",
        "from": "T20n1188_002:0819c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛聖眾大祕密王。",
        "from": "T20n1188_002:0819c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜稱讚廣大道場。",
        "from": "T20n1188_002:0820a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼佛。",
        "from": "T20n1188_002:0820a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讚金剛掌菩薩金剛手菩薩言。",
        "from": "T20n1188_002:0820a02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "T20n1188_002:0820a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能顯示文殊所說諸佛祕密最勝名義。",
        "from": "T20n1188_002:0820a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "利益安樂一切眾生。",
        "from": "T20n1188_002:0820a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等不久當得阿耨多羅三藐三菩提。",
        "from": "T20n1188_002:0820a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大解脫於幻網中作大導師。",
        "from": "T20n1188_002:0820a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大清淨甚深妙義開發引導。",
        "from": "T20n1188_002:0820a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生入佛境界。",
        "from": "T20n1188_002:0820a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今證明。",
        "from": "T20n1188_002:0820a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等所說最勝名義。",
        "from": "T20n1188_002:0820a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經於一萬六千大祕密教智慧藏中。",
        "from": "T20n1188_002:0820a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第流出為三摩地輪。",
        "from": "T20n1188_002:0820a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摧伏魔怨消除煩惱。",
        "from": "T20n1188_002:0820a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度諸眾生遠離輪迴到菩提岸。",
        "from": "T20n1188_002:0820a09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊所說最勝名義經卷下",
        "from": "T20n1188_002:0820a11_0",
        "context_with_next_sentence": false
    }
]