[
    {
        "source_sentence": "佛祖綱目卷第六(宙字號)",
        "from": "X85n1594_006:0572b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甲子(周幽王五年起)辛未(平王元年)王戌(桓王元年)癸亥(周桓王二年止)(庚子)四祖優波毱多傳法提多迦優波毱多。",
        "from": "X85n1594_006:0572b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叱利國人。",
        "from": "X85n1594_006:0572b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "(一名優波崛多。",
        "from": "X85n1594_006:0572b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一名鄔波毱多)姓首陀。",
        "from": "X85n1594_006:0572b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父善意。",
        "from": "X85n1594_006:0572b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七出家。",
        "from": "X85n1594_006:0572b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十證果。",
        "from": "X85n1594_006:0572b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨方行化。",
        "from": "X85n1594_006:0572b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至摩突羅國。",
        "from": "X85n1594_006:0572b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得度者甚眾。",
        "from": "X85n1594_006:0572b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繇是魔宮震動。",
        "from": "X85n1594_006:0572b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬愁怖。",
        "from": "X85n1594_006:0572b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂竭其魔力。",
        "from": "X85n1594_006:0572b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以害正法。",
        "from": "X85n1594_006:0572b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多即入三昧。",
        "from": "X85n1594_006:0572b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀其所繇。",
        "from": "X85n1594_006:0572b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬復何便。",
        "from": "X85n1594_006:0572b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "密持瓔珞。",
        "from": "X85n1594_006:0572b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "縻毱多頸。",
        "from": "X85n1594_006:0572b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及毱多出定。",
        "from": "X85n1594_006:0572b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取人狗蛇三屍。",
        "from": "X85n1594_006:0572b12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化為華鬘。",
        "from": "X85n1594_006:0572b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輭言諭波旬曰。",
        "from": "X85n1594_006:0572b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝與我瓔珞。",
        "from": "X85n1594_006:0572b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚是珍妙。",
        "from": "X85n1594_006:0572b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾此華鬘。",
        "from": "X85n1594_006:0572b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以相酬奉。",
        "from": "X85n1594_006:0572b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬大喜。",
        "from": "X85n1594_006:0572b14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "引頸受之。",
        "from": "X85n1594_006:0572b14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即變為三種臭屍。",
        "from": "X85n1594_006:0572b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蟲蛆壞爛。",
        "from": "X85n1594_006:0572b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬厭惡。",
        "from": "X85n1594_006:0572b15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大生憂惱。",
        "from": "X85n1594_006:0572b15_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡其神力。",
        "from": "X85n1594_006:0572b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫能移動。",
        "from": "X85n1594_006:0572b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃升六欲天。",
        "from": "X85n1594_006:0572b15_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告諸天主。",
        "from": "X85n1594_006:0572b16_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又詣梵王。",
        "from": "X85n1594_006:0572b16_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求其解免。",
        "from": "X85n1594_006:0572b16_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼各告言。",
        "from": "X85n1594_006:0572b16_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十力弟子所作神變。",
        "from": "X85n1594_006:0572b16_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我輩凡陋。",
        "from": "X85n1594_006:0572b17_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何能去之。",
        "from": "X85n1594_006:0572b17_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬曰。",
        "from": "X85n1594_006:0572b17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然則柰何。",
        "from": "X85n1594_006:0572b17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵王曰。",
        "from": "X85n1594_006:0572b18_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝可歸心尊者。",
        "from": "X85n1594_006:0572b18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能除斷。",
        "from": "X85n1594_006:0572b18_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃為說偈。",
        "from": "X85n1594_006:0572b18_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其回向曰。",
        "from": "X85n1594_006:0572b18_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若因地倒。",
        "from": "X85n1594_006:0572b19_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還因地起。",
        "from": "X85n1594_006:0572b19_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離地求起。",
        "from": "X85n1594_006:0572b19_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終無其理。",
        "from": "X85n1594_006:0572b19_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬受教已。",
        "from": "X85n1594_006:0572b19_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即下天宮。",
        "from": "X85n1594_006:0572b20_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至毱多所。",
        "from": "X85n1594_006:0572b20_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮足哀懺。",
        "from": "X85n1594_006:0572b20_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多曰。",
        "from": "X85n1594_006:0572b20_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝自今去。",
        "from": "X85n1594_006:0572b20_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於如來正法。",
        "from": "X85n1594_006:0572b21_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更作嬈害否。",
        "from": "X85n1594_006:0572b21_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬曰。",
        "from": "X85n1594_006:0572b21_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我誓回向佛道。",
        "from": "X85n1594_006:0572b21_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永斷不善。",
        "from": "X85n1594_006:0572b22_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多曰。",
        "from": "X85n1594_006:0572b22_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若然者。",
        "from": "X85n1594_006:0572b22_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝可口自唱言。",
        "from": "X85n1594_006:0572b22_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皈依三寶。",
        "from": "X85n1594_006:0572b22_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波旬合掌三唱。",
        "from": "X85n1594_006:0572b23_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "華鬘悉除。",
        "from": "X85n1594_006:0572b23_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃歡喜踴躍。",
        "from": "X85n1594_006:0572b23_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說偈曰。",
        "from": "X85n1594_006:0572b23_54",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首三界尊。",
        "from": "X85n1594_006:0572b24_55",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十力聖弟子。",
        "from": "X85n1594_006:0572b24_56",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今願回向。",
        "from": "X85n1594_006:0572b24_57",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿令有劣弱。",
        "from": "X85n1594_006:0572b24_58",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是偈已。",
        "from": "X85n1594_006:0572c01_59",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禮而去。",
        "from": "X85n1594_006:0572c01_60",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多一日。",
        "from": "X85n1594_006:0572c01_61",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因訪一老比丘尼。",
        "from": "X85n1594_006:0572c01_62",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔入門。",
        "from": "X85n1594_006:0572c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便觸撒鉢盂。",
        "from": "X85n1594_006:0572c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "比丘尼曰。",
        "from": "X85n1594_006:0572c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛在日。",
        "from": "X85n1594_006:0572c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六群比丘甚是麤行。",
        "from": "X85n1594_006:0572c02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "數來我舍。",
        "from": "X85n1594_006:0572c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚不如是。",
        "from": "X85n1594_006:0572c03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者紹祖位人。",
        "from": "X85n1594_006:0572c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得與麼麤行。",
        "from": "X85n1594_006:0572c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多乃休去。",
        "from": "X85n1594_006:0572c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多在世化導。",
        "from": "X85n1594_006:0572c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證果最多。",
        "from": "X85n1594_006:0572c04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每度一人。",
        "from": "X85n1594_006:0572c04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一籌置於石室。",
        "from": "X85n1594_006:0572c05_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其室縱十八肘。",
        "from": "X85n1594_006:0572c05_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣十二肘。",
        "from": "X85n1594_006:0572c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "充滿其間。",
        "from": "X85n1594_006:0572c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最後有一長者子。",
        "from": "X85n1594_006:0572c06_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名曰香眾。",
        "from": "X85n1594_006:0572c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來禮毱多。",
        "from": "X85n1594_006:0572c06_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求出家毱多問曰。",
        "from": "X85n1594_006:0572c06_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝身出家。",
        "from": "X85n1594_006:0572c07_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心出家。",
        "from": "X85n1594_006:0572c07_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X85n1594_006:0572c07_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我來出家。",
        "from": "X85n1594_006:0572c07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非為身心。",
        "from": "X85n1594_006:0572c07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多曰。",
        "from": "X85n1594_006:0572c08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不為身心。",
        "from": "X85n1594_006:0572c08_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復誰出家。",
        "from": "X85n1594_006:0572c08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "X85n1594_006:0572c08_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫出家者。",
        "from": "X85n1594_006:0572c08_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我我故。",
        "from": "X85n1594_006:0572c08_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無我我故。",
        "from": "X85n1594_006:0572c09_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即心不生滅。",
        "from": "X85n1594_006:0572c09_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不生滅。",
        "from": "X85n1594_006:0572c09_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是常道。",
        "from": "X85n1594_006:0572c09_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛亦常心無形相。",
        "from": "X85n1594_006:0572c09_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其體亦然。",
        "from": "X85n1594_006:0572c10_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多曰。",
        "from": "X85n1594_006:0572c10_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當大悟。",
        "from": "X85n1594_006:0572c10_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心自通達。",
        "from": "X85n1594_006:0572c10_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜依佛法僧。",
        "from": "X85n1594_006:0572c11_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "紹隆聖種。",
        "from": "X85n1594_006:0572c11_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即為剃髮。",
        "from": "X85n1594_006:0572c11_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授具足戒。",
        "from": "X85n1594_006:0572c11_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍告之曰。",
        "from": "X85n1594_006:0572c11_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝父嘗夢金日而生。",
        "from": "X85n1594_006:0572c12_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝可名提多迦。",
        "from": "X85n1594_006:0572c12_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復謂曰。",
        "from": "X85n1594_006:0572c12_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來以大法眼藏。",
        "from": "X85n1594_006:0572c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第傳授。",
        "from": "X85n1594_006:0572c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至於我。",
        "from": "X85n1594_006:0572c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今復付汝。",
        "from": "X85n1594_006:0572c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聽吾偈曰。",
        "from": "X85n1594_006:0572c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心自本來心。",
        "from": "X85n1594_006:0572c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本心非有法。",
        "from": "X85n1594_006:0572c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有法有本心。",
        "from": "X85n1594_006:0572c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非心非本法。",
        "from": "X85n1594_006:0572c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "付法已。",
        "from": "X85n1594_006:0572c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃踴身虗空。",
        "from": "X85n1594_006:0572c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呈十八變。",
        "from": "X85n1594_006:0572c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "却復本座。",
        "from": "X85n1594_006:0572c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跏趺而逝。",
        "from": "X85n1594_006:0572c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時號無相好佛。",
        "from": "X85n1594_006:0572c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提多迦以室內籌。",
        "from": "X85n1594_006:0572c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用焚其軀。",
        "from": "X85n1594_006:0572c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收舍利建塔。",
        "from": "X85n1594_006:0572c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當此土周平王三十年庚子歲也○有著身見者。",
        "from": "X85n1594_006:0572c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求度於毱多。",
        "from": "X85n1594_006:0572c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多曰。",
        "from": "X85n1594_006:0572c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求度之法。",
        "from": "X85n1594_006:0572c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要信吾言。",
        "from": "X85n1594_006:0572c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不違吾教。",
        "from": "X85n1594_006:0572c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人曰。",
        "from": "X85n1594_006:0572c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既來投。",
        "from": "X85n1594_006:0572c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師固當聞命。",
        "from": "X85n1594_006:0572c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多乃化一險崖山聳喬木。",
        "from": "X85n1594_006:0572c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令其上樹。",
        "from": "X85n1594_006:0572c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又於樹下。",
        "from": "X85n1594_006:0572c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化作大坑。",
        "from": "X85n1594_006:0572c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深廣千肘。",
        "from": "X85n1594_006:0572c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又令放脚。",
        "from": "X85n1594_006:0572c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人受教。",
        "from": "X85n1594_006:0572c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即放二脚。",
        "from": "X85n1594_006:0572c21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令放一手。",
        "from": "X85n1594_006:0572c21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便放一手。",
        "from": "X85n1594_006:0572c21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令復放手。",
        "from": "X85n1594_006:0572c22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人答言。",
        "from": "X85n1594_006:0572c22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復放手。",
        "from": "X85n1594_006:0572c22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便墮坑死。",
        "from": "X85n1594_006:0572c22_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毱多曰。",
        "from": "X85n1594_006:0572c22_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先約受教。",
        "from": "X85n1594_006:0572c23_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何違我。",
        "from": "X85n1594_006:0572c23_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時其人。",
        "from": "X85n1594_006:0572c23_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身愛即滅。",
        "from": "X85n1594_006:0572c23_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "放手而墮。",
        "from": "X85n1594_006:0572c23_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見樹坑。",
        "from": "X85n1594_006:0572c24_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即證道果。",
        "from": "X85n1594_006:0572c24_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛祖綱目卷第六",
        "from": "X85n1594_006:0573a02_0",
        "context_with_next_sentence": false
    }
]