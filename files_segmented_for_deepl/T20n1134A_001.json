[
    {
        "source_sentence": "金剛壽命陀羅尼經法",
        "from": "T20n1134A_001:0576a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特進試鴻臚卿大辨正",
        "from": "T20n1134A_001:0576a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣智三藏奉　詔譯",
        "from": "T20n1134A_001:0576a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘盧遮那如來。",
        "from": "T20n1134A_001:0576a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於色界頂第四禪成等正覺。",
        "from": "T20n1134A_001:0576a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即下須彌山頂。",
        "from": "T20n1134A_001:0576a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於金剛寶樓閣。",
        "from": "T20n1134A_001:0576a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡虛空遍法界一切如來皆悉雲集。",
        "from": "T20n1134A_001:0576a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前後圍繞。",
        "from": "T20n1134A_001:0576a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異口同音。",
        "from": "T20n1134A_001:0576a13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊轉妙法輪甚深祕密。",
        "from": "T20n1134A_001:0576a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂金剛界輪。",
        "from": "T20n1134A_001:0576a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降三世教令輪。",
        "from": "T20n1134A_001:0576a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍調伏法輪。",
        "from": "T20n1134A_001:0576a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切義成就輪。",
        "from": "T20n1134A_001:0576a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是四輪從毘盧遮那心出。",
        "from": "T20n1134A_001:0576a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一輪皆有三十七聖者。",
        "from": "T20n1134A_001:0576a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一真言一一三摩地。",
        "from": "T20n1134A_001:0576a17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一契印威儀軌持。",
        "from": "T20n1134A_001:0576a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲願力。",
        "from": "T20n1134A_001:0576a18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於雜染佛剎。",
        "from": "T20n1134A_001:0576a18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於淨妙世界。",
        "from": "T20n1134A_001:0576a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或隱或顯。",
        "from": "T20n1134A_001:0576a19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉輪利樂度諸眾生各各不同。",
        "from": "T20n1134A_001:0576a19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘盧遮那佛。",
        "from": "T20n1134A_001:0576a20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受諸如來請已。",
        "from": "T20n1134A_001:0576a20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲轉法輪時。",
        "from": "T20n1134A_001:0576a20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即入三摩地。",
        "from": "T20n1134A_001:0576a21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀見摩醯首羅大自在天。",
        "from": "T20n1134A_001:0576a21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剛強難化執持邪見。",
        "from": "T20n1134A_001:0576a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非我寂靜大悲之身堪任調伏。",
        "from": "T20n1134A_001:0576a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是世尊入悲怒三摩地。",
        "from": "T20n1134A_001:0576a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從胸臆間現出五峯金剛大菩提心。",
        "from": "T20n1134A_001:0576a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出四面八臂威德熾盛奇特難覩降三世金剛菩薩身。",
        "from": "T20n1134A_001:0576a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍禮毘盧遮那如來及一切諸佛。",
        "from": "T20n1134A_001:0576a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願世尊示教於我。",
        "from": "T20n1134A_001:0576a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何所作為。",
        "from": "T20n1134A_001:0576a27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告降三世菩薩。",
        "from": "T20n1134A_001:0576a27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝今降伏彼難調伏諸大天等。",
        "from": "T20n1134A_001:0576a27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令悉歸依諸佛法僧發菩提心。",
        "from": "T20n1134A_001:0576a28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尋即降伏一切諸天等。",
        "from": "T20n1134A_001:0576a29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡歸依佛法僧眾。",
        "from": "T20n1134A_001:0576a29_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯大自在天。",
        "from": "T20n1134A_001:0576b01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "特大威德而生拒敵。",
        "from": "T20n1134A_001:0576b01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降三世菩薩種種苦治乃至於死(左踏大天右踏天后)於是毘盧遮那如來。",
        "from": "T20n1134A_001:0576b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入大悲怒三摩地已。",
        "from": "T20n1134A_001:0576b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說金剛壽命陀羅尼。",
        "from": "T20n1134A_001:0576b03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復入金剛壽命三昧耶。",
        "from": "T20n1134A_001:0576b04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及結密印加持魔醯首羅天王。",
        "from": "T20n1134A_001:0576b04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還復得甦重增壽命。",
        "from": "T20n1134A_001:0576b05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依諸佛發菩提心。",
        "from": "T20n1134A_001:0576b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灌頂受記證得八地位。",
        "from": "T20n1134A_001:0576b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛壽命真言曰。",
        "from": "T20n1134A_001:0576b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵　嚩　日羅(二合)　喻　灑　娑嚩(二合)　賀佛告執金剛菩薩若有善男子善女人。",
        "from": "T20n1134A_001:0576b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持念誦每日三時時別千遍。",
        "from": "T20n1134A_001:0576b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過現所有惡業因緣短命夭壽。",
        "from": "T20n1134A_001:0576b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由持此真言故信心清淨。",
        "from": "T20n1134A_001:0576b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "業障消滅更增壽命。",
        "from": "T20n1134A_001:0576b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有修習三摩地者。",
        "from": "T20n1134A_001:0576b13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現生不轉父母生身。",
        "from": "T20n1134A_001:0576b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲五神通陵空自在。",
        "from": "T20n1134A_001:0576b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說三摩地門。",
        "from": "T20n1134A_001:0576b14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結跏趺坐閉目端身。",
        "from": "T20n1134A_001:0576b15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手重疊或結定印。",
        "from": "T20n1134A_001:0576b15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安於臍下。",
        "from": "T20n1134A_001:0576b16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於虛空中遍想諸佛了了分明。",
        "from": "T20n1134A_001:0576b16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自身中當其心上。",
        "from": "T20n1134A_001:0576b17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀淨滿月光明瑩徹。",
        "from": "T20n1134A_001:0576b17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有五股金剛杵形。",
        "from": "T20n1134A_001:0576b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒熾漸大如等己身。",
        "from": "T20n1134A_001:0576b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "變為降三世金剛菩薩。",
        "from": "T20n1134A_001:0576b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂有毘盧遮那如來。",
        "from": "T20n1134A_001:0576b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從佛身上遍體毛孔中。",
        "from": "T20n1134A_001:0576b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出白甘露灌注自身。",
        "from": "T20n1134A_001:0576b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次心中復觀。",
        "from": "T20n1134A_001:0576b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛薩埵菩薩。",
        "from": "T20n1134A_001:0576b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相好殊勝威德自在。",
        "from": "T20n1134A_001:0576b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結金剛壽命加持甲冑密印。",
        "from": "T20n1134A_001:0576b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以二手作金剛拳。",
        "from": "T20n1134A_001:0576b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以進力右押左。",
        "from": "T20n1134A_001:0576b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相鉤安於頂上。",
        "from": "T20n1134A_001:0576b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即誦壽命真言七遍。",
        "from": "T20n1134A_001:0576b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安於額前分手繫項。",
        "from": "T20n1134A_001:0576b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "繫項後已。",
        "from": "T20n1134A_001:0576b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直舒進力旋轉如環。",
        "from": "T20n1134A_001:0576b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下至心上相纏如繫甲勢。",
        "from": "T20n1134A_001:0576b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至背後復繫。",
        "from": "T20n1134A_001:0576b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來至臍兩膝腰後。",
        "from": "T20n1134A_001:0576b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當心二肩頂前頂後。",
        "from": "T20n1134A_001:0576b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復至額前重繫項後。",
        "from": "T20n1134A_001:0576b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛拳繫漸垂兩手。",
        "from": "T20n1134A_001:0576b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "徐徐而下如垂帶勢。",
        "from": "T20n1134A_001:0576b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當誦真言成被甲冑護身被甲真言曰。",
        "from": "T20n1134A_001:0576b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)(一)　碪(讁簪切)(二)　嚩　日囉(二合)　欲(三)由結密印加持威力。",
        "from": "T20n1134A_001:0576c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身如金剛堅固難壞。",
        "from": "T20n1134A_001:0576c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸災橫見者歡喜。",
        "from": "T20n1134A_001:0576c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切人民生大恭敬。",
        "from": "T20n1134A_001:0576c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次說護摩祕密之法。",
        "from": "T20n1134A_001:0576c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂除災延壽增益。",
        "from": "T20n1134A_001:0576c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吉祥富饒辯才無礙。",
        "from": "T20n1134A_001:0576c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至疫疾風雨等難。",
        "from": "T20n1134A_001:0576c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法作之一切願滿。",
        "from": "T20n1134A_001:0576c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩壇法有其四種。",
        "from": "T20n1134A_001:0576c07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂方圓三角蓮華。",
        "from": "T20n1134A_001:0576c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其所求依方而坐。",
        "from": "T20n1134A_001:0576c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本尊形像依法畫之。",
        "from": "T20n1134A_001:0576c08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗壇軌儀如餘部說。",
        "from": "T20n1134A_001:0576c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今略說除災延壽建壇之法。",
        "from": "T20n1134A_001:0576c09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨治一室堀深一肘。",
        "from": "T20n1134A_001:0576c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除去穢惡不淨之物灰骨瓦石。",
        "from": "T20n1134A_001:0576c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨土填之。",
        "from": "T20n1134A_001:0576c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一方壇量廣三肘。",
        "from": "T20n1134A_001:0576c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞿摩塗飾如淨鏡面掘地之時若得餘寶。",
        "from": "T20n1134A_001:0576c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大吉祥殊勝之相。",
        "from": "T20n1134A_001:0576c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求諸願速得成就。",
        "from": "T20n1134A_001:0576c13_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有異物。",
        "from": "T20n1134A_001:0576c14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當取河岸淨土填之平治如法。",
        "from": "T20n1134A_001:0576c14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸眾香和瞿摩夷重重塗飾。",
        "from": "T20n1134A_001:0576c15_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於壇中心用白粉。",
        "from": "T20n1134A_001:0576c15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作一肘半金剛甲冑。",
        "from": "T20n1134A_001:0576c16_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "中央穿作一肘火爐。",
        "from": "T20n1134A_001:0576c16_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或深半肘隨其所宜。",
        "from": "T20n1134A_001:0576c17_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若其不能穿之。",
        "from": "T20n1134A_001:0576c17_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取一火爐安於壇心亦得。",
        "from": "T20n1134A_001:0576c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印位華緣依位作之。",
        "from": "T20n1134A_001:0576c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "設壇既了懸諸幡蓋。",
        "from": "T20n1134A_001:0576c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安置金剛三世尊像。",
        "from": "T20n1134A_001:0576c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養之物華菓飲食布壇四周。",
        "from": "T20n1134A_001:0576c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護摩藥物置行者前。",
        "from": "T20n1134A_001:0576c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以四瓶置壇四角。",
        "from": "T20n1134A_001:0576c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面對本尊依法念誦。",
        "from": "T20n1134A_001:0576c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先辦乳木長十指量麁如大指二十一。",
        "from": "T20n1134A_001:0576c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘇搵兩頭爐中燒。",
        "from": "T20n1134A_001:0576c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "炭火熾盛。",
        "from": "T20n1134A_001:0576c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦延命真言加持火木。",
        "from": "T20n1134A_001:0576c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每誦一遍一擲火中。",
        "from": "T20n1134A_001:0576c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火既熾已。",
        "from": "T20n1134A_001:0576c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於光焰中觀作八葉蓮華。",
        "from": "T20n1134A_001:0576c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於胎中當觀字。",
        "from": "T20n1134A_001:0576c25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明晃曜遍照成大金剛壽命菩薩。",
        "from": "T20n1134A_001:0576c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以四字明引請菩薩。",
        "from": "T20n1134A_001:0576c26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "降入壇中受諸供養。",
        "from": "T20n1134A_001:0576c27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四字密言曰。",
        "from": "T20n1134A_001:0576c27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "弱　吽　鑁　斛",
        "from": "T20n1134A_001:0576c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以右手作半金剛印。",
        "from": "T20n1134A_001:0577a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以水灑火令淨。",
        "from": "T20n1134A_001:0577a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次取一器盛滿融蘇。",
        "from": "T20n1134A_001:0577a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取骨蔞草一千八莖乃至一百八莖。",
        "from": "T20n1134A_001:0577a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以搵其蘇。",
        "from": "T20n1134A_001:0577a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦壽命真言。",
        "from": "T20n1134A_001:0577a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其草數。",
        "from": "T20n1134A_001:0577a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一誦一擲於其火中。",
        "from": "T20n1134A_001:0577a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既擲盡已。",
        "from": "T20n1134A_001:0577a04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次復燒擲諸香乳酪。",
        "from": "T20n1134A_001:0577a04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前念誦課數畢已。",
        "from": "T20n1134A_001:0577a05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿杓傾蘇於火中。",
        "from": "T20n1134A_001:0577a05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初後如是。",
        "from": "T20n1134A_001:0577a06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能於三長齋月或自生月乃至生日作是供養。",
        "from": "T20n1134A_001:0577a06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除災難增益壽命。",
        "from": "T20n1134A_001:0577a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具大福智勝願圓滿。",
        "from": "T20n1134A_001:0577a07_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行來出入官位高遷。",
        "from": "T20n1134A_001:0577a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "富饒財寶皆悉稱意。",
        "from": "T20n1134A_001:0577a08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求男女并及聰明。",
        "from": "T20n1134A_001:0577a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當候大陽虧時加持牛蘇服之。",
        "from": "T20n1134A_001:0577a09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得若加持九節菖蒲。",
        "from": "T20n1134A_001:0577a10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令煙暖光等三相現已。",
        "from": "T20n1134A_001:0577a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而取服之即滿其願。",
        "from": "T20n1134A_001:0577a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日誦萬言辯說無礙。",
        "from": "T20n1134A_001:0577a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若求陵空隱顯自在。",
        "from": "T20n1134A_001:0577a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以牛黃一依前法無間加持。",
        "from": "T20n1134A_001:0577a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三相現已塗足點額。",
        "from": "T20n1134A_001:0577a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遊空自在。",
        "from": "T20n1134A_001:0577a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更求餘一切諸願。",
        "from": "T20n1134A_001:0577a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但於餘部隨心作之。",
        "from": "T20n1134A_001:0577a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆成就無不遂者。",
        "from": "T20n1134A_001:0577a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦使國土安寧無諸災疫。",
        "from": "T20n1134A_001:0577a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風雨以時人民安樂。",
        "from": "T20n1134A_001:0577a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切賢聖擁護其身。",
        "from": "T20n1134A_001:0577a16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說勝利不可窮盡。",
        "from": "T20n1134A_001:0577a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具如瑜伽經中廣說。",
        "from": "T20n1134A_001:0577a17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作息災延壽法。",
        "from": "T20n1134A_001:0577a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向北坐當作圓壇。",
        "from": "T20n1134A_001:0577a18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀諸聖眾悉皆白色。",
        "from": "T20n1134A_001:0577a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身著白衣供養白食。",
        "from": "T20n1134A_001:0577a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸供養具一切皆白燒沈香。",
        "from": "T20n1134A_001:0577a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作增益富饒法者。",
        "from": "T20n1134A_001:0577a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向東坐。",
        "from": "T20n1134A_001:0577a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身及聖尊及身衣服。",
        "from": "T20n1134A_001:0577a21_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并及供養食菓器物。",
        "from": "T20n1134A_001:0577a21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切黃色燒白檀香。",
        "from": "T20n1134A_001:0577a22_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作調伏法者。",
        "from": "T20n1134A_001:0577a22_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向南坐。",
        "from": "T20n1134A_001:0577a23_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身及本尊衣服供具。",
        "from": "T20n1134A_001:0577a23_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切盡皆深青黑色。",
        "from": "T20n1134A_001:0577a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒安息香。",
        "from": "T20n1134A_001:0577a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作敬愛法者。",
        "from": "T20n1134A_001:0577a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向西坐。",
        "from": "T20n1134A_001:0577a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身及本尊衣服供具。",
        "from": "T20n1134A_001:0577a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切盡皆赤色。",
        "from": "T20n1134A_001:0577a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒蘇合等香字。",
        "from": "T20n1134A_001:0577a26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是普賢延命種子也。",
        "from": "T20n1134A_001:0577a26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛壽命陀羅尼經法",
        "from": "T20n1134A_001:0577a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儀軌震部卷二。",
        "from": "T20n1134A_001:0577a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大明藏杜凾所入。",
        "from": "T20n1134A_001:0577a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛壽命陀羅尼念誦法。",
        "from": "T20n1134A_001:0577a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛智與沙門不空譯經。",
        "from": "T20n1134A_001:0577b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與此經同本。",
        "from": "T20n1134A_001:0577b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而脫落不少。",
        "from": "T20n1134A_001:0577b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今得天永元庚寅年。",
        "from": "T20n1134A_001:0577b02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "延寶四年五月。",
        "from": "T20n1134A_001:0577b03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天明元辛丑年五月。",
        "from": "T20n1134A_001:0577b03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三傳之本於智積院慈順僧正所檢閱。",
        "from": "T20n1134A_001:0577b04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "記其異冠。",
        "from": "T20n1134A_001:0577b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命工令壽。",
        "from": "T20n1134A_001:0577b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此本者。",
        "from": "T20n1134A_001:0577b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍照金剛及慈覺智證三師之請來。",
        "from": "T20n1134A_001:0577b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實為善本。",
        "from": "T20n1134A_001:0577b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時享和改元辛酉年秋八月二十一之夜。",
        "from": "T20n1134A_001:0577b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "校于燈下。",
        "from": "T20n1134A_001:0577b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　　　　　　豐山總持院沙門快道誌　一校加筆了　享和癸亥孟夏初六　　　　　慈順",
        "from": "T20n1134A_001:0577b09_0",
        "context_with_next_sentence": false
    }
]