[
    {
        "source_sentence": "摩利支天一印法",
        "from": "T21n1259_001:0285c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二印。",
        "from": "T21n1259_001:0285c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者身印。",
        "from": "T21n1259_001:0285c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者隱形印也。",
        "from": "T21n1259_001:0285c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先作身印當心誦真言七遍。",
        "from": "T21n1259_001:0285c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持身五處。",
        "from": "T21n1259_001:0285c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂五處者。",
        "from": "T21n1259_001:0285c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一心二額三左肩四右肩五頂是也。",
        "from": "T21n1259_001:0285c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身印相虛心合拳。",
        "from": "T21n1259_001:0285c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二中指指端屈纏著二頭指端。",
        "from": "T21n1259_001:0285c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大金剛輪印也真言曰。",
        "from": "T21n1259_001:0285c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)　摩　利　支　娑嚩　訶",
        "from": "T21n1259_001:0285c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次作隱形印當心念誦一百八遍。",
        "from": "T21n1259_001:0285c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依此印真言加持力故。",
        "from": "T21n1259_001:0285c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切天魔惡鬼不善邪魔外道不得其便。",
        "from": "T21n1259_001:0285c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無見其行者形體。",
        "from": "T21n1259_001:0285c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖求覓空盡邪力。",
        "from": "T21n1259_001:0285c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得持明行者形體。",
        "from": "T21n1259_001:0285c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故每七種所行時。",
        "from": "T21n1259_001:0285c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以印真言可加持。",
        "from": "T21n1259_001:0285c16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七種所行時者。",
        "from": "T21n1259_001:0285c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一睡眠時。",
        "from": "T21n1259_001:0285c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二覺悟時。",
        "from": "T21n1259_001:0285c17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三沐浴時。",
        "from": "T21n1259_001:0285c17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四遠行時。",
        "from": "T21n1259_001:0285c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五逢客時。",
        "from": "T21n1259_001:0285c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六飲食時。",
        "from": "T21n1259_001:0285c18_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七行廁時。",
        "from": "T21n1259_001:0285c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身印加持五處。",
        "from": "T21n1259_001:0285c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以身印想隱入金剛堅固智拳城中。",
        "from": "T21n1259_001:0285c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其隱形印。",
        "from": "T21n1259_001:0285c19_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手作虛掌。",
        "from": "T21n1259_001:0285c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拳之舒右掌五輪。",
        "from": "T21n1259_001:0285c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覆其左虛掌拳上。",
        "from": "T21n1259_001:0285c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不令間隙。",
        "from": "T21n1259_001:0285c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其心身。",
        "from": "T21n1259_001:0285c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T21n1259_001:0285c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引)　阿　爾　底也(引)　摩　利　支　娑嚩　賀摩利支天一印法",
        "from": "T21n1259_001:0285c23_0",
        "context_with_next_sentence": false
    }
]