[
    {
        "source_sentence": "金剛頂超勝三界經說文殊五",
        "from": "T20n1172_001:0709a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字真言勝相一卷",
        "from": "T20n1172_001:0709a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1172_001:0709a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑒",
        "from": "T20n1172_001:0709a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T20n1172_001:0709a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T20n1172_001:0709a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時金剛手菩薩摩訶薩等一切菩薩。",
        "from": "T20n1172_001:0709a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆於毘盧遮那佛前。",
        "from": "T20n1172_001:0709a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各自說心真言印。",
        "from": "T20n1172_001:0709a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是曼殊室唎菩薩摩訶薩。",
        "from": "T20n1172_001:0709a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從座而起白佛言。",
        "from": "T20n1172_001:0709a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我今為欲利益未來一切諸有情故。",
        "from": "T20n1172_001:0709a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速得成就摩訶般若波羅蜜多。",
        "from": "T20n1172_001:0709a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人纔誦一遍。",
        "from": "T20n1172_001:0709a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如誦八萬四千十二圍陀藏經。",
        "from": "T20n1172_001:0709a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦兩遍。",
        "from": "T20n1172_001:0709a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利普賢隨逐。",
        "from": "T20n1172_001:0709a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四眾圍遶加被。",
        "from": "T20n1172_001:0709a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是慈無畏護法善神在其人前。",
        "from": "T20n1172_001:0709a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難白文殊師利言。",
        "from": "T20n1172_001:0709a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當說此真言。",
        "from": "T20n1172_001:0709a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時有十萬億佛現。",
        "from": "T20n1172_001:0709a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸佛一一毛孔出十萬億菩薩。",
        "from": "T20n1172_001:0709a22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一菩薩毛孔各出十萬億龍王。",
        "from": "T20n1172_001:0709a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一龍王毛孔各出十萬億龍女。",
        "from": "T20n1172_001:0709a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一龍女毛孔復出十萬億青象。",
        "from": "T20n1172_001:0709a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一青象毛孔各出十萬億白象。",
        "from": "T20n1172_001:0709a25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一白象毛孔各出十萬億香象。",
        "from": "T20n1172_001:0709a26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一香象毛孔復出十萬億山象。",
        "from": "T20n1172_001:0709a27_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一山象毛孔。",
        "from": "T20n1172_001:0709a27_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復現十萬億寶院。",
        "from": "T20n1172_001:0709a28_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一寶院復現十萬億八功德水池。",
        "from": "T20n1172_001:0709a28_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其池四寶合成。",
        "from": "T20n1172_001:0709a29_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一寶池皆出十萬億閻浮檀金光。",
        "from": "T20n1172_001:0709b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復於一一光中皆現十萬億圓光。",
        "from": "T20n1172_001:0709b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一圓光化出十萬億天女。",
        "from": "T20n1172_001:0709b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚴持種種供養。",
        "from": "T20n1172_001:0709b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是殑伽沙數四眾。",
        "from": "T20n1172_001:0709b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時共集大會。",
        "from": "T20n1172_001:0709b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同音說此真言。",
        "from": "T20n1172_001:0709b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現三摩地三昧。",
        "from": "T20n1172_001:0709b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今略說少耳。",
        "from": "T20n1172_001:0709b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知其功德無量。",
        "from": "T20n1172_001:0709b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說五字真言曰。",
        "from": "T20n1172_001:0709b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿囉跛左曩",
        "from": "T20n1172_001:0709b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若善男子善女人。",
        "from": "T20n1172_001:0709b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有能持此真言。",
        "from": "T20n1172_001:0709b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "纔誦一遍即入如來一切法平等。",
        "from": "T20n1172_001:0709b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切文字亦皆平等。",
        "from": "T20n1172_001:0709b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速得成就摩訶般若。",
        "from": "T20n1172_001:0709b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸弟子受此心真言時。",
        "from": "T20n1172_001:0709b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令結密印。",
        "from": "T20n1172_001:0709b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二手金剛縛。",
        "from": "T20n1172_001:0709b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並建忍願屈上節如劍形。",
        "from": "T20n1172_001:0709b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印上承華散壇供養。",
        "from": "T20n1172_001:0709b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然應告言。",
        "from": "T20n1172_001:0709b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此心法門一切如來祕密最勝。",
        "from": "T20n1172_001:0709b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慎勿輕爾為愚人說。",
        "from": "T20n1172_001:0709b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "破汝三昧戒。",
        "from": "T20n1172_001:0709b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善諦思惟。",
        "from": "T20n1172_001:0709b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿者是無生義。",
        "from": "T20n1172_001:0709b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉者清淨無染離塵垢義。",
        "from": "T20n1172_001:0709b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跛者亦無第一義諦諸法平等義。",
        "from": "T20n1172_001:0709b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左者諸法無有諸行義。",
        "from": "T20n1172_001:0709b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩者諸法無有性相。",
        "from": "T20n1172_001:0709b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說言文字皆不可得義。",
        "from": "T20n1172_001:0709b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以曩字無有性相故。",
        "from": "T20n1172_001:0709b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左字無有諸行。",
        "from": "T20n1172_001:0709b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左字無諸行故。",
        "from": "T20n1172_001:0709b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跛字無第一義諦。",
        "from": "T20n1172_001:0709b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以跛字無第一義諦故。",
        "from": "T20n1172_001:0709b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囉字無有塵垢。",
        "from": "T20n1172_001:0709b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以囉字無有塵垢故。",
        "from": "T20n1172_001:0709b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿字法本不生義。",
        "from": "T20n1172_001:0709b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子當觀是心本來清淨無所染著。",
        "from": "T20n1172_001:0709b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離我我所分別之相。",
        "from": "T20n1172_001:0709b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入此門者名三摩地。",
        "from": "T20n1172_001:0709b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是真修習。",
        "from": "T20n1172_001:0709b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知是人如來印可。",
        "from": "T20n1172_001:0709b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殊勝功德不可思議。",
        "from": "T20n1172_001:0709b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦一遍能除行人一切苦難。",
        "from": "T20n1172_001:0709b27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦兩遍除滅億劫生死重罪。",
        "from": "T20n1172_001:0709b28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦三遍三昧現前。",
        "from": "T20n1172_001:0709b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦四遍總持不忘。",
        "from": "T20n1172_001:0709b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若誦五遍速成無上菩提。",
        "from": "T20n1172_001:0709c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能一心獨處閑靜。",
        "from": "T20n1172_001:0709c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵書五字輪壇。",
        "from": "T20n1172_001:0709c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依法念誦滿一月已。",
        "from": "T20n1172_001:0709c02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曼殊菩薩即現其身。",
        "from": "T20n1172_001:0709c02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於空中演說法要。",
        "from": "T20n1172_001:0709c03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時行者得宿命智。",
        "from": "T20n1172_001:0709c03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辯才無礙神足自在。",
        "from": "T20n1172_001:0709c04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勝願成就福智具足。",
        "from": "T20n1172_001:0709c04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速能階證如來法身。",
        "from": "T20n1172_001:0709c05_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但心信受。",
        "from": "T20n1172_001:0709c05_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經十六生決成正覺。",
        "from": "T20n1172_001:0709c06_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若力不辦建立壇場。",
        "from": "T20n1172_001:0709c06_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香花供養及畫本尊。",
        "from": "T20n1172_001:0709c07_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以用香泥塗舍利塔。",
        "from": "T20n1172_001:0709c07_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梵寫五字真言。",
        "from": "T20n1172_001:0709c08_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "旋遶念誦五十萬遍。",
        "from": "T20n1172_001:0709c08_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊菩薩現其人前而為說法。",
        "from": "T20n1172_001:0709c08_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當得宿命辯才。",
        "from": "T20n1172_001:0709c09_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來諸菩薩等。",
        "from": "T20n1172_001:0709c09_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及執金剛恒沙聖眾。",
        "from": "T20n1172_001:0709c10_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常加護念速滿諸願。",
        "from": "T20n1172_001:0709c10_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾證菩提。",
        "from": "T20n1172_001:0709c11_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣如金剛頂經說。",
        "from": "T20n1172_001:0709c11_41",
        "context_with_next_sentence": false
    }
]