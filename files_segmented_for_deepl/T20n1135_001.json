[
    {
        "source_sentence": "佛說一切如來金剛壽命陀羅尼",
        "from": "T20n1135_001:0578a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經",
        "from": "T20n1135_001:0578a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿肅國",
        "from": "T20n1135_001:0578a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "公食邑三千戶賜紫贈司空諡大鑑",
        "from": "T20n1135_001:0578a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正號大廣智大興善寺三藏沙門",
        "from": "T20n1135_001:0578a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不空奉　詔譯",
        "from": "T20n1135_001:0578a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1135_001:0578a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在殑伽河側。",
        "from": "T20n1135_001:0578a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸比丘及大菩薩無量天人大眾俱。",
        "from": "T20n1135_001:0578a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告毘沙門等四天王言。",
        "from": "T20n1135_001:0578a20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有四種法甚可怖畏。",
        "from": "T20n1135_001:0578a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男若女童男童女。",
        "from": "T20n1135_001:0578a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有情無能免者。",
        "from": "T20n1135_001:0578a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂生老病死。",
        "from": "T20n1135_001:0578a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中一法最為遍惱。",
        "from": "T20n1135_001:0578a23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難可對治。",
        "from": "T20n1135_001:0578a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂死怖。",
        "from": "T20n1135_001:0578a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我愍是故說對治法。",
        "from": "T20n1135_001:0578a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時四天王白佛言。",
        "from": "T20n1135_001:0578a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊我於今日為獲大利。",
        "from": "T20n1135_001:0578a25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願世尊為眾生故宣說是法。",
        "from": "T20n1135_001:0578a25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊面向東方。",
        "from": "T20n1135_001:0578a26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彈指召集一切如來。",
        "from": "T20n1135_001:0578a26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是誓言。",
        "from": "T20n1135_001:0578a27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有十方一切如來應正等覺。",
        "from": "T20n1135_001:0578a27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為眾生故。",
        "from": "T20n1135_001:0578a28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證菩提者咸皆助我。",
        "from": "T20n1135_001:0578a28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令我以一切如來威神力故。",
        "from": "T20n1135_001:0578a28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉令一切眾生轉非命業。",
        "from": "T20n1135_001:0578a29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使增壽命。",
        "from": "T20n1135_001:0578b01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我昔未為眾生轉此法輪。",
        "from": "T20n1135_001:0578b01_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於今方轉。",
        "from": "T20n1135_001:0578b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能令眾生壽命色力皆得成就無夭死怖。",
        "from": "T20n1135_001:0578b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是南西北方四維上下。",
        "from": "T20n1135_001:0578b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "召集驚告亦復如是。",
        "from": "T20n1135_001:0578b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時十方盡佛眼所到。",
        "from": "T20n1135_001:0578b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若干世界一切如來皆悉赴集。",
        "from": "T20n1135_001:0578b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿虛空數如微塵。",
        "from": "T20n1135_001:0578b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時一切諸佛為加持故。",
        "from": "T20n1135_001:0578b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "異口同音。",
        "from": "T20n1135_001:0578b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說一切如來金剛壽命陀羅尼曰。",
        "from": "T20n1135_001:0578b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛胵他(一)者犁(二)者攞者犁(三)彌娜知薩嚩(二合)薩底(二合)稽(四)斫訖浪(二合)蘖南(五)鉢囉(二合)舍滿都薩婆路誐(六)薩婆薩怛嚩(二合)南(七)阿娜[齒*束](八)俱娜[齒*來](九)遮[口*(隸-木+士)]遮[口*(隸-木+士)](十)係麼澆(牛矯反)哩(十一)係麼儞鏟尼(十二)係麼尸棄(十三)矯囉微(十四)矯囉迷(十五)係俱囉微(十六)信囉[口*犁](十七)俱麼底(十八)微捨麼抳(十九)戍戍毘嚩(二十)阿者犁(二十一)彌者犁(二十二)麼尾覽麼(二十三)戶毛戶毛(二十四)唵(二十五)麼折囉諭師(某甲二十六)薩嚩訶爾時十方佛所。",
        "from": "T20n1135_001:0578b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切執金剛菩薩異口同音亦說延命陀羅尼曰。",
        "from": "T20n1135_001:0578b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吽(引)吽(引)尸棄薩嚩(二合引)訶(引)如是一切如來及十方執金剛菩薩。",
        "from": "T20n1135_001:0578b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是陀羅尼已隱而不現。",
        "from": "T20n1135_001:0578b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時毘沙門天王白佛言。",
        "from": "T20n1135_001:0578b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦以佛神力。",
        "from": "T20n1135_001:0578b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切眾生加持護念。",
        "from": "T20n1135_001:0578b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除非命故說陀羅尼。",
        "from": "T20n1135_001:0578b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始尾(二合下同)帝(一)始尾怛犁(二)栗利爾時毘樓勒叉天王又白佛言。",
        "from": "T20n1135_001:0578b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦以佛神力。",
        "from": "T20n1135_001:0578b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為多眾生除夭命故。",
        "from": "T20n1135_001:0578b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說陀囉尼。",
        "from": "T20n1135_001:0578b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩登霓(一)摩登儗尼(二)輸麼輸麼謀(三)爾時提頭賴吒天王亦白佛言。",
        "from": "T20n1135_001:0578b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦為諸眾生。",
        "from": "T20n1135_001:0578b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除死怖故說陀羅尼。",
        "from": "T20n1135_001:0578c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "者[口*禮]者[口*禮](一)者囉[口*犁]爾時毘樓博叉天王亦白佛言。",
        "from": "T20n1135_001:0578c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦以佛神力故。",
        "from": "T20n1135_001:0578c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令一切眾生除非命故說陀羅尼。",
        "from": "T20n1135_001:0578c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未臨(一)麼麼麼",
        "from": "T20n1135_001:0578c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告四天王言。",
        "from": "T20n1135_001:0578c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有讀誦此經。",
        "from": "T20n1135_001:0578c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日日受持乃至一遍。",
        "from": "T20n1135_001:0578c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當敬彼善男子善女人。",
        "from": "T20n1135_001:0578c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應如佛想終不墮三惡道定增壽命。",
        "from": "T20n1135_001:0578c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人每日。",
        "from": "T20n1135_001:0578c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一切眾生轉讀此經。",
        "from": "T20n1135_001:0578c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終無夭死短命之怖。",
        "from": "T20n1135_001:0578c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無惡夢魘魅呪。",
        "from": "T20n1135_001:0578c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咀惡形羅剎鬼神之怖。",
        "from": "T20n1135_001:0578c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不為水火兵毒之所傷害。",
        "from": "T20n1135_001:0578c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸佛菩薩攝受護念其處。",
        "from": "T20n1135_001:0578c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦為佛所護持。",
        "from": "T20n1135_001:0578c12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說是經已。",
        "from": "T20n1135_001:0578c12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毘沙門天王等一切大眾。",
        "from": "T20n1135_001:0578c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T20n1135_001:0578c13_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說一切如來金剛壽命陀羅尼經",
        "from": "T20n1135_001:0578c14_0",
        "context_with_next_sentence": false
    }
]