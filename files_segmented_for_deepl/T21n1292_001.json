[
    {
        "source_sentence": "法華十羅剎法",
        "from": "T21n1292_001:0377a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先欲作印可入釋迦定印。",
        "from": "T21n1292_001:0377a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一道場印　二手檀慧內相交。",
        "from": "T21n1292_001:0377a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二水環。",
        "from": "T21n1292_001:0377a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各以二空押二火甲。",
        "from": "T21n1292_001:0377a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘度相拄頭即成。",
        "from": "T21n1292_001:0377a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明二十八品。",
        "from": "T21n1292_001:0377a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二法華印　以二水二火內相叉。",
        "from": "T21n1292_001:0377a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以檀度押力頭。",
        "from": "T21n1292_001:0377a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以風押智。",
        "from": "T21n1292_001:0377a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以禪捻慧。",
        "from": "T21n1292_001:0377a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成呪曰(菱印也)。",
        "from": "T21n1292_001:0377a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薩嚩達摩奔荼梨伽安爾曼爾",
        "from": "T21n1292_001:0377a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三羅剎　二手各以水鉤火背。",
        "from": "T21n1292_001:0377a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以空掘水甲。",
        "from": "T21n1292_001:0377a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以右風押左慧。",
        "from": "T21n1292_001:0377a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以右火押左力。",
        "from": "T21n1292_001:0377a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭又以右檀押左願即成。",
        "from": "T21n1292_001:0377a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1292_001:0377a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛姪他弭履弭履母樓母樓護娑婆賀",
        "from": "T21n1292_001:0377a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四以前印以左於上即成呪曰。",
        "from": "T21n1292_001:0377a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怛儞也他弭履弭履母護娑婆賀",
        "from": "T21n1292_001:0377a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五以先印各各別相。",
        "from": "T21n1292_001:0377a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背以檀慧相向。",
        "from": "T21n1292_001:0377a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以右風鉤左力。",
        "from": "T21n1292_001:0377a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成呪曰。",
        "from": "T21n1292_001:0377a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵八萬金剛童子迦那迦那護蘇護蘇罰罰罰(向鬼門雷電被打印也)六羅剎縛印　以前印。",
        "from": "T21n1292_001:0377a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以大指相交。",
        "from": "T21n1292_001:0377b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二水閒二地指押上。",
        "from": "T21n1292_001:0377b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以無二風屈頭鉤。",
        "from": "T21n1292_001:0377b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "押空頭成。",
        "from": "T21n1292_001:0377b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1292_001:0377b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊徥履(云云　如經說任理頌羅剎呪也)七修多羅藏印　二手外二火相交。",
        "from": "T21n1292_001:0377b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二水鉤上。",
        "from": "T21n1292_001:0377b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以二空頭相拄。",
        "from": "T21n1292_001:0377b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二地交內入。",
        "from": "T21n1292_001:0377b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1292_001:0377b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八羅剎護法華藏印　二手各自火後越。",
        "from": "T21n1292_001:0377b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二水鉤。",
        "from": "T21n1292_001:0377b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "風指相交。",
        "from": "T21n1292_001:0377b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又各二地二空理環成連集。",
        "from": "T21n1292_001:0377b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1292_001:0377b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵弊孕鑁友吒(他本蓮華也)",
        "from": "T21n1292_001:0377b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九羅剎微塵陀落印　二手各以二火鉤水背上。",
        "from": "T21n1292_001:0377b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即左右相搏取。",
        "from": "T21n1292_001:0377b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持右地押左力度面。",
        "from": "T21n1292_001:0377b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又右風押左智上。",
        "from": "T21n1292_001:0377b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以左慧押右禪上成。",
        "from": "T21n1292_001:0377b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1292_001:0377b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵呪罰智莎呵",
        "from": "T21n1292_001:0377b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十天地魔目大縛印　左手覆右手仰。",
        "from": "T21n1292_001:0377b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即以右地各勾左慧。",
        "from": "T21n1292_001:0377b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "叉以右水勾左願。",
        "from": "T21n1292_001:0377b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即各以空鎖連。",
        "from": "T21n1292_001:0377b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "背合以右火勾左方度。",
        "from": "T21n1292_001:0377b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又各以風相合成。",
        "from": "T21n1292_001:0377b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪曰。",
        "from": "T21n1292_001:0377b19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵莊叶都羅縛[棣木+齒]縛[棣木+齒]都禁縛禁嚩惡惡十羅剎形樣　藍婆形如藥叉。",
        "from": "T21n1292_001:0377b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣色青。",
        "from": "T21n1292_001:0377b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手獨股當右肩。",
        "from": "T21n1292_001:0377b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持念珠。",
        "from": "T21n1292_001:0377b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即立左膝當居彼上。",
        "from": "T21n1292_001:0377b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面肉色也　毘藍婆。",
        "from": "T21n1292_001:0377b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如龍王如圓滿月也。",
        "from": "T21n1292_001:0377b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如向大海。",
        "from": "T21n1292_001:0377b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把風雲。",
        "from": "T21n1292_001:0377b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把念珠也。",
        "from": "T21n1292_001:0377b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣色碧綠也。",
        "from": "T21n1292_001:0377b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面色白前立鏡臺也　曲齒形如天女仙。",
        "from": "T21n1292_001:0377b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣色青面伏低。",
        "from": "T21n1292_001:0377b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前捧香花長跪居。",
        "from": "T21n1292_001:0377b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半跏坐也　花齒。",
        "from": "T21n1292_001:0377b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如尼女。",
        "from": "T21n1292_001:0377b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣色紫色也。",
        "from": "T21n1292_001:0377b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把花。",
        "from": "T21n1292_001:0377b27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手把花盤面少低也　黑齒。",
        "from": "T21n1292_001:0377b28_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如神女。",
        "from": "T21n1292_001:0377b28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣色都妙色也。",
        "from": "T21n1292_001:0377b29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手取叉左手軍持也。",
        "from": "T21n1292_001:0377b29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如守護之形半跏坐也　多髮。",
        "from": "T21n1292_001:0377b29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如童子滿月肉色。",
        "from": "T21n1292_001:0377c01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾達女右手銅環取。",
        "from": "T21n1292_001:0377c02_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手如舞長跪居也。",
        "from": "T21n1292_001:0377c02_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無厭足形如頂經之形。",
        "from": "T21n1292_001:0377c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒守護衣色淺[口*孤]也　持瓔珞。",
        "from": "T21n1292_001:0377c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如吉祥天女。",
        "from": "T21n1292_001:0377c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左右之手持瓔珞也。",
        "from": "T21n1292_001:0377c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣色金也。",
        "from": "T21n1292_001:0377c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面色肉色結跏趺坐也　白幸帝。",
        "from": "T21n1292_001:0377c06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如頂鳴。",
        "from": "T21n1292_001:0377c07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "女形衣色紅青也。",
        "from": "T21n1292_001:0377c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手把裳。",
        "from": "T21n1292_001:0377c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手持獨股。",
        "from": "T21n1292_001:0377c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如打物形立膝居也　奪一切衣眾生精氣。",
        "from": "T21n1292_001:0377c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "形如梵王帝釋女。",
        "from": "T21n1292_001:0377c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帶鎧伏甲。",
        "from": "T21n1292_001:0377c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出頂馬頭他。",
        "from": "T21n1292_001:0377c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忿怒形。",
        "from": "T21n1292_001:0377c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手持杵左手持三股。",
        "from": "T21n1292_001:0377c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣色棶雜色也。",
        "from": "T21n1292_001:0377c10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結跏趺坐也。",
        "from": "T21n1292_001:0377c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八羅剎呪曰。",
        "from": "T21n1292_001:0377c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵帝履帝履[穴/又][穴/又]醯醯比吽鑁吽發吒十羅剎八龍王攝印護身呪　難陀拔難陀等部類眷屬皆護我可言加加加。",
        "from": "T21n1292_001:0377c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　序品方便譬喻品　　信解藥草授記品　化城五百弟子品　　授學無學法師品　多寶佛塔提婆品　　勸持安樂踊出品　如來壽量分別品　　隨喜法師功德品　不輕神力囑累品　　藥王妙音菩薩品　普門陀羅尼嚴王品　　普賢菩薩勸發品　(已上呪)。",
        "from": "T21n1292_001:0377c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十羅剎本心呪曰。",
        "from": "T21n1292_001:0377c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵怛姪他唵伊致明(一)伊致明底明(二)盧弭盧弭(三)薩馱訶薩馱訶(四)薩婆[非/云]婆呵又呪曰。",
        "from": "T21n1292_001:0377c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一唵縛羅陀縛特迷虎鉢娑呵",
        "from": "T21n1292_001:0377c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二唵呵羅呵羅里醯娑呵",
        "from": "T21n1292_001:0377c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三唵南陀吉羅尼莎呵",
        "from": "T21n1292_001:0377c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四唵帝摩里帝摩里莎訶",
        "from": "T21n1292_001:0378a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五唵鉢羅尼鉢羅尼尼陀里莎訶",
        "from": "T21n1292_001:0378a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六唵俱及陀羅尼陀羅尼莎訶",
        "from": "T21n1292_001:0378a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七唵娑里娑里迦羅帝迦羅帝莎呵",
        "from": "T21n1292_001:0378a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八唵儞陀儞陀契陀契陀羅尼莎呵",
        "from": "T21n1292_001:0378a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九唵百帝百帝真陀莎呵",
        "from": "T21n1292_001:0378a06_0",
        "context_with_next_sentence": false
    }
]