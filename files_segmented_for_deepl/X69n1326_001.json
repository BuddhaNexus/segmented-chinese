[
    {
        "source_sentence": " 五家語錄序",
        "from": "X69n1326_001:0021a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威音王已前。",
        "from": "X69n1326_001:0021a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有無量恒河沙祖佛出世。",
        "from": "X69n1326_001:0021a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以十方世界為口。",
        "from": "X69n1326_001:0021a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出無量言辭。",
        "from": "X69n1326_001:0021a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "談無量妙義。",
        "from": "X69n1326_001:0021a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而時人猶認三十二相迦陵仙音。",
        "from": "X69n1326_001:0021a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為佛真法身。",
        "from": "X69n1326_001:0021a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故萬不已。",
        "from": "X69n1326_001:0021a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱入化火道場。",
        "from": "X69n1326_001:0021a07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各遺舍利八斛四斗。",
        "from": "X69n1326_001:0021a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而輪王收取。",
        "from": "X69n1326_001:0021a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建造九級窣堵波。",
        "from": "X69n1326_001:0021a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至第九級結頂去不得處。",
        "from": "X69n1326_001:0021a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用千丈旃檀香木。",
        "from": "X69n1326_001:0021a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大數百圍。",
        "from": "X69n1326_001:0021a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從空錐下。",
        "from": "X69n1326_001:0021a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自塔之結角交加處。",
        "from": "X69n1326_001:0021a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "透頂透底。",
        "from": "X69n1326_001:0021a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聳立格外。",
        "from": "X69n1326_001:0021a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上有重重寶盤。",
        "from": "X69n1326_001:0021a11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "上之更上。",
        "from": "X69n1326_001:0021a11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復有銳然金頂。",
        "from": "X69n1326_001:0021a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離頂入空。",
        "from": "X69n1326_001:0021a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "湧出五色寶光。",
        "from": "X69n1326_001:0021a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每一一光。",
        "from": "X69n1326_001:0021a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各現出。",
        "from": "X69n1326_001:0021a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非青非黃。",
        "from": "X69n1326_001:0021a12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非赤白黑。",
        "from": "X69n1326_001:0021a12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歷辨而明。",
        "from": "X69n1326_001:0021a13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髏分不得之象。",
        "from": "X69n1326_001:0021a13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人數盡層級。",
        "from": "X69n1326_001:0021a13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "看透頂尖。",
        "from": "X69n1326_001:0021a13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辨析五光。",
        "from": "X69n1326_001:0021a14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心識路絕。",
        "from": "X69n1326_001:0021a14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不知不覺。",
        "from": "X69n1326_001:0021a14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "目眩耳鳴。",
        "from": "X69n1326_001:0021a14_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轟然若雷之破頂。",
        "from": "X69n1326_001:0021a14_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自己全身。",
        "from": "X69n1326_001:0021a15_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化為舍利之光。",
        "from": "X69n1326_001:0021a15_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終日在太虗空中。",
        "from": "X69n1326_001:0021a15_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為祥為瑞。",
        "from": "X69n1326_001:0021a16_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或隱或顯。",
        "from": "X69n1326_001:0021a16_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普使見聞眾生。",
        "from": "X69n1326_001:0021a16_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同入五光三昧。",
        "from": "X69n1326_001:0021a16_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫此一代時教。",
        "from": "X69n1326_001:0021a17_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五家宗旨之的派也。",
        "from": "X69n1326_001:0021a17_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何則。",
        "from": "X69n1326_001:0021a17_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崇高九級者。",
        "from": "X69n1326_001:0021a17_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五時之教義也。",
        "from": "X69n1326_001:0021a18_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人心思可到。",
        "from": "X69n1326_001:0021a18_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受用可著者也。",
        "from": "X69n1326_001:0021a18_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結頂處者。",
        "from": "X69n1326_001:0021a19_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來禪四句結縛。",
        "from": "X69n1326_001:0021a19_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三世諸佛。",
        "from": "X69n1326_001:0021a19_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口掛壁上。",
        "from": "X69n1326_001:0021a19_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大聦明人。",
        "from": "X69n1326_001:0021a20_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心如木石處也。",
        "from": "X69n1326_001:0021a20_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "千丈旃檀從空破頂者。",
        "from": "X69n1326_001:0021a20_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祖師禪向格外轉身。",
        "from": "X69n1326_001:0021a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打翻窠臼。",
        "from": "X69n1326_001:0021a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直截痛快。",
        "from": "X69n1326_001:0021a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是也。",
        "from": "X69n1326_001:0021a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重重寶盤上有金頂者。",
        "from": "X69n1326_001:0021a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉出轉高。",
        "from": "X69n1326_001:0021a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愈結愈銳。",
        "from": "X69n1326_001:0021a22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是也。",
        "from": "X69n1326_001:0021a22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此從迦葉破顏。",
        "from": "X69n1326_001:0021a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至六祖兩枝。",
        "from": "X69n1326_001:0021a23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各見此面目也。",
        "from": "X69n1326_001:0021a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離頂入空放五色光者。",
        "from": "X69n1326_001:0021a23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五派分宗。",
        "from": "X69n1326_001:0021a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各建旨趣之謂也。",
        "from": "X69n1326_001:0021a24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五色而非青黃赤白黑歷歷而明類分不得者。",
        "from": "X69n1326_001:0021a24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五家各出宗旨之面目也。",
        "from": "X69n1326_001:0021b01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令人看到教之級盡。",
        "from": "X69n1326_001:0021b02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗之頂尖。",
        "from": "X69n1326_001:0021b02_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心絕聞雷。",
        "from": "X69n1326_001:0021b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化為全身舍利之光。",
        "from": "X69n1326_001:0021b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在太虗空中為祥為瑞。",
        "from": "X69n1326_001:0021b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或隱或顯。",
        "from": "X69n1326_001:0021b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使見聞眾生同入五光三昧者。",
        "from": "X69n1326_001:0021b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入不思議境。",
        "from": "X69n1326_001:0021b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得大自在。",
        "from": "X69n1326_001:0021b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入魔入佛處也。",
        "from": "X69n1326_001:0021b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五宗之所以為教宗之最上一大事因緣也。",
        "from": "X69n1326_001:0021b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今之為佛弟子者。",
        "from": "X69n1326_001:0021b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入教則不能出於九級。",
        "from": "X69n1326_001:0021b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而透結頂。",
        "from": "X69n1326_001:0021b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入宗則限於錐木。",
        "from": "X69n1326_001:0021b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無重盤。",
        "from": "X69n1326_001:0021b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故聞五光者。",
        "from": "X69n1326_001:0021b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆怒而罵。",
        "from": "X69n1326_001:0021b08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "笑而唾曰。",
        "from": "X69n1326_001:0021b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼烏用此光而不色有。",
        "from": "X69n1326_001:0021b08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以復無為恠為妖。",
        "from": "X69n1326_001:0021b08_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "碎我身心者哉。",
        "from": "X69n1326_001:0021b09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是善出語者。",
        "from": "X69n1326_001:0021b09_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "貴迅捷。",
        "from": "X69n1326_001:0021b09_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善作用者。",
        "from": "X69n1326_001:0021b10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重孤硬。",
        "from": "X69n1326_001:0021b10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "致數五宗為旁出。",
        "from": "X69n1326_001:0021b10_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正旨為義學。",
        "from": "X69n1326_001:0021b10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剖其梨。",
        "from": "X69n1326_001:0021b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "削其語。",
        "from": "X69n1326_001:0021b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使臨濟雲門溈仰法眼曹洞之真兒孫。",
        "from": "X69n1326_001:0021b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭破腦裂。",
        "from": "X69n1326_001:0021b12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "務令法無防制。",
        "from": "X69n1326_001:0021b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道可胡傳。",
        "from": "X69n1326_001:0021b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則兒孫遍天下。",
        "from": "X69n1326_001:0021b12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五宗掃跡。",
        "from": "X69n1326_001:0021b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方為一大快事。",
        "from": "X69n1326_001:0021b13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫何更有靈山。",
        "from": "X69n1326_001:0021b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願再來。",
        "from": "X69n1326_001:0021b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護持末法正宗者。",
        "from": "X69n1326_001:0021b14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海昌之黎肩居士也。",
        "from": "X69n1326_001:0021b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參尋既久。",
        "from": "X69n1326_001:0021b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入法益深。",
        "from": "X69n1326_001:0021b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "救法如救頭然。",
        "from": "X69n1326_001:0021b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "護世如護眼目。",
        "from": "X69n1326_001:0021b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搜五家之語錄。",
        "from": "X69n1326_001:0021b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "梓以廣布於閻浮提。",
        "from": "X69n1326_001:0021c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間使有目者。",
        "from": "X69n1326_001:0021c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆見塔頂放光佛身常在。",
        "from": "X69n1326_001:0021c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入其目者。",
        "from": "X69n1326_001:0021c02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆得五光任運自如三昧。",
        "from": "X69n1326_001:0021c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此居士無功之功。",
        "from": "X69n1326_001:0021c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曷可以浮圖功德。",
        "from": "X69n1326_001:0021c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為較量者哉。",
        "from": "X69n1326_001:0021c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屬余序。",
        "from": "X69n1326_001:0021c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愧不能發五宗正趣。",
        "from": "X69n1326_001:0021c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聊於塔頂上。",
        "from": "X69n1326_001:0021c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加聚沙一掬。",
        "from": "X69n1326_001:0021c04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿使人更向五光中。",
        "from": "X69n1326_001:0021c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分無分有。",
        "from": "X69n1326_001:0021c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說是說非。",
        "from": "X69n1326_001:0021c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失取雙目。",
        "from": "X69n1326_001:0021c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吳門鄧蔚山天壽聖恩禪寺於密法藏和南撰 五宗源流圖東土初祖達摩二祖慧可三祖僧璨四祖道信五祖弘忍六祖慧能(大鑒下一世)南嶽讓(二世)馬祖一(三世)百丈海(四世)黃檗運(五世)臨濟玄溈山佑仰山寂(三世)天王悟(四世)龍潭信(五世)德山鑒(六世)雪峰存(七世)雲門偃玄沙備(八世)羅漢琛(九世)清涼益(大鑒下一世)青原思(二世)石頭遷(三世)藥山儼(四世)雲巖晟(五世)洞山价(六世)曹山寂自達磨西來。",
        "from": "X69n1326_001:0021c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以傳二祖。",
        "from": "X69n1326_001:0022a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二祖傳三祖。",
        "from": "X69n1326_001:0022a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三祖傳四祖。",
        "from": "X69n1326_001:0022a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四祖傳五祖。",
        "from": "X69n1326_001:0022a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五祖傳六祖。",
        "from": "X69n1326_001:0022a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六祖遂出南嶽青原二宗。",
        "from": "X69n1326_001:0022a02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南嶽出馬祖一。",
        "from": "X69n1326_001:0022a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一出百丈海。",
        "from": "X69n1326_001:0022a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海出黃檗運。",
        "from": "X69n1326_001:0022a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運得臨濟玄。",
        "from": "X69n1326_001:0022a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立正宗。",
        "from": "X69n1326_001:0022a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故成臨濟宗。",
        "from": "X69n1326_001:0022a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "丈次出溈山祐。",
        "from": "X69n1326_001:0022a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祐得仰山寂。",
        "from": "X69n1326_001:0022a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "共相唱和。",
        "from": "X69n1326_001:0022a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成溈仰宗。",
        "from": "X69n1326_001:0022a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次青原出石頭遷。",
        "from": "X69n1326_001:0022a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遷出藥山儼。",
        "from": "X69n1326_001:0022a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儼出雲巖晟。",
        "from": "X69n1326_001:0022a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晟出洞山价。",
        "from": "X69n1326_001:0022a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "价得曹山寂。",
        "from": "X69n1326_001:0022a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成曹洞宗。",
        "from": "X69n1326_001:0022a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬祖次出天王悟。",
        "from": "X69n1326_001:0022a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟出龍潭信。",
        "from": "X69n1326_001:0022a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信出德山鑒。",
        "from": "X69n1326_001:0022a07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鑒出雪峰存。",
        "from": "X69n1326_001:0022a07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存得雲門偃。",
        "from": "X69n1326_001:0022a08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成雲門宗。",
        "from": "X69n1326_001:0022a08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存次出玄沙備。",
        "from": "X69n1326_001:0022a08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備出羅漢琛。",
        "from": "X69n1326_001:0022a08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琛得清凉益。",
        "from": "X69n1326_001:0022a09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成法眼宗。",
        "from": "X69n1326_001:0022a09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然雲門法眼二宗。",
        "from": "X69n1326_001:0022a09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "景德傳燈。",
        "from": "X69n1326_001:0022a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謬收青原下。",
        "from": "X69n1326_001:0022a10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "據雪竇顯禪師塔銘。",
        "from": "X69n1326_001:0022a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敘為馬祖大寂九世孫。",
        "from": "X69n1326_001:0022a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逆遡以上。",
        "from": "X69n1326_001:0022a11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雪竇嗣智門祚。",
        "from": "X69n1326_001:0022a11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祚嗣香林遠。",
        "from": "X69n1326_001:0022a11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠嗣雪峰存。",
        "from": "X69n1326_001:0022a11_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲門既嗣雪峰。",
        "from": "X69n1326_001:0022a12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玄沙亦嗣雪峰。",
        "from": "X69n1326_001:0022a12_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則二宗俱屬馬大師。",
        "from": "X69n1326_001:0022a12_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無疑矣。",
        "from": "X69n1326_001:0022a13_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋時于越雲壑瑞禪師。",
        "from": "X69n1326_001:0022a13_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "集心燈錄。",
        "from": "X69n1326_001:0022a13_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦以二宗屬南嶽枝派。",
        "from": "X69n1326_001:0022a13_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世罕知者。",
        "from": "X69n1326_001:0022a14_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其書不甚流傳。",
        "from": "X69n1326_001:0022a14_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "葢時無開闢手眼。",
        "from": "X69n1326_001:0022a14_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踵謬襲訛。",
        "from": "X69n1326_001:0022a15_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫相訂正。",
        "from": "X69n1326_001:0022a15_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "茲閱五燈會元。",
        "from": "X69n1326_001:0022a15_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天王悟傳後。",
        "from": "X69n1326_001:0022a15_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "詳載其說。",
        "from": "X69n1326_001:0022a16_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綜覈源流。",
        "from": "X69n1326_001:0022a16_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "辨析明確。",
        "from": "X69n1326_001:0022a16_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "質諸金粟悟徑山信真寂印諸尊宿。",
        "from": "X69n1326_001:0022a16_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俱同是見。",
        "from": "X69n1326_001:0022a17_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰著五宗源流圖。",
        "from": "X69n1326_001:0022a17_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "改定傳後。",
        "from": "X69n1326_001:0022a18_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具擇法眼者。",
        "from": "X69n1326_001:0022a18_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當自證之。",
        "from": "X69n1326_001:0022a18_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 五家語錄序",
        "from": "X69n1326_001:0022b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五家語者。",
        "from": "X69n1326_001:0022b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自達磨西來至六傳。",
        "from": "X69n1326_001:0022b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "再四世。",
        "from": "X69n1326_001:0022b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法遍中華。",
        "from": "X69n1326_001:0022b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪備眾體。",
        "from": "X69n1326_001:0022b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "機語不一。",
        "from": "X69n1326_001:0022b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無心而分。",
        "from": "X69n1326_001:0022b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自成五家。",
        "from": "X69n1326_001:0022b06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂偽仰臨濟曹洞雲門法眼。",
        "from": "X69n1326_001:0022b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然機用雖有五家。",
        "from": "X69n1326_001:0022b07_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無非直指之旨。",
        "from": "X69n1326_001:0022b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今黎眉居士刻茲語錄。",
        "from": "X69n1326_001:0022b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復以前人辯證之言。",
        "from": "X69n1326_001:0022b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定為圖式。",
        "from": "X69n1326_001:0022b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可謂承上啟下。",
        "from": "X69n1326_001:0022b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先後包含。",
        "from": "X69n1326_001:0022b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "閱是語者。",
        "from": "X69n1326_001:0022b09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可盡五家差別之元。",
        "from": "X69n1326_001:0022b09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以明自己差別之智。",
        "from": "X69n1326_001:0022b10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總歸當人本地風光。",
        "from": "X69n1326_001:0022b10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "全機大用。",
        "from": "X69n1326_001:0022b10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出於文字之表。",
        "from": "X69n1326_001:0022b11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則雖見有五家兒孫。",
        "from": "X69n1326_001:0022b11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空王佛田厙奴。",
        "from": "X69n1326_001:0022b11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至狐狼野干到來。",
        "from": "X69n1326_001:0022b12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "總與三十棒。",
        "from": "X69n1326_001:0022b12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為甚如此。",
        "from": "X69n1326_001:0022b12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "者裏放過即不可。",
        "from": "X69n1326_001:0022b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "崇禎壬申孟春於明天童方丈密雲圓悟題并書 五家語錄序世尊在雪山。",
        "from": "X69n1326_001:0022b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "覩明星大悟。",
        "from": "X69n1326_001:0022b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歎曰。",
        "from": "X69n1326_001:0022b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奇哉眾生。",
        "from": "X69n1326_001:0022b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆具如來智慧德相。",
        "from": "X69n1326_001:0022b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但因妄想執著。",
        "from": "X69n1326_001:0022b23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而不證得。",
        "from": "X69n1326_001:0022b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二部葛藤。",
        "from": "X69n1326_001:0022b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從這裏起。",
        "from": "X69n1326_001:0022b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若論直指人心。",
        "from": "X69n1326_001:0022b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尚未會在。",
        "from": "X69n1326_001:0022b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟菩提達磨。",
        "from": "X69n1326_001:0022b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶較些子。",
        "from": "X69n1326_001:0022b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飄然一鉢。",
        "from": "X69n1326_001:0022c01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過我震旦。",
        "from": "X69n1326_001:0022c01_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教外別傳。",
        "from": "X69n1326_001:0022c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "接上上根。",
        "from": "X69n1326_001:0022c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "劈頭遇著不唧[口*留]漢。",
        "from": "X69n1326_001:0022c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拂袖空山九年冷坐。",
        "from": "X69n1326_001:0022c02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後得慧可。",
        "from": "X69n1326_001:0022c02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以續慧命。",
        "from": "X69n1326_001:0022c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遞相授受。",
        "from": "X69n1326_001:0022c03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至六祖大鑒。",
        "from": "X69n1326_001:0022c03_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣鉢不傳。",
        "from": "X69n1326_001:0022c03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "單傳其法。",
        "from": "X69n1326_001:0022c03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一花開五葉之讖。",
        "from": "X69n1326_001:0022c03_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其若是乎。",
        "from": "X69n1326_001:0022c04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六祖下出二枝。",
        "from": "X69n1326_001:0022c04_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南嶽讓。",
        "from": "X69n1326_001:0022c04_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青原思。",
        "from": "X69n1326_001:0022c04_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南嶽下出溈仰臨濟雲門法眼。",
        "from": "X69n1326_001:0022c05_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "青原下出曹洞。",
        "from": "X69n1326_001:0022c05_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "溈仰父子。",
        "from": "X69n1326_001:0022c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金針玉線。",
        "from": "X69n1326_001:0022c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有九十七種圓相。",
        "from": "X69n1326_001:0022c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟有四賓主四料揀四喝三玄三要。",
        "from": "X69n1326_001:0022c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一句中具三玄。",
        "from": "X69n1326_001:0022c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一玄中具三要。",
        "from": "X69n1326_001:0022c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有玄有要。",
        "from": "X69n1326_001:0022c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是吾宗旨。",
        "from": "X69n1326_001:0022c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即將建立法幢。",
        "from": "X69n1326_001:0022c08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高竪宗風。",
        "from": "X69n1326_001:0022c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "擾亂兜率陀天。",
        "from": "X69n1326_001:0022c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星飛斗散。",
        "from": "X69n1326_001:0022c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "臨濟光明赫赤。",
        "from": "X69n1326_001:0022c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誰敢正視。",
        "from": "X69n1326_001:0022c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將示滅。",
        "from": "X69n1326_001:0022c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說傳法偈曰。",
        "from": "X69n1326_001:0022c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沿流不止問如何。",
        "from": "X69n1326_001:0022c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真照無邊說似他。",
        "from": "X69n1326_001:0022c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離相離名人不稟。",
        "from": "X69n1326_001:0022c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吹毛用了急須磨。",
        "from": "X69n1326_001:0022c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沿流不止一句。",
        "from": "X69n1326_001:0022c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以讖源流久長。",
        "from": "X69n1326_001:0022c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘溈仰雲門法眼三宗。",
        "from": "X69n1326_001:0022c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "絕去久之。",
        "from": "X69n1326_001:0022c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖有其宗。",
        "from": "X69n1326_001:0022c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無其人也。",
        "from": "X69n1326_001:0022c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曹洞一宗。",
        "from": "X69n1326_001:0022c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "似乎影響之間。",
        "from": "X69n1326_001:0022c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "半似半不是。",
        "from": "X69n1326_001:0022c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若氷月霜花。",
        "from": "X69n1326_001:0022c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西風掃地。",
        "from": "X69n1326_001:0022c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必矣。",
        "from": "X69n1326_001:0022c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宋時大慧杲禪師。",
        "from": "X69n1326_001:0022c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作略諸方。",
        "from": "X69n1326_001:0022c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "柄殺活刀。",
        "from": "X69n1326_001:0022c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復起臨濟之風。",
        "from": "X69n1326_001:0022c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五逆聞雷。",
        "from": "X69n1326_001:0022c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不假言而自證。",
        "from": "X69n1326_001:0022c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "近時時道邊淺。",
        "from": "X69n1326_001:0022c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乾剝剝以為禪。",
        "from": "X69n1326_001:0022c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾不相聞有血盆口摩天俊鷂之奇。",
        "from": "X69n1326_001:0022c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不用棒喝。",
        "from": "X69n1326_001:0022c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超越古人之見。",
        "from": "X69n1326_001:0022c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生殺之機。",
        "from": "X69n1326_001:0022c18_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "毫無一語。",
        "from": "X69n1326_001:0022c18_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不是伊不濟。",
        "from": "X69n1326_001:0022c18_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只是伊不到闊綽無涯岸處。",
        "from": "X69n1326_001:0022c18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既立邊淺涯岸之鄉。",
        "from": "X69n1326_001:0022c19_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命根原不曾斷。",
        "from": "X69n1326_001:0022c19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "那得如古人。",
        "from": "X69n1326_001:0022c20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "星光四野。",
        "from": "X69n1326_001:0022c20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "月色一天。",
        "from": "X69n1326_001:0022c20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孤朗々地。",
        "from": "X69n1326_001:0022c20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無水不印。",
        "from": "X69n1326_001:0022c20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左之右之。",
        "from": "X69n1326_001:0022c21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所不備。",
        "from": "X69n1326_001:0022c21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嗟々今人有虗名之悟。",
        "from": "X69n1326_001:0022c21_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而無實證之功。",
        "from": "X69n1326_001:0022c21_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作用不如。",
        "from": "X69n1326_001:0022c22_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "霄壤相隔。",
        "from": "X69n1326_001:0022c22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宗門下衰。",
        "from": "X69n1326_001:0022c22_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "啾啾亂啼。",
        "from": "X69n1326_001:0022c22_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命殞昏暗。",
        "from": "X69n1326_001:0022c23_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不出俗人之手。",
        "from": "X69n1326_001:0022c23_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "習氣現前。",
        "from": "X69n1326_001:0022c23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難迯生死之關。",
        "from": "X69n1326_001:0022c23_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要如古宿上堂告寂。",
        "from": "X69n1326_001:0022c24_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在還家。",
        "from": "X69n1326_001:0022c24_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敢保驢年未夢見在。",
        "from": "X69n1326_001:0022c24_40",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海昌黎眉居士。",
        "from": "X69n1326_001:0023a01_41",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "留心此道有年。",
        "from": "X69n1326_001:0023a01_42",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不辭辛苦。",
        "from": "X69n1326_001:0023a01_43",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "參訪知識。",
        "from": "X69n1326_001:0023a01_44",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "領得歸來。",
        "from": "X69n1326_001:0023a02_45",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受用自如。",
        "from": "X69n1326_001:0023a02_46",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具擇法眼。",
        "from": "X69n1326_001:0023a02_47",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "備縱奪機。",
        "from": "X69n1326_001:0023a02_48",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遇其中人。",
        "from": "X69n1326_001:0023a02_49",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "露一言半句。",
        "from": "X69n1326_001:0023a03_50",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如玉在石。",
        "from": "X69n1326_001:0023a03_51",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "等閑人。",
        "from": "X69n1326_001:0023a03_52",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安能容易得知乎。",
        "from": "X69n1326_001:0023a03_53",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "搜尋五家尊宿語錄。",
        "from": "X69n1326_001:0023a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彚刻成編。",
        "from": "X69n1326_001:0023a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以垂不朽。",
        "from": "X69n1326_001:0023a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俾見者聞者。",
        "from": "X69n1326_001:0023a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發宿命智。",
        "from": "X69n1326_001:0023a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知有向上一著離相離名無位真人在。",
        "from": "X69n1326_001:0023a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不行生死之紆徑。",
        "from": "X69n1326_001:0023a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "高踏祖佛之頂[寧*頁]。",
        "from": "X69n1326_001:0023a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正法眼藏。",
        "from": "X69n1326_001:0023a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正欲開示。",
        "from": "X69n1326_001:0023a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空王佛出頭來。",
        "from": "X69n1326_001:0023a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作麼生商量。",
        "from": "X69n1326_001:0023a07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "田厙奴聻。",
        "from": "X69n1326_001:0023a07_6",
        "context_with_next_sentence": false
    }
]