[
    {
        "source_sentence": "佛說化珠保命真經",
        "from": "X01n0025_001:0415c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥王菩薩化理流布",
        "from": "X01n0025_001:0415c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "X01n0025_001:0415c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛在兜率羅天。",
        "from": "X01n0025_001:0415c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂摩尼珠[火*毘]燿如日。",
        "from": "X01n0025_001:0415c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "照見娑婆世界眾生因緣。",
        "from": "X01n0025_001:0415c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部恭敬瞻仰。",
        "from": "X01n0025_001:0415c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥王藥上二法王子。",
        "from": "X01n0025_001:0415c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從坐起頂禮。",
        "from": "X01n0025_001:0415c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言。",
        "from": "X01n0025_001:0415c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X01n0025_001:0415c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生疾厄不等。",
        "from": "X01n0025_001:0415c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即如[病丙+魯]痘。",
        "from": "X01n0025_001:0415c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盛行世間。",
        "from": "X01n0025_001:0415c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或生胎中。",
        "from": "X01n0025_001:0415c09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或在懷抱。",
        "from": "X01n0025_001:0415c09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或五六歲。",
        "from": "X01n0025_001:0415c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至長壯。",
        "from": "X01n0025_001:0415c10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "邪氣流染。",
        "from": "X01n0025_001:0415c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "存沒哀苦。",
        "from": "X01n0025_001:0415c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何因緣。",
        "from": "X01n0025_001:0415c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得此惡證。",
        "from": "X01n0025_001:0415c10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "X01n0025_001:0415c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "X01n0025_001:0415c11_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾當為汝細說如是因緣。",
        "from": "X01n0025_001:0415c11_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯摩尼珠。",
        "from": "X01n0025_001:0415c11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是歷劫來。",
        "from": "X01n0025_001:0415c12_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛之所凝結。",
        "from": "X01n0025_001:0415c12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虗空直陽。",
        "from": "X01n0025_001:0415c12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "主持世界中有情無情。",
        "from": "X01n0025_001:0415c12_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆被普照。",
        "from": "X01n0025_001:0415c13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在人身中。",
        "from": "X01n0025_001:0415c13_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性水精明。",
        "from": "X01n0025_001:0415c13_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "珠現其中。",
        "from": "X01n0025_001:0415c13_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如萬川月。",
        "from": "X01n0025_001:0415c14_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非一非多。",
        "from": "X01n0025_001:0415c14_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "靈光道炁。",
        "from": "X01n0025_001:0415c14_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爰[企止+了]陰陽。",
        "from": "X01n0025_001:0415c14_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣諸眾生。",
        "from": "X01n0025_001:0415c14_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無始劫來。",
        "from": "X01n0025_001:0415c15_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "薰染真性。",
        "from": "X01n0025_001:0415c15_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加以慾氣粗濁。",
        "from": "X01n0025_001:0415c15_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精血交媾。",
        "from": "X01n0025_001:0415c15_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以故受生之後。",
        "from": "X01n0025_001:0415c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真妄搏擊。",
        "from": "X01n0025_001:0415c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "清濁衡衡。",
        "from": "X01n0025_001:0415c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此性體中。",
        "from": "X01n0025_001:0415c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直摩尼珠。",
        "from": "X01n0025_001:0415c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "既有清濁明暗諸相。",
        "from": "X01n0025_001:0415c17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼形骸裡。",
        "from": "X01n0025_001:0415c17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幻摩尼珠。",
        "from": "X01n0025_001:0415c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遂有[病丙+魯]痘。",
        "from": "X01n0025_001:0415c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "厚薄諸證。",
        "from": "X01n0025_001:0415c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一法子再白佛言。",
        "from": "X01n0025_001:0415c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛性慈悲。",
        "from": "X01n0025_001:0415c18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼普照。",
        "from": "X01n0025_001:0415c18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合得生理。",
        "from": "X01n0025_001:0415c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣何沒故。",
        "from": "X01n0025_001:0415c19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告王子。",
        "from": "X01n0025_001:0415c19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真性不壞。",
        "from": "X01n0025_001:0415c19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生生不減。",
        "from": "X01n0025_001:0415c19_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣眾生心。",
        "from": "X01n0025_001:0415c20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妄起善惡。",
        "from": "X01n0025_001:0415c20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善氣輕清。",
        "from": "X01n0025_001:0415c20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡氣重濁。",
        "from": "X01n0025_001:0415c20_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "輕清為陽。",
        "from": "X01n0025_001:0415c20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重濁為陰。",
        "from": "X01n0025_001:0415c21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陽清珠明。",
        "from": "X01n0025_001:0415c21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性以制命。",
        "from": "X01n0025_001:0415c21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍居陽界。",
        "from": "X01n0025_001:0415c21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰濁珠暗。",
        "from": "X01n0025_001:0415c21_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命與性離。",
        "from": "X01n0025_001:0415c22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仍還陰界。",
        "from": "X01n0025_001:0415c22_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非摩珠有明有暗。",
        "from": "X01n0025_001:0415c22_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼眾生自別明暗。",
        "from": "X01n0025_001:0415c22_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰陽異路。",
        "from": "X01n0025_001:0415c23_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善惡異報。",
        "from": "X01n0025_001:0415c23_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[病丙+魯]痘存沒。",
        "from": "X01n0025_001:0415c23_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是不齊。",
        "from": "X01n0025_001:0415c23_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "藥王菩薩再起白佛。",
        "from": "X01n0025_001:0415c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頗有眾生。",
        "from": "X01n0025_001:0415c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現居陽界。",
        "from": "X01n0025_001:0415c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於[病丙+魯]痘時。",
        "from": "X01n0025_001:0415c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸惡鬼。",
        "from": "X01n0025_001:0416a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或男或女。",
        "from": "X01n0025_001:0416a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種惡形。",
        "from": "X01n0025_001:0416a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "提抱小兒。",
        "from": "X01n0025_001:0416a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或至水邊。",
        "from": "X01n0025_001:0416a01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或以船載。",
        "from": "X01n0025_001:0416a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為何以故。",
        "from": "X01n0025_001:0416a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現諸形像。",
        "from": "X01n0025_001:0416a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "X01n0025_001:0416a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一氣氤氳。",
        "from": "X01n0025_001:0416a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如水鴻潒。",
        "from": "X01n0025_001:0416a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "人處其中。",
        "from": "X01n0025_001:0416a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如魚游水。",
        "from": "X01n0025_001:0416a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰氣既多。",
        "from": "X01n0025_001:0416a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰神隨之。",
        "from": "X01n0025_001:0416a03_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神離其舍。",
        "from": "X01n0025_001:0416a03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而游鴻潒。",
        "from": "X01n0025_001:0416a04_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人捨宅。",
        "from": "X01n0025_001:0416a04_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而涉泛濫。",
        "from": "X01n0025_001:0416a04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至修善人。",
        "from": "X01n0025_001:0416a04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陽氣陽神。",
        "from": "X01n0025_001:0416a04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸善形。",
        "from": "X01n0025_001:0416a05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦復如是。",
        "from": "X01n0025_001:0416a05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼命終人。",
        "from": "X01n0025_001:0416a05_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過奈河橋。",
        "from": "X01n0025_001:0416a05_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有牛頭夜刃凶神。",
        "from": "X01n0025_001:0416a05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或有蓮華幢幡吉相。",
        "from": "X01n0025_001:0416a06_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦皆如是。",
        "from": "X01n0025_001:0416a06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惟心。",
        "from": "X01n0025_001:0416a06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更復何疑。",
        "from": "X01n0025_001:0416a07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時藥王菩薩再白佛言。",
        "from": "X01n0025_001:0416a07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬰兒始孩。",
        "from": "X01n0025_001:0416a07_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何惡業。",
        "from": "X01n0025_001:0416a07_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而得惡報。",
        "from": "X01n0025_001:0416a08_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又若善男信女。",
        "from": "X01n0025_001:0416a08_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將以何法。",
        "from": "X01n0025_001:0416a08_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而得消灾。",
        "from": "X01n0025_001:0416a08_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願佛慈悲更為演說。",
        "from": "X01n0025_001:0416a09_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俾諸眾生堅固善念。",
        "from": "X01n0025_001:0416a09_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "X01n0025_001:0416a09_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "X01n0025_001:0416a09_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝能諸眾生。",
        "from": "X01n0025_001:0416a10_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求大饒益。",
        "from": "X01n0025_001:0416a10_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今當說。",
        "from": "X01n0025_001:0416a10_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嬰孩何罪。",
        "from": "X01n0025_001:0416a10_39",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "緣世界眾生生死輪回。",
        "from": "X01n0025_001:0416a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前世惡業。",
        "from": "X01n0025_001:0416a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其性陰濁。",
        "from": "X01n0025_001:0416a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神氣不清淨。",
        "from": "X01n0025_001:0416a11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼不照。",
        "from": "X01n0025_001:0416a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以致[病丙+魯]亡。",
        "from": "X01n0025_001:0416a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又或父母先人惡業相感。",
        "from": "X01n0025_001:0416a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "陰陽相纏。",
        "from": "X01n0025_001:0416a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欠債殺生。",
        "from": "X01n0025_001:0416a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淫穢不淨。",
        "from": "X01n0025_001:0416a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "致兒命不保。",
        "from": "X01n0025_001:0416a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "死沈受河。",
        "from": "X01n0025_001:0416a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "哀苦痛哭。",
        "from": "X01n0025_001:0416a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受其罪報。",
        "from": "X01n0025_001:0416a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曾不知愆。",
        "from": "X01n0025_001:0416a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "服藥求醫。",
        "from": "X01n0025_001:0416a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨天尤人。",
        "from": "X01n0025_001:0416a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何益哉。",
        "from": "X01n0025_001:0416a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "X01n0025_001:0416a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常存善心。",
        "from": "X01n0025_001:0416a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈悲喜捨。",
        "from": "X01n0025_001:0416a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時持殺盜淫邪之戒。",
        "from": "X01n0025_001:0416a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便生端生有福之兒。",
        "from": "X01n0025_001:0416a16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神靈氣清。",
        "from": "X01n0025_001:0416a16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聦明智慧。",
        "from": "X01n0025_001:0416a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與摩尼珠。",
        "from": "X01n0025_001:0416a17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "渾合無二。",
        "from": "X01n0025_001:0416a17_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有何灾疹能侵。",
        "from": "X01n0025_001:0416a17_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人於兒子[病丙+魯]痘時。",
        "from": "X01n0025_001:0416a17_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頓發善心。",
        "from": "X01n0025_001:0416a18_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自悔惡業。",
        "from": "X01n0025_001:0416a18_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母齋戒。",
        "from": "X01n0025_001:0416a18_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不殺不淫。",
        "from": "X01n0025_001:0416a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香華燿燭。",
        "from": "X01n0025_001:0416a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晨夕頂禮。",
        "from": "X01n0025_001:0416a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦尊佛寶號。",
        "from": "X01n0025_001:0416a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及秘密神呪。",
        "from": "X01n0025_001:0416a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滿一千遍。",
        "from": "X01n0025_001:0416a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或五千四千餘遍。",
        "from": "X01n0025_001:0416a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即有佛聖降靈。",
        "from": "X01n0025_001:0416a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼照燿。",
        "from": "X01n0025_001:0416a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍守護。",
        "from": "X01n0025_001:0416a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡鬼逃亡。",
        "from": "X01n0025_001:0416a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "孩兒保命。",
        "from": "X01n0025_001:0416a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "父母延年。",
        "from": "X01n0025_001:0416a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大饒益。",
        "from": "X01n0025_001:0416a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思議。",
        "from": "X01n0025_001:0416a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時二法王子懽喜踊躍。",
        "from": "X01n0025_001:0416a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽首白佛。",
        "from": "X01n0025_001:0416a22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有人持誦佛號。",
        "from": "X01n0025_001:0416a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及神呪者。",
        "from": "X01n0025_001:0416a23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當分身。",
        "from": "X01n0025_001:0416a23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍至其處。",
        "from": "X01n0025_001:0416a23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普埀救護。",
        "from": "X01n0025_001:0416a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俾得安寧。",
        "from": "X01n0025_001:0416a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於是佛對藥王藥上二大菩薩。",
        "from": "X01n0025_001:0416a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而說呪曰。",
        "from": "X01n0025_001:0416b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵波羅蜜多唵悉達多般怛那",
        "from": "X01n0025_001:0416b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無毗婆尸佛南無尸棄佛",
        "from": "X01n0025_001:0416b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無毗舍浮佛南無拘留孫佛",
        "from": "X01n0025_001:0416b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無毗那含牟尼佛南無迦葉佛",
        "from": "X01n0025_001:0416b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無本師釋迦牟尼佛南無當來彌勒尊南無大慈大悲救苦救難靈感觀世音菩薩摩訶薩南無誓度眾生消灾增壽藥王藥上菩薩摩訶薩唵波羅蜜多唵悉多般怛那佛說化珠保命真經",
        "from": "X01n0025_001:0416b06_0",
        "context_with_next_sentence": false
    }
]