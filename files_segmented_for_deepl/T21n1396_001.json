[
    {
        "source_sentence": "佛說拔除罪障呪王經一卷",
        "from": "T21n1396_001:0913a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唐三藏法師義淨奉　制譯",
        "from": "T21n1396_001:0913a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T21n1396_001:0913a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時薄伽梵。",
        "from": "T21n1396_001:0913a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在淨居天上。",
        "from": "T21n1396_001:0913a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為諸大眾說微妙法。",
        "from": "T21n1396_001:0913a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時曼殊室利菩薩摩訶薩於大眾中。",
        "from": "T21n1396_001:0913a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從座起偏露右肩。",
        "from": "T21n1396_001:0913a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右膝著地合掌向佛。",
        "from": "T21n1396_001:0913a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瞻仰尊容目未曾捨。",
        "from": "T21n1396_001:0913a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言世尊。",
        "from": "T21n1396_001:0913a09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大法門甚深微妙實為希有。",
        "from": "T21n1396_001:0913a09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然於當來五濁世時。",
        "from": "T21n1396_001:0913a10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惡業眾生具貪恚癡。",
        "from": "T21n1396_001:0913a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欺誑不信樂行非法。",
        "from": "T21n1396_001:0913a11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不修戒品專行諂偽。",
        "from": "T21n1396_001:0913a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼於呪法所有威力神通變現。",
        "from": "T21n1396_001:0913a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸國土隨其方處。",
        "from": "T21n1396_001:0913a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求者獲益功德成就。",
        "from": "T21n1396_001:0913a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念誦儀軌祭祠法式。",
        "from": "T21n1396_001:0913a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆不信受亦不依行。",
        "from": "T21n1396_001:0913a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂非佛說而為謗毀惡障纏心。",
        "from": "T21n1396_001:0913a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便命過墮大地獄。",
        "from": "T21n1396_001:0913a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受極苦惱無有出期。",
        "from": "T21n1396_001:0913a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊此等眾生既遭大苦云何救護。",
        "from": "T21n1396_001:0913a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願慈悲拔濟令出。",
        "from": "T21n1396_001:0913a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時釋迦牟尼如來。",
        "from": "T21n1396_001:0913a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即便舒手摩曼殊室利法王子頂。",
        "from": "T21n1396_001:0913a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "告言善哉曼殊室利。",
        "from": "T21n1396_001:0913a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲利益一切有情。",
        "from": "T21n1396_001:0913a19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能問如來如是義利。",
        "from": "T21n1396_001:0913a20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝當諦聽極善作意。",
        "from": "T21n1396_001:0913a20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為汝說此呪法。",
        "from": "T21n1396_001:0913a21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安樂人天利益一切。",
        "from": "T21n1396_001:0913a21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸呪法最為殊勝。",
        "from": "T21n1396_001:0913a22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "祕密甚深順菩提道依真法界獲大功德。",
        "from": "T21n1396_001:0913a22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於臨終時必能憶此大力呪王。",
        "from": "T21n1396_001:0913a23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正念現前當生善處。",
        "from": "T21n1396_001:0913a24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T21n1396_001:0913a24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南麼薩婆怛他揭羝驃　阿羅喝驃　唵拘摩囉乎嚧比那　毘輸和　三婆婆阿揭車阿揭車　洛呼洛呼　勃纜勃攬　唅唅　氏那市(市一反)曼殊室利也　蘇室哩也　多賴也漫(引聲)薩婆毒溪驃癹癹　苫麼也苫麼也　阿弭都嗢婆菩嗢婆婆　波跛謎那勢也　莎訶曼殊室利。",
        "from": "T21n1396_001:0913a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此即是汝最勝根本呪藏心王神呪。",
        "from": "T21n1396_001:0913b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除一切苦惱厄難。",
        "from": "T21n1396_001:0913b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡罪障悉皆銷滅。",
        "from": "T21n1396_001:0913b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長壽無疾富樂無窮。",
        "from": "T21n1396_001:0913b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言詞聰辯眾人愛敬。",
        "from": "T21n1396_001:0913b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦能通利一切呪王。",
        "from": "T21n1396_001:0913b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時釋迦牟尼如來說是語時。",
        "from": "T21n1396_001:0913b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛剎土及此大地山河江海。",
        "from": "T21n1396_001:0913b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡有情界六種震動。",
        "from": "T21n1396_001:0913b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切惡趣餓鬼傍生。",
        "from": "T21n1396_001:0913b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "琰魔王界地獄有情。",
        "from": "T21n1396_001:0913b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦惱停息皆得安樂。",
        "from": "T21n1396_001:0913b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛復告曼殊室利。",
        "from": "T21n1396_001:0913b08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天若人若男若女。",
        "from": "T21n1396_001:0913b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在家出家諸有情類。",
        "from": "T21n1396_001:0913b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若能至心受持此呪王者。",
        "from": "T21n1396_001:0913b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不被惡人之所欺謗魔不得便。",
        "from": "T21n1396_001:0913b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨不能害毒不能傷。",
        "from": "T21n1396_001:0913b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切障礙毘那夜迦皆自退散。",
        "from": "T21n1396_001:0913b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其受持者應起此心。",
        "from": "T21n1396_001:0913b12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛世尊難思議法。",
        "from": "T21n1396_001:0913b12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯佛與佛乃能證知。",
        "from": "T21n1396_001:0913b13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今豈敢輒生誹謗。",
        "from": "T21n1396_001:0913b13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊說此法已。",
        "from": "T21n1396_001:0913b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼諸大眾并淨居天。",
        "from": "T21n1396_001:0913b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及餘諸天無量百千俱胝。",
        "from": "T21n1396_001:0913b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍藥叉健達婆阿蘇羅等。",
        "from": "T21n1396_001:0913b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禁呪成就諸大仙人。",
        "from": "T21n1396_001:0913b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及曼殊室利法王子。",
        "from": "T21n1396_001:0913b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀自在菩薩慈氏菩薩。",
        "from": "T21n1396_001:0913b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大勢至菩薩執金剛菩薩。",
        "from": "T21n1396_001:0913b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等諸大菩薩。",
        "from": "T21n1396_001:0913b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及阿僧祇世界諸佛剎土。",
        "from": "T21n1396_001:0913b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切如來并諸菩薩當共證知。",
        "from": "T21n1396_001:0913b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此大呪王是不思議法。",
        "from": "T21n1396_001:0913b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是大神呪是大明呪。",
        "from": "T21n1396_001:0913b21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無上呪是無等等呪。",
        "from": "T21n1396_001:0913b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能除罪苦障真實不虛。",
        "from": "T21n1396_001:0913b22_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時諸天大眾聞說呪王。",
        "from": "T21n1396_001:0913b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜頂戴奉持。",
        "from": "T21n1396_001:0913b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說拔除罪障呪王經",
        "from": "T21n1396_001:0913b25_0",
        "context_with_next_sentence": false
    }
]