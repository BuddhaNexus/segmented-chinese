        "from": "T19n0924C_001:0032c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次散念誦。",
        "from": "T19n0924C_001:0032c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先禮三寶。",
        "from": "T19n0924C_001:0032c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次禮本尊。",
        "from": "T19n0924C_001:0032c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後普禮諸佛。",
        "from": "T19n0924C_001:0032c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持誦人先麻座。",
        "from": "T19n0924C_001:0032c24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捺婆等座謂跏趺半跏賢麻座。",
        "from": "T19n0924C_001:0032c24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令身端直。",
        "from": "T19n0924C_001:0032c25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次把數珠。",
        "from": "T19n0924C_001:0032c25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右三迴轉。",
        "from": "T19n0924C_001:0032c25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次安珠二掌中頂戴(云云)次結根本印。",
        "from": "T19n0924C_001:0032c26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本真言(七遍)。",
        "from": "T19n0924C_001:0032c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次念誦了。",
        "from": "T19n0924C_001:0032c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即頂戴珠。",
        "from": "T19n0924C_001:0032c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發願安置本處。",
        "from": "T19n0924C_001:0032c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結根本印。",
        "from": "T19n0924C_001:0032c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦根本真言(七遍)。",
        "from": "T19n0924C_001:0032c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次更奉塗香華燒香飯燈。",
        "from": "T19n0924C_001:0032c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次運心供養。",
        "from": "T19n0924C_001:0032c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次讚。",
        "from": "T19n0924C_001:0032c29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉閼伽。",
        "from": "T19n0924C_001:0032c29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次發願。",
        "from": "T19n0924C_001:0033a01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次解界。",
        "from": "T19n0924C_001:0033a01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次示三昧。",
        "from": "T19n0924C_001:0033a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及禮拜諸尊。",
        "from": "T19n0924C_001:0033a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次誦現在諸如來之偈。",
        "from": "T19n0924C_001:0033a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉送真言。",
        "from": "T19n0924C_001:0033a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次迴九方便。",
        "from": "T19n0924C_001:0033a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次迴向。",
        "from": "T19n0924C_001:0033a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次三部三昧耶。",
        "from": "T19n0924C_001:0033a03_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次護身。",
        "from": "T19n0924C_001:0033a03_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次護本尊等。",
        "from": "T19n0924C_001:0033a03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次遍禮諸尊。",
        "from": "T19n0924C_001:0033a04_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後去。",
        "from": "T19n0924C_001:0033a04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當堂道場。",
        "from": "T19n0924C_001:0033a04_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "任意經行。",
        "from": "T19n0924C_001:0033a04_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小指檀慧(上左下右)無名指戒方(上左下右)中指忍願(上右下左)頭指進力(上左下右)大指禪智(上左下右)。",
        "from": "T19n0924C_001:0033a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善來尊者。",
        "from": "T19n0924C_001:0033a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善來尊者。",
        "from": "T19n0924C_001:0033a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由本願力。",
        "from": "T19n0924C_001:0033a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而來降赴。",
        "from": "T19n0924C_001:0033a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "顧乘加持。",
        "from": "T19n0924C_001:0033a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "授此請。",
        "from": "T19n0924C_001:0033a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并受我所辦諸供養。",
        "from": "T19n0924C_001:0033a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起大慈悲。",
        "from": "T19n0924C_001:0033a09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯垂納受。",
        "from": "T19n0924C_001:0033a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　現在諸如來　　救世諸菩薩",
        "from": "T19n0924C_001:0033a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不斷大乘教　　到殊勝位者",
        "from": "T19n0924C_001:0033a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　唯願聖天眾　　決定證知我",
        "from": "T19n0924C_001:0033a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　各當隨處安　　後時垂哀赴",
        "from": "T19n0924C_001:0033a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終頌。",
        "from": "T19n0924C_001:0033a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我今念誦祕密呪　　普通三界得清淨　願我生中除業障　　福德圓滿利有情　生生世世恒受持　　四恩眾生共成佛藥師儀軌一具(終　傳教大師將來)",
        "from": "T19n0924C_001:0033a16_0",
        "context_with_next_sentence": false
    }
]