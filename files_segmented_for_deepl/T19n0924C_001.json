[
    {
        "source_sentence": "藥師儀軌一具",
        "from": "T19n0924C_001:0030c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲入向道場者。",
        "from": "T19n0924C_001:0030c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先澡浴清淨。",
        "from": "T19n0924C_001:0030c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以真言手印。",
        "from": "T19n0924C_001:0030c05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "灑淨所換之衣。",
        "from": "T19n0924C_001:0030c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次以真言加持其衣而著。",
        "from": "T19n0924C_001:0030c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵微莾羅莎縛賀",
        "from": "T19n0924C_001:0030c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次即衣著。",
        "from": "T19n0924C_001:0030c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢哩嚩羅多嚩曰里尼吽",
        "from": "T19n0924C_001:0030c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次飲水灑淨。",
        "from": "T19n0924C_001:0030c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵摩訶入嚩羅吽",
        "from": "T19n0924C_001:0030c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次入向道場時。",
        "from": "T19n0924C_001:0030c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不起貪欲瞋恚等。",
        "from": "T19n0924C_001:0030c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專念本尊而住(住自他本清淨。",
        "from": "T19n0924C_001:0030c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "步步想踐連華徃)次至道場門外灑淨(云云)。",
        "from": "T19n0924C_001:0030c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次香水灑身。",
        "from": "T19n0924C_001:0030c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以後稱吽字三聲。",
        "from": "T19n0924C_001:0030c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後道場并觀想聖眾。",
        "from": "T19n0924C_001:0030c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿道場。",
        "from": "T19n0924C_001:0030c16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如影重之。",
        "from": "T19n0924C_001:0030c16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不相妨礙。",
        "from": "T19n0924C_001:0030c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次入道場已。",
        "from": "T19n0924C_001:0030c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "面向於本尊。",
        "from": "T19n0924C_001:0030c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌作禮。",
        "from": "T19n0924C_001:0030c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮三部尊。",
        "from": "T19n0924C_001:0030c17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及本尊已。",
        "from": "T19n0924C_001:0030c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舉頭諦觀佛菩薩金剛像也。",
        "from": "T19n0924C_001:0030c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向佛陳說三世宿愆惡障。",
        "from": "T19n0924C_001:0030c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "重懺本尊所。",
        "from": "T19n0924C_001:0030c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦如是懺禮。",
        "from": "T19n0924C_001:0030c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T19n0924C_001:0030c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　我從過去世　　流轉於生死",
        "from": "T19n0924C_001:0030c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　今對大聖尊　　盡心而懺悔",
        "from": "T19n0924C_001:0030c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如先佛所懺　　我今亦如是",
        "from": "T19n0924C_001:0030c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　願承加持力　　眾生悉清淨",
        "from": "T19n0924C_001:0030c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以此大願故　　自他獲無垢",
        "from": "T19n0924C_001:0030c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次香水淨三業。",
        "from": "T19n0924C_001:0030c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩嚩(二合)婆嚩輸馱薩嚩達麼薩嚩(二合)婆嚩輸度啥次定心已。",
        "from": "T19n0924C_001:0030c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作三部三昧耶。",
        "from": "T19n0924C_001:0030c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0030c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵怛他(引)蘖覩納婆(二合)縛(引)也娑嚩(二合引)訶(佛)唵跛娜謨(二合)納婆(二合)縛(引)也娑嚩(二合引)賀(蓮)唵嚩曰盧(二合)納婆(二合)縛也婆嚩(二合引)賀(引)(已上三部三昧耶)。",
        "from": "T19n0924C_001:0030c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次遣除身中毘那夜迦。",
        "from": "T19n0924C_001:0031a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵闍波沒[口*栗]帝賀囊迦囊吽泮吒次辟除諸障。",
        "from": "T19n0924C_001:0031a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩謨嚩日羅也吽賀囊莾他尾特夢婆輸[車*差]羅也泮吒(七遍)次辟除諸物所有諸難(在多意云云)真言曰。",
        "from": "T19n0924C_001:0031a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳哩枳哩羅嚩嗟捺羅[合*牛]泮吒(三遍)次去垢真言曰。",
        "from": "T19n0924C_001:0031a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳哩枳哩嚩日羅[合*牛]泮吒(七遍)次清淨。",
        "from": "T19n0924C_001:0031a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵爾囊爾迦",
        "from": "T19n0924C_001:0031a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次光澤。",
        "from": "T19n0924C_001:0031a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵帝誓帝惹徒尾寧徒提沙馱吽泮吒(三遍)次神分。",
        "from": "T19n0924C_001:0031a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次唱禮。",
        "from": "T19n0924C_001:0031a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切恭敬。",
        "from": "T19n0924C_001:0031a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次供文。",
        "from": "T19n0924C_001:0031a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次四方四佛四菩薩。",
        "from": "T19n0924C_001:0031a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次本尊稱名三遍。",
        "from": "T19n0924C_001:0031a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次三部界會一切佛菩薩等。",
        "from": "T19n0924C_001:0031a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次驚覺。",
        "from": "T19n0924C_001:0031a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩曰魯(二合用)底瑟吒(二合)吽(印者。",
        "from": "T19n0924C_001:0031a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手各金剛拳。",
        "from": "T19n0924C_001:0031a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舒頭指。",
        "from": "T19n0924C_001:0031a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各頭側相柱二小指相交。",
        "from": "T19n0924C_001:0031a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "每誦一遍。",
        "from": "T19n0924C_001:0031a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作印拳動)。",
        "from": "T19n0924C_001:0031a21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次九方便　至心歸命。",
        "from": "T19n0924C_001:0031a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命十方正等覺　　三世一切具三身　歸命一切大乘法　　歸命不退菩提眾　歸命諸明真實言　　歸命一切諸密印　以身口意清淨業　　慇懃無量恭敬禮歸命頂禮藥師瑠璃光佛。",
        "from": "T19n0924C_001:0031a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次禮方便。",
        "from": "T19n0924C_001:0031a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰(印者。",
        "from": "T19n0924C_001:0031a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以金剛部印。",
        "from": "T19n0924C_001:0031a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當額誦呪禮拜)。",
        "from": "T19n0924C_001:0031a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵(引一)曩莫薩縛怛他(引)蘗多(二)迦(引)野弭縛(引)吃質(二合)多(三)縛日羅(二合)滿娜南迦嚕弭次至心歸依。",
        "from": "T19n0924C_001:0031a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言(印者金剛合掌)次施身方便了。",
        "from": "T19n0924C_001:0031b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮。",
        "from": "T19n0924C_001:0031b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施身方便。",
        "from": "T19n0924C_001:0031b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言如例。",
        "from": "T19n0924C_001:0031b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印者。",
        "from": "T19n0924C_001:0031b03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌。",
        "from": "T19n0924C_001:0031b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "投身舒耳。",
        "from": "T19n0924C_001:0031b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若莊校投者。",
        "from": "T19n0924C_001:0031b04_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心耳。",
        "from": "T19n0924C_001:0031b04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至心發願了。",
        "from": "T19n0924C_001:0031b04_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮。",
        "from": "T19n0924C_001:0031b05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發菩提心方便真言。",
        "from": "T19n0924C_001:0031b05_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至心懺悔了。",
        "from": "T19n0924C_001:0031b05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸命頂禮。",
        "from": "T19n0924C_001:0031b06_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言(印者。",
        "from": "T19n0924C_001:0031b06_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先金剛合掌。",
        "from": "T19n0924C_001:0031b06_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大指頭指舒著)。",
        "from": "T19n0924C_001:0031b06_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次至心隨喜了。",
        "from": "T19n0924C_001:0031b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次歸命頂禮。",
        "from": "T19n0924C_001:0031b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言(印者。",
        "from": "T19n0924C_001:0031b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌)次至心勸請了。",
        "from": "T19n0924C_001:0031b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次歸命頂禮。",
        "from": "T19n0924C_001:0031b08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言(印者。",
        "from": "T19n0924C_001:0031b08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金剛合掌)次至心奉請誦了。",
        "from": "T19n0924C_001:0031b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次歸命頂禮。",
        "from": "T19n0924C_001:0031b09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言(先作向清印。",
        "from": "T19n0924C_001:0031b09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入內大指更出舌。",
        "from": "T19n0924C_001:0031b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭指鉤召)次至心迴向誦了。",
        "from": "T19n0924C_001:0031b10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次歸命頂禮。",
        "from": "T19n0924C_001:0031b10_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次真言如例(印金剛合掌云云)。",
        "from": "T19n0924C_001:0031b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次護身。",
        "from": "T19n0924C_001:0031b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日羅(二合引)嚚儞(二合)嚩羅捻跛跢(二合引)也娑縛(二合)賀次大護身真言曰。",
        "from": "T19n0924C_001:0031b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵入嚩羅囊也[合*牛]泮吒",
        "from": "T19n0924C_001:0031b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次被甲真言曰。",
        "from": "T19n0924C_001:0031b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵度比度比迦也度比鉢羅入嚩里寧娑婆賀次結髮。",
        "from": "T19n0924C_001:0031b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇悉地羯哩莎嚩賀",
        "from": "T19n0924C_001:0031b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結地界。",
        "from": "T19n0924C_001:0031b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里枳里嚩日羅(二合)縛曰里(二合)部律(二合半音)滿馱滿馱吽發吒(半音)次結方隅金剛幡。",
        "from": "T19n0924C_001:0031b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩羅薩羅縛日羅(二合)伽羅吽泮吒(半音)次置寶座等。",
        "from": "T19n0924C_001:0031b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵毘莾嚕挪地[合*牛](三遍)",
        "from": "T19n0924C_001:0031b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次阿者羅吽(三遍)。",
        "from": "T19n0924C_001:0031b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿者羅微[口*(肄聿+(企止+米))]娑婆賀(三遍)唵迦莾羅娑婆賀(三遍)唵囊莾薩嚩怛他蘖哆南薩羅嚩他欠烏跢蘖帝娑破羅呬莾闍伽伽囊釰娑嚩賀(三遍)次以我功德力。",
        "from": "T19n0924C_001:0031b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來加持力。",
        "from": "T19n0924C_001:0031c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以法界力。",
        "from": "T19n0924C_001:0031c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普供養而住。",
        "from": "T19n0924C_001:0031c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次大虛空藏普通供養真言并印。",
        "from": "T19n0924C_001:0031c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵誐誐曩三(去)婆嚩嚩日羅(二合)斛(引)次遏伽以辨事真言持誦。",
        "from": "T19n0924C_001:0031c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後以部母呪呪之。",
        "from": "T19n0924C_001:0031c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於左邊。",
        "from": "T19n0924C_001:0031c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次部母。",
        "from": "T19n0924C_001:0031c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜牟婆伽婆都烏瑟尼沙耶唵盧盧娑普嚧若嚩羅帝瑟佗悉陀嚕者儞薩嚩羅他娑但儞莎訶次於寶座上。",
        "from": "T19n0924C_001:0031c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀本尊種子字。",
        "from": "T19n0924C_001:0031c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "并結根本印(云云)。",
        "from": "T19n0924C_001:0031c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次轉字成本尊。",
        "from": "T19n0924C_001:0031c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "曩莫三滿多沒馱(引)南",
        "from": "T19n0924C_001:0031c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次欲請本尊。",
        "from": "T19n0924C_001:0031c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先執香爐。",
        "from": "T19n0924C_001:0031c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言。",
        "from": "T19n0924C_001:0031c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨治空中道路。",
        "from": "T19n0924C_001:0031c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵蘇悉地迦里入嚩(二合)里多南多暮羅(二合)曳入嚩(二合)羅滿馱滿馱賀囊賀囊[合*牛]泮吒次發願啟白眾。",
        "from": "T19n0924C_001:0031c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉請本尊。",
        "from": "T19n0924C_001:0031c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更先想座已也。",
        "from": "T19n0924C_001:0031c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成就車輅。",
        "from": "T19n0924C_001:0031c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "送本尊所(云云)。",
        "from": "T19n0924C_001:0031c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶車輅。",
        "from": "T19n0924C_001:0031c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵都嚕都嚕吽",
        "from": "T19n0924C_001:0031c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次請車輅。",
        "from": "T19n0924C_001:0031c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0031c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜麼悉底哩野(二合)地尾(二合)迦(引)南怛他(引)[(薩文+(立一))/木]多(引)南(引一)唵嚩曰郎(二合)銀儞野(三合引)羯唎沙也(二合)莎訶次召請奉本尊。",
        "from": "T19n0924C_001:0031c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真真言曰。",
        "from": "T19n0924C_001:0032a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵枳里枳里嚩日羅吽泮吒(七遍)",
        "from": "T19n0924C_001:0032a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次視三昧耶。",
        "from": "T19n0924C_001:0032a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵昇羯隸三莾烟莎訶(七遍)",
        "from": "T19n0924C_001:0032a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉閼伽。",
        "from": "T19n0924C_001:0032a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵藥乞私囊那也見捺羅達弩鉢捨莎訶(三遍)次奉蓮華座(誦三部真言。",
        "from": "T19n0924C_001:0032a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獻成用道用)。",
        "from": "T19n0924C_001:0032a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵味羅味羅也莎訶",
        "from": "T19n0924C_001:0032a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵鉢那莾味羅也莎訶",
        "from": "T19n0924C_001:0032a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵縛日羅味羅也莎訶",
        "from": "T19n0924C_001:0032a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次陳善來世尊真言。",
        "from": "T19n0924C_001:0032a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "持誦奉獻。",
        "from": "T19n0924C_001:0032a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結上方金剛網。",
        "from": "T19n0924C_001:0032a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾娑普(二合)羅捺落(二合)乞刃(二合)縛日羅(二合)半惹羅吽泮吒次上下方。",
        "from": "T19n0924C_001:0032a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次火院真言曰。",
        "from": "T19n0924C_001:0032a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿三莾擬儞吽泮吒",
        "from": "T19n0924C_001:0032a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次重結大界。",
        "from": "T19n0924C_001:0032a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵昇羯隸莾訶三摩焰莎訶(七遍)",
        "from": "T19n0924C_001:0032a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次奉塗香。",
        "from": "T19n0924C_001:0032a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032a20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伊[口*半]言馱輸婆儞微夜(二合)輸者也輸者輸者輸者也莾夜儞吠儞始薄訖底也(二合)鉢羅(二合)底仡[口*栗]呬也鉢羅(二合)四椰[口*半]唵阿賀羅阿賀羅薩羅縛尾儞夜達羅布爾帝莎訶(七遍)奉華真言伊[口*半]蘇莾囊素儞尾夜輪者輸輸者輸囊也莽夜寧唵伏儞多薄訖底(二合)夜鉢羅底仡[口*栗]呬也鉢羅四挪[口*半]唵阿賀羅阿賀羅薩羅縛尾儞夜達羅布爾帝莎訶奉燒香。",
        "from": "T19n0924C_001:0032a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿閻縛囊沙鉢底羅素賀[口*栗]儞也健陀值也蘇羅部若曩莽夜寧吠儞始薄訖底夜度報閻鉢羅底仡[口*栗]呬也難唵阿(去)賀羅阿賀羅薩羅縛尾儞夜達羅布爾莎訶奉食。",
        "from": "T19n0924C_001:0032b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "奧灑涕難羅素賀[口*栗](二合)他也(二合)[目*壹]灑涌多羅(二合)設怒麼里莽也寧唵吠儞始薄訖底夜(二合)鉢羅底仡呬也(二合)鉢羅四椰吽唵阿(去)羅賀阿羅賀薩羅縛尾儞夜(二合)達羅布爾帝莎訶奉燈。",
        "from": "T19n0924C_001:0032b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "羅訖蒭(二合)近曩(二合)灑者(二合)跛尾怛羅(二合)室者(二合)瞻謨尾曇莽曩(短名)輸婆莽夜寧吠儞始薄訖底夜(二合)儞報閻鉢羅底訖[口*栗](二合)呬也難唵阿路迦也薩羅尾儞夜(二合)達羅布爾帝莎婆賀次運心供養。",
        "from": "T19n0924C_001:0032b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以心運想。",
        "from": "T19n0924C_001:0032b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "水陸諸華。",
        "from": "T19n0924C_001:0032b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無曰主所。",
        "from": "T19n0924C_001:0032b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿虛空。",
        "from": "T19n0924C_001:0032b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡十方界。",
        "from": "T19n0924C_001:0032b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及以人天。",
        "from": "T19n0924C_001:0032b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙塗香雲。",
        "from": "T19n0924C_001:0032b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒香燈明。",
        "from": "T19n0924C_001:0032b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幢幡寶蓋。",
        "from": "T19n0924C_001:0032b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種鼓樂。",
        "from": "T19n0924C_001:0032b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歌舞妓唱。",
        "from": "T19n0924C_001:0032b19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真珠羅網。",
        "from": "T19n0924C_001:0032b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "懸諸寶領。",
        "from": "T19n0924C_001:0032b20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "髣白拂微妙聲。",
        "from": "T19n0924C_001:0032b20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鋒矜羯尼網。",
        "from": "T19n0924C_001:0032b20_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如意寶樹。",
        "from": "T19n0924C_001:0032b21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服之雲天。",
        "from": "T19n0924C_001:0032b21_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸廚膳上。",
        "from": "T19n0924C_001:0032b21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙香美味。",
        "from": "T19n0924C_001:0032b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宮殿樓閣寶柱。",
        "from": "T19n0924C_001:0032b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴身。",
        "from": "T19n0924C_001:0032b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭冠瓔珞。",
        "from": "T19n0924C_001:0032b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等雲。",
        "from": "T19n0924C_001:0032b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者運心。",
        "from": "T19n0924C_001:0032b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿虛空。",
        "from": "T19n0924C_001:0032b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以至誠心。",
        "from": "T19n0924C_001:0032b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是供養。",
        "from": "T19n0924C_001:0032b23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "最為勝上。",
        "from": "T19n0924C_001:0032b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故行者以決定心。",
        "from": "T19n0924C_001:0032b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而行此法。",
        "from": "T19n0924C_001:0032b24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心供養。",
        "from": "T19n0924C_001:0032b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦持真言。",
        "from": "T19n0924C_001:0032b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及作手印。",
        "from": "T19n0924C_001:0032b25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如上所想。",
        "from": "T19n0924C_001:0032b25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉成就。",
        "from": "T19n0924C_001:0032b26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0924C_001:0032b26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵薩羅嚩他龕搵娜[(薩文+(立一))/木]帝娑頗羅呬門伽伽曩釰莎訶(七遍)次讚歎佛曰。",
        "from": "T19n0924C_001:0032b27_0",
        "context_with_next_sentence": true
    },
    {
