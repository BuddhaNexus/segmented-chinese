[
    {
        "source_sentence": "金剛小引",
        "from": "X37n0674_011:0779b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禾中古資聖寺為鹽官安國師道場。",
        "from": "X37n0674_011:0779b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俍亭和尚重來繼席。",
        "from": "X37n0674_011:0779b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "國師嘗云。",
        "from": "X37n0674_011:0779b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虗空為鼓。",
        "from": "X37n0674_011:0779b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "須彌為椎。",
        "from": "X37n0674_011:0779b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚麼人打得。",
        "from": "X37n0674_011:0779b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俍亭和尚則豈非其人哉。",
        "from": "X37n0674_011:0779b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海內嚮慕翹勤非一日矣。",
        "from": "X37n0674_011:0779b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夏五之月大周直叩座前飽聆禪悅。",
        "from": "X37n0674_011:0779b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真炎暑中一帙清涼散也。",
        "from": "X37n0674_011:0779b22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別後挂帆而北。",
        "from": "X37n0674_011:0779b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好一柄犀牛扇子直扇清風過于虎阜。",
        "from": "X37n0674_011:0779b23_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "快甚快甚。",
        "from": "X37n0674_011:0779b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至讀所為金剛妙義。",
        "from": "X37n0674_011:0779b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則橫說竪說。",
        "from": "X37n0674_011:0779b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "權說實說。",
        "from": "X37n0674_011:0779c01_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七縱八橫。",
        "from": "X37n0674_011:0779c01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翻天倒地。",
        "from": "X37n0674_011:0779c01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所不說。",
        "from": "X37n0674_011:0779c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究未嘗說。",
        "from": "X37n0674_011:0779c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空生云。",
        "from": "X37n0674_011:0779c02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未曾得聞如是之經。",
        "from": "X37n0674_011:0779c02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大周亦云。",
        "from": "X37n0674_011:0779c02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "未曾得聞如是之說也。",
        "from": "X37n0674_011:0779c02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因合掌恭敬而為之引。",
        "from": "X37n0674_011:0779c03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲棲弟子大周拜題",
        "from": "X37n0674_011:0779c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 雲溪俍亭挺和尚拈金剛經五十",
        "from": "X37n0674_011:0779c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三則",
        "from": "X37n0674_011:0779c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "枯宅居士　楊師益　編",
        "from": "X37n0674_011:0779c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敷座而坐",
        "from": "X37n0674_011:0779c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0779c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時忽有人。",
        "from": "X37n0674_011:0779c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大展坐具。",
        "from": "X37n0674_011:0779c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作禮三拜。",
        "from": "X37n0674_011:0779c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拂袖便行。",
        "from": "X37n0674_011:0779c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也許他具一隻眼。",
        "from": "X37n0674_011:0779c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或喝散大眾。",
        "from": "X37n0674_011:0779c14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掀倒繩床。",
        "from": "X37n0674_011:0779c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也許他具一隻眼。",
        "from": "X37n0674_011:0779c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或將老瞿曇一脚蹋翻。",
        "from": "X37n0674_011:0779c15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說老老大大。",
        "from": "X37n0674_011:0779c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作這般行徑。",
        "from": "X37n0674_011:0779c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也許他具一隻眼。",
        "from": "X37n0674_011:0779c16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何應住云何降伏其心",
        "from": "X37n0674_011:0779c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0779c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空生空生。",
        "from": "X37n0674_011:0779c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "囈語作麼。",
        "from": "X37n0674_011:0779c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好與一頓痛棒。",
        "from": "X37n0674_011:0779c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "趂出在三門外。",
        "from": "X37n0674_011:0779c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不許相見。",
        "from": "X37n0674_011:0779c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或者有箇入頭。",
        "from": "X37n0674_011:0779c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也不見得。",
        "from": "X37n0674_011:0779c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善哉善哉。",
        "from": "X37n0674_011:0779c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "教壞人家男女。",
        "from": "X37n0674_011:0779c20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應如是住如是降伏其心",
        "from": "X37n0674_011:0779c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0779c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空生空生。",
        "from": "X37n0674_011:0779c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錯過一機了也。",
        "from": "X37n0674_011:0779c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時若舉起便薦。",
        "from": "X37n0674_011:0779c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呵呵大笑一場說。",
        "from": "X37n0674_011:0779c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "原來佛法無多子。",
        "from": "X37n0674_011:0779c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈不通身慶快。",
        "from": "X37n0674_011:0779c23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不唧[口*留]漢。",
        "from": "X37n0674_011:0779c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更疑他三十年。",
        "from": "X37n0674_011:0779c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皆令入無餘涅槃而滅度之",
        "from": "X37n0674_011:0780a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老瞿曇口門尚窄。",
        "from": "X37n0674_011:0780a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也只說得一半。",
        "from": "X37n0674_011:0780a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "狗子便成佛也。",
        "from": "X37n0674_011:0780a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "香臺又作麼生。",
        "from": "X37n0674_011:0780a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩但應如所教住",
        "from": "X37n0674_011:0780a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切忌動著。",
        "from": "X37n0674_011:0780a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "動著三十棒。",
        "from": "X37n0674_011:0780a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見諸相非相即見如來",
        "from": "X37n0674_011:0780a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法眼云。",
        "from": "X37n0674_011:0780a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸相非相即不見如來。",
        "from": "X37n0674_011:0780a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天童云。",
        "from": "X37n0674_011:0780a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一箇說如來禪。",
        "from": "X37n0674_011:0780a07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一箇說祖師禪。",
        "from": "X37n0674_011:0780a08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "會得甚奇特。",
        "from": "X37n0674_011:0780a08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不會也相許。",
        "from": "X37n0674_011:0780a08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且道會得的也便罷了。",
        "from": "X37n0674_011:0780a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不會的却怎生相許。",
        "from": "X37n0674_011:0780a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聻。",
        "from": "X37n0674_011:0780a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不見道。",
        "from": "X37n0674_011:0780a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見怪不怪。",
        "from": "X37n0674_011:0780a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其怪自敗。",
        "from": "X37n0674_011:0780a10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "法尚應捨何況非法",
        "from": "X37n0674_011:0780a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "火爐裏拈取一莖眉毛得麼。",
        "from": "X37n0674_011:0780a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呀呀墮了也。",
        "from": "X37n0674_011:0780a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來得阿耨多羅三藐三菩提耶如來有所說法耶拈云。",
        "from": "X37n0674_011:0780a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時空生只合拍手歸眾。",
        "from": "X37n0674_011:0780a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂佛法者即非佛法",
        "from": "X37n0674_011:0780a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "綿州附子益州薑。",
        "from": "X37n0674_011:0780a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實無有法名阿羅漢",
        "from": "X37n0674_011:0780a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切忌坐在無事甲裏。",
        "from": "X37n0674_011:0780a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於法實無所得",
        "from": "X37n0674_011:0780a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "情知情知。",
        "from": "X37n0674_011:0780a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應無所住而生其心",
        "from": "X37n0674_011:0780a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有者道石女起舞。",
        "from": "X37n0674_011:0780a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "木人興歌。",
        "from": "X37n0674_011:0780a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道得十分。",
        "from": "X37n0674_011:0780a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦當了也。",
        "from": "X37n0674_011:0780a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住住住。",
        "from": "X37n0674_011:0780a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫將箇現成句子。",
        "from": "X37n0674_011:0780a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錯理會他。",
        "from": "X37n0674_011:0780a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒河沙",
        "from": "X37n0674_011:0780b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好世尊撒泥撒沙。",
        "from": "X37n0674_011:0780b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迷昧人家眼目。",
        "from": "X37n0674_011:0780b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨說是經",
        "from": "X37n0674_011:0780b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "古人云。",
        "from": "X37n0674_011:0780b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咳嗽掉臂無非佛事。",
        "from": "X37n0674_011:0780b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫不是隨說是經麼。",
        "from": "X37n0674_011:0780b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知解宗徒未免依文解義了也。",
        "from": "X37n0674_011:0780b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非般若波羅蜜名般若波羅蜜",
        "from": "X37n0674_011:0780b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爪籬木杓與我安放一邊。",
        "from": "X37n0674_011:0780b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來無所說",
        "from": "X37n0674_011:0780b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老瞿曇直得一時口啞。",
        "from": "X37n0674_011:0780b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微塵非微塵世界非世界",
        "from": "X37n0674_011:0780b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且莫向鬼窟裏作活計。",
        "from": "X37n0674_011:0780b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十二相即是非相是名三十二相",
        "from": "X37n0674_011:0780b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一棒打殺與狗子喫了也。",
        "from": "X37n0674_011:0780b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時如來性命却在別人手裏。",
        "from": "X37n0674_011:0780b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離一切諸相即名諸佛",
        "from": "X37n0674_011:0780b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是諸佛出身處。",
        "from": "X37n0674_011:0780b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東山水上行。",
        "from": "X37n0674_011:0780b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "老韶陽指東話西。",
        "from": "X37n0674_011:0780b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "扭壞他人鼻孔。",
        "from": "X37n0674_011:0780b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此法無實無虗",
        "from": "X37n0674_011:0780b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八角磨盤空裏走。",
        "from": "X37n0674_011:0780b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錯錯錯。",
        "from": "X37n0674_011:0780b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "話墮了也。",
        "from": "X37n0674_011:0780b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "在在處處若有此經",
        "from": "X37n0674_011:0780b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫眼花好。",
        "from": "X37n0674_011:0780b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為人輕賤",
        "from": "X37n0674_011:0780b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "太尊貴生。",
        "from": "X37n0674_011:0780b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "算數譬喻所不能及",
        "from": "X37n0674_011:0780b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前三三。",
        "from": "X37n0674_011:0780c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後三三。",
        "from": "X37n0674_011:0780c01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟是多少。",
        "from": "X37n0674_011:0780c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "果報不可思議",
        "from": "X37n0674_011:0780c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燈籠緣壁上。",
        "from": "X37n0674_011:0780c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天台與他寒山竹筒拾得笤帚。",
        "from": "X37n0674_011:0780c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時向摩耶夫人腹中。",
        "from": "X37n0674_011:0780c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現身作佛去也。",
        "from": "X37n0674_011:0780c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "還有人信得及麼。",
        "from": "X37n0674_011:0780c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若信不及。",
        "from": "X37n0674_011:0780c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十棒自領出去。",
        "from": "X37n0674_011:0780c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若信得及。",
        "from": "X37n0674_011:0780c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三十棒自領出去。",
        "from": "X37n0674_011:0780c05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何應住云何降伏其心",
        "from": "X37n0674_011:0780c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時好與一喝。",
        "from": "X37n0674_011:0780c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "待他耳聾三日。",
        "from": "X37n0674_011:0780c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "別作商量也。",
        "from": "X37n0674_011:0780c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不致婆婆和和。",
        "from": "X37n0674_011:0780c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說箇不了。",
        "from": "X37n0674_011:0780c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當生如是心",
        "from": "X37n0674_011:0780c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五臺山上雲蒸飯。",
        "from": "X37n0674_011:0780c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十字街前狗矢天。",
        "from": "X37n0674_011:0780c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎竿頭上煎[飢几+追]子。",
        "from": "X37n0674_011:0780c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三箇猢猻夜簸錢。",
        "from": "X37n0674_011:0780c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說即儘說。",
        "from": "X37n0674_011:0780c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恐太煞了也。",
        "from": "X37n0674_011:0780c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "滅度無量眾生實無眾生得滅度者",
        "from": "X37n0674_011:0780c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何是行鳥道。",
        "from": "X37n0674_011:0780c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不逢一人。",
        "from": "X37n0674_011:0780c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又說不行鳥道。",
        "from": "X37n0674_011:0780c14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且道行的是。",
        "from": "X37n0674_011:0780c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不行的是。",
        "from": "X37n0674_011:0780c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實無有法發阿耨多羅三藐三菩提心者拈云。",
        "from": "X37n0674_011:0780c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樹倒藤枯句。",
        "from": "X37n0674_011:0780c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸何處了也。",
        "from": "X37n0674_011:0780c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住住住。",
        "from": "X37n0674_011:0780c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且與我覓取盋盂柄來。",
        "from": "X37n0674_011:0780c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實無有法得阿耨多羅三藐三菩提",
        "from": "X37n0674_011:0780c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "落空亡的外道請過一邊。",
        "from": "X37n0674_011:0780c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且莫喚鐘作甕。",
        "from": "X37n0674_011:0780c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如來者即諸法如義",
        "from": "X37n0674_011:0780c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0780c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喚作如如。",
        "from": "X37n0674_011:0780c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "早是變了也。",
        "from": "X37n0674_011:0780c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王老師通身負墮。",
        "from": "X37n0674_011:0780c22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口巴巴道。",
        "from": "X37n0674_011:0780c22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向異類中行。",
        "from": "X37n0674_011:0780c23_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法皆是佛法",
        "from": "X37n0674_011:0780c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雲門乾矢橛。",
        "from": "X37n0674_011:0781a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "洞山麻三觔。",
        "from": "X37n0674_011:0781a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莫便是這箇道理麼。",
        "from": "X37n0674_011:0781a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咄。",
        "from": "X37n0674_011:0781a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "錯會不少。",
        "from": "X37n0674_011:0781a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非大身是名大身",
        "from": "X37n0674_011:0781a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭長三寸。",
        "from": "X37n0674_011:0781a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頸長一尺。",
        "from": "X37n0674_011:0781a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟是甚麼人。",
        "from": "X37n0674_011:0781a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喚取來。",
        "from": "X37n0674_011:0781a04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我要與他相見。",
        "from": "X37n0674_011:0781a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實無有法名為菩薩",
        "from": "X37n0674_011:0781a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金果朝來猿摘去。",
        "from": "X37n0674_011:0781a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "玉花晚後鳳銜歸。",
        "from": "X37n0674_011:0781a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "道伴交肩。",
        "from": "X37n0674_011:0781a07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "這裏莫錯認好。",
        "from": "X37n0674_011:0781a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸心非心是名為心",
        "from": "X37n0674_011:0781a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有者道剎竿頭上仰蓮心。",
        "from": "X37n0674_011:0781a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分明是這箇也。",
        "from": "X37n0674_011:0781a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且道你面皮厚多少。",
        "from": "X37n0674_011:0781a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過去心不可得現在心不可得未來心不可得拈云。",
        "from": "X37n0674_011:0781a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周金剛好不丈夫。",
        "from": "X37n0674_011:0781a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "被他老婆一問。",
        "from": "X37n0674_011:0781a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "直得口啞。",
        "from": "X37n0674_011:0781a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是箇皮裏有血的。",
        "from": "X37n0674_011:0781a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何不與他餅子一擔打翻。",
        "from": "X37n0674_011:0781a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福德無故得福德多",
        "from": "X37n0674_011:0781a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真園頭作許多痴福。",
        "from": "X37n0674_011:0781a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "著甚來由。",
        "from": "X37n0674_011:0781a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "好與他念法華一齊趂出。",
        "from": "X37n0674_011:0781a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "免得後人說勝說負。",
        "from": "X37n0674_011:0781a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色身非色身諸相非諸相",
        "from": "X37n0674_011:0781a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金香爐下鐵崑崙。",
        "from": "X37n0674_011:0781a19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "這裏作何分別。",
        "from": "X37n0674_011:0781a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人言如來有所說法即為謗佛",
        "from": "X37n0674_011:0781a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "掩耳盜鈴漢。",
        "from": "X37n0674_011:0781a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說法者無法可說",
        "from": "X37n0674_011:0781a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聾人爭得聞。",
        "from": "X37n0674_011:0781a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛得阿耨多羅為無所得耶",
        "from": "X37n0674_011:0781a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當時翻轉面皮。",
        "from": "X37n0674_011:0781b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "痛與一頓。",
        "from": "X37n0674_011:0781b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儘教扶起沙盆。",
        "from": "X37n0674_011:0781b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是如是乃至無有少法可得",
        "from": "X37n0674_011:0781b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "東瓜直儱侗。",
        "from": "X37n0674_011:0781b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是法平等無有高下",
        "from": "X37n0674_011:0781b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切忌顢頇。",
        "from": "X37n0674_011:0781b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修一切善法",
        "from": "X37n0674_011:0781b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塗污了也。",
        "from": "X37n0674_011:0781b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即非善法是名善法",
        "from": "X37n0674_011:0781b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牛頭出馬頭回。",
        "from": "X37n0674_011:0781b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至算數譬喻所不能及",
        "from": "X37n0674_011:0781b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "撮空謾語。",
        "from": "X37n0674_011:0781b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡夫非凡夫",
        "from": "X37n0674_011:0781b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "李順王小二烏有先生無是公。",
        "from": "X37n0674_011:0781b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一齊向水牯牛隊中。",
        "from": "X37n0674_011:0781b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "打殺了三世諸佛也。",
        "from": "X37n0674_011:0781b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟有謝三郎不在其內。",
        "from": "X37n0674_011:0781b14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若以色見我以音聲求我是人行邪道不能見如來拈云。",
        "from": "X37n0674_011:0781b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "切忌當面錯過。",
        "from": "X37n0674_011:0781b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於法不說斷滅相",
        "from": "X37n0674_011:0781b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此事不得孤滯。",
        "from": "X37n0674_011:0781b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然雖枯木糝花。",
        "from": "X37n0674_011:0781b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要且眼中著屑了也。",
        "from": "X37n0674_011:0781b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "闍黎闍黎放過著。",
        "from": "X37n0674_011:0781b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩不受福德",
        "from": "X37n0674_011:0781b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖則貧無立錐。",
        "from": "X37n0674_011:0781b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "只恐富嫌千口。",
        "from": "X37n0674_011:0781b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善現善現。",
        "from": "X37n0674_011:0781b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且與我拈取華房百寶來。",
        "from": "X37n0674_011:0781b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所從來亦無所去",
        "from": "X37n0674_011:0781b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釘釘著懸挂著。",
        "from": "X37n0674_011:0781c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一合相非一合相是名一合相",
        "from": "X37n0674_011:0781c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "江南三月鷓鴣啼。",
        "from": "X37n0674_011:0781c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "算來只在秋江上。",
        "from": "X37n0674_011:0781c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且道古人語有差別沒差別。",
        "from": "X37n0674_011:0781c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聻。",
        "from": "X37n0674_011:0781c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是知如是見如是信解不生法相",
        "from": "X37n0674_011:0781c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈云。",
        "from": "X37n0674_011:0781c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大家吹滅火。",
        "from": "X37n0674_011:0781c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一路暗中行。",
        "from": "X37n0674_011:0781c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "饒你投明到來。",
        "from": "X37n0674_011:0781c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "也須三十拄杖。",
        "from": "X37n0674_011:0781c06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切有為法如夢幻泡影如露亦如電應作如是觀拈云。",
        "from": "X37n0674_011:0781c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "牡丹花且置。",
        "from": "X37n0674_011:0781c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即如栢樹子又作麼生。",
        "from": "X37n0674_011:0781c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無為法有為法變作兩箇熱鐵丸兒。",
        "from": "X37n0674_011:0781c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時穿向金剛鼻孔裏去也。",
        "from": "X37n0674_011:0781c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具眼人惺惺著。",
        "from": "X37n0674_011:0781c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "拈金剛經五十三則(終)",
        "from": "X37n0674_011:0781c14_0",
        "context_with_next_sentence": false
    }
]