[
    {
        "source_sentence": "大般涅槃經集解卷第三十七",
        "from": "T37n1763_037:0496c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　明慈悲有實益",
        "from": "T37n1763_037:0496c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　廣明慈心中布施發願事",
        "from": "T37n1763_037:0496c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　廣明菩薩所脩之慈即萬行",
        "from": "T37n1763_037:0496c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　廣明慈益象見師子力士移石盧至長者斯那大多女人及提婆達多憍薩羅賊流離太子梵行品之第二迦葉菩薩白佛言世尊(至)如是之觀非虛妄耶。",
        "from": "T37n1763_037:0496c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0496c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0496c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三門明四等。",
        "from": "T37n1763_037:0496c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是實行也。",
        "from": "T37n1763_037:0496c27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難有兩意。",
        "from": "T37n1763_037:0496c27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此以虛行為難也。",
        "from": "T37n1763_037:0496c28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_037:0496c28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "難有三重。",
        "from": "T37n1763_037:0496c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此初重云。",
        "from": "T37n1763_037:0496c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生在苦。",
        "from": "T37n1763_037:0496c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不蒙救拔。",
        "from": "T37n1763_037:0496c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證慈非實也。",
        "from": "T37n1763_037:0496c29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_037:0497a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下大段之第三也。",
        "from": "T37n1763_037:0497a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明慈是實行。",
        "from": "T37n1763_037:0497a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將辨此理故。",
        "from": "T37n1763_037:0497a01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉先作虛妄之難也。",
        "from": "T37n1763_037:0497a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此中凡六翻。",
        "from": "T37n1763_037:0497a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此下第一釋慈體是實行。",
        "from": "T37n1763_037:0497a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉引五事。",
        "from": "T37n1763_037:0497a03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證其無實之難也。",
        "from": "T37n1763_037:0497a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊若非虛妄實與樂者(至)威德力故一切受樂。",
        "from": "T37n1763_037:0497a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_037:0497a06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二重難。",
        "from": "T37n1763_037:0497a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其必益。",
        "from": "T37n1763_037:0497a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是則眾生必應離苦也。",
        "from": "T37n1763_037:0497a06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若當真實不得樂者(至)云何得與此義相應。",
        "from": "T37n1763_037:0497a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第二難。",
        "from": "T37n1763_037:0497a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明虛行不應得實報也。",
        "from": "T37n1763_037:0497a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_037:0497a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三重難。",
        "from": "T37n1763_037:0497a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以果驗因。",
        "from": "T37n1763_037:0497a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二事相違。",
        "from": "T37n1763_037:0497a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛實未辨。",
        "from": "T37n1763_037:0497a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請求解釋也。",
        "from": "T37n1763_037:0497a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善哉善哉善男子(至)諸佛菩薩真實不虛。",
        "from": "T37n1763_037:0497a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497a13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初偈說世間慈也。",
        "from": "T37n1763_037:0497a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行人以施是實行。",
        "from": "T37n1763_037:0497a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故以施福。",
        "from": "T37n1763_037:0497a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "脩世間慈。",
        "from": "T37n1763_037:0497a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不及其一況出世間也。",
        "from": "T37n1763_037:0497a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者。",
        "from": "T37n1763_037:0497a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖慈一人。",
        "from": "T37n1763_037:0497a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與世之樂。",
        "from": "T37n1763_037:0497a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施不得爾。",
        "from": "T37n1763_037:0497a15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "昇降可知矣。",
        "from": "T37n1763_037:0497a16_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_037:0497a16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就此明慈有實益門中。",
        "from": "T37n1763_037:0497a16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有五翻為證也。",
        "from": "T37n1763_037:0497a17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初明脩慈實能轉境。",
        "from": "T37n1763_037:0497a17_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二明慈能斷煩惱。",
        "from": "T37n1763_037:0497a17_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三明慈為萬善根本。",
        "from": "T37n1763_037:0497a18_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四明慈即萬行。",
        "from": "T37n1763_037:0497a18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五引果地之慈。",
        "from": "T37n1763_037:0497a19_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "證真實不虛也。",
        "from": "T37n1763_037:0497a19_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛是窮慈之極。",
        "from": "T37n1763_037:0497a20_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得知因地亦有實益也。",
        "from": "T37n1763_037:0497a20_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖可有此五翻。",
        "from": "T37n1763_037:0497a20_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若欲對向三問者。",
        "from": "T37n1763_037:0497a21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五翻悉證慈有實益。",
        "from": "T37n1763_037:0497a21_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並答初問。",
        "from": "T37n1763_037:0497a22_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下當更題。",
        "from": "T37n1763_037:0497a22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二問出別答也。",
        "from": "T37n1763_037:0497a22_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此偈言。",
        "from": "T37n1763_037:0497a23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "慈之功用。",
        "from": "T37n1763_037:0497a23_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃於諸行為勝。",
        "from": "T37n1763_037:0497a23_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈但實益而已。",
        "from": "T37n1763_037:0497a23_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可以聲聞是虛。",
        "from": "T37n1763_037:0497a24_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令菩薩益虛也。",
        "from": "T37n1763_037:0497a24_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二問既釋。",
        "from": "T37n1763_037:0497a25_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則第三問自然釋矣。",
        "from": "T37n1763_037:0497a25_37",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寶亮曰。",
        "from": "T37n1763_037:0497a25_38",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛將答難。",
        "from": "T37n1763_037:0497a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先歎之也。",
        "from": "T37n1763_037:0497a26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈明實益格量功德。",
        "from": "T37n1763_037:0497a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者。",
        "from": "T37n1763_037:0497a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有人止於一人邊與慈。",
        "from": "T37n1763_037:0497a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲與其樂。",
        "from": "T37n1763_037:0497a27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始終必得出生死也。",
        "from": "T37n1763_037:0497a27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答人雖復供養無量五通神仙。",
        "from": "T37n1763_037:0497a28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為福田。",
        "from": "T37n1763_037:0497a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不如於一人邊起慈。",
        "from": "T37n1763_037:0497a29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六分中之一也。",
        "from": "T37n1763_037:0497a29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "供養仙人。",
        "from": "T37n1763_037:0497b01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃得生死之報。",
        "from": "T37n1763_037:0497b01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若拔一人苦。",
        "from": "T37n1763_037:0497b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實獲出世之利也。",
        "from": "T37n1763_037:0497b02_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又化無量人。",
        "from": "T37n1763_037:0497b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得出於生死。",
        "from": "T37n1763_037:0497b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知慈是實也。",
        "from": "T37n1763_037:0497b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "釋慈有實益之義。",
        "from": "T37n1763_037:0497b03_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文齊於此。",
        "from": "T37n1763_037:0497b03_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何知耶善男子菩薩(至)是實思惟非不真實。",
        "from": "T37n1763_037:0497b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五事證慈。",
        "from": "T37n1763_037:0497b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此第一明能令境雖不實。",
        "from": "T37n1763_037:0497b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而物得實益。",
        "from": "T37n1763_037:0497b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如由金得樂。",
        "from": "T37n1763_037:0497b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "土即為金。",
        "from": "T37n1763_037:0497b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無金生樂。",
        "from": "T37n1763_037:0497b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "金即為土無異。",
        "from": "T37n1763_037:0497b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小大相容。",
        "from": "T37n1763_037:0497b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當事皆實。",
        "from": "T37n1763_037:0497b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而得其用也。",
        "from": "T37n1763_037:0497b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子云何名為(至)以是故名真實思惟。",
        "from": "T37n1763_037:0497b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497b11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二以斷結。",
        "from": "T37n1763_037:0497b11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故知非虛。",
        "from": "T37n1763_037:0497b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子菩薩摩訶薩(至)阿耨多羅三藐三菩提。",
        "from": "T37n1763_037:0497b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三事以慈能生萬善。",
        "from": "T37n1763_037:0497b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈得非實耶。",
        "from": "T37n1763_037:0497b13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_037:0497b14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身被毒箭者。",
        "from": "T37n1763_037:0497b14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂慳貪箭也。",
        "from": "T37n1763_037:0497b14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眷屬欲令安隱者。",
        "from": "T37n1763_037:0497b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩也。",
        "from": "T37n1763_037:0497b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即命良醫而為拔箭者。",
        "from": "T37n1763_037:0497b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸福田。",
        "from": "T37n1763_037:0497b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "往就乞也。",
        "from": "T37n1763_037:0497b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且待莫觸者。",
        "from": "T37n1763_037:0497b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方復優量。",
        "from": "T37n1763_037:0497b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可施不可施。",
        "from": "T37n1763_037:0497b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或財物多少。",
        "from": "T37n1763_037:0497b17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時節可不也。",
        "from": "T37n1763_037:0497b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩亦爾。",
        "from": "T37n1763_037:0497b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若行施時者。",
        "from": "T37n1763_037:0497b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合譬也。",
        "from": "T37n1763_037:0497b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次善男子菩薩摩訶薩(至)常應勤發如是誓願。",
        "from": "T37n1763_037:0497b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_037:0497b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言依離身一尺六寸者。",
        "from": "T37n1763_037:0497b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如下經文。",
        "from": "T37n1763_037:0497b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又曰。",
        "from": "T37n1763_037:0497b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "衣服離身四寸不墮。",
        "from": "T37n1763_037:0497b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "便是一邊。",
        "from": "T37n1763_037:0497b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四寸四邊。",
        "from": "T37n1763_037:0497b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一尺六寸也。",
        "from": "T37n1763_037:0497b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此則舉其四邊。",
        "from": "T37n1763_037:0497b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼乃言其一面耳。",
        "from": "T37n1763_037:0497b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子一切聲聞緣覺菩薩(至)以是義故實非虛妄。",
        "from": "T37n1763_037:0497b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六度是菩薩別行。",
        "from": "T37n1763_037:0497b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說施一事。",
        "from": "T37n1763_037:0497b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘五可知也。",
        "from": "T37n1763_037:0497b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其餘諸行。",
        "from": "T37n1763_037:0497b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與三乘共行。",
        "from": "T37n1763_037:0497b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由佛說故。",
        "from": "T37n1763_037:0497b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以慈為本也。",
        "from": "T37n1763_037:0497b27_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先說人後說法。",
        "from": "T37n1763_037:0497b27_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不淨至無我等觀。",
        "from": "T37n1763_037:0497b27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說行相也。",
        "from": "T37n1763_037:0497b28_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燸法至見道修道。",
        "from": "T37n1763_037:0497b28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說得果深淺也。",
        "from": "T37n1763_037:0497b28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正勤至八聖道。",
        "from": "T37n1763_037:0497b29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說深淺法之名也。",
        "from": "T37n1763_037:0497b29_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四禪至無諍三昧。",
        "from": "T37n1763_037:0497b29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說定名也。",
        "from": "T37n1763_037:0497c01_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "他心智至佛智。",
        "from": "T37n1763_037:0497c01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說智名也。",
        "from": "T37n1763_037:0497c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂皆以慈為本也。",
        "from": "T37n1763_037:0497c02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子能為善者名實思惟(至)如是無量無邊功德。",
        "from": "T37n1763_037:0497c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四事說萬行即慈。",
        "from": "T37n1763_037:0497c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除苦為慈。",
        "from": "T37n1763_037:0497c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三乘皆是滅苦。",
        "from": "T37n1763_037:0497c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "要得實者。",
        "from": "T37n1763_037:0497c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則滅苦也。",
        "from": "T37n1763_037:0497c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "思惟是慈。",
        "from": "T37n1763_037:0497c05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡苦名乘也。",
        "from": "T37n1763_037:0497c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦葉菩薩白佛言世尊(至)所脩慈心為無利益。",
        "from": "T37n1763_037:0497c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五事明慈。",
        "from": "T37n1763_037:0497c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現有實益。",
        "from": "T37n1763_037:0497c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故發問。",
        "from": "T37n1763_037:0497c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言善男子菩薩之慈(至)是實思惟非無利益。",
        "from": "T37n1763_037:0497c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二種必受苦。",
        "from": "T37n1763_037:0497c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一以不救故。",
        "from": "T37n1763_037:0497c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二以雖得小樂。",
        "from": "T37n1763_037:0497c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更增重苦也。",
        "from": "T37n1763_037:0497c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂一闡提者。",
        "from": "T37n1763_037:0497c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫定受苦報。",
        "from": "T37n1763_037:0497c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦非但闡提。",
        "from": "T37n1763_037:0497c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘業亦有。",
        "from": "T37n1763_037:0497c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而其相難辨也。",
        "from": "T37n1763_037:0497c13_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受苦不定者。",
        "from": "T37n1763_037:0497c14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂與樂則得。",
        "from": "T37n1763_037:0497c14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不與則不得也。",
        "from": "T37n1763_037:0497c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遙見師子者。",
        "from": "T37n1763_037:0497c15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夫不慈之物。",
        "from": "T37n1763_037:0497c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有遇之者。",
        "from": "T37n1763_037:0497c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則便生懼。",
        "from": "T37n1763_037:0497c15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大悲之人。",
        "from": "T37n1763_037:0497c16_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有逆值者。",
        "from": "T37n1763_037:0497c16_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實自然生樂。",
        "from": "T37n1763_037:0497c16_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以憑師有在也。",
        "from": "T37n1763_037:0497c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧宗曰。",
        "from": "T37n1763_037:0497c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "前有兩關。",
        "from": "T37n1763_037:0497c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "同明慈有實益。",
        "from": "T37n1763_037:0497c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已遣一關。",
        "from": "T37n1763_037:0497c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "將顯事迹。",
        "from": "T37n1763_037:0497c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故迦葉重有前問。",
        "from": "T37n1763_037:0497c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即釋。",
        "from": "T37n1763_037:0497c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子我說是慈有無量(至)大涅槃經亦不可思議。",
        "from": "T37n1763_037:0497c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "案。",
        "from": "T37n1763_037:0497c21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "僧亮曰。",
        "from": "T37n1763_037:0497c21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "明法身無緣。",
        "from": "T37n1763_037:0497c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有感斯應。",
        "from": "T37n1763_037:0497c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以顯大慈之功無出入異者也。",
        "from": "T37n1763_037:0497c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大般涅槃經集解卷第三十七",
        "from": "T37n1763_037:0497c23_0",
        "context_with_next_sentence": false
    }
]