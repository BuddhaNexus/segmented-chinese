[
    {
        "source_sentence": "大乘百法明門論本事分中略錄",
        "from": "T31n1614_001:0855b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名數",
        "from": "T31n1614_001:0855b11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天親菩薩造",
        "from": "T31n1614_001:0855b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐三藏法師玄奘譯",
        "from": "T31n1614_001:0855b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如世尊言。",
        "from": "T31n1614_001:0855b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法無我。",
        "from": "T31n1614_001:0855b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等一切法。",
        "from": "T31n1614_001:0855b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為無我。",
        "from": "T31n1614_001:0855b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切法者。",
        "from": "T31n1614_001:0855b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略有五種。",
        "from": "T31n1614_001:0855b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者心法。",
        "from": "T31n1614_001:0855b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者心所有法。",
        "from": "T31n1614_001:0855b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者色法。",
        "from": "T31n1614_001:0855b17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者心不相應行法。",
        "from": "T31n1614_001:0855b17_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者無為法。",
        "from": "T31n1614_001:0855b17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切最勝故。",
        "from": "T31n1614_001:0855b18_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與此相應故。",
        "from": "T31n1614_001:0855b18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二所現影故。",
        "from": "T31n1614_001:0855b18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三分位差別故。",
        "from": "T31n1614_001:0855b19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四所顯示故。",
        "from": "T31n1614_001:0855b19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是次第。",
        "from": "T31n1614_001:0855b19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一心法略有八種。",
        "from": "T31n1614_001:0855b20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一眼識。",
        "from": "T31n1614_001:0855b20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二耳識。",
        "from": "T31n1614_001:0855b20_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三鼻識。",
        "from": "T31n1614_001:0855b20_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四舌識。",
        "from": "T31n1614_001:0855b21_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五身識。",
        "from": "T31n1614_001:0855b21_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六意識。",
        "from": "T31n1614_001:0855b21_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七末那識。",
        "from": "T31n1614_001:0855b21_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八阿賴耶識。",
        "from": "T31n1614_001:0855b21_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二心所有法。",
        "from": "T31n1614_001:0855b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略有五十一種。",
        "from": "T31n1614_001:0855b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分為六位。",
        "from": "T31n1614_001:0855b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍行有五。",
        "from": "T31n1614_001:0855b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二別境有五。",
        "from": "T31n1614_001:0855b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三善有十一。",
        "from": "T31n1614_001:0855b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四煩惱有六。",
        "from": "T31n1614_001:0855b24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五隨煩惱有二十。",
        "from": "T31n1614_001:0855b25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六不定有四。",
        "from": "T31n1614_001:0855b25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一遍行五者。",
        "from": "T31n1614_001:0855b25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一作意二觸三受四想五思。",
        "from": "T31n1614_001:0855b26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二別境五者。",
        "from": "T31n1614_001:0855b26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一欲二勝解三念四定五慧。",
        "from": "T31n1614_001:0855b27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三善十一者。",
        "from": "T31n1614_001:0855b27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一信二精進。",
        "from": "T31n1614_001:0855b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三慚四愧。",
        "from": "T31n1614_001:0855b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五無貪六無嗔七無癡。",
        "from": "T31n1614_001:0855b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八輕安九不放逸十行捨十一不害。",
        "from": "T31n1614_001:0855b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四煩惱六者。",
        "from": "T31n1614_001:0855b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一貪二嗔。",
        "from": "T31n1614_001:0855c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三慢四無明。",
        "from": "T31n1614_001:0855c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五疑六不正見。",
        "from": "T31n1614_001:0855c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五隨煩惱二十者。",
        "from": "T31n1614_001:0855c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一忿二恨。",
        "from": "T31n1614_001:0855c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三惱四覆。",
        "from": "T31n1614_001:0855c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五誑六諂。",
        "from": "T31n1614_001:0855c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七憍八害。",
        "from": "T31n1614_001:0855c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九嫉十慳。",
        "from": "T31n1614_001:0855c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一無慚十二無愧。",
        "from": "T31n1614_001:0855c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三不信十四懈怠。",
        "from": "T31n1614_001:0855c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五放逸十六惛沈。",
        "from": "T31n1614_001:0855c04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七掉舉十八失念。",
        "from": "T31n1614_001:0855c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九不正知二十散亂。",
        "from": "T31n1614_001:0855c05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六不定四者。",
        "from": "T31n1614_001:0855c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一睡眠二惡作。",
        "from": "T31n1614_001:0855c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三尋四伺。",
        "from": "T31n1614_001:0855c06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三色法。",
        "from": "T31n1614_001:0855c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略有十一種。",
        "from": "T31n1614_001:0855c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一眼二耳三鼻四舌五身。",
        "from": "T31n1614_001:0855c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六色七聲八香九味十觸。",
        "from": "T31n1614_001:0855c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一法處所攝色。",
        "from": "T31n1614_001:0855c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四心不相應行法。",
        "from": "T31n1614_001:0855c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略有二十四種。",
        "from": "T31n1614_001:0855c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一得二命根。",
        "from": "T31n1614_001:0855c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三眾同分。",
        "from": "T31n1614_001:0855c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四異生性。",
        "from": "T31n1614_001:0855c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五無想定。",
        "from": "T31n1614_001:0855c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六滅盡定。",
        "from": "T31n1614_001:0855c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七無想報。",
        "from": "T31n1614_001:0855c12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八名身九句身十文身。",
        "from": "T31n1614_001:0855c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十一生十二老。",
        "from": "T31n1614_001:0855c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十三住十四無常。",
        "from": "T31n1614_001:0855c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十五流轉。",
        "from": "T31n1614_001:0855c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十六定異。",
        "from": "T31n1614_001:0855c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十七相應。",
        "from": "T31n1614_001:0855c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八勢速。",
        "from": "T31n1614_001:0855c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十九次第。",
        "from": "T31n1614_001:0855c14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十方。",
        "from": "T31n1614_001:0855c14_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十一時。",
        "from": "T31n1614_001:0855c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十二數。",
        "from": "T31n1614_001:0855c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十三和合性。",
        "from": "T31n1614_001:0855c15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二十四不和合性。",
        "from": "T31n1614_001:0855c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五無為法者。",
        "from": "T31n1614_001:0855c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略有六種。",
        "from": "T31n1614_001:0855c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一虛空無為。",
        "from": "T31n1614_001:0855c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二擇滅無為。",
        "from": "T31n1614_001:0855c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三非擇滅無為。",
        "from": "T31n1614_001:0855c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四不動滅無為。",
        "from": "T31n1614_001:0855c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五想受滅無為。",
        "from": "T31n1614_001:0855c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六真如無為。",
        "from": "T31n1614_001:0855c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言無我者。",
        "from": "T31n1614_001:0855c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略有二種。",
        "from": "T31n1614_001:0855c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一補特伽羅無我。",
        "from": "T31n1614_001:0855c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二法無我。",
        "from": "T31n1614_001:0855c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘百法明門論",
        "from": "T31n1614_001:0855c22_0",
        "context_with_next_sentence": false
    }
]