[
    {
        "source_sentence": "大方廣佛華嚴經入法界品四十",
        "from": "T19n1019_001:0707c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二字觀門",
        "from": "T19n1019_001:0707c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T19n1019_001:0707c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅國公食邑三千戶賜紫贈司",
        "from": "T19n1019_001:0707c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空諡大鑒正號大廣智大興善",
        "from": "T19n1019_001:0707c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺三藏沙門不空奉　詔譯",
        "from": "T19n1019_001:0707c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時善財童子從天宮下。",
        "from": "T19n1019_001:0707c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "向迦毘羅城。",
        "from": "T19n1019_001:0707c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至善知眾藝童子所。",
        "from": "T19n1019_001:0707c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭頂禮敬於一面立。",
        "from": "T19n1019_001:0707c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白言聖者。",
        "from": "T19n1019_001:0707c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我已發阿耨多羅三藐三菩提心。",
        "from": "T19n1019_001:0707c16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而未知菩薩云何學菩薩行。",
        "from": "T19n1019_001:0707c16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何修菩薩道。",
        "from": "T19n1019_001:0707c17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我聞聖者善能教誨。",
        "from": "T19n1019_001:0707c17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願為我說。",
        "from": "T19n1019_001:0707c18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時彼童子告善財言。",
        "from": "T19n1019_001:0707c18_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子我得菩薩解脫。",
        "from": "T19n1019_001:0707c19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名善知眾藝。",
        "from": "T19n1019_001:0707c19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我恒稱持入此解脫根本之字。",
        "from": "T19n1019_001:0707c19_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　阿(上)字時名由菩薩威德入無差別境界般若波羅蜜門。",
        "from": "T19n1019_001:0707c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法本不生故。",
        "from": "T19n1019_001:0707c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　囉字時。",
        "from": "T19n1019_001:0707c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入無邊際差別般若波羅蜜門。",
        "from": "T19n1019_001:0707c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法離塵垢故。",
        "from": "T19n1019_001:0707c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　跛字時。",
        "from": "T19n1019_001:0707c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入法界際般若波羅蜜門。",
        "from": "T19n1019_001:0707c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法勝義諦不可得故。",
        "from": "T19n1019_001:0707c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　左(輕呼)字時入普輪斷差別般若波羅蜜門。",
        "from": "T19n1019_001:0707c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法無諸行故。",
        "from": "T19n1019_001:0707c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　曩(舌頭呼)字時。",
        "from": "T19n1019_001:0707c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入無阿賴耶際般若波羅蜜門。",
        "from": "T19n1019_001:0707c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法性相不可得故。",
        "from": "T19n1019_001:0708a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　攞字時。",
        "from": "T19n1019_001:0708a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入無垢般若波羅蜜門。",
        "from": "T19n1019_001:0708a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法出世間故。",
        "from": "T19n1019_001:0708a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛支因緣永不現故。",
        "from": "T19n1019_001:0708a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　娜字時。",
        "from": "T19n1019_001:0708a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入不退轉加行般若波羅蜜門。",
        "from": "T19n1019_001:0708a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法調伏寂靜真如平等無分別故。",
        "from": "T19n1019_001:0708a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　麼字時。",
        "from": "T19n1019_001:0708a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入金剛場般若波羅蜜門悟一切法離縛解故。",
        "from": "T19n1019_001:0708a06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　拏(上)字時。",
        "from": "T19n1019_001:0708a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入普遍輪般若波羅蜜門。",
        "from": "T19n1019_001:0708a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法離熱矯穢得清凉故。",
        "from": "T19n1019_001:0708a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　灑字時。",
        "from": "T19n1019_001:0708a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入海藏般若波羅蜜門。",
        "from": "T19n1019_001:0708a10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法無罣礙故。",
        "from": "T19n1019_001:0708a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　嚩字時。",
        "from": "T19n1019_001:0708a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入普遍生安住般若波羅蜜門。",
        "from": "T19n1019_001:0708a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法言語道斷故。",
        "from": "T19n1019_001:0708a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　多(上)字時。",
        "from": "T19n1019_001:0708a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入照曜塵垢般若波羅蜜門。",
        "from": "T19n1019_001:0708a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法真如不動故。",
        "from": "T19n1019_001:0708a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　野字時。",
        "from": "T19n1019_001:0708a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入差別積聚般若波羅蜜門。",
        "from": "T19n1019_001:0708a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法如實不生故。",
        "from": "T19n1019_001:0708a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　瑟吒(二合上)字時。",
        "from": "T19n1019_001:0708a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入普遍光明息除熱惱般若波羅蜜門。",
        "from": "T19n1019_001:0708a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法制伏任持相不可得故。",
        "from": "T19n1019_001:0708a19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　迦(上)字時。",
        "from": "T19n1019_001:0708a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入差別種類般若波羅蜜門。",
        "from": "T19n1019_001:0708a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法作者不可得故。",
        "from": "T19n1019_001:0708a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　娑(上)字時。",
        "from": "T19n1019_001:0708a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入現前降霔大雨般若波羅蜜門。",
        "from": "T19n1019_001:0708a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法時平等性不可得故。",
        "from": "T19n1019_001:0708a24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　莽(輕呼)字時。",
        "from": "T19n1019_001:0708a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入大迅疾眾峯般若波羅蜜門。",
        "from": "T19n1019_001:0708a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法我所執性不可得故。",
        "from": "T19n1019_001:0708a26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　誐字時。",
        "from": "T19n1019_001:0708a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入普遍輪長養般若波羅蜜門。",
        "from": "T19n1019_001:0708a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法行取性不可得故。",
        "from": "T19n1019_001:0708a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　他(上)字時。",
        "from": "T19n1019_001:0708a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入真如無差別般若波羅蜜門。",
        "from": "T19n1019_001:0708a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法處所不可得故。",
        "from": "T19n1019_001:0708b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　惹字時。",
        "from": "T19n1019_001:0708b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入世間流轉窮源清淨般若波羅蜜門。",
        "from": "T19n1019_001:0708b02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法能所生起不可得故。",
        "from": "T19n1019_001:0708b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　娑嚩(二合)字時。",
        "from": "T19n1019_001:0708b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入念一切佛莊嚴般若波羅蜜門。",
        "from": "T19n1019_001:0708b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法安隱性不可得故。",
        "from": "T19n1019_001:0708b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　馱字時。",
        "from": "T19n1019_001:0708b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入觀察法界道場般若波羅蜜門。",
        "from": "T19n1019_001:0708b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法能持界性不可得故。",
        "from": "T19n1019_001:0708b07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　捨字時。",
        "from": "T19n1019_001:0708b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入隨順一切佛教般若波羅蜜門。",
        "from": "T19n1019_001:0708b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法寂靜性不可得故。",
        "from": "T19n1019_001:0708b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　佉(上)字時入現行因地智慧藏般若波羅蜜門。",
        "from": "T19n1019_001:0708b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法如虛空性不可得故。",
        "from": "T19n1019_001:0708b11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　訖灑(二合)字時。",
        "from": "T19n1019_001:0708b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入決擇息諸業海藏般若波羅蜜門。",
        "from": "T19n1019_001:0708b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法窮盡性不可得故。",
        "from": "T19n1019_001:0708b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　娑多(二合上)字時。",
        "from": "T19n1019_001:0708b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入摧諸煩惱清淨光明般若波羅蜜門。",
        "from": "T19n1019_001:0708b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法住持處非處令不動轉性不可得故。",
        "from": "T19n1019_001:0708b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　孃(輕呼上)字時。",
        "from": "T19n1019_001:0708b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入生世間了別般若波羅蜜門。",
        "from": "T19n1019_001:0708b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法能所知性不可得故。",
        "from": "T19n1019_001:0708b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　囉他(二合上)字時。",
        "from": "T19n1019_001:0708b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入逆生死輪智道場般若波羅蜜門。",
        "from": "T19n1019_001:0708b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法執著義性不可得故。",
        "from": "T19n1019_001:0708b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　婆(引去)字時。",
        "from": "T19n1019_001:0708b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入一切宮殿道場莊嚴般若波羅蜜門。",
        "from": "T19n1019_001:0708b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法可破壞性不可得故。",
        "from": "T19n1019_001:0708b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　磋(上)字時。",
        "from": "T19n1019_001:0708b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入修行加行藏蓋差別道場般若波羅蜜門。",
        "from": "T19n1019_001:0708b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法欲樂覆性不可得故。",
        "from": "T19n1019_001:0708b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　娑麼(二合)字時。",
        "from": "T19n1019_001:0708b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入現見十方諸佛旋般若波羅蜜門。",
        "from": "T19n1019_001:0708b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法可憶念性不可得故。",
        "from": "T19n1019_001:0708b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　訶嚩(二合)字時。",
        "from": "T19n1019_001:0708b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入觀察一切眾生堪任力遍生海藏般若波羅蜜門。",
        "from": "T19n1019_001:0708b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法可呼召性不可得故。",
        "from": "T19n1019_001:0708b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　哆娑(二合)字時。",
        "from": "T19n1019_001:0708c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入一切功德海趣入修行源底般若波羅蜜門。",
        "from": "T19n1019_001:0708c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法勇健性不可得故。",
        "from": "T19n1019_001:0708c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　伽(去)字時。",
        "from": "T19n1019_001:0708c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入持一切法雲堅固海藏般若波羅蜜門。",
        "from": "T19n1019_001:0708c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法厚平等性不可得故。",
        "from": "T19n1019_001:0708c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　姹(上)字時。",
        "from": "T19n1019_001:0708c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入願往詣十方現前見一切佛般若波羅蜜門。",
        "from": "T19n1019_001:0708c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法積集性不可得故。",
        "from": "T19n1019_001:0708c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　儜(上)字時。",
        "from": "T19n1019_001:0708c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入字輪積集俱胝字般若波羅蜜門。",
        "from": "T19n1019_001:0708c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法離諸諠諍無往無來行住坐臥不可得故。",
        "from": "T19n1019_001:0708c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　頗字時。",
        "from": "T19n1019_001:0708c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入成熟一切眾生際往詣道場般若波羅蜜門。",
        "from": "T19n1019_001:0708c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法遍滿果報不可得故。",
        "from": "T19n1019_001:0708c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　塞迦(二合上)字時。",
        "from": "T19n1019_001:0708c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入無著無礙解脫地藏光明輪普照般若波羅蜜門。",
        "from": "T19n1019_001:0708c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法積聚蘊性不可得故。",
        "from": "T19n1019_001:0708c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　也娑(上二合)字時。",
        "from": "T19n1019_001:0708c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入宣說一切佛法境界般若波羅蜜門。",
        "from": "T19n1019_001:0708c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法衰老性相不可得故。",
        "from": "T19n1019_001:0708c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　室左(二合上)字時。",
        "from": "T19n1019_001:0708c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入一切虛空以法雲雷震吼普照般若波羅蜜門。",
        "from": "T19n1019_001:0708c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法聚集足跡不可得故。",
        "from": "T19n1019_001:0708c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　吒(上)字時。",
        "from": "T19n1019_001:0708c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入無我利益眾生究竟邊際般若波羅蜜門。",
        "from": "T19n1019_001:0708c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法相驅迫性不可得故。",
        "from": "T19n1019_001:0708c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": " 　荼(引去)字時。",
        "from": "T19n1019_001:0708c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入法輪無差別藏般若波羅蜜門。",
        "from": "T19n1019_001:0708c28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悟一切法究竟處所不可得故。",
        "from": "T19n1019_001:0708c29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子我稱如是入諸解脫根本字時。",
        "from": "T19n1019_001:0709a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此四十二般若波羅蜜為首。",
        "from": "T19n1019_001:0709a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入無量無數般若波羅蜜門。",
        "from": "T19n1019_001:0709a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又善男子如是字門。",
        "from": "T19n1019_001:0709a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是能悟入法空邊際。",
        "from": "T19n1019_001:0709a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除如是字。",
        "from": "T19n1019_001:0709a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "表諸法空。",
        "from": "T19n1019_001:0709a05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更不可得。",
        "from": "T19n1019_001:0709a05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故如是字義不可宣說。",
        "from": "T19n1019_001:0709a05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可顯示。",
        "from": "T19n1019_001:0709a06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可執取。",
        "from": "T19n1019_001:0709a06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可書持。",
        "from": "T19n1019_001:0709a06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可觀察。",
        "from": "T19n1019_001:0709a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "離諸相故。",
        "from": "T19n1019_001:0709a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子譬如虛空是一切物所歸趣處。",
        "from": "T19n1019_001:0709a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸字門亦復如是。",
        "from": "T19n1019_001:0709a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法空義皆入此門。",
        "from": "T19n1019_001:0709a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方得顯了。",
        "from": "T19n1019_001:0709a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若菩薩摩訶薩於如是入諸字門。",
        "from": "T19n1019_001:0709a09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得善巧智。",
        "from": "T19n1019_001:0709a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸言音所詮所表。",
        "from": "T19n1019_001:0709a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆無罣礙。",
        "from": "T19n1019_001:0709a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於一切法平等空性。",
        "from": "T19n1019_001:0709a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡能證持。",
        "from": "T19n1019_001:0709a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於眾言音咸得善巧。",
        "from": "T19n1019_001:0709a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若菩薩摩訶薩。",
        "from": "T19n1019_001:0709a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能聽如是入諸字門。",
        "from": "T19n1019_001:0709a12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "印阿(上)字印。",
        "from": "T19n1019_001:0709a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞已受持。",
        "from": "T19n1019_001:0709a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦通利為他解說。",
        "from": "T19n1019_001:0709a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不貪名利。",
        "from": "T19n1019_001:0709a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此因緣得二十種殊勝功德。",
        "from": "T19n1019_001:0709a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等二十。",
        "from": "T19n1019_001:0709a15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂得強憶念。",
        "from": "T19n1019_001:0709a15_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得勝慚愧。",
        "from": "T19n1019_001:0709a15_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得堅固力。",
        "from": "T19n1019_001:0709a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得法旨趣。",
        "from": "T19n1019_001:0709a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得增上覺。",
        "from": "T19n1019_001:0709a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得殊勝慧。",
        "from": "T19n1019_001:0709a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無礙辯。",
        "from": "T19n1019_001:0709a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得總持門。",
        "from": "T19n1019_001:0709a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無疑惑。",
        "from": "T19n1019_001:0709a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得違順語不生恚愛。",
        "from": "T19n1019_001:0709a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得無高下平等而住。",
        "from": "T19n1019_001:0709a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得於有情言音善巧。",
        "from": "T19n1019_001:0709a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得蘊善巧處善巧界善巧。",
        "from": "T19n1019_001:0709a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得緣起善巧因善巧緣善巧法善巧。",
        "from": "T19n1019_001:0709a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得根勝劣智善巧他心智善巧。",
        "from": "T19n1019_001:0709a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得觀星曆善巧。",
        "from": "T19n1019_001:0709a21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得天耳智善巧宿住隨念智善巧神境智善巧死生智善巧。",
        "from": "T19n1019_001:0709a21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得漏盡智善巧。",
        "from": "T19n1019_001:0709a22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得說處非處智善巧。",
        "from": "T19n1019_001:0709a23_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得往來等威儀路善巧。",
        "from": "T19n1019_001:0709a23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為得二十種殊勝功德。",
        "from": "T19n1019_001:0709a24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子我唯知此入諸解脫根本字智。",
        "from": "T19n1019_001:0709a24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如諸菩薩摩訶薩。",
        "from": "T19n1019_001:0709a25_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能於一切世間善巧之法。",
        "from": "T19n1019_001:0709a26_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以智通達。",
        "from": "T19n1019_001:0709a26_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "到於彼岸。",
        "from": "T19n1019_001:0709a26_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我云何能知能說彼功德行。",
        "from": "T19n1019_001:0709a27_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時善財童子。",
        "from": "T19n1019_001:0709a27_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭面敬禮眾藝之足。",
        "from": "T19n1019_001:0709a28_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遶無數匝戀仰",
        "from": "T19n1019_001:0709a28_27",
        "context_with_next_sentence": false
    }
]