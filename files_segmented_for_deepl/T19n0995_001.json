[
    {
        "source_sentence": "新譯仁王般若經陀羅尼念誦軌",
        "from": "T19n0995_001:0519b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "儀序",
        "from": "T19n0995_001:0519b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大興善寺翻經沙門慧靈述",
        "from": "T19n0995_001:0519b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我皇帝聖德廣運仁育群品。",
        "from": "T19n0995_001:0519b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦既纂曆吹大法螺。",
        "from": "T19n0995_001:0519b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "刊梵言之輕重。",
        "from": "T19n0995_001:0519b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "警迷徒之耳目。",
        "from": "T19n0995_001:0519b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偉矣哉。",
        "from": "T19n0995_001:0519b28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迺辟興善寺大廣智三藏不空。",
        "from": "T19n0995_001:0519b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與義學沙門良賁等一十四人。",
        "from": "T19n0995_001:0519b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府魚朝恩翰林學士常袞等。",
        "from": "T19n0995_001:0519c01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "去歲夏四月。",
        "from": "T19n0995_001:0519c02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於南桃園再譯斯經。",
        "from": "T19n0995_001:0519c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "秋九月詔資聖西明。",
        "from": "T19n0995_001:0519c02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百座敷闡。",
        "from": "T19n0995_001:0519c03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下紫微而五雲抱出。",
        "from": "T19n0995_001:0519c03_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經長衢而萬姓作禮。",
        "from": "T19n0995_001:0519c04_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阡郭充滿猶墻堵焉。",
        "from": "T19n0995_001:0519c04_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "稽緇衣覽青史。",
        "from": "T19n0995_001:0519c05_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自摩騰入漢僧會遊矣。",
        "from": "T19n0995_001:0519c05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "瑞法之來莫與京者。",
        "from": "T19n0995_001:0519c05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經云若未來世有諸國王。",
        "from": "T19n0995_001:0519c06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立正法護三寶者。",
        "from": "T19n0995_001:0519c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我令五方菩薩往護其國令無災難。",
        "from": "T19n0995_001:0519c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又云五菩薩自於佛前發弘誓言。",
        "from": "T19n0995_001:0519c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我有陀羅尼能加持擁護。",
        "from": "T19n0995_001:0519c09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是一切佛本所修行速疾之門。",
        "from": "T19n0995_001:0519c09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人得聞一經於耳。",
        "from": "T19n0995_001:0519c10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有罪障悉皆消滅況復習誦而令通利。",
        "from": "T19n0995_001:0519c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛即讚言若誦持此陀羅尼者。",
        "from": "T19n0995_001:0519c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我及十方諸佛悉常擁護。",
        "from": "T19n0995_001:0519c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡鬼神敬之如佛。",
        "from": "T19n0995_001:0519c13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不久當得阿耨菩提。",
        "from": "T19n0995_001:0519c13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則知此陀羅尼。",
        "from": "T19n0995_001:0519c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "字母之根底眾瑜伽之藪澤。",
        "from": "T19n0995_001:0519c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如彼水木歸其本源。",
        "from": "T19n0995_001:0519c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故菩薩演之王者建之。",
        "from": "T19n0995_001:0519c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "黎人念之諸佛讚之。",
        "from": "T19n0995_001:0519c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "俾爾昌而熾俾爾福而利。",
        "from": "T19n0995_001:0519c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於可以見聖人之心。",
        "from": "T19n0995_001:0519c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其益既弘其軌亦妙。",
        "from": "T19n0995_001:0519c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苟于誠不克曷降之以嘉。",
        "from": "T19n0995_001:0519c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三藏是以譯具多之文。",
        "from": "T19n0995_001:0519c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良賁法師乃受從簡素。",
        "from": "T19n0995_001:0519c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "始夫處所方便終其觀行儀則。",
        "from": "T19n0995_001:0519c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修為十門第以位次。",
        "from": "T19n0995_001:0519c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一昭著庶無懵焉。",
        "from": "T19n0995_001:0519c20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "凡我道俗將保厥躬遂求願。",
        "from": "T19n0995_001:0519c21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "踐菩提之路登仁壽之域者。",
        "from": "T19n0995_001:0519c21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何莫由斯之道哉。",
        "from": "T19n0995_001:0519c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁王般若念誦法一卷",
        "from": "T19n0995_001:0520a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開府儀同三司特進試鴻臚卿",
        "from": "T19n0995_001:0520a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "肅國公食邑三千戶賜紫贈司",
        "from": "T19n0995_001:0520a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空謚大鑒正號大廣智大興善",
        "from": "T19n0995_001:0520a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寺三藏沙門不空奉　詔譯",
        "from": "T19n0995_001:0520a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛告波斯匿王。",
        "from": "T19n0995_001:0520a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今為王說仁王護國般若波羅蜜多持明之法。",
        "from": "T19n0995_001:0520a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "王當諦受一心善聽。",
        "from": "T19n0995_001:0520a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先明入道場儀軌。",
        "from": "T19n0995_001:0520a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸仁王為求息災。",
        "from": "T19n0995_001:0520a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先須沐浴著新淨衣。",
        "from": "T19n0995_001:0520a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若法行王者受近住戒。",
        "from": "T19n0995_001:0520a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應起殷重大乘之心。",
        "from": "T19n0995_001:0520a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "欲求成就不惜身命。",
        "from": "T19n0995_001:0520a12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於無邊有情廣起悲願濟度之心能如是者速得成就。",
        "from": "T19n0995_001:0520a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入道場已五體投地。",
        "from": "T19n0995_001:0520a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍禮法界一切三寶右膝著地懺悔三業一切罪障。",
        "from": "T19n0995_001:0520a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勸十方佛轉正法輪。",
        "from": "T19n0995_001:0520a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請諸如來久住於世。",
        "from": "T19n0995_001:0520a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨喜三乘所修福智。",
        "from": "T19n0995_001:0520a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我某甲所修功德。",
        "from": "T19n0995_001:0520a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆迴向無上菩提。",
        "from": "T19n0995_001:0520a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願共法界一切有情。",
        "from": "T19n0995_001:0520a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求悉地速得滿足。",
        "from": "T19n0995_001:0520a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結跏趺坐。",
        "from": "T19n0995_001:0520a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如其闕緣不得澡浴。",
        "from": "T19n0995_001:0520a19_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二手塗香發慇重心。",
        "from": "T19n0995_001:0520a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結清淨印。",
        "from": "T19n0995_001:0520a20_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手當心虛心合掌如未敷蓮華誦真言曰。",
        "from": "T19n0995_001:0520a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵娑嚩(二合)婆(引)嚩輸(入)鐸(引)薩嚩達磨(引)娑嚩(二合)婆嚩輸度撼誦此真言三遍。",
        "from": "T19n0995_001:0520a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正誦之時運心廣布。",
        "from": "T19n0995_001:0520a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法本來清淨。",
        "from": "T19n0995_001:0520a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故我身亦悉清淨。",
        "from": "T19n0995_001:0520a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即閉目運想。",
        "from": "T19n0995_001:0520a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿虛空一切諸佛菩薩道場眾會。",
        "from": "T19n0995_001:0520a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "執持種種上妙香花。",
        "from": "T19n0995_001:0520a27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業至誠頭面禮敬。",
        "from": "T19n0995_001:0520a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一結佛部三昧耶印",
        "from": "T19n0995_001:0520a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手當心。",
        "from": "T19n0995_001:0520b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "內相叉作拳。",
        "from": "T19n0995_001:0520b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "並竪二大拇指誦真言曰。",
        "from": "T19n0995_001:0520b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵爾那爾迦娑嚩(二合引)訶(去引)不出聲誦此真言三遍。",
        "from": "T19n0995_001:0520b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "下皆准知。",
        "from": "T19n0995_001:0520b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T19n0995_001:0520b04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結此印契誦此佛部三昧耶真言故。",
        "from": "T19n0995_001:0520b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方法界一切諸佛。",
        "from": "T19n0995_001:0520b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆雲集遍滿虛空。",
        "from": "T19n0995_001:0520b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持行者離諸障惱。",
        "from": "T19n0995_001:0520b06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業清淨所修行願速得成就。",
        "from": "T19n0995_001:0520b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二結諸菩薩部三昧耶印",
        "from": "T19n0995_001:0520b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩手當心如前作拳。",
        "from": "T19n0995_001:0520b10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左大拇指屈於掌中誦真言曰。",
        "from": "T19n0995_001:0520b10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵阿(引)嚧(引)力迦娑嚩(二合)訶(去)準前誦三遍。",
        "from": "T19n0995_001:0520b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T19n0995_001:0520b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結此印契誦此諸菩薩部三昧耶真言故。",
        "from": "T19n0995_001:0520b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得觀自在菩薩等。",
        "from": "T19n0995_001:0520b14_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方法界一切菩薩。",
        "from": "T19n0995_001:0520b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悉皆雲集遍滿虛空。",
        "from": "T19n0995_001:0520b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持行法行者三業清淨無諸災難謂諸菩薩承本悲願。",
        "from": "T19n0995_001:0520b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令所求者皆悉滿足。",
        "from": "T19n0995_001:0520b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第三結金剛部三昧耶印",
        "from": "T19n0995_001:0520b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右如前印舒左大拇指。",
        "from": "T19n0995_001:0520b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "屈右大拇指於掌中。",
        "from": "T19n0995_001:0520b19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T19n0995_001:0520b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合)地力迦娑嚩(二合引)訶准前誦三遍於頂上散。",
        "from": "T19n0995_001:0520b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結此印契誦金剛部三昧耶真言故。",
        "from": "T19n0995_001:0520b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即得十方法界一切金剛現威怒身。",
        "from": "T19n0995_001:0520b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如雲而集滿虛空界。",
        "from": "T19n0995_001:0520b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "加持行法行者。",
        "from": "T19n0995_001:0520b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三業堅固猶如金剛。",
        "from": "T19n0995_001:0520b25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼聖者承佛威神以自願力。",
        "from": "T19n0995_001:0520b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大則護持國界令無災難。",
        "from": "T19n0995_001:0520b26_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "小則乃至一身令無諸厄。",
        "from": "T19n0995_001:0520b26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第四結護身印",
        "from": "T19n0995_001:0520b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又用三部所結印契。",
        "from": "T19n0995_001:0520b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及誦真言五處加持。",
        "from": "T19n0995_001:0520b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂額左肩右肩心喉五處。",
        "from": "T19n0995_001:0520b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T19n0995_001:0520c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成被金剛堅固甲冑。",
        "from": "T19n0995_001:0520c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由此加持遍行者身。",
        "from": "T19n0995_001:0520c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "威光赫[亦/火]一切諸魔作障惱者。",
        "from": "T19n0995_001:0520c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼不敢覩疾走而去。",
        "from": "T19n0995_001:0520c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第五結辟除印及金剛方隅寶界印",
        "from": "T19n0995_001:0520c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右以前金剛部印契。",
        "from": "T19n0995_001:0520c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦彼真言繞壇左轉三匝。",
        "from": "T19n0995_001:0520c05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能辟除大力諸魔隨佛菩薩。",
        "from": "T19n0995_001:0520c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有隱者遠去他界。",
        "from": "T19n0995_001:0520c06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨心大小右轉三匝。",
        "from": "T19n0995_001:0520c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成金剛方隅寶界。",
        "from": "T19n0995_001:0520c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩尚不違越。",
        "from": "T19n0995_001:0520c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "況障惱者能得其便。",
        "from": "T19n0995_001:0520c08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T19n0995_001:0520c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第六結請聖眾降壇印",
        "from": "T19n0995_001:0520c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右用前三部印契及誦真言。",
        "from": "T19n0995_001:0520c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大拇指向身招請。",
        "from": "T19n0995_001:0520c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三遍三招。",
        "from": "T19n0995_001:0520c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即前滿空三部聖眾。",
        "from": "T19n0995_001:0520c12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各依本位不相障礙。",
        "from": "T19n0995_001:0520c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "寂然而住頂上散。",
        "from": "T19n0995_001:0520c13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第七獻閼伽香水印",
        "from": "T19n0995_001:0520c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右以兩手持捧摩尼寶器盛香水。",
        "from": "T19n0995_001:0520c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於眉間誦真言曰。",
        "from": "T19n0995_001:0520c15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵嚩日囉(二合引)娜迦吽(引)",
        "from": "T19n0995_001:0520c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "準上誦三遍。",
        "from": "T19n0995_001:0520c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "運心廣布。",
        "from": "T19n0995_001:0520c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第普浴一切聖眾。",
        "from": "T19n0995_001:0520c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T19n0995_001:0520c19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由獻閼伽故。",
        "from": "T19n0995_001:0520c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從勝解行地乃至法雲地。",
        "from": "T19n0995_001:0520c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於地地中十方法界諸佛菩薩。",
        "from": "T19n0995_001:0520c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉加護諸佛灌頂。",
        "from": "T19n0995_001:0520c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八獻寶座印",
        "from": "T19n0995_001:0520c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右以兩手當心虛心合掌。",
        "from": "T19n0995_001:0520c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二大拇指及二小指。",
        "from": "T19n0995_001:0520c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相附少屈。",
        "from": "T19n0995_001:0520c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘之六指各散。",
        "from": "T19n0995_001:0520c24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "微屈如開敷蓮華。",
        "from": "T19n0995_001:0520c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真言曰。",
        "from": "T19n0995_001:0520c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵迦磨攞娑嚩(二合引)訶",
        "from": "T19n0995_001:0520c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結印契及誦真言。",
        "from": "T19n0995_001:0520c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獻寶座令諸聖眾皆如實受用。",
        "from": "T19n0995_001:0520c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則令行法行者至果位中。",
        "from": "T19n0995_001:0520c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獲得金剛堅固寶座。",
        "from": "T19n0995_001:0520c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九結普供養印",
        "from": "T19n0995_001:0521a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右以兩手合掌。",
        "from": "T19n0995_001:0521a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五指互交以右押左。",
        "from": "T19n0995_001:0521a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於心上誦真言曰。",
        "from": "T19n0995_001:0521a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜莫三曼多沒馱(引)南(引)薩嚩他欠(平)烏娜誐(二合)諦薩頗(二合)囉呬(引)[牟*含]誐誐曩(上)劍娑嚩(二合引)訶由結此印誦真言故蓮心廣布。",
        "from": "T19n0995_001:0521a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周遍法界諸佛菩薩道場海會。",
        "from": "T19n0995_001:0521a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普雨一切諸供養具。",
        "from": "T19n0995_001:0521a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初誦一遍。",
        "from": "T19n0995_001:0521a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "塵沙寶器滿盛塗香普塗聖眾。",
        "from": "T19n0995_001:0521a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦第二遍。",
        "from": "T19n0995_001:0521a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種種花鬘普遍莊嚴。",
        "from": "T19n0995_001:0521a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦第三遍。",
        "from": "T19n0995_001:0521a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "燒種種香普遍供養。",
        "from": "T19n0995_001:0521a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦第四遍。",
        "from": "T19n0995_001:0521a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雨諸天中上妙飲食。",
        "from": "T19n0995_001:0521a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於寶器普遍供養。",
        "from": "T19n0995_001:0521a11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦第五遍。",
        "from": "T19n0995_001:0521a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雨諸摩尼以為燈明普遍供養。",
        "from": "T19n0995_001:0521a12_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛菩薩由誦真言加持力故。",
        "from": "T19n0995_001:0521a13_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獻香等於諸海會悉皆真實聖眾受用。",
        "from": "T19n0995_001:0521a14_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行法行者於當來世常獲是報。",
        "from": "T19n0995_001:0521a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十結般若波羅蜜多根本印",
        "from": "T19n0995_001:0521a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又以兩手背相附。",
        "from": "T19n0995_001:0521a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "收二頭指。",
        "from": "T19n0995_001:0521a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以二小指屈於掌中。",
        "from": "T19n0995_001:0521a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大拇指各押二指頭。",
        "from": "T19n0995_001:0521a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於心上。",
        "from": "T19n0995_001:0521a18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦經中陀羅尼七遍。",
        "from": "T19n0995_001:0521a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由結此印誦陀羅尼故。",
        "from": "T19n0995_001:0521a19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行法行者自身即便變成般若波羅蜜多菩薩。",
        "from": "T19n0995_001:0521a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而為一切諸佛之母。",
        "from": "T19n0995_001:0521a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其菩薩像結跏趺坐白蓮華上。",
        "from": "T19n0995_001:0521a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身黃金色。",
        "from": "T19n0995_001:0521a22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾寶瓔珞遍身莊嚴。",
        "from": "T19n0995_001:0521a22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "首戴寶冠繫冠白繒。",
        "from": "T19n0995_001:0521a22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兩邊垂下。",
        "from": "T19n0995_001:0521a23_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手當心持般若梵夾。",
        "from": "T19n0995_001:0521a23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "右手當乳作說法印。",
        "from": "T19n0995_001:0521a24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以大拇指押無名指頭。",
        "from": "T19n0995_001:0521a24_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即想菩薩從頂至足身諸毛孔。",
        "from": "T19n0995_001:0521a25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "流出光明作種種色。",
        "from": "T19n0995_001:0521a25_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍滿法界。",
        "from": "T19n0995_001:0521a26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一光中化無量佛。",
        "from": "T19n0995_001:0521a26_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍虛空界諸世界中。",
        "from": "T19n0995_001:0521a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "普為眾生當根宣說般若波羅蜜多甚深之法。",
        "from": "T19n0995_001:0521a27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令悟解。",
        "from": "T19n0995_001:0521a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "住三摩地行法行王作此觀已。",
        "from": "T19n0995_001:0521a28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頂上散印。",
        "from": "T19n0995_001:0521a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "手持數珠置於掌中。",
        "from": "T19n0995_001:0521a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "合掌當心。",
        "from": "T19n0995_001:0521b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T19n0995_001:0521b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唵尾嚧者那(引)麼攞娑嚩(二合引)訶(引)誦此三遍加持數珠頂上戴已。",
        "from": "T19n0995_001:0521b02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後當心。",
        "from": "T19n0995_001:0521b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "左手承珠右手移珠。",
        "from": "T19n0995_001:0521b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "念念相應住佛母三昧。",
        "from": "T19n0995_001:0521b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀心莫間斷。",
        "from": "T19n0995_001:0521b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦一百八遍或二十一遍。",
        "from": "T19n0995_001:0521b05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦真言曰。",
        "from": "T19n0995_001:0521b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "娜謨囉怛娜(二合)怛囉(二合)夜野(一)娜莫(引)阿哩夜(二合)吠(無蓋反)[口*路]者娜野(二)怛他(引)櫱多(引)夜囉訶(二合)諦(三)三貌三沒馱(引)野(四)娜莫阿(引)哩野(二合五)三滿多跋捺囉(二合引)野(六)[曰/月]地薩怛嚩(二合引)野(七)摩賀薩怛嚩(二合引)野(八)摩賀迦(引)嚕抳迦(引)野(九)怛儞野(二合)他(十引)枳穰(二合)娜鉢囉(二合)儞(引)閉(十一)惡乞叉(二合)野句勢(十二)鉢囉(二合)底婆(引)娜嚩底(十三)薩嚩沒馱(引)嚩路枳諦(十四)喻誐跛哩儞澁跛(二合)寧(十五)儼避(引)囉努囉嚩誐(引)係(十六)底哩野(三合)特嚩(二合十七)跛哩儞澁跛(二合)寧(十八)[曰/月]地質多散惹娜儞(十九)薩嚩(引)毘曬迦(引)毘色訖諦(二合二十)達磨娑(引)誐囉三步諦(二十一)阿慕伽室囉(二合)嚩儜(二十二)摩賀三滿多跋捺囉(二合)步彌(二十三)涅(奴逸反)哩野(二合)諦(二十四)尾野(二合)羯囉拏(二十五)跛哩鉢囉(二合)跛儞(二十六)薩嚩悉馱(二十七)娜麼塞訖哩(二合)諦(二十八)薩嚩冒地薩怛嚩(二合二十九)散惹娜儞(三十)婆誐嚩底(丁以反引三十一)沒馱(引)麼諦(三十二)阿囉嬭迦囉嬭(三十三)阿囉拏迦囉嬭(三十四)摩賀鉢囉(二合)枳穰(二合三十五)播囉弭諦娑嚩(二合引)賀(三十六)如是依前至心誦念。",
        "from": "T19n0995_001:0521b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍數足已。",
        "from": "T19n0995_001:0521b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即頂戴數珠置於本處。",
        "from": "T19n0995_001:0521b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "結三摩地印橫舒兩手以右押左。",
        "from": "T19n0995_001:0521c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "置於臍下。",
        "from": "T19n0995_001:0521c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "端身閉目頭少微屈專注心上。",
        "from": "T19n0995_001:0521c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諦觀圓明鏡智上縱廣一肘。",
        "from": "T19n0995_001:0521c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸遍法界。",
        "from": "T19n0995_001:0521c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "布字行列右旋次第觀。",
        "from": "T19n0995_001:0521c03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一字光明徹照。",
        "from": "T19n0995_001:0521c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從外向內至於地字。",
        "from": "T19n0995_001:0521c04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從內向外漸觀諸字。",
        "from": "T19n0995_001:0521c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "周而復始。",
        "from": "T19n0995_001:0521c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "至第三遍心善寂定。",
        "from": "T19n0995_001:0521c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了了分明觀所詮義。",
        "from": "T19n0995_001:0521c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生不滅一一平等。",
        "from": "T19n0995_001:0521c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆遍法界非動非靜。",
        "from": "T19n0995_001:0521c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定慧雙運永離諸相。",
        "from": "T19n0995_001:0521c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是般若波羅蜜多三摩地觀。",
        "from": "T19n0995_001:0521c08_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從此却結般若波羅蜜多印。",
        "from": "T19n0995_001:0521c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦陀羅尼七遍。",
        "from": "T19n0995_001:0521c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於頂上散。",
        "from": "T19n0995_001:0521c10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結普供養印。",
        "from": "T19n0995_001:0521c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前運心次第供養。",
        "from": "T19n0995_001:0521c10_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "對聖眾前。",
        "from": "T19n0995_001:0521c11_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以向所修所生功德。",
        "from": "T19n0995_001:0521c11_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡將資益所求諸願。",
        "from": "T19n0995_001:0521c11_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為國為家利他滿足。",
        "from": "T19n0995_001:0521c12_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後迴施眾生。",
        "from": "T19n0995_001:0521c12_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴嚴淨土迴向實際。",
        "from": "T19n0995_001:0521c13_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迴求無上菩提。",
        "from": "T19n0995_001:0521c13_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願共有情速至彼岸。",
        "from": "T19n0995_001:0521c13_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結前結界印。",
        "from": "T19n0995_001:0521c14_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前真言三遍左轉即成解界。",
        "from": "T19n0995_001:0521c14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次結前三部印。",
        "from": "T19n0995_001:0521c15_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦前真言三遍。",
        "from": "T19n0995_001:0521c15_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆以大拇指向外撥之。",
        "from": "T19n0995_001:0521c16_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即成發遣聖眾各歸本土。",
        "from": "T19n0995_001:0521c16_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行者作禮而去。",
        "from": "T19n0995_001:0521c17_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如常經行。",
        "from": "T19n0995_001:0521c17_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受持讀誦大乘勿散動也。",
        "from": "T19n0995_001:0521c17_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是五更至於晨朝以為初時。",
        "from": "T19n0995_001:0521c18_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從日午後至於未時為第二時。",
        "from": "T19n0995_001:0521c19_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從黃昏後至於中夜為第三時。",
        "from": "T19n0995_001:0521c19_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從中夜後至於五更為第四時。",
        "from": "T19n0995_001:0521c20_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是時中各依本數。",
        "from": "T19n0995_001:0521c21_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "精勤不怠一切他敵自然降伏。",
        "from": "T19n0995_001:0521c21_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切災難永不復生。",
        "from": "T19n0995_001:0521c22_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時波斯匿王聞佛說已。",
        "from": "T19n0995_001:0521c22_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即從坐起作禮圍繞。",
        "from": "T19n0995_001:0521c23_36",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踊躍信受奉行。",
        "from": "T19n0995_001:0521c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "仁王般若念誦法一卷",
        "from": "T19n0995_001:0522a01_0",
        "context_with_next_sentence": false
    }
]