[
    {
        "source_sentence": "大乘集菩薩學論卷第二十",
        "from": "T32n1636_020:0127b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "西天譯經三藏朝散大夫",
        "from": "T32n1636_020:0127b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "試鴻臚少卿宣梵大師賜紫",
        "from": "T32n1636_020:0127b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沙門臣日稱等奉　詔譯",
        "from": "T32n1636_020:0127b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "正命受用品第十五之餘",
        "from": "T32n1636_020:0127b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_020:0127b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此復明尸羅清淨。",
        "from": "T32n1636_020:0127b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如虛空藏經云。",
        "from": "T32n1636_020:0127b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遠離聲聞辟支佛心。",
        "from": "T32n1636_020:0127b20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於菩提心不生退轉。",
        "from": "T32n1636_020:0127b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於尸羅而得清淨。",
        "from": "T32n1636_020:0127b21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子。",
        "from": "T32n1636_020:0127b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如虛空體性清淨。",
        "from": "T32n1636_020:0127b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩持戒清淨亦爾。",
        "from": "T32n1636_020:0127b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空無垢。",
        "from": "T32n1636_020:0127b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩持戒無垢亦爾。",
        "from": "T32n1636_020:0127b23_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空寂靜菩薩持戒寂靜亦爾。",
        "from": "T32n1636_020:0127b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空無壞。",
        "from": "T32n1636_020:0127b24_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩持戒無壞亦爾。",
        "from": "T32n1636_020:0127b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如虛空無能過勝。",
        "from": "T32n1636_020:0127b25_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩持戒於有情中無有勝者。",
        "from": "T32n1636_020:0127b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如虛空清淨平等。",
        "from": "T32n1636_020:0127b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩行忍於諸有情平等和合清淨亦爾。",
        "from": "T32n1636_020:0127b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有人手持利斧入娑羅林斷其枝葉。",
        "from": "T32n1636_020:0127b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知彼樹無有瞋恚。",
        "from": "T32n1636_020:0127b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生分別誰能斷者。",
        "from": "T32n1636_020:0127b29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不分別誰所斷者。",
        "from": "T32n1636_020:0127b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩行忍亦復如是。",
        "from": "T32n1636_020:0127c01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為菩薩最上忍辱猶如虛空。",
        "from": "T32n1636_020:0127c01_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又寶髻經說。",
        "from": "T32n1636_020:0127c02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "身精進清淨。",
        "from": "T32n1636_020:0127c02_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂若此身猶如影響。",
        "from": "T32n1636_020:0127c03_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有言說自性無記。",
        "from": "T32n1636_020:0127c03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "了知是心畢竟空寂。",
        "from": "T32n1636_020:0127c04_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當以大悲被慈甲冑。",
        "from": "T32n1636_020:0127c04_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足諸行深修禪定。",
        "from": "T32n1636_020:0127c05_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於功德法無令缺減。",
        "from": "T32n1636_020:0127c05_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以菩提心觀諸眾生無有缺減。",
        "from": "T32n1636_020:0127c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂行布施乃至方便無有缺減。",
        "from": "T32n1636_020:0127c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於慈悲喜捨深心相應無有缺減。",
        "from": "T32n1636_020:0127c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於布施愛語利行同事無有缺減。",
        "from": "T32n1636_020:0127c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於正念正知無有缺減。",
        "from": "T32n1636_020:0127c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於念處正勤神足根力覺支聖道無有缺減。",
        "from": "T32n1636_020:0127c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至於奢摩他毘鉢舍那無有缺減。",
        "from": "T32n1636_020:0127c10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "捨業煩惱自性無知。",
        "from": "T32n1636_020:0127c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修檢其身無令縱逸。",
        "from": "T32n1636_020:0127c11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常勤佛事成熟有情。",
        "from": "T32n1636_020:0127c12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "咸得清涼安住寂靜。",
        "from": "T32n1636_020:0127c12_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子。",
        "from": "T32n1636_020:0127c13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名菩薩修禪波羅蜜多行清淨。",
        "from": "T32n1636_020:0127c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是乃至智慧波羅蜜多當如是知。",
        "from": "T32n1636_020:0127c14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增長勝力品第十六",
        "from": "T32n1636_020:0127c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_020:0127c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此明三種增長勝力。",
        "from": "T32n1636_020:0127c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂於所行行常無厭足。",
        "from": "T32n1636_020:0127c16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所度眾生不生懈退。",
        "from": "T32n1636_020:0127c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "求佛妙智堅固勇猛。",
        "from": "T32n1636_020:0127c17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是三種力非諸聲聞所能行故。",
        "from": "T32n1636_020:0127c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如寶雲經說。",
        "from": "T32n1636_020:0127c18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸眾生自性怯弱。",
        "from": "T32n1636_020:0127c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯除菩薩顯現增長。",
        "from": "T32n1636_020:0127c19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如來祕密經說。",
        "from": "T32n1636_020:0127c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時阿闍世王復白佛言。",
        "from": "T32n1636_020:0127c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "T32n1636_020:0127c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩具修幾法即能獲得如是勝力。",
        "from": "T32n1636_020:0127c21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛言。",
        "from": "T32n1636_020:0127c21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大王菩薩若修十法獲斯勝利。",
        "from": "T32n1636_020:0127c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何等為十。",
        "from": "T32n1636_020:0127c22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者菩薩寧捨身命終不棄捨無上正法。",
        "from": "T32n1636_020:0127c22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者於一切眾生作謙下想不增慢心。",
        "from": "T32n1636_020:0127c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者於彼劣弱眾生起愍念心不生損害。",
        "from": "T32n1636_020:0127c24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者見飢渴眾生施妙飲食。",
        "from": "T32n1636_020:0127c25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者見怖畏眾生施其無畏。",
        "from": "T32n1636_020:0127c26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者見疾病眾生施藥救療。",
        "from": "T32n1636_020:0127c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者見貧乏眾生惠令滿足。",
        "from": "T32n1636_020:0127c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者見佛塔廟形像圖拭圓淨。",
        "from": "T32n1636_020:0127c28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者出歡喜言安慰眾生。",
        "from": "T32n1636_020:0127c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者見彼負重疲困苦惱眾生為除重擔。",
        "from": "T32n1636_020:0127c29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若具如是十法。",
        "from": "T32n1636_020:0128a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即能獲得如是最勝之力。",
        "from": "T32n1636_020:0128a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又海意經說。",
        "from": "T32n1636_020:0128a02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若能發起精進。",
        "from": "T32n1636_020:0128a02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常所堅固勤行樂欲。",
        "from": "T32n1636_020:0128a03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所起精進無有休息。",
        "from": "T32n1636_020:0128a03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而諸菩薩即於阿耨多羅三藐三菩提不為難得。",
        "from": "T32n1636_020:0128a04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T32n1636_020:0128a05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "海意。",
        "from": "T32n1636_020:0128a05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由精進故乃得菩提。",
        "from": "T32n1636_020:0128a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若懈怠者於佛菩提遠中復遠。",
        "from": "T32n1636_020:0128a06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無懈怠者能行布施。",
        "from": "T32n1636_020:0128a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無懈怠者能集智慧。",
        "from": "T32n1636_020:0128a07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如月燈經云。",
        "from": "T32n1636_020:0128a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如水中生優鉢羅花。",
        "from": "T32n1636_020:0128a08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知次第而得增長。",
        "from": "T32n1636_020:0128a08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當漸修學布施等行。",
        "from": "T32n1636_020:0128a09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如能斷金剛經云。",
        "from": "T32n1636_020:0128a09_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若菩薩善住布施。",
        "from": "T32n1636_020:0128a10_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所獲福聚不可稱量。",
        "from": "T32n1636_020:0128a10_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如大般若說。",
        "from": "T32n1636_020:0128a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復次舍利子。",
        "from": "T32n1636_020:0128a11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若菩薩摩訶薩樂欲修習般若波羅蜜多。",
        "from": "T32n1636_020:0128a11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非少布施而得圓滿。",
        "from": "T32n1636_020:0128a12_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當施與一切眾生金銀珍寶園林舍宅種種所須隨其意樂。",
        "from": "T32n1636_020:0128a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以一切相智方便善巧。",
        "from": "T32n1636_020:0128a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能迴向無量阿僧祇一切眾生。",
        "from": "T32n1636_020:0128a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當學般若波羅蜜多。",
        "from": "T32n1636_020:0128a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_020:0128a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此明大悲空藏自性清淨受用福聚速疾增長。",
        "from": "T32n1636_020:0128a17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若無大悲非菩薩行。",
        "from": "T32n1636_020:0128a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立諸善此為根本。",
        "from": "T32n1636_020:0128a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是深心堅固大悲現前。",
        "from": "T32n1636_020:0128a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勇猛修作諸相應行。",
        "from": "T32n1636_020:0128a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能獲得尸羅清淨。",
        "from": "T32n1636_020:0128a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故有頌云。",
        "from": "T32n1636_020:0128a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如人善鬪戰　　利器心堅勇",
        "from": "T32n1636_020:0128a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　若小有懈退　　則為彼所執",
        "from": "T32n1636_020:0128a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如善財往詣聖慈善知識所。",
        "from": "T32n1636_020:0128a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即自念言。",
        "from": "T32n1636_020:0128a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我此身於過去世。",
        "from": "T32n1636_020:0128a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無勇猛心。",
        "from": "T32n1636_020:0128a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無堅固意。",
        "from": "T32n1636_020:0128a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無清淨因。",
        "from": "T32n1636_020:0128a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受諸輪轉。",
        "from": "T32n1636_020:0128a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心樂流蕩顛倒分別。",
        "from": "T32n1636_020:0128a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自邪思惟樂習欲行。",
        "from": "T32n1636_020:0128a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取著世間無利事業。",
        "from": "T32n1636_020:0128a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於自身所獲義利。",
        "from": "T32n1636_020:0128a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起不平等悉皆棄捨。",
        "from": "T32n1636_020:0128a27_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於現在世起大勇悍。",
        "from": "T32n1636_020:0128a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揀擇分別真實思惟。",
        "from": "T32n1636_020:0128a28_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸菩薩所行正行當勤修作。",
        "from": "T32n1636_020:0128a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸眾生起增上心多所饒益。",
        "from": "T32n1636_020:0128a29_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸佛所發大精進作大善利。",
        "from": "T32n1636_020:0128b01_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴諸根增長願力。",
        "from": "T32n1636_020:0128b01_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "讀誦經典心淨信解。",
        "from": "T32n1636_020:0128b02_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝持身心不生高舉。",
        "from": "T32n1636_020:0128b02_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常樂出離生老病死憂惱苦海。",
        "from": "T32n1636_020:0128b03_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而於後際樂行菩薩所行之行。",
        "from": "T32n1636_020:0128b03_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當往詣一切佛剎。",
        "from": "T32n1636_020:0128b04_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "敬受如來之所教誨。",
        "from": "T32n1636_020:0128b05_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "親近供養善說法師。",
        "from": "T32n1636_020:0128b05_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂求相應諸佛正法。",
        "from": "T32n1636_020:0128b06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承事供給諸善知識。",
        "from": "T32n1636_020:0128b06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "開示演說一切佛法。",
        "from": "T32n1636_020:0128b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若能如是思惟觀察。",
        "from": "T32n1636_020:0128b07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能增長願力智身。",
        "from": "T32n1636_020:0128b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "度脫眾生植眾德本。",
        "from": "T32n1636_020:0128b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如無盡意經說。",
        "from": "T32n1636_020:0128b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩獨一無侶勇猛堅固畢竟攝取阿耨多羅三藐三菩提。",
        "from": "T32n1636_020:0128b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "深心自修不假他作。",
        "from": "T32n1636_020:0128b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以精進鎧而自莊嚴。",
        "from": "T32n1636_020:0128b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如諸眾生所作善業。",
        "from": "T32n1636_020:0128b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦如是悉當作之。",
        "from": "T32n1636_020:0128b12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸菩薩從初發心所修諸行。",
        "from": "T32n1636_020:0128b12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我亦當行。",
        "from": "T32n1636_020:0128b13_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "施非我伴我是施伴。",
        "from": "T32n1636_020:0128b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒忍精進禪定智慧。",
        "from": "T32n1636_020:0128b13_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非是我伴。",
        "from": "T32n1636_020:0128b14_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我是彼伴。",
        "from": "T32n1636_020:0128b14_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸波羅蜜不能使我。",
        "from": "T32n1636_020:0128b14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而我能使諸波羅蜜。",
        "from": "T32n1636_020:0128b15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切善根皆亦如是。",
        "from": "T32n1636_020:0128b15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至於金剛坐道場壞諸魔眾。",
        "from": "T32n1636_020:0128b16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一剎那頃以平等相應慧得成阿耨多羅三藐三菩提如金剛幢經說。",
        "from": "T32n1636_020:0128b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如日天子出現世間獨一無侶。",
        "from": "T32n1636_020:0128b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所歷境界而無退轉。",
        "from": "T32n1636_020:0128b19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於生盲者不生厭患。",
        "from": "T32n1636_020:0128b19_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於羅睺阿修羅王不生厭患。",
        "from": "T32n1636_020:0128b20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於乾闥婆城不生厭患。",
        "from": "T32n1636_020:0128b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於閻浮提方處穢惡不生厭患。",
        "from": "T32n1636_020:0128b21_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於四天下地界微塵不生厭患。",
        "from": "T32n1636_020:0128b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸高山煙雲等障皆無厭患。",
        "from": "T32n1636_020:0128b22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩出現世間亦復如是。",
        "from": "T32n1636_020:0128b23_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無分別智正念了知。",
        "from": "T32n1636_020:0128b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若為眾生之所損害。",
        "from": "T32n1636_020:0128b24_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生厭患心無退轉。",
        "from": "T32n1636_020:0128b25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若於菩薩廣大善根而生嫉妬。",
        "from": "T32n1636_020:0128b25_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩於彼不生厭患心無退轉。",
        "from": "T32n1636_020:0128b26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復眾生為邪見垢之所染污。",
        "from": "T32n1636_020:0128b27_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩於彼不生厭患心無退轉。",
        "from": "T32n1636_020:0128b27_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見眾生常為瞋恚之所纏縛。",
        "from": "T32n1636_020:0128b28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩於彼亦不遠離。",
        "from": "T32n1636_020:0128b29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若見眾生愚癡覆障煩惱垢重。",
        "from": "T32n1636_020:0128b29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而復破壞菩提種子。",
        "from": "T32n1636_020:0128c01_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間無能救護。",
        "from": "T32n1636_020:0128c01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩於彼不生輕慢。",
        "from": "T32n1636_020:0128c02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故。",
        "from": "T32n1636_020:0128c02_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩大悲不見眾生有過失故。",
        "from": "T32n1636_020:0128c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "猶如日輪出現世間。",
        "from": "T32n1636_020:0128c03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令明顯無有曀障。",
        "from": "T32n1636_020:0128c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復眾生愚癡所覆。",
        "from": "T32n1636_020:0128c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不信諸佛不聞正法不識僧田。",
        "from": "T32n1636_020:0128c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自所造作種種苦因。",
        "from": "T32n1636_020:0128c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或墮地獄傍生鬼界。",
        "from": "T32n1636_020:0128c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時菩薩見彼眾生造是業已。",
        "from": "T32n1636_020:0128c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心不動亂亦不揀擇無有驚畏。",
        "from": "T32n1636_020:0128c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發堅勇心不生退轉。",
        "from": "T32n1636_020:0128c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決定代彼受諸苦惱。",
        "from": "T32n1636_020:0128c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T32n1636_020:0128c09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當荷負彼諸眾生。",
        "from": "T32n1636_020:0128c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至世間生老病死苦惱之難。",
        "from": "T32n1636_020:0128c09_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八無暇難。",
        "from": "T32n1636_020:0128c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸輪回難。",
        "from": "T32n1636_020:0128c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡見難。",
        "from": "T32n1636_020:0128c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壞善法難。",
        "from": "T32n1636_020:0128c11_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生無智難。",
        "from": "T32n1636_020:0128c11_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當畢竟令脫是難。",
        "from": "T32n1636_020:0128c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸眾生無明所蔽。",
        "from": "T32n1636_020:0128c12_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愛網所著。",
        "from": "T32n1636_020:0128c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有結所縛。",
        "from": "T32n1636_020:0128c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸苦籠縶不生覺了無求出離。",
        "from": "T32n1636_020:0128c13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常懷疑惑與願相違。",
        "from": "T32n1636_020:0128c13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於輪回海一向漂沒。",
        "from": "T32n1636_020:0128c14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當安住一切智王。",
        "from": "T32n1636_020:0128c14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生成就義利皆得解脫。",
        "from": "T32n1636_020:0128c15_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯我一人能為救護。",
        "from": "T32n1636_020:0128c15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假使一切世界悉為惡趣。",
        "from": "T32n1636_020:0128c16_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "受苦眾生充滿其中。",
        "from": "T32n1636_020:0128c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以我所集一切善根。",
        "from": "T32n1636_020:0128c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "平等迴向無不與者。",
        "from": "T32n1636_020:0128c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至最後邊際所經時分。",
        "from": "T32n1636_020:0128c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一惡趣消滅無餘。",
        "from": "T32n1636_020:0128c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一眾生皆得解脫。",
        "from": "T32n1636_020:0128c19_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若使一人未離苦者。",
        "from": "T32n1636_020:0128c19_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當以身質而出之。",
        "from": "T32n1636_020:0128c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願諸眾生因我身故得盡苦際獲安隱樂。",
        "from": "T32n1636_020:0128c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各樂出真實語言。",
        "from": "T32n1636_020:0128c21_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "勿相欺誑不生損害。",
        "from": "T32n1636_020:0128c22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當令發一切智心離五欲境行菩薩行。",
        "from": "T32n1636_020:0128c22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "畢竟安住無上正等菩提。",
        "from": "T32n1636_020:0128c23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所以者何。",
        "from": "T32n1636_020:0128c24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸眾生由著欲故攝屬魔境。",
        "from": "T32n1636_020:0128c24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛世尊之所訶毀。",
        "from": "T32n1636_020:0128c25_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當知貪欲眾苦之本。",
        "from": "T32n1636_020:0128c25_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是緣故。",
        "from": "T32n1636_020:0128c26_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或作諍訟。",
        "from": "T32n1636_020:0128c26_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或相鬪戰。",
        "from": "T32n1636_020:0128c26_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "起諸煩惱。",
        "from": "T32n1636_020:0128c26_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後墮地獄餓鬼畜生。",
        "from": "T32n1636_020:0128c27_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至斷生天業遠離諸佛。",
        "from": "T32n1636_020:0128c27_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何由能發無上智王。",
        "from": "T32n1636_020:0128c28_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸眾生為欲所沒。",
        "from": "T32n1636_020:0128c28_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "熾然燒煮無量過患。",
        "from": "T32n1636_020:0128c29_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我以善根平等回向。",
        "from": "T32n1636_020:0128c29_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令諸眾生皆悉捨離。",
        "from": "T32n1636_020:0129a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "樂求佛智得涅盤樂。",
        "from": "T32n1636_020:0129a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當為彼作大導師。",
        "from": "T32n1636_020:0129a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以方便智令達彼岸。",
        "from": "T32n1636_020:0129a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如日出照四大洲。",
        "from": "T32n1636_020:0129a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸境界咸令顯現。",
        "from": "T32n1636_020:0129a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若王宮殿聚落城邑人民往來所作事業。",
        "from": "T32n1636_020:0129a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成熟苗稼滋榮草木。",
        "from": "T32n1636_020:0129a04_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是日天子光明威德。",
        "from": "T32n1636_020:0129a05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨出世間決定無二。",
        "from": "T32n1636_020:0129a05_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是菩薩從初發心。",
        "from": "T32n1636_020:0129a06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "見諸眾生不種善根。",
        "from": "T32n1636_020:0129a06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即作是念。",
        "from": "T32n1636_020:0129a07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當救護一切眾生。",
        "from": "T32n1636_020:0129a07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當解脫一切眾生。",
        "from": "T32n1636_020:0129a07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當照明一切眾生。",
        "from": "T32n1636_020:0129a08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當教誨一切眾生。",
        "from": "T32n1636_020:0129a08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當入解一切眾生。",
        "from": "T32n1636_020:0129a09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當攝受一切眾生。",
        "from": "T32n1636_020:0129a09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當成熟一切眾生皆令安隱斷諸疑惑。",
        "from": "T32n1636_020:0129a10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又如日輪出現世間。",
        "from": "T32n1636_020:0129a11_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "光明普照不假他求。",
        "from": "T32n1636_020:0129a11_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩摩訶薩亦復如是。",
        "from": "T32n1636_020:0129a12_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出現世間見苦眾生。",
        "from": "T32n1636_020:0129a12_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不待他請而方救護。",
        "from": "T32n1636_020:0129a13_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無少善根而不回向。",
        "from": "T32n1636_020:0129a13_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以諸眾生而為莊嚴。",
        "from": "T32n1636_020:0129a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又無盡意經云。",
        "from": "T32n1636_020:0129a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩不計劫數而求菩提。",
        "from": "T32n1636_020:0129a14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從生死本至于今日。",
        "from": "T32n1636_020:0129a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所經時分不可稱量。",
        "from": "T32n1636_020:0129a15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於若干劫而作莊嚴。",
        "from": "T32n1636_020:0129a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至一覩諸佛一發道心。",
        "from": "T32n1636_020:0129a16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所經諸佛如殑伽沙。",
        "from": "T32n1636_020:0129a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "承事供養無有懈倦。",
        "from": "T32n1636_020:0129a17_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "方能解了一切眾生心之所行。",
        "from": "T32n1636_020:0129a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名菩薩無盡莊嚴。",
        "from": "T32n1636_020:0129a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是修習檀波羅蜜菩提分法。",
        "from": "T32n1636_020:0129a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能具足相好莊嚴。",
        "from": "T32n1636_020:0129a20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又寶雲經云。",
        "from": "T32n1636_020:0129a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩見諸眾生盲無慧眼。",
        "from": "T32n1636_020:0129a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "[怡台+龍]悷難調破戒懶墮眾惡悉具。",
        "from": "T32n1636_020:0129a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是時菩薩深起厭離求生淨土。",
        "from": "T32n1636_020:0129a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願我不聞諸惡之名。",
        "from": "T32n1636_020:0129a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作是念已。",
        "from": "T32n1636_020:0129a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復更思惟。",
        "from": "T32n1636_020:0129a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是諸眾生愚癡瘖啞無涅盤分。",
        "from": "T32n1636_020:0129a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生信心遠離諸佛。",
        "from": "T32n1636_020:0129a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我當調伏而救拔之。",
        "from": "T32n1636_020:0129a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "發是心時。",
        "from": "T32n1636_020:0129a25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切魔宮悉皆震動。",
        "from": "T32n1636_020:0129a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十方諸佛同聲讚歎。",
        "from": "T32n1636_020:0129a26_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是人不久坐於道場得阿耨多羅三藐三菩提。",
        "from": "T32n1636_020:0129a26_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1636_020:0129a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是漸次思惟觀察。",
        "from": "T32n1636_020:0129a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能增長無量福聚。",
        "from": "T32n1636_020:0129a28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應當質直深心堅固修作。",
        "from": "T32n1636_020:0129a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如法集經說。",
        "from": "T32n1636_020:0129a29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於佛法中質直為本。",
        "from": "T32n1636_020:0129a29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若諸菩薩無質直心。",
        "from": "T32n1636_020:0129b01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則是遠離一切佛法。",
        "from": "T32n1636_020:0129b01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若是具足深心堅固。",
        "from": "T32n1636_020:0129b02_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於未曾聞深妙之法而生渴仰。",
        "from": "T32n1636_020:0129b02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或於空中山林樹間。",
        "from": "T32n1636_020:0129b03_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然出於深妙法音而得解了。",
        "from": "T32n1636_020:0129b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故菩薩當如是行。",
        "from": "T32n1636_020:0129b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人有足則能遊行。",
        "from": "T32n1636_020:0129b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若具質直深心。",
        "from": "T32n1636_020:0129b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能修行一切佛法。",
        "from": "T32n1636_020:0129b06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人有身則有壽命。",
        "from": "T32n1636_020:0129b06_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若具質直深心。",
        "from": "T32n1636_020:0129b07_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能獲得諸佛菩提。",
        "from": "T32n1636_020:0129b07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如人有命則有財利。",
        "from": "T32n1636_020:0129b08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若具質直深心。",
        "from": "T32n1636_020:0129b08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而能獲得諸佛聖財。",
        "from": "T32n1636_020:0129b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如大炬其焰熾然。",
        "from": "T32n1636_020:0129b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若具質直深心。",
        "from": "T32n1636_020:0129b09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸佛法則能明了。",
        "from": "T32n1636_020:0129b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如有雲則能降雨。",
        "from": "T32n1636_020:0129b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若具質直深心。",
        "from": "T32n1636_020:0129b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則能宣布諸佛法雨。",
        "from": "T32n1636_020:0129b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故菩薩應當了知質直深心善自守護。",
        "from": "T32n1636_020:0129b12_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "譬如樹根腐敗則不能生枝葉花果。",
        "from": "T32n1636_020:0129b13_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "菩薩若無質直深心。",
        "from": "T32n1636_020:0129b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸善法不復生長。",
        "from": "T32n1636_020:0129b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不能取諸佛菩提。",
        "from": "T32n1636_020:0129b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大乘集菩薩學論卷第二十",
        "from": "T32n1636_020:0129b16_0",
        "context_with_next_sentence": false
    }
]