[
    {
        "source_sentence": "佛說六字神呪王經",
        "from": "T20n1045a_001:0039c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "失譯人名今附梁錄",
        "from": "T20n1045a_001:0039c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞。",
        "from": "T20n1045a_001:0039c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時佛在舍衛國祇陀林中。",
        "from": "T20n1045a_001:0039c09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時有一外道旃陀羅女。",
        "from": "T20n1045a_001:0039c09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "專行眾惡符書厭禱。",
        "from": "T20n1045a_001:0039c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "或事山神樹神樹下鬼神。",
        "from": "T20n1045a_001:0039c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日月五星南斗北辰。",
        "from": "T20n1045a_001:0039c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切魍魎雜魔邪魅。",
        "from": "T20n1045a_001:0039c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惑尊者阿難及諸善人。",
        "from": "T20n1045a_001:0039c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等恒河沙數。",
        "from": "T20n1045a_001:0039c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時如來因見阿難恍惚。",
        "from": "T20n1045a_001:0039c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "憐愍一切故。",
        "from": "T20n1045a_001:0039c14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為說此六字神呪王經。",
        "from": "T20n1045a_001:0039c14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "先佛所說我今亦說。",
        "from": "T20n1045a_001:0039c15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0039c15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "安陀隸　槃陀隸　迦羅知　趐由知　帝闍波帝　頻頭波帝　陀頭隸　陀究帝隸　陀究摩帝　修摩帝　安陀羅　槃陀羅　檀陀羅　提兜羅　阿那延陀　曼陀波帝　阿那　阿那夜　摩頭　摩波帝　迦羅吒　翅由羅　浮登伽彌　帝闍波帝　頻頭摩帝　阿羅婆　伽婆帝　毘咤　毘提膩　至吒　毘提膩　悉波呵若有為某甲作惡呪詛。",
        "from": "T20n1045a_001:0039c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作已若當作。",
        "from": "T20n1045a_001:0039c25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天若龍。",
        "from": "T20n1045a_001:0039c25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夜叉若羅剎。",
        "from": "T20n1045a_001:0039c26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若餓鬼若鳩[目*杲]茶。",
        "from": "T20n1045a_001:0039c26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若富多那若毘舍闍。",
        "from": "T20n1045a_001:0039c26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若阿波摩羅若憂波摩羅。",
        "from": "T20n1045a_001:0039c27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若吉遮若佉傴陀。",
        "from": "T20n1045a_001:0039c27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若毘陀羅若半毘陀羅。",
        "from": "T20n1045a_001:0039c28_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若沙門若婆羅門。",
        "from": "T20n1045a_001:0039c28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若剎利若毘舍若首陀。",
        "from": "T20n1045a_001:0039c29_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若摩登伽女若栴陀羅若栴陀羅女。",
        "from": "T20n1045a_001:0039c29_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若奴若婢。",
        "from": "T20n1045a_001:0040a01_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若男若女。",
        "from": "T20n1045a_001:0040a01_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若外道若尼乾。",
        "from": "T20n1045a_001:0040a02_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若外道出家男女。",
        "from": "T20n1045a_001:0040a02_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等種種悉能呪詛[示*厭]蠱。",
        "from": "T20n1045a_001:0040a02_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今以此六字神呪王經。",
        "from": "T20n1045a_001:0040a03_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天上作天上滅之。",
        "from": "T20n1045a_001:0040a04_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天下作天下滅之。",
        "from": "T20n1045a_001:0040a04_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若雲漢中作雲漢中滅之。",
        "from": "T20n1045a_001:0040a05_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若虛空中作虛空中滅之。",
        "from": "T20n1045a_001:0040a05_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若地上作地上滅之。",
        "from": "T20n1045a_001:0040a06_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若地下作地下滅之。",
        "from": "T20n1045a_001:0040a06_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若八表外作八表外滅之。",
        "from": "T20n1045a_001:0040a07_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若六合內作六合內滅之。",
        "from": "T20n1045a_001:0040a07_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若隨風飃中作隨風飃中滅之。",
        "from": "T20n1045a_001:0040a08_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若四海大水邊作四海大水邊滅之。",
        "from": "T20n1045a_001:0040a09_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若江河邊作江河邊滅之。",
        "from": "T20n1045a_001:0040a09_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若淮濟邊作淮濟邊滅之。",
        "from": "T20n1045a_001:0040a10_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若陂溏邊作陂溏邊滅之。",
        "from": "T20n1045a_001:0040a11_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若雜水邊作雜水邊滅之。",
        "from": "T20n1045a_001:0040a11_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若大山邊作大山邊滅之。",
        "from": "T20n1045a_001:0040a12_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若五岳邊作五岳邊滅之。",
        "from": "T20n1045a_001:0040a12_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若五岳中作五岳中滅之。",
        "from": "T20n1045a_001:0040a13_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若坑谷邊作坑谷邊滅之。",
        "from": "T20n1045a_001:0040a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若坑谷中作坑谷中滅之。",
        "from": "T20n1045a_001:0040a14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若茂林邊作茂林邊滅之。",
        "from": "T20n1045a_001:0040a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若茂林中作茂林中滅之。",
        "from": "T20n1045a_001:0040a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若樹木邊作樹木邊滅之。",
        "from": "T20n1045a_001:0040a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若樹下根中作樹下根中滅之。",
        "from": "T20n1045a_001:0040a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若大道徑邊作大道徑邊滅之。",
        "from": "T20n1045a_001:0040a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若大道徑中作大道徑中滅之。",
        "from": "T20n1045a_001:0040a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若小四交道邊作小四交道邊滅之。",
        "from": "T20n1045a_001:0040a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若城外門作城外門滅之。",
        "from": "T20n1045a_001:0040a19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若城內作城內滅之。",
        "from": "T20n1045a_001:0040a20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若宮外門作宮外門滅之。",
        "from": "T20n1045a_001:0040a20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若宮內作宮內滅之。",
        "from": "T20n1045a_001:0040a21_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若婆羅門作悉皆滅之。",
        "from": "T20n1045a_001:0040a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若塚墓邊作塚墓邊滅之。",
        "from": "T20n1045a_001:0040a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若塚墓中作塚墓中滅之。",
        "from": "T20n1045a_001:0040a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若垣牆邊作垣牆邊滅之。",
        "from": "T20n1045a_001:0040a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若家舍宅中作家舍宅中滅之。",
        "from": "T20n1045a_001:0040a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若屋宕作屋宕滅之。",
        "from": "T20n1045a_001:0040a25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若柱邊作柱邊滅之。",
        "from": "T20n1045a_001:0040a25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若柱趺下作柱趺下滅之。",
        "from": "T20n1045a_001:0040a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若坋土邊作坋土邊滅之。",
        "from": "T20n1045a_001:0040a26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若坋土中作坋土中滅之。",
        "from": "T20n1045a_001:0040a27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若井邊作井邊滅之。",
        "from": "T20n1045a_001:0040a27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若井中作井中滅之。",
        "from": "T20n1045a_001:0040a28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若碓磨邊作碓磨邊滅之。",
        "from": "T20n1045a_001:0040a28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爐竈邊作爐竈邊滅之。",
        "from": "T20n1045a_001:0040a29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若爐竈中作爐竈中滅之。",
        "from": "T20n1045a_001:0040a29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若火邊作火邊滅之。",
        "from": "T20n1045a_001:0040b01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若火中作火中滅之。",
        "from": "T20n1045a_001:0040b01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若廁溷邊作廁溷邊滅之。",
        "from": "T20n1045a_001:0040b02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若廁溷便中作廁溷便中滅之。",
        "from": "T20n1045a_001:0040b02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若車乘具邊作車乘具邊滅之。",
        "from": "T20n1045a_001:0040b03_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若車乘具中作車乘具中滅之。",
        "from": "T20n1045a_001:0040b04_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若鞍馬服飾邊作鞍馬服飾邊滅之。",
        "from": "T20n1045a_001:0040b05_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若床敷邊作床敷邊滅之。",
        "from": "T20n1045a_001:0040b05_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若床敷中作床敷寂滅之。",
        "from": "T20n1045a_001:0040b06_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若衣被氈褥邊作衣被氈褥邊滅之。",
        "from": "T20n1045a_001:0040b07_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若靴帽中作靴帽中滅之。",
        "from": "T20n1045a_001:0040b07_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若雜器中作雜器中滅之。",
        "from": "T20n1045a_001:0040b08_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若飲食中作飲食中滅之。",
        "from": "T20n1045a_001:0040b09_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若側近人作者悉皆滅之。",
        "from": "T20n1045a_001:0040b09_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夜作夜滅之。",
        "from": "T20n1045a_001:0040b10_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若晝作晝滅之。",
        "from": "T20n1045a_001:0040b10_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "晝夜常作悉皆滅之。",
        "from": "T20n1045a_001:0040b10_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此呪能斷帝釋呪道。",
        "from": "T20n1045a_001:0040b11_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能斷梵天呪道。",
        "from": "T20n1045a_001:0040b11_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能斷四天大王呪道。",
        "from": "T20n1045a_001:0040b12_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與一切所有邪鬼呪方道術悉皆能斷。",
        "from": "T20n1045a_001:0040b12_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有為某甲作惡呪詛。",
        "from": "T20n1045a_001:0040b13_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與毒藥悉皆滅之。",
        "from": "T20n1045a_001:0040b14_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若佉傴陀若毘陀羅。",
        "from": "T20n1045a_001:0040b14_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若吉遮若富多那所為悉皆滅之。",
        "from": "T20n1045a_001:0040b15_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾惡都盡眾善補處。",
        "from": "T20n1045a_001:0040b16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此呪常吉。",
        "from": "T20n1045a_001:0040b16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此真實章句。",
        "from": "T20n1045a_001:0040b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使某甲晝安夜安晝夜常安。",
        "from": "T20n1045a_001:0040b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得受百歲得見百秋。",
        "from": "T20n1045a_001:0040b17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故世間最上無過於佛。",
        "from": "T20n1045a_001:0040b18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天尊所上眾神仰奉。",
        "from": "T20n1045a_001:0040b18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有行惡呪道。",
        "from": "T20n1045a_001:0040b19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作已作若當作。",
        "from": "T20n1045a_001:0040b19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若中作若成就若未成就悉皆滅之。",
        "from": "T20n1045a_001:0040b20_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與一切眾毒能害人者。",
        "from": "T20n1045a_001:0040b20_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此六字神呪王經皆消滅之。",
        "from": "T20n1045a_001:0040b21_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0040b22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉智　佉住　佉毘智　緘壽　緘壽　多智　婆智若有為某甲作惡呪詛。",
        "from": "T20n1045a_001:0040b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若作已若當作。",
        "from": "T20n1045a_001:0040b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若天若龍。",
        "from": "T20n1045a_001:0040b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若夜叉若羅剎。",
        "from": "T20n1045a_001:0040b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及與一切能作惡呪詛悉皆滅之。",
        "from": "T20n1045a_001:0040b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若佉傴陀若毘陀羅若富多那所為悉皆滅之。",
        "from": "T20n1045a_001:0040b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾惡都盡眾善補處。",
        "from": "T20n1045a_001:0040b28_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此呪常吉。",
        "from": "T20n1045a_001:0040b28_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此真實章句。",
        "from": "T20n1045a_001:0040b29_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使某甲晝安夜安晝夜常安。",
        "from": "T20n1045a_001:0040b29_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得受百歲得見百秋。",
        "from": "T20n1045a_001:0040c01_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故世間最妙無過於法。",
        "from": "T20n1045a_001:0040c01_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天人所欽眾魔奉受離欲無著。",
        "from": "T20n1045a_001:0040c02_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0040c02_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉智　佉住　佉毘智　緘壽　緘壽　多智婆智何以故。",
        "from": "T20n1045a_001:0040c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世間最貴無過於僧。",
        "from": "T20n1045a_001:0040c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "良善福田賢聖所敬眾善守護。",
        "from": "T20n1045a_001:0040c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0040c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佉智　佉住　佉毘智　緘壽　緘壽　多智　婆智　兜帝　阿彌帝　阿周帝若有為某甲作惡呪詛。",
        "from": "T20n1045a_001:0040c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若和合毒藥。",
        "from": "T20n1045a_001:0040c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若真若偽所作非法。",
        "from": "T20n1045a_001:0040c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若前身若今身。",
        "from": "T20n1045a_001:0040c11_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行業眾惡一切重罪。",
        "from": "T20n1045a_001:0040c11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉滅之。",
        "from": "T20n1045a_001:0040c12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0040c12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩烋波　烏摩陀　蓰陀禰　佉傴他　波提　蓰陀禰　頻頭摩提　至帝　至多提婆　蓰陀禰　阿渒　旃陀渒　悉波訶若有人誦持六字神呪王經。",
        "from": "T20n1045a_001:0040c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "假令呪枯樹可得還生枝葉。",
        "from": "T20n1045a_001:0040c16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何況呪人身。",
        "from": "T20n1045a_001:0040c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "使某甲得受百歲得見百秋。",
        "from": "T20n1045a_001:0040c17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛所說阿難所傳。",
        "from": "T20n1045a_001:0040c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若人讀誦通利悉皆自護。",
        "from": "T20n1045a_001:0040c18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾惡遠身恒與善居。",
        "from": "T20n1045a_001:0040c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若呪他者能除彼人眾苦惡患。",
        "from": "T20n1045a_001:0040c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦者斷五辛至心鮮潔然後乃能行之。",
        "from": "T20n1045a_001:0040c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸難凶禍無有遺餘吉無不利。",
        "from": "T20n1045a_001:0040c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0040c22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呵呵遮利　呵呵那彌　呵呵浮丘陀摩提　呵呵浮丘陀　尼劬沙　呵呵尼呵　陀佉傴陀　尼毘沙　佉傴陀　伊禰　彌禰　陀弊　陀陀弊　悉波呵南無佛觀世音菩薩摩訶薩。",
        "from": "T20n1045a_001:0040c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無法。",
        "from": "T20n1045a_001:0040c27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無比丘僧。",
        "from": "T20n1045a_001:0040c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禮彼大士。",
        "from": "T20n1045a_001:0040c28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後說是神呪。",
        "from": "T20n1045a_001:0040c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "願此呪常吉。",
        "from": "T20n1045a_001:0040c28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觀世音觀照我身觀世音觀照我身憐愍我故。",
        "from": "T20n1045a_001:0040c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0041a01_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "烏呵尼　無呵尼　阿陀　尼阿陀　毘阿陀　闍毘尼　耽毘尼　安陀隸　那陀隸　斯鼻提　安陀　羅婆斯　薩陸豆　率吒婆羅　豆率吒　貿耽闍波　夜耽波　夜無呵夜　尼波羅　夜耶波某　羅提　槃某遮提　盧婁至吒　般陀斯　至吒般陀斯　悉波呵此呪能護一切厄難。",
        "from": "T20n1045a_001:0041a02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鮮淨光明解脫無垢觀照一切濟度眾難迷荒惡魔。",
        "from": "T20n1045a_001:0041a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現世護身。",
        "from": "T20n1045a_001:0041a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "師子狂象虎狼猨猴。",
        "from": "T20n1045a_001:0041a09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及諸毒獸欲害人者皆悉被縛。",
        "from": "T20n1045a_001:0041a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "我今為某甲及共行伴。",
        "from": "T20n1045a_001:0041a11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切呪力所向之處願無不吉。",
        "from": "T20n1045a_001:0041a11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0041a12_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "知醯利　知利　收利　摩登耆　旃陀利　羅叉羅叉　羅耶　悉波呵南無佛陀。",
        "from": "T20n1045a_001:0041a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無達摩。",
        "from": "T20n1045a_001:0041a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無僧伽。",
        "from": "T20n1045a_001:0041a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "必願此呪令大吉。",
        "from": "T20n1045a_001:0041a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善用此呪法。",
        "from": "T20n1045a_001:0041a16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當用之日洗浴鮮潔淨除垢穢。",
        "from": "T20n1045a_001:0041a16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不得行婬不食五辛飲酒。",
        "from": "T20n1045a_001:0041a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "噉肉以白疊綖手提其綖。",
        "from": "T20n1045a_001:0041a17_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦呪七遍一遍一結。",
        "from": "T20n1045a_001:0041a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有官事被言。",
        "from": "T20n1045a_001:0041a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有鬪諍。",
        "from": "T20n1045a_001:0041a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有呪詛。",
        "from": "T20n1045a_001:0041a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及一切眾惡悉皆滅之。",
        "from": "T20n1045a_001:0041a19_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "呪訖此綖繫著其人淨衣帶中。",
        "from": "T20n1045a_001:0041a20_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "事過之後。",
        "from": "T20n1045a_001:0041a20_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請佛及僧燒眾名香。",
        "from": "T20n1045a_001:0041a21_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即就靜坐讀大乘經設供齋會任力有無報佛慈恩主自解願永絕八難善願從心即說呪曰。",
        "from": "T20n1045a_001:0041a21_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "醯豆摩帝　耶舍波帝　波沙　無婆帝　烏受波帝　欝多羅尼　阿叉　夜波帝　阿[少/兔]羶　帝目視　婆羅婆某帝婆羅婆只豆　悉波呵南無當來過去現在常住三世無量諸佛。",
        "from": "T20n1045a_001:0041a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "文殊師利普賢彌勒首楞嚴三昧。",
        "from": "T20n1045a_001:0041a28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "請佛觀世音消伏毒害。",
        "from": "T20n1045a_001:0041a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛空藏寶幢孔雀陀羅尼六字章句。",
        "from": "T20n1045a_001:0041b01_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "堅意菩薩不污菩薩。",
        "from": "T20n1045a_001:0041b02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各稱一切得道賢聖。",
        "from": "T20n1045a_001:0041b02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歸依如是菩薩摩訶薩眾。",
        "from": "T20n1045a_001:0041b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如前三稱三寶。",
        "from": "T20n1045a_001:0041b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後說此大神陀羅尼呪。",
        "from": "T20n1045a_001:0041b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即說呪曰。",
        "from": "T20n1045a_001:0041b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "迦羅尼　摩帝毘　怒多尼　伽帝婆羅　毘耶舍　首羅　婆隸　陀[少/兔]提　散陀婆提　欝多羅尼　舍陀某佉　舍陀娑羅毘舍　羶陀　迦梨舍　婆薩那　婆伽帝　究舍羅　婆娑尼　阿[少/兔]散帝　毘散帝　知帝　阿知帝　湯那伽帝　阿湯那伽帝　阿婆娑利　舍婆羅禰目隸　牟羅牟羅　婆陀禰　舍尼　婆羅陸　須梨婆帝　殊帝　婆羅陸　須毘摩隸　菴摩隸此呪能除一切冥闇如來所持諸天敬愛。",
        "from": "T20n1045a_001:0041b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善神擁護梵天解悟。",
        "from": "T20n1045a_001:0041b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "帝釋奉事護世營護。",
        "from": "T20n1045a_001:0041b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "決了大聖仙賢受持和合。",
        "from": "T20n1045a_001:0041b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾生解脫繫縛。",
        "from": "T20n1045a_001:0041b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四天以下及人民眾斷絕煩惱降伏魔怨。",
        "from": "T20n1045a_001:0041b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "化令外道摧惡就善。",
        "from": "T20n1045a_001:0041b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諍嫉論說憍慢之心。",
        "from": "T20n1045a_001:0041b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善無不慈增崇善根。",
        "from": "T20n1045a_001:0041b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超越法師不捨大眾。",
        "from": "T20n1045a_001:0041b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "悅人學習守護經典。",
        "from": "T20n1045a_001:0041b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚深法藏利益三寶。",
        "from": "T20n1045a_001:0041b20_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "愍念一切眾生之類。",
        "from": "T20n1045a_001:0041b20_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "莊嚴眾義有如是利。",
        "from": "T20n1045a_001:0041b21_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名無盡藏此陀羅尼句非護能護。",
        "from": "T20n1045a_001:0041b21_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若復有人能讀誦者。",
        "from": "T20n1045a_001:0041b22_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得三十三無疑畏難。",
        "from": "T20n1045a_001:0041b22_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "用此呪法清晨早起洗手漱口燒香禮拜三遍讀誦。",
        "from": "T20n1045a_001:0041b23_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所行之處言語談論。",
        "from": "T20n1045a_001:0041b24_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切向處無常不勝。",
        "from": "T20n1045a_001:0041b24_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眾人貴敬見者歡喜。",
        "from": "T20n1045a_001:0041b25_20",
