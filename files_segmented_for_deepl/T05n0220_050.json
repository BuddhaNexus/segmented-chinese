        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何以故？",
        "from": "T05n0220_050:0280c20_35",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "色處畢竟不可得故。",
        "from": "T05n0220_050:0280c20_36",
        "context_with_next_sentence": true
    },
