[
    {
        "source_sentence": "諸佛心陀羅尼經",
        "from": "T19n0918_001:0001a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大唐三藏法師玄奘奉　詔譯",
        "from": "T19n0918_001:0001a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是我聞一時薄伽梵住如來境眾寶道場。",
        "from": "T19n0918_001:0001a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛所都諸佛所樂。",
        "from": "T19n0918_001:0001a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "智無疑滯菩薩妙宮。",
        "from": "T19n0918_001:0001a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具諸微妙種種嚴飾常演法音大功德殿。",
        "from": "T19n0918_001:0001a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與無央數大菩薩俱。",
        "from": "T19n0918_001:0001a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆是如來法身真子。",
        "from": "T19n0918_001:0001a09_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從諸佛土而來集會。",
        "from": "T19n0918_001:0001a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無量天人阿素洛等。",
        "from": "T19n0918_001:0001a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應真大眾前後圍遶。",
        "from": "T19n0918_001:0001a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸菩薩摩訶薩言。",
        "from": "T19n0918_001:0001a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子有陀羅尼名諸佛心。",
        "from": "T19n0918_001:0001a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "殑伽沙等諸佛同說。",
        "from": "T19n0918_001:0001a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能遍饒益諸怖畏者。",
        "from": "T19n0918_001:0001a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有至心受持讀誦。",
        "from": "T19n0918_001:0001a14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "超百千劫生死劇苦。",
        "from": "T19n0918_001:0001a14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "定於無上正等菩提。",
        "from": "T19n0918_001:0001a15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能速修行永無退轉。",
        "from": "T19n0918_001:0001a15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無上正等菩提。",
        "from": "T19n0918_001:0001a16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "終不枉生無佛世界。",
        "from": "T19n0918_001:0001a16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒善悟解諸陀羅尼。",
        "from": "T19n0918_001:0001a17_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常見如來親近供養。",
        "from": "T19n0918_001:0001a17_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒憶宿命深信因果。",
        "from": "T19n0918_001:0001a18_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能使現世人非人等怨害皆除。",
        "from": "T19n0918_001:0001a18_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疾病不侵無有中夭。",
        "from": "T19n0918_001:0001a19_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸惡魔事皆悉殄滅。",
        "from": "T19n0918_001:0001a19_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所有惡業無不消除。",
        "from": "T19n0918_001:0001a20_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切魔軍驚怖退散。",
        "from": "T19n0918_001:0001a20_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善男子此陀羅尼文字章句。",
        "from": "T19n0918_001:0001a21_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是一切佛共所稱揚。",
        "from": "T19n0918_001:0001a21_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即是諸佛文字章句。",
        "from": "T19n0918_001:0001a22_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝應諦聽陀羅尼曰。",
        "from": "T19n0918_001:0001a22_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛睇　蘇佛睇　莫訶佛睇　壹底佛睇　呾呾囉佛睇　佛睇佛睇　三摩佛睇　頞鞞佛睇　沒栗度佛睇　佛陀　末底　佛睇　莫訶佛陀末底佛睇　薩縛佛陀　頞奴末帝　薩縛佛陀　頞奴若帝　佛陀佛陀佛陀佛陀佛陀佛陀佛陀佛陀佛陀佛陀　阿難都　佛陀毘沙耶　阿難多　達摩提舍耶　醫建多　末捺斯迦洛　僧泣多　達摩婆筏那　醫多儞　薩縛佛睇毘　陀喇尼　三般羅迦始多　頞奴劍　波邪薩埵南　薩縛達　摩喃母達邏尼　呾姪他　黍睇蘇黍睇　輸達泥　僧輸達泥　涅末麗　末羅罯波揭帝　揭底三末底羯爛帝　羯臘謎　羯臘摩裟揭麗　娑揭[口*洛]末底　輸計毘輸計　戌迦攝末泥　扇帝鄔波扇帝　般刺扇多頞縛婆(去聲)細薩縛奔若般唎賓稚帝　薩縛達摩般喇底曼稚帝　喝[口*洛]喝[口*洛]　末藍喝[口*洛]　折[口*洛]折[口*洛]　珊折[口*洛]　折羅　折羅　珊折羅　呾羅　呾羅　珊呾羅　三摩呾羅　儞囇儞囇　蘇儞囇　纈履那末底　路迦達囇　路迦陀刺尼　達[口*洛]達[口*洛]　侘[口*洛]侘[口*洛]　鶻魯陀　鶻魯陀　莫訶毘闍耶婆呬尼　喝那喝那　薩縛佛睇　四閉囇史多　薩筏若般替　薩　筏若波羅弭帝　莫訶般刺底婆(去聲)那珊半泥三縵多路計　佛陀毘沙曳　佛陀般賴底曼稚帝　薄伽筏底　薩[口*洛]薩[口*洛]　般賴薩[口*洛]　般賴薩[口*洛]　毘薩[口*洛]毘薩[口*洛]　薩縛度沙阿波揭帝　莎訶佛說如是一切佛心具大威德陀羅尼已。",
        "from": "T19n0918_001:0001a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即時三千大千世界大地大海。",
        "from": "T19n0918_001:0001b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "妙高山王一切同時十八震動。",
        "from": "T19n0918_001:0001b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸天宮殿皆悉傾搖。",
        "from": "T19n0918_001:0001b25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兇悖魔軍威光失滅。",
        "from": "T19n0918_001:0001b25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "相顧惶恐戰懼懷憂。",
        "from": "T19n0918_001:0001b26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時三千界所應度者。",
        "from": "T19n0918_001:0001b26_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "驚覩此相俱三失聲。",
        "from": "T19n0918_001:0001b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯諸天眾信三寶者。",
        "from": "T19n0918_001:0001b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "歡喜踊躍。",
        "from": "T19n0918_001:0001b28_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各捧天華遙散佛上。",
        "from": "T19n0918_001:0001b28_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "兼勸眾魔歸依佛法。",
        "from": "T19n0918_001:0001b28_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時世尊告諸菩薩。",
        "from": "T19n0918_001:0001c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾今愍念一切有情說陀羅尼令脫苦難。",
        "from": "T19n0918_001:0001c01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宜正憶念宣布世間。",
        "from": "T19n0918_001:0001c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆令受持獲勝利樂。",
        "from": "T19n0918_001:0001c02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "時諸菩薩及餘眾會。",
        "from": "T19n0918_001:0001c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜信受奉行。",
        "from": "T19n0918_001:0001c03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸佛心陀羅尼經",
        "from": "T19n0918_001:0001c05_0",
        "context_with_next_sentence": false
    }
]