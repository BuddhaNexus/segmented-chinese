[
    {
        "source_sentence": "十二因緣論一卷",
        "from": "T32n1651_001:0480c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "淨意菩薩造",
        "from": "T32n1651_001:0480c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後魏北印度三藏菩提流支譯",
        "from": "T32n1651_001:0480c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　歸命牟尼尊　　妙法比丘僧",
        "from": "T32n1651_001:0480c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　略作因緣論　　為義顯現故",
        "from": "T32n1651_001:0480c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　牟尼所演說　　十二勝上分",
        "from": "T32n1651_001:0480c19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　因緣所生法　　彼為三所攝",
        "from": "T32n1651_001:0480c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　煩惱業及苦　　次說應當知",
        "from": "T32n1651_001:0480c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　煩惱初八九　　業二及以十",
        "from": "T32n1651_001:0480c22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　餘七說為苦　　三攝十二法",
        "from": "T32n1651_001:0480c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　從三故生二　　從二故生七",
        "from": "T32n1651_001:0480c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　從七復生三　　是故如輪轉",
        "from": "T32n1651_001:0480c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一切世間法　　唯因果無人",
        "from": "T32n1651_001:0480c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　但從諸空法　　唯生於空法",
        "from": "T32n1651_001:0480c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　誦燈印鏡響　　日珠種子水",
        "from": "T32n1651_001:0480c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸陰轉不轉　　智者善思量",
        "from": "T32n1651_001:0480c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有弟子成就。",
        "from": "T32n1651_001:0481a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨所聞法堪能受持。",
        "from": "T32n1651_001:0481a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令不忘失於如來法。",
        "from": "T32n1651_001:0481a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂事非事及性相等。",
        "from": "T32n1651_001:0481a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是義中心懷疑惑。",
        "from": "T32n1651_001:0481a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為得知故問言。",
        "from": "T32n1651_001:0481a03_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者。",
        "from": "T32n1651_001:0481a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　牟尼所演說　　十二勝上分",
        "from": "T32n1651_001:0481a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　因緣所生法　　彼為三所攝",
        "from": "T32n1651_001:0481a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如是等諸事　　今為知請問",
        "from": "T32n1651_001:0481a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　願為我解釋　　除斷我疑網",
        "from": "T32n1651_001:0481a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　師見弟子意　　於法生渴仰",
        "from": "T32n1651_001:0481a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　恭敬請示故　　即答言汝聽",
        "from": "T32n1651_001:0481a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　十二勝上分　　彼為三所攝",
        "from": "T32n1651_001:0481a10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　煩惱業及苦　　次說應當知",
        "from": "T32n1651_001:0481a11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是中十及二　　故曰為十二",
        "from": "T32n1651_001:0481a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以彼不異分　　故名為勝分",
        "from": "T32n1651_001:0481a13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如車輿分故　　說勝分應知",
        "from": "T32n1651_001:0481a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言牟尼者。",
        "from": "T32n1651_001:0481a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名為寂滅。",
        "from": "T32n1651_001:0481a15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名無分別。",
        "from": "T32n1651_001:0481a15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名為定。",
        "from": "T32n1651_001:0481a15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦名無言說。",
        "from": "T32n1651_001:0481a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼牟尼所演。",
        "from": "T32n1651_001:0481a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "宣暢辯說。",
        "from": "T32n1651_001:0481a16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名假名。",
        "from": "T32n1651_001:0481a16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然彼非是大人丈夫。",
        "from": "T32n1651_001:0481a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在定時。",
        "from": "T32n1651_001:0481a17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性相所生。",
        "from": "T32n1651_001:0481a17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但唯因緣所生成故。",
        "from": "T32n1651_001:0481a18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼十二分。",
        "from": "T32n1651_001:0481a18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於煩惱業及以苦處。",
        "from": "T32n1651_001:0481a18_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三法迭互共作因緣。",
        "from": "T32n1651_001:0481a19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如拒瓶案。",
        "from": "T32n1651_001:0481a19_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是三處所攝應知。",
        "from": "T32n1651_001:0481a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1651_001:0481a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者為煩惱。",
        "from": "T32n1651_001:0481a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者為業。",
        "from": "T32n1651_001:0481a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者為苦。",
        "from": "T32n1651_001:0481a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而得有此諸因緣法勝分攝成。",
        "from": "T32n1651_001:0481a21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1651_001:0481a23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於此十二勝上分中。",
        "from": "T32n1651_001:0481a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初為無明。",
        "from": "T32n1651_001:0481a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第八為愛。",
        "from": "T32n1651_001:0481a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第九為取。",
        "from": "T32n1651_001:0481a24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此三勝分是煩惱所攝。",
        "from": "T32n1651_001:0481a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第二為行。",
        "from": "T32n1651_001:0481a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第十為有。",
        "from": "T32n1651_001:0481a25_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此二勝分是業所攝。",
        "from": "T32n1651_001:0481a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "餘七勝分是苦所攝。",
        "from": "T32n1651_001:0481a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是煩惱業苦等三。",
        "from": "T32n1651_001:0481a26_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝十二分應知。",
        "from": "T32n1651_001:0481a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "言餘七者。",
        "from": "T32n1651_001:0481a27_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂識名色六入觸受及生老死。",
        "from": "T32n1651_001:0481a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恩愛別離。",
        "from": "T32n1651_001:0481a28_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "怨憎合會。",
        "from": "T32n1651_001:0481a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所求不得。",
        "from": "T32n1651_001:0481a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等法生一切苦。",
        "from": "T32n1651_001:0481a28_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸分。",
        "from": "T32n1651_001:0481a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於向所說。",
        "from": "T32n1651_001:0481a29_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "煩惱業苦。",
        "from": "T32n1651_001:0481a29_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為根本。",
        "from": "T32n1651_001:0481b01_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應知。",
        "from": "T32n1651_001:0481b01_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "攝十二分。",
        "from": "T32n1651_001:0481b01_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有三事。",
        "from": "T32n1651_001:0481b01_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更無餘法。",
        "from": "T32n1651_001:0481b01_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切經中但有此分。",
        "from": "T32n1651_001:0481b02_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "更無有餘。",
        "from": "T32n1651_001:0481b02_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1651_001:0481b03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已知此等諸勝分義。",
        "from": "T32n1651_001:0481b03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為我解釋煩惱業苦在於何處。",
        "from": "T32n1651_001:0481b03_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復云何成一切諸事。",
        "from": "T32n1651_001:0481b04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1651_001:0481b04_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從三生二。",
        "from": "T32n1651_001:0481b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三是煩惱。",
        "from": "T32n1651_001:0481b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二是業。",
        "from": "T32n1651_001:0481b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從煩惱而生於業。",
        "from": "T32n1651_001:0481b05_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從二生七。",
        "from": "T32n1651_001:0481b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者是苦。",
        "from": "T32n1651_001:0481b06_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從於業而生於苦。",
        "from": "T32n1651_001:0481b06_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從七生三者。",
        "from": "T32n1651_001:0481b07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂從於苦而生煩惱。",
        "from": "T32n1651_001:0481b07_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此說煩惱業苦三種迭互相生。",
        "from": "T32n1651_001:0481b07_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故生有輪轉不定。",
        "from": "T32n1651_001:0481b08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所言有者。",
        "from": "T32n1651_001:0481b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂欲色無色界等。",
        "from": "T32n1651_001:0481b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼中不住。",
        "from": "T32n1651_001:0481b09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "喻如輪轉。",
        "from": "T32n1651_001:0481b09_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以彼有故。",
        "from": "T32n1651_001:0481b10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間凡夫眾生。",
        "from": "T32n1651_001:0481b10_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次第上下猶如輪轉。",
        "from": "T32n1651_001:0481b10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有中不定。",
        "from": "T32n1651_001:0481b11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不定故。",
        "from": "T32n1651_001:0481b11_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說有三處。",
        "from": "T32n1651_001:0481b11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1651_001:0481b13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼造一切身。",
        "from": "T32n1651_001:0481b13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在眾生。",
        "from": "T32n1651_001:0481b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "何者是彼作事云何。",
        "from": "T32n1651_001:0481b13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1651_001:0481b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈言。",
        "from": "T32n1651_001:0481b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間法。",
        "from": "T32n1651_001:0481b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯因果無人。",
        "from": "T32n1651_001:0481b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "除假說故有。",
        "from": "T32n1651_001:0481b15_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是正思量。",
        "from": "T32n1651_001:0481b16_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼非說性。",
        "from": "T32n1651_001:0481b16_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故見作眾生不成。",
        "from": "T32n1651_001:0481b16_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1651_001:0481b18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若如是者。",
        "from": "T32n1651_001:0481b18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何得從現在世間而取未來世間。",
        "from": "T32n1651_001:0481b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1651_001:0481b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無有一毫等法從現在世間而取未來世間。",
        "from": "T32n1651_001:0481b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故偈言。",
        "from": "T32n1651_001:0481b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但從諸空法。",
        "from": "T32n1651_001:0481b21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯生於空法。",
        "from": "T32n1651_001:0481b21_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此明自我我所空。",
        "from": "T32n1651_001:0481b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂煩惱業處。",
        "from": "T32n1651_001:0481b22_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此五法行性離無我。",
        "from": "T32n1651_001:0481b22_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應如是取。",
        "from": "T32n1651_001:0481b23_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "問曰。",
        "from": "T32n1651_001:0481b24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若性無我法中。",
        "from": "T32n1651_001:0481b24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而行性無我者。",
        "from": "T32n1651_001:0481b24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今說何為證。",
        "from": "T32n1651_001:0481b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "答曰。",
        "from": "T32n1651_001:0481b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈言。",
        "from": "T32n1651_001:0481b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "誦燈印鏡響。",
        "from": "T32n1651_001:0481b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日珠種子水。",
        "from": "T32n1651_001:0481b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等諸喻。",
        "from": "T32n1651_001:0481b26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為證可取。",
        "from": "T32n1651_001:0481b27_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信無自體。",
        "from": "T32n1651_001:0481b27_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "性假名故。",
        "from": "T32n1651_001:0481b27_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有言。",
        "from": "T32n1651_001:0481b27_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "現在世未來世者。",
        "from": "T32n1651_001:0481b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如師所誦。",
        "from": "T32n1651_001:0481b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "實不從師轉至弟子。",
        "from": "T32n1651_001:0481b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雖不從師轉至弟子。",
        "from": "T32n1651_001:0481b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "豈可不成授弟子義。",
        "from": "T32n1651_001:0481b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "可言弟子無因而得。",
        "from": "T32n1651_001:0481c01_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遮護妄計無因患故。",
        "from": "T32n1651_001:0481c01_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是臨命終時。",
        "from": "T32n1651_001:0481c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心識不至未來世間。",
        "from": "T32n1651_001:0481c02_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "防常患故。",
        "from": "T32n1651_001:0481c02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非未來身從餘處來。",
        "from": "T32n1651_001:0481c03_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遮護妄計無因患故。",
        "from": "T32n1651_001:0481c03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如師誦為因。",
        "from": "T32n1651_001:0481c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "令弟子得。",
        "from": "T32n1651_001:0481c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼不可說。",
        "from": "T32n1651_001:0481c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以為即是亦不可說。",
        "from": "T32n1651_001:0481c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為一向異。",
        "from": "T32n1651_001:0481c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是臨命終時。",
        "from": "T32n1651_001:0481c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心識為因。",
        "from": "T32n1651_001:0481c05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故得生後身心識。",
        "from": "T32n1651_001:0481c06_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而彼心識。",
        "from": "T32n1651_001:0481c06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可說一不可說異。",
        "from": "T32n1651_001:0481c06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不離彼。",
        "from": "T32n1651_001:0481c07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不即彼。",
        "from": "T32n1651_001:0481c07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是從燈生燈。",
        "from": "T32n1651_001:0481c07_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從印生印。",
        "from": "T32n1651_001:0481c08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從鏡生像。",
        "from": "T32n1651_001:0481c08_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從聲有響。",
        "from": "T32n1651_001:0481c08_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從日從珠出生於火。",
        "from": "T32n1651_001:0481c08_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "從子生芽。",
        "from": "T32n1651_001:0481c09_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如安石榴菴羅果等。",
        "from": "T32n1651_001:0481c09_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "口生涎水。",
        "from": "T32n1651_001:0481c10_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是等法。",
        "from": "T32n1651_001:0481c10_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不名即彼。",
        "from": "T32n1651_001:0481c10_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不名異彼。",
        "from": "T32n1651_001:0481c10_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一切諸因緣法。",
        "from": "T32n1651_001:0481c11_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "轉不轉事。",
        "from": "T32n1651_001:0481c11_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸有智者。",
        "from": "T32n1651_001:0481c11_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善思量應知。",
        "from": "T32n1651_001:0481c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是中陰者。",
        "from": "T32n1651_001:0481c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂說色受想行識。",
        "from": "T32n1651_001:0481c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼託生者。",
        "from": "T32n1651_001:0481c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此諸陰滅。",
        "from": "T32n1651_001:0481c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "因彼滅陰。",
        "from": "T32n1651_001:0481c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "後相似生。",
        "from": "T32n1651_001:0481c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然實無有一毫等法從此至彼。",
        "from": "T32n1651_001:0481c13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此是世間漸次之義。",
        "from": "T32n1651_001:0481c14_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以是義故。",
        "from": "T32n1651_001:0481c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切世間。",
        "from": "T32n1651_001:0481c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無常不淨苦無我等。",
        "from": "T32n1651_001:0481c15_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以能觀察如是事故。",
        "from": "T32n1651_001:0481c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於諸法中不生疑惑。",
        "from": "T32n1651_001:0481c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不疑惑故則不生染。",
        "from": "T32n1651_001:0481c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生染故則不生著。",
        "from": "T32n1651_001:0481c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不生著故則不虛渴。",
        "from": "T32n1651_001:0481c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不虛渴故則不造業。",
        "from": "T32n1651_001:0481c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無業故則不取事。",
        "from": "T32n1651_001:0481c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不取事故不造有為行。",
        "from": "T32n1651_001:0481c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無有為行。",
        "from": "T32n1651_001:0481c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故則不復生。",
        "from": "T32n1651_001:0481c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以不生故無有一切身心等苦。",
        "from": "T32n1651_001:0481c20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是不造五種因故。",
        "from": "T32n1651_001:0481c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則於彼處無七種果。",
        "from": "T32n1651_001:0481c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以無果故名為解脫。",
        "from": "T32n1651_001:0481c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是作故。",
        "from": "T32n1651_001:0481c22_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則是釋成不生不滅不常不斷。",
        "from": "T32n1651_001:0481c22_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有邊無邊如是等句。",
        "from": "T32n1651_001:0481c23_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於中有偈。",
        "from": "T32n1651_001:0481c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　不見無緣生　　決定是正義",
        "from": "T32n1651_001:0481c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於諸最妙事　　是故不成斷",
        "from": "T32n1651_001:0481c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　於中無所減　　亦復無所增",
        "from": "T32n1651_001:0481c27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　應見如諦實　　隨狀及如彼",
        "from": "T32n1651_001:0481c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十二因緣論一卷",
        "from": "T32n1651_001:0481c29_0",
        "context_with_next_sentence": false
    }
]