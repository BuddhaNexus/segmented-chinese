[
    {
        "source_sentence": "阿毘達磨俱舍論實義疏卷第三",
        "from": "T29n1561_003:0326b19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尊者安惠造",
        "from": "T29n1561_003:0326b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三科一聚二生門三種族。",
        "from": "T29n1561_003:0326b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聚蘊義。",
        "from": "T29n1561_003:0326b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生門義。",
        "from": "T29n1561_003:0326b22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "心心所法生長門義。",
        "from": "T29n1561_003:0326b23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "生於眼識以眼為門。",
        "from": "T29n1561_003:0326b23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此經證門義有六。",
        "from": "T29n1561_003:0326b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然心所法有十二。",
        "from": "T29n1561_003:0326b24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故契經說。",
        "from": "T29n1561_003:0326b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼色為緣生於眼識。",
        "from": "T29n1561_003:0326b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三和合。",
        "from": "T29n1561_003:0326b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "觸起俱受想思如是。",
        "from": "T29n1561_003:0326b25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至意法為緣生於意識三和合。",
        "from": "T29n1561_003:0326b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "種族義是界義。",
        "from": "T29n1561_003:0326b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如一山中有多銅鐵金銀界族說名多界。",
        "from": "T29n1561_003:0326b27_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一身或一相續。",
        "from": "T29n1561_003:0326b28_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有十八類。",
        "from": "T29n1561_003:0326b28_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸法種族名十八界。",
        "from": "T29n1561_003:0326b29_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "眼根現在未來過去是。",
        "from": "T29n1561_003:0326b29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無明顛倒頌曰。",
        "from": "T29n1561_003:0326c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　如魚處泥中　　躍水令渾濁",
        "from": "T29n1561_003:0326c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　無明中倒想　　污患亦復然",
        "from": "T29n1561_003:0326c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊名想頌曰。",
        "from": "T29n1561_003:0326c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　牟尼說法蘊　　數有八十千",
        "from": "T29n1561_003:0326c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　彼體語或名　　此必行蘊攝",
        "from": "T29n1561_003:0326c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論頌曰。",
        "from": "T29n1561_003:0326c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　空界謂竅隟　　傳說是明闇",
        "from": "T29n1561_003:0326c09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　識界有漏識　　有情生所依",
        "from": "T29n1561_003:0326c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論卷第二",
        "from": "T29n1561_003:0326c11_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十八界中幾有見幾無見。",
        "from": "T29n1561_003:0326c12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾有對幾無對。",
        "from": "T29n1561_003:0326c12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "幾善幾不善幾無記。",
        "from": "T29n1561_003:0326c12_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頌曰。",
        "from": "T29n1561_003:0326c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　一有見謂色　　十有色有對",
        "from": "T29n1561_003:0326c14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　此八除色聲　　無記得三種",
        "from": "T29n1561_003:0326c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五無分別　　由計度隨念",
        "from": "T29n1561_003:0326c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以意地散惠　　意諸念為體",
        "from": "T29n1561_003:0326c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T29n1561_003:0326c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說分別略有三種。",
        "from": "T29n1561_003:0326c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一自性分別。",
        "from": "T29n1561_003:0326c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二計度分別。",
        "from": "T29n1561_003:0326c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三隨念分別。",
        "from": "T29n1561_003:0326c19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "由五識身雖有自性而無餘二。",
        "from": "T29n1561_003:0326c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "說有八種分別非也。",
        "from": "T29n1561_003:0326c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有自性分別。",
        "from": "T29n1561_003:0326c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "而",
        "from": "T29n1561_003:0326c20_8",
        "context_with_next_sentence": false
    }
]