        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是一一諸境界中。",
        "from": "X74n1470_008:0180a02_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各有無數欄楯。",
        "from": "X74n1470_008:0180a03_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無數宮殿。",
        "from": "X74n1470_008:0180a03_9",
        "context_with_next_sentence": true
    },
    {
