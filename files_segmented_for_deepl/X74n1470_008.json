        "from": "X74n1470_008:0179c22_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶帳。",
        "from": "X74n1470_008:0179c22_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶網。",
        "from": "X74n1470_008:0179c22_10",
        "context_with_next_sentence": true
    },
    {
