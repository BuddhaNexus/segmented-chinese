        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可思不可量。",
        "from": "X74n1470_008:0179c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不可說不可說。",
        "from": "X74n1470_008:0179c20_5",
        "context_with_next_sentence": true
    },
    {
