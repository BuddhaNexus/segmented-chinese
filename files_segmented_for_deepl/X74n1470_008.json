        "from": "X74n1470_008:0179c23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶燄。",
        "from": "X74n1470_008:0179c23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶雲。",
        "from": "X74n1470_008:0179c23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶座。",
        "from": "X74n1470_008:0179c23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶經行地。",
        "from": "X74n1470_008:0179c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶所住處。",
        "from": "X74n1470_008:0179c24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶剎摩尼寶山。",
        "from": "X74n1470_008:0179c24_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶海。",
        "from": "X74n1470_008:0180a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩尼寶河。",
