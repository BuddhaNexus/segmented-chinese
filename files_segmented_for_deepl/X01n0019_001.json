[
    {
        "source_sentence": "佛說斷溫經",
        "from": "X01n0019_001:0403c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "爾時佛在王舍城中。",
        "from": "X01n0019_001:0403c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與諸大眾俱會。",
        "from": "X01n0019_001:0403c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難長脆合掌。",
        "from": "X01n0019_001:0403c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "白佛言。",
        "from": "X01n0019_001:0403c07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "世尊。",
        "from": "X01n0019_001:0403c07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "彼南閻浮提世界。",
        "from": "X01n0019_001:0403c07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忽遇天行溫氣瘴烈。",
        "from": "X01n0019_001:0403c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如猛火橫延。",
        "from": "X01n0019_001:0403c08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "及死亡無數。",
        "from": "X01n0019_001:0403c08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如何免之。",
        "from": "X01n0019_001:0403c08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛告阿難。",
        "from": "X01n0019_001:0403c08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝可諦聽。",
        "from": "X01n0019_001:0403c08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "吾當為汝說。",
        "from": "X01n0019_001:0403c09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此病有鬼神。",
        "from": "X01n0019_001:0403c09_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常吐惡毒。",
        "from": "X01n0019_001:0403c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以害人民。",
        "from": "X01n0019_001:0403c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "初得病時。",
        "from": "X01n0019_001:0403c10_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "頭痛寒熱。",
        "from": "X01n0019_001:0403c10_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "百支節俱苦痛畢切。",
        "from": "X01n0019_001:0403c10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿難言。",
        "from": "X01n0019_001:0403c10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有善男子善女人。",
        "from": "X01n0019_001:0403c10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "能知七鬼神名字。",
        "from": "X01n0019_001:0403c11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作崇敬。",
        "from": "X01n0019_001:0403c11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當除此病患乎。",
        "from": "X01n0019_001:0403c11_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為汝即說除病呪曰。",
        "from": "X01n0019_001:0403c12_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無佛陀耶。",
        "from": "X01n0019_001:0403c13_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無達磨耶。",
        "from": "X01n0019_001:0403c13_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無僧伽耶。",
        "from": "X01n0019_001:0403c13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "南無觀世音菩薩摩訶薩。",
        "from": "X01n0019_001:0403c13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "疫病消除娑婆訶爾時十方諸佛。",
        "from": "X01n0019_001:0403c14_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "諸大菩薩。",
        "from": "X01n0019_001:0403c15_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切賢聖僧。",
        "from": "X01n0019_001:0403c15_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "聞我說金剛秘密呪。",
        "from": "X01n0019_001:0403c15_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦說七鬼神名字曰。",
        "from": "X01n0019_001:0403c16_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夢多難鬼。",
        "from": "X01n0019_001:0403c16_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿佉尼鬼。",
        "from": "X01n0019_001:0403c16_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "尼佉尸鬼。",
        "from": "X01n0019_001:0403c16_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿伽那鬼。",
        "from": "X01n0019_001:0403c17_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "波羅尼鬼。",
        "from": "X01n0019_001:0403c17_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "阿毗羅鬼。",
        "from": "X01n0019_001:0403c17_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "婆提梨鬼也。",
        "from": "X01n0019_001:0403c17_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "汝等當知。",
        "from": "X01n0019_001:0403c18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此神名思念。",
        "from": "X01n0019_001:0403c18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "速疾遠離。",
        "from": "X01n0019_001:0403c18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不敢侵害。",
        "from": "X01n0019_001:0403c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其人所被毒氣普皆消散。",
        "from": "X01n0019_001:0403c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "得金剛密迹。",
        "from": "X01n0019_001:0403c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其身安樂。",
        "from": "X01n0019_001:0403c19_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無諸患難。",
        "from": "X01n0019_001:0403c19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說是經已。",
        "from": "X01n0019_001:0403c20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天龍八部。",
        "from": "X01n0019_001:0403c20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切大眾。",
        "from": "X01n0019_001:0403c20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆大歡喜。",
        "from": "X01n0019_001:0403c20_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "信受奉行。",
        "from": "X01n0019_001:0403c20_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "佛說斷溫經",
        "from": "X01n0019_001:0403c23_0",
        "context_with_next_sentence": false
    }
]