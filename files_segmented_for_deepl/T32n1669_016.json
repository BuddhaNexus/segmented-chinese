[
    {
        "source_sentence": "大宗地玄文本論卷第十六",
        "from": "T32n1669_016:0688a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_016:0688a19_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_016:0688a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎出離繫縛地清白解脫道路大決擇分第三十一如是。",
        "from": "T32n1669_016:0688a21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "已說最極廣大俱行山王無盡海海。",
        "from": "T32n1669_016:0688a23_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大決擇分。",
        "from": "T32n1669_016:0688a23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說出離繫縛地清白解脫道路大決擇分。",
        "from": "T32n1669_016:0688a24_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_016:0688a25_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_016:0688a25_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　解脫道路中　　有二十無為",
        "from": "T32n1669_016:0688a26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　謂十空十有　　如是諸無為",
        "from": "T32n1669_016:0688a27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一位中　　皆悉具足有",
        "from": "T32n1669_016:0688a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　依法位立轉　　有二重重超",
        "from": "T32n1669_016:0688a29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_016:0688b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就出離繫縛地清白解脫道路分中。",
        "from": "T32n1669_016:0688b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有二十無為常法。",
        "from": "T32n1669_016:0688b01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂十空十有無為各差別故。",
        "from": "T32n1669_016:0688b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為十空無為一者廣大虛空自然常住離造作空無為。",
        "from": "T32n1669_016:0688b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者大虛空影空無為。",
        "from": "T32n1669_016:0688b04_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者虛空影影空無為。",
        "from": "T32n1669_016:0688b05_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者破影無所有空無為。",
        "from": "T32n1669_016:0688b05_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者空空俱非空無為。",
        "from": "T32n1669_016:0688b06_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者離言絕說空無為。",
        "from": "T32n1669_016:0688b06_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者絕離未畢空無為。",
        "from": "T32n1669_016:0688b07_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者絕離心解空無為。",
        "from": "T32n1669_016:0688b07_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者絕離窮窮空無為。",
        "from": "T32n1669_016:0688b08_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者無障無礙大空大空空無為。",
        "from": "T32n1669_016:0688b08_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_016:0688b09_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為十有無為。",
        "from": "T32n1669_016:0688b09_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者一切言說決定常住無破非空無為。",
        "from": "T32n1669_016:0688b10_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者一切心識決定常住無破非空無為。",
        "from": "T32n1669_016:0688b11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者一切大種決定常住無破非空無為。",
        "from": "T32n1669_016:0688b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "四者一切俱非決定常住無破非空無為。",
        "from": "T32n1669_016:0688b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五者一切有實決定常住無破非空無為。",
        "from": "T32n1669_016:0688b14_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "六者一切性大決定常住無破非空無為。",
        "from": "T32n1669_016:0688b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七者一切今光決定常住無破非空無為。",
        "from": "T32n1669_016:0688b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八者一切有名決定常住無破非空無為。",
        "from": "T32n1669_016:0688b16_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "九者一切無名決定常住無破非空無為。",
        "from": "T32n1669_016:0688b17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十者廣大圓滿自性本有一切種有決定常住無破非空無為。",
        "from": "T32n1669_016:0688b18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為十。",
        "from": "T32n1669_016:0688b20_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈解脫道路中有二十無為。",
        "from": "T32n1669_016:0688b20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂十空十有故。",
        "from": "T32n1669_016:0688b21_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是二十種無為法。",
        "from": "T32n1669_016:0688b21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一種金剛位中。",
        "from": "T32n1669_016:0688b21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足圓滿無闕失轉。",
        "from": "T32n1669_016:0688b22_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈如是諸無為五十一位中皆悉具足有故。",
        "from": "T32n1669_016:0688b22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "依如是等二十無為。",
        "from": "T32n1669_016:0688b23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一位建立轉相。",
        "from": "T32n1669_016:0688b24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有二種。",
        "from": "T32n1669_016:0688b24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為二。",
        "from": "T32n1669_016:0688b25_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者重重該攝無障礙門。",
        "from": "T32n1669_016:0688b25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者次第亂轉超過門。",
        "from": "T32n1669_016:0688b25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為二。",
        "from": "T32n1669_016:0688b26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈依法位立轉有二重重超故。",
        "from": "T32n1669_016:0688b26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "且重重該攝無障礙門形相如何。",
        "from": "T32n1669_016:0688b27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_016:0688b28_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　二十法一一　　各攝後二十",
        "from": "T32n1669_016:0688b29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一種位　　一一皆各各",
        "from": "T32n1669_016:0688c01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　攝五十一位　　亦此相違攝",
        "from": "T32n1669_016:0688c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　以此因緣故　　建立重重名",
        "from": "T32n1669_016:0688c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_016:0688c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何名為重重門相。",
        "from": "T32n1669_016:0688c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂該攝故。",
        "from": "T32n1669_016:0688c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何該攝。",
        "from": "T32n1669_016:0688c04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂二十種無為常法信心具足。",
        "from": "T32n1669_016:0688c05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各攝後諸位各二十種無為法故。",
        "from": "T32n1669_016:0688c05_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如說信心餘位亦爾。",
        "from": "T32n1669_016:0688c06_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈二十法一一各攝後二十故。",
        "from": "T32n1669_016:0688c07_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一種位。",
        "from": "T32n1669_016:0688c07_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各攝五十一。",
        "from": "T32n1669_016:0688c08_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無障礙。",
        "from": "T32n1669_016:0688c08_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一種位一一皆各各攝五十一位故。",
        "from": "T32n1669_016:0688c08_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦一一法攝一切位。",
        "from": "T32n1669_016:0688c09_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一位位攝一切法。",
        "from": "T32n1669_016:0688c10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦無障礙。",
        "from": "T32n1669_016:0688c10_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈亦此相違攝故。",
        "from": "T32n1669_016:0688c10_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以如是二種該攝門故。",
        "from": "T32n1669_016:0688c11_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "立重重名。",
        "from": "T32n1669_016:0688c11_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈以此因緣故建立重重名故。",
        "from": "T32n1669_016:0688c12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說重重該攝無障礙門。",
        "from": "T32n1669_016:0688c12_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說次第亂轉超過門。",
        "from": "T32n1669_016:0688c13_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_016:0688c14_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_016:0688c14_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　五十一位中　　隨一經五十",
        "from": "T32n1669_016:0688c15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　漸漸增法數　　周遍廣大轉",
        "from": "T32n1669_016:0688c16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_016:0688c17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_016:0688c17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示五十一種金剛位中。",
        "from": "T32n1669_016:0688c17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以信為初經五十位。",
        "from": "T32n1669_016:0688c18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以發心住而為其初經五十位。",
        "from": "T32n1669_016:0688c18_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至以最極地而為其初經五十位。",
        "from": "T32n1669_016:0688c19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若第一轉。",
        "from": "T32n1669_016:0688c20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增四十一百數法位轉。",
        "from": "T32n1669_016:0688c20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若第二轉。",
        "from": "T32n1669_016:0688c21_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "增八十二百數法位轉。",
        "from": "T32n1669_016:0688c21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至最後地故。",
        "from": "T32n1669_016:0688c21_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈五十一位中隨一經五十漸漸增法數周遍廣大轉故。",
        "from": "T32n1669_016:0688c22_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "蘊高山王品類修多羅中。",
        "from": "T32n1669_016:0688c23_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_016:0688c24_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無破地地門中。",
        "from": "T32n1669_016:0688c24_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有寂靜寶其數眾多。",
        "from": "T32n1669_016:0688c24_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "空寂靜寶其數眾多。",
        "from": "T32n1669_016:0688c25_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "若有行者入此門中。",
        "from": "T32n1669_016:0688c25_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通達諸法無為大道。",
        "from": "T32n1669_016:0688c26_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無所障礙無所疑畏。",
        "from": "T32n1669_016:0688c26_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其心自在決定常住大安樂。",
        "from": "T32n1669_016:0688c27_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "漸漸增長常功德海。",
        "from": "T32n1669_016:0688c27_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_016:0688c28_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫山王根本地地無礙自在大決擇分第三十二如是已說出離繫縛地清白解脫道路大決擇分。",
        "from": "T32n1669_016:0688c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說解脫山王根本地地無礙自在大決擇分。",
        "from": "T32n1669_016:0689a03_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_016:0689a04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_016:0689a04_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　根本山王中　　空有互相生",
        "from": "T32n1669_016:0689a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　諸位相生故　　轉勝廣大轉",
        "from": "T32n1669_016:0689a06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_016:0689a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今此偈中為明何義。",
        "from": "T32n1669_016:0689a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為欲現示十空無為。",
        "from": "T32n1669_016:0689a07_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各出生十有無為常法。",
        "from": "T32n1669_016:0689a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "十有無為。",
        "from": "T32n1669_016:0689a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各出生十空無為常法。",
        "from": "T32n1669_016:0689a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "五十一位。",
        "from": "T32n1669_016:0689a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各出生五十依重重等門。",
        "from": "T32n1669_016:0689a09_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "圓滿廣大轉故。",
        "from": "T32n1669_016:0689a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈根本山王中空有互相生諸位相生故轉勝廣大轉故。",
        "from": "T32n1669_016:0689a11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "摩訶衍海修多羅中。",
        "from": "T32n1669_016:0689a12_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_016:0689a12_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "解脫海中。",
        "from": "T32n1669_016:0689a13_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有空有亦有有空其數眾多。",
        "from": "T32n1669_016:0689a13_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是空有。",
        "from": "T32n1669_016:0689a14_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是常滅非無常量。",
        "from": "T32n1669_016:0689a14_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是功德非過患品。",
        "from": "T32n1669_016:0689a14_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說言解脫藏海。",
        "from": "T32n1669_016:0689a15_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_016:0689a15_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎大宗地玄文本論卷第十六",
        "from": "T32n1669_016:0689a16_14",
        "context_with_next_sentence": false
    }
]