[
    {
        "source_sentence": "大宗地玄文本論卷第三",
        "from": "T32n1669_003:0672c21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "馬鳴菩薩造",
        "from": "T32n1669_003:0672c23_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "真諦三藏譯",
        "from": "T32n1669_003:0672c24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "◎金輪山王道路大決擇分第五",
        "from": "T32n1669_003:0672c25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說金剛寶輪山王大決擇分。",
        "from": "T32n1669_003:0672c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說金輪山王道路大決擇分。",
        "from": "T32n1669_003:0672c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_003:0672c27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_003:0672c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　山王道路中　　總有十五位",
        "from": "T32n1669_003:0672c28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　體五種位中　　各有三用故",
        "from": "T32n1669_003:0672c29_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_003:0673a01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此金輪山王道路中。",
        "from": "T32n1669_003:0673a01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立幾位為道路量。",
        "from": "T32n1669_003:0673a01_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說雖無量。",
        "from": "T32n1669_003:0673a02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說有十五種位。",
        "from": "T32n1669_003:0673a02_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以如是位為道路分。",
        "from": "T32n1669_003:0673a02_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈山王道路中總有十五位故。",
        "from": "T32n1669_003:0673a03_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何因緣。",
        "from": "T32n1669_003:0673a04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "分明了知此道路中有十五種根本之位。",
        "from": "T32n1669_003:0673a04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "謂彼金剛大力寶輪山王體五種位中。",
        "from": "T32n1669_003:0673a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆悉各各有三種自在作用故。",
        "from": "T32n1669_003:0673a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此義故。",
        "from": "T32n1669_003:0673a06_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "成立十五差別名數。",
        "from": "T32n1669_003:0673a07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈體五種位中各有三用故。",
        "from": "T32n1669_003:0673a07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故方修多羅中。",
        "from": "T32n1669_003:0673a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_003:0673a08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一區王民行地地中。",
        "from": "T32n1669_003:0673a08_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有十五種體分業數位無所餘位。",
        "from": "T32n1669_003:0673a09_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_003:0673a10_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然明神妙理修多羅中。",
        "from": "T32n1669_003:0673a10_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_003:0673a10_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有二十五種差別位者。",
        "from": "T32n1669_003:0673a11_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "取王家轉非取作轉故。",
        "from": "T32n1669_003:0673a11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "造作轉攝十五種位。",
        "from": "T32n1669_003:0673a12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "名字形相當如何耶。",
        "from": "T32n1669_003:0673a12_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_003:0673a13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　明了及遠數　　相續并三合",
        "from": "T32n1669_003:0673a14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　遍動遍不動　　字等兼廣大",
        "from": "T32n1669_003:0673a15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　遍到不遍到　　融立與俱離",
        "from": "T32n1669_003:0673a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　是名十五名　　如次應觀察",
        "from": "T32n1669_003:0673a17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_003:0673a18_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就次第漸轉體中。",
        "from": "T32n1669_003:0673a18_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "則有三種用。",
        "from": "T32n1669_003:0673a18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_003:0673a18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者分明了達審地作用。",
        "from": "T32n1669_003:0673a19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不亂次第建立位地。",
        "from": "T32n1669_003:0673a19_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "行道分明名義俱了故。",
        "from": "T32n1669_003:0673a20_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈明了故。",
        "from": "T32n1669_003:0673a20_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者遠行遠修無數作用。",
        "from": "T32n1669_003:0673a20_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "經無量劫過此諸位。",
        "from": "T32n1669_003:0673a21_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "修集功德無窮盡故。",
        "from": "T32n1669_003:0673a22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈及遠數故。",
        "from": "T32n1669_003:0673a22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者無斷無絕恒轉作用。",
        "from": "T32n1669_003:0673a22_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "剎那剎那中間中間。",
        "from": "T32n1669_003:0673a23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常恒不息自然轉故。",
        "from": "T32n1669_003:0673a23_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈相續故。",
        "from": "T32n1669_003:0673a24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_003:0673a24_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就諸法等是體中。",
        "from": "T32n1669_003:0673a24_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有三種用。",
        "from": "T32n1669_003:0673a25_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_003:0673a25_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者能詮能了一合作用。",
        "from": "T32n1669_003:0673a25_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "善巧言說無礙覺慧。",
        "from": "T32n1669_003:0673a26_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其數無量一金剛故。",
        "from": "T32n1669_003:0673a26_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者所詮所證一合作用。",
        "from": "T32n1669_003:0673a27_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "甚深極義勝妙玄理。",
        "from": "T32n1669_003:0673a27_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯是一區唯是一身。",
        "from": "T32n1669_003:0673a28_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "無二岐故。",
        "from": "T32n1669_003:0673a28_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者隨應有名一合作用。",
        "from": "T32n1669_003:0673a29_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其所應一切有名一切皆悉一金身故是名為三。",
        "from": "T32n1669_003:0673a29_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈并三合故。",
        "from": "T32n1669_003:0673b01_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就無餘究竟體中。",
        "from": "T32n1669_003:0673b02_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有三種用。",
        "from": "T32n1669_003:0673b02_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_003:0673b02_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者周遍圓滿動轉作用。",
        "from": "T32n1669_003:0673b03_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "第一時中依一位中。",
        "from": "T32n1669_003:0673b03_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "通攝一切而究竟轉故。",
        "from": "T32n1669_003:0673b04_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈遍動故。",
        "from": "T32n1669_003:0673b04_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者周遍圓滿不動作用。",
        "from": "T32n1669_003:0673b05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是轉者所餘一切無量位中。",
        "from": "T32n1669_003:0673b05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "不移不轉常決定故。",
        "from": "T32n1669_003:0673b06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈遍不動故。",
        "from": "T32n1669_003:0673b06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者名句文字無別作用。",
        "from": "T32n1669_003:0673b07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "隨其先唱與餘一故。",
        "from": "T32n1669_003:0673b07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈字等故。",
        "from": "T32n1669_003:0673b08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_003:0673b08_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就周遍圓滿體中。",
        "from": "T32n1669_003:0673b08_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有三種用。",
        "from": "T32n1669_003:0673b09_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_003:0673b09_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者無量無邊廣大作用。",
        "from": "T32n1669_003:0673b09_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其法自體出現業相。",
        "from": "T32n1669_003:0673b10_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡極廣大無分界故。",
        "from": "T32n1669_003:0673b10_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈廣大故。",
        "from": "T32n1669_003:0673b11_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者無礙通達遍到作用。",
        "from": "T32n1669_003:0673b11_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一時建立一切位故。",
        "from": "T32n1669_003:0673b12_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈遍到故。",
        "from": "T32n1669_003:0673b12_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者極極無數不遍作用。",
        "from": "T32n1669_003:0673b12_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "遍通經過而唯有一邊故。",
        "from": "T32n1669_003:0673b13_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈不遍到故。",
        "from": "T32n1669_003:0673b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_003:0673b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "就俱非絕離體中。",
        "from": "T32n1669_003:0673b14_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦有三種用。",
        "from": "T32n1669_003:0673b15_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_003:0673b15_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者消融無所建立作用。",
        "from": "T32n1669_003:0673b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法皆悉遣除無所許故。",
        "from": "T32n1669_003:0673b16_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈融故。",
        "from": "T32n1669_003:0673b16_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者建立諸法悉持作用。",
        "from": "T32n1669_003:0673b17_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一切諸法以俱非義是理成故。",
        "from": "T32n1669_003:0673b17_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈立故。",
        "from": "T32n1669_003:0673b18_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者消融建立俱絕作用。",
        "from": "T32n1669_003:0673b18_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "究竟絕道廣建立故。",
        "from": "T32n1669_003:0673b19_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈與俱離故。",
        "from": "T32n1669_003:0673b19_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_003:0673b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是為十五種位名字焉。",
        "from": "T32n1669_003:0673b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此道路位。",
        "from": "T32n1669_003:0673b20_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大利根者乃能通達。",
        "from": "T32n1669_003:0673b21_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鈍根眾生決定難了。",
        "from": "T32n1669_003:0673b21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈是名十五名。",
        "from": "T32n1669_003:0673b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如次應觀察故。",
        "from": "T32n1669_003:0673b22_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸位。",
        "from": "T32n1669_003:0673b22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦一時出興亦異時出興。",
        "from": "T32n1669_003:0673b22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦俱時出興。",
        "from": "T32n1669_003:0673b23_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦一處轉。",
        "from": "T32n1669_003:0673b23_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦異處轉。",
        "from": "T32n1669_003:0673b24_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦俱處轉。",
        "from": "T32n1669_003:0673b24_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦不出興亦不移轉。",
        "from": "T32n1669_003:0673b24_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦唯一種亦是多種。",
        "from": "T32n1669_003:0673b25_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於彼本法有作功用有作方便。",
        "from": "T32n1669_003:0673b25_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自然自在建立造作。",
        "from": "T32n1669_003:0673b26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "應審觀察。",
        "from": "T32n1669_003:0673b26_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "獨一山王摩訶山王大決擇分第六",
        "from": "T32n1669_003:0673b27_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是已說金輪山王道路大決擇分。",
        "from": "T32n1669_003:0673b28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次當說獨一山王摩訶山王大決擇分。",
        "from": "T32n1669_003:0673b28_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其相云何。",
        "from": "T32n1669_003:0673b29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "偈曰。",
        "from": "T32n1669_003:0673b29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　摩訶山王中　　總有千二百",
        "from": "T32n1669_003:0673c02_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　七十五種位　　謂五十一種",
        "from": "T32n1669_003:0673c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　大根本位中　　皆一一各各",
        "from": "T32n1669_003:0673c04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "　漸轉等五位　　具足安立故",
        "from": "T32n1669_003:0673c05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "論曰。",
        "from": "T32n1669_003:0673c06_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "此獨一山王摩訶山王體中。",
        "from": "T32n1669_003:0673c06_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "建立幾位以為體分。",
        "from": "T32n1669_003:0673c06_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "廣說雖無量。",
        "from": "T32n1669_003:0673c07_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "略說其要。",
        "from": "T32n1669_003:0673c07_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯有一千二百七十五種決定位故。",
        "from": "T32n1669_003:0673c07_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈摩訶山王中總有千二百七十五種位故。",
        "from": "T32n1669_003:0673c08_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以何因緣。",
        "from": "T32n1669_003:0673c09_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "山王體中有千二百七十五位分明現知。",
        "from": "T32n1669_003:0673c09_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所謂常恒五十一種根本位中。",
        "from": "T32n1669_003:0673c10_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一一各各次第漸轉諸法等。",
        "from": "T32n1669_003:0673c11_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是無餘究竟周遍圓滿俱非絕離五種位具足轉故。",
        "from": "T32n1669_003:0673c12_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦漸轉等五種位中。",
        "from": "T32n1669_003:0673c13_12",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "各各開示漸轉等五種別位故。",
        "from": "T32n1669_003:0673c13_13",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故成立一千二百七十五位。",
        "from": "T32n1669_003:0673c14_14",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如偈謂五十一種大根本位中。",
        "from": "T32n1669_003:0673c15_15",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "皆一一各各漸轉等五位具足安立故。",
        "from": "T32n1669_003:0673c15_16",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故此獨一山王摩訶山王體中。",
        "from": "T32n1669_003:0673c16_17",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是千二百七十五種位亦一一時轉。",
        "from": "T32n1669_003:0673c17_18",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦異一時轉。",
        "from": "T32n1669_003:0673c18_19",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦俱一時轉。",
        "from": "T32n1669_003:0673c18_20",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦皆非轉。",
        "from": "T32n1669_003:0673c18_21",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦一時一處轉。",
        "from": "T32n1669_003:0673c19_22",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦一時異處轉。",
        "from": "T32n1669_003:0673c19_23",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦異時異處轉。",
        "from": "T32n1669_003:0673c19_24",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦皆非轉。",
        "from": "T32n1669_003:0673c20_25",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "自在自然無障無礙。",
        "from": "T32n1669_003:0673c20_26",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是故說言無盡虛空大陀陀筏羅法界本藏地地出生無窮無極廣德大海法門藏焉。",
        "from": "T32n1669_003:0673c20_27",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大智莊嚴法界性身甚深修多羅中。",
        "from": "T32n1669_003:0673c22_28",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "作如是說。",
        "from": "T32n1669_003:0673c23_29",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "非比非喻難得惟說。",
        "from": "T32n1669_003:0673c23_30",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大海中藏大海一體。",
        "from": "T32n1669_003:0673c24_31",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "本地地中三品德類。",
        "from": "T32n1669_003:0673c24_32",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "具足圓滿無所闕失。",
        "from": "T32n1669_003:0673c25_33",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "云何為三。",
        "from": "T32n1669_003:0673c25_34",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一者上品德類。",
        "from": "T32n1669_003:0673c26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其位名字眾多無數。",
        "from": "T32n1669_003:0673c26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與十方世界微塵之數其量等故。",
        "from": "T32n1669_003:0673c26_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "二者中品德類其位名字。",
        "from": "T32n1669_003:0673c27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "與百百百億三千大千世界微塵之數其量等故。",
        "from": "T32n1669_003:0673c28_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "三者下品德類。",
        "from": "T32n1669_003:0673c29_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "其位名字。",
        "from": "T32n1669_003:0673c29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "有一千二百七十五位故。",
        "from": "T32n1669_003:0673c29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "是名為三。",
        "from": "T32n1669_003:0674a01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "如是諸位一一一一一一。",
        "from": "T32n1669_003:0674a01_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至無量無量無量無量無量無量。",
        "from": "T32n1669_003:0674a02_10",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "乃至廣說故。",
        "from": "T32n1669_003:0674a03_11",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "大宗地玄文本論卷第三",
        "from": "T32n1669_003:0674a04_0",
        "context_with_next_sentence": false
    }
]