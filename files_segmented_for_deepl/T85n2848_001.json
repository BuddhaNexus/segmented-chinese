[
    {
        "source_sentence": "迴向文",
        "from": "T85n2848_001:1299a03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "復持勝善次用莊嚴。",
        "from": "T85n2848_001:1299a05_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "當今大唐聖主。",
        "from": "T85n2848_001:1299a05_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願聖壽剋昌皇□□□金輪與沈輪齊轉。",
        "from": "T85n2848_001:1299a05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "舜日與佛日□□高視百王長新萬劫　皇大子潛星。",
        "from": "T85n2848_001:1299a06_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "少海澄闌。",
        "from": "T85n2848_001:1299a08_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盤石增高。",
        "from": "T85n2848_001:1299a08_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟城作鎮。",
        "from": "T85n2848_001:1299a08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "故持勝善次用莊嚴。",
        "from": "T85n2848_001:1299a09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "今六和尚伏願長承帝澤。",
        "from": "T85n2848_001:1299a09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為灌頂之國師。",
        "from": "T85n2848_001:1299a10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永鎮台階。",
        "from": "T85n2848_001:1299a10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "贊明王之治化。",
        "from": "T85n2848_001:1299a10_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝善次用莊嚴　都僧統和尚。",
        "from": "T85n2848_001:1299a12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願智山峯崇聳。",
        "from": "T85n2848_001:1299a12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德海洪深。",
        "from": "T85n2848_001:1299a13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命等松筠。",
        "from": "T85n2848_001:1299a13_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福慶遐遠。",
        "from": "T85n2848_001:1299a13_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "即使長承國寵。",
        "from": "T85n2848_001:1299a13_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永扇慈風。",
        "from": "T85n2848_001:1299a14_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嘉聲溢於□中。",
        "from": "T85n2848_001:1299a14_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "雅範流於像季。",
        "from": "T85n2848_001:1299a14_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝善次用莊嚴　尚書貴體。",
        "from": "T85n2848_001:1299a16_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願福山永固。",
        "from": "T85n2848_001:1299a16_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "神壽無疆。",
        "from": "T85n2848_001:1299a17_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功業高於雲山。",
        "from": "T85n2848_001:1299a17_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "德量深於巨海。",
        "from": "T85n2848_001:1299a17_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "出持旌節以靜萬方。",
        "from": "T85n2848_001:1299a18_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入坐朝堂百寮取則。",
        "from": "T85n2848_001:1299a18_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "來逢元日恒保□春。",
        "from": "T85n2848_001:1299a19_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命等松筠壽同劫石。",
        "from": "T85n2848_001:1299a19_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持勝福次用莊嚴　大夫伏願形同大地歷千載。",
        "from": "T85n2848_001:1299a20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "所恒安福比山河。",
        "from": "T85n2848_001:1299a21_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "跨萬齡而永固。",
        "from": "T85n2848_001:1299a21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "然後振芳聲於帝里。",
        "from": "T85n2848_001:1299a21_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "翔翼雲衢流雅譽。",
        "from": "T85n2848_001:1299a22_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "於鸞池濯鱗漢水。",
        "from": "T85n2848_001:1299a22_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "又持勝善次用莊嚴　安姚二侍御。",
        "from": "T85n2848_001:1299a24_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願聳瓊臺於天外。",
        "from": "T85n2848_001:1299a24_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "飛紫蓋於雲衢。",
        "from": "T85n2848_001:1299a25_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "命與天長福將地遠。",
        "from": "T85n2848_001:1299a25_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "保離區□動止獲安。",
        "from": "T85n2848_001:1299a26_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "嚮震八方往還清吉。",
        "from": "T85n2848_001:1299a26_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝善次用莊嚴　尚書姟子紹千秋之寵。",
        "from": "T85n2848_001:1299a28_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恒居萬代之榮。",
        "from": "T85n2848_001:1299a29_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "忠孝雙全。",
        "from": "T85n2848_001:1299a29_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "朱門繼襲。",
        "from": "T85n2848_001:1299a29_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝善次用莊嚴　釋門教授等。",
        "from": "T85n2848_001:1299b01_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願榮貴轉新香名霞布。",
        "from": "T85n2848_001:1299b01_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "入信帝坐出統僧拳。",
        "from": "T85n2848_001:1299b02_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "群生因類次昇高。",
        "from": "T85n2848_001:1299b02_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "苦海恒施於寶筏。",
        "from": "T85n2848_001:1299b03_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝善次用莊嚴　釋門法律等。",
        "from": "T85n2848_001:1299b04_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願色力增固。",
        "from": "T85n2848_001:1299b04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽命霞延。",
        "from": "T85n2848_001:1299b05_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常為苦海之津梁。",
        "from": "T85n2848_001:1299b05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "鎮作法門之牆壍。",
        "from": "T85n2848_001:1299b05_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福次用莊嚴　都督公等。",
        "from": "T85n2848_001:1299b07_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "壽遐遠日往月來。",
        "from": "T85n2848_001:1299b07_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "福慶相暉。",
        "from": "T85n2848_001:1299b08_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "天長地久。",
        "from": "T85n2848_001:1299b08_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福次用莊嚴　都部落使等。",
        "from": "T85n2848_001:1299b09_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願福祿唯承歡誤。",
        "from": "T85n2848_001:1299b09_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "日新榮名[利-禾+堯]昌。",
        "from": "T85n2848_001:1299b10_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "美譽霞備城隍尞彩。",
        "from": "T85n2848_001:1299b10_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "盡亦盡忠。",
        "from": "T85n2848_001:1299b11_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "部落官尞。",
        "from": "T85n2848_001:1299b11_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "為唯清唯直。",
        "from": "T85n2848_001:1299b11_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福次用莊嚴　尊宿大德等。",
        "from": "T85n2848_001:1299b12_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "長垂像訓匡護法城。",
        "from": "T85n2848_001:1299b12_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "永耀慈燈紀綱梵宇。",
        "from": "T85n2848_001:1299b13_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝善次用莊嚴　諸法將大德等。",
        "from": "T85n2848_001:1299b14_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願智山岌立德水澄清。",
        "from": "T85n2848_001:1299b14_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "禪枝茂而覺花敫。",
        "from": "T85n2848_001:1299b15_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "戒香熏而惠珠朗。",
        "from": "T85n2848_001:1299b15_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福次用莊嚴　諸尼大德等。",
        "from": "T85n2848_001:1299b17_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願四依恒滿六度專精。",
        "from": "T85n2848_001:1299b17_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "意樹恒春心燈永耀。",
        "from": "T85n2848_001:1299b18_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "八功德水去垢除災。",
        "from": "T85n2848_001:1299b18_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "七淨妙花莊嚴法體。",
        "from": "T85n2848_001:1299b19_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福次用莊嚴　卿官父杖等。",
        "from": "T85n2848_001:1299b20_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "惟願壽同松柏福比江湖。",
        "from": "T85n2848_001:1299b20_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "萬歲千秋莫風莫變。",
        "from": "T85n2848_001:1299b21_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福次用莊嚴　諸父杖等。",
        "from": "T85n2848_001:1299b22_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "唯願一聞千悟得大總持。",
        "from": "T85n2848_001:1299b22_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "罪垢不日而消除。",
        "from": "T85n2848_001:1299b23_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "功德善芽而增長。",
        "from": "T85n2848_001:1299b23_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "亦願春秋納慶寒暑順宜。",
        "from": "T85n2848_001:1299b24_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "保富貴於千齡。",
        "from": "T85n2848_001:1299b24_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "演休祥於累劫。",
        "from": "T85n2848_001:1299b25_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福次用莊嚴　李和尚等。",
        "from": "T85n2848_001:1299b26_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "但某乙久蒙示訓。",
        "from": "T85n2848_001:1299b26_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "虛接鼎堂。",
        "from": "T85n2848_001:1299b27_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "恨生前不盡指揮。",
        "from": "T85n2848_001:1299b27_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "沒後慚昇法座。",
        "from": "T85n2848_001:1299b27_4",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "以此思忖倍益□□開讚之物奉申迴向。",
        "from": "T85n2848_001:1299b28_5",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "一唯願神遊奈苑欽覺□以尋真。",
        "from": "T85n2848_001:1299b29_6",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "夕慇花臺染戒香而奉聖金輪展轉。",
        "from": "T85n2848_001:1299b29_7",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "常馳歡喜之園室茂斯遊。",
        "from": "T85n2848_001:1299c01_8",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "逈汎□功德水。",
        "from": "T85n2848_001:1299c02_9",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "次持勝福次莊嚴　某乙過□父母所生魂路。",
        "from": "T85n2848_001:1299c03_0",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "伏願足步金□神遊□界。",
        "from": "T85n2848_001:1299c04_1",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "過慈舟於定水永竭昏河。",
        "from": "T85n2848_001:1299c04_2",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "揮惠劍於相材長被受□。",
        "from": "T85n2848_001:1299c05_3",
        "context_with_next_sentence": true
    },
    {
        "source_sentence": "□於親族成□□潔遠□枝羅俱霑潤益。",
        "from": "T85n2848_001:1299c05_4",
        "context_with_next_sentence": false
    }
]