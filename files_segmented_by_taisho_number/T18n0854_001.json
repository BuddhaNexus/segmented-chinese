{
    "T18n0854_001:0164c02": "No. 854",
    "T18n0854_001:0164c03": "胎藏梵字真言上卷",
    "T18n0854_001:0164c05": "灑淨真言：",
    "T18n0854_001:0164c06": "􇖵􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􇩄􄕙􄙺􄗫􇟄􄑘􄑘",
    "T18n0854_001:0164c07": "􄖩􄙺􄗫􇟄􄙺􄗥􆩗􄖭􄑘􄕝􇟄􇩄􆮧􄕙􄙘􄙦",
    "T18n0854_001:0164c08": "􇧼􇟄􄕻􇩷􄕼􄕛􄙘􄙪􄕻􄖫􇟄􇩉􄚩􇖴",
    "T18n0854_001:0164c09": "持地真言曰：",
    "T18n0854_001:0164c10": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙺􇩆􄕗􄕤􄑘􄕘􇟄􄕽􆵌",
    "T18n0854_001:0164c11": "􄖪􄕽􆵍􄕝􆿅􄑼􄙐􇟄􄙘􄗥􄙐􇟄􅋌􄗽􄕑􇟄",
    "T18n0854_001:0164c12": "􇩄􆮧􄕙􄖵􄙀􄙦􇧼􇩉􄚩􇖴",
    "T18n0854_001:0164c13": "持香水真言曰（押紙：已下經二，具緣品）：",
    "T18n0854_001:0164c14": "􄖩􄗰􄙺􄗥􇪪􈎭􇧺􈁀􇟄􆿅􅍉􄗷􇩉􄚩􇖴",
    "T18n0854_001:0164c15": "略奉持護摩真言曰。",
    "T18n0854_001:0164c16": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􆿔􄗥􄚩􄙣􆩘􄑘􄕗􄙣􆩘",
    "T18n0854_001:0164c17": "􄑀􄗽􇟄􇩄􄙢􄗥􄕻􇩷􄖫􈋷􄕗􇟄􆿅􄗚􄙖􇩉􄚩",
    "T18n0854_001:0164c18": "􄙖􄕻􅟇􄙺􈍇􄖪􄪽􆻤􇟄􇩉􄚩􇖴",
    "T18n0854_001:0164c19": "大力大護明妃真言曰：",
    "T18n0854_001:0164c20": "􄖩􄗰􄙺􇩆􄕗􄕤􄑘􄕝􄢢􇟄􄙺􇩆􄗙􄗱􄙘􄑘􄕝􇧸",
    "T18n0854_001:0164c21": "􇟄􄙘􈂫􈁃􄑒􇧸􇟄􄙺􇩆􄕤􇟄􄚲􄑖􇟄􄗽􄚴􄗥􄚩",
    "T18n0854_001:0164c22": "􄙖􄙐􇟄􄙺􅞣􄕗􄕤􄑘􄕗􇟄􄖹􅝯􄖫􈅉􄕝􇩴",
    "T18n0854_001:0164c23": "􇩴􄩟􈁆􄩟􈁆􇟄􆿅􇩄􄖫􄚨􄕝􇟄􇩉􄚩􇖴",
    "T18n0854_001:0164c24": "入佛三昧耶真言曰：",
    "T18n0854_001:0164c25": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􄙺􄗫􄩠􄙺􄗫􇟄􄙺",
    "T18n0854_001:0164c26": "􄗥􄗷􇩉􄚩􇖴",
    "T18n0854_001:0164c27": "法界生真言曰：",
    "T18n0854_001:0164c28": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄕻􇩷􄕻􄕛􇟄􇩉􄚩􄙖",
    "T18n0854_001:0164c29": "􄑈􄚲􇖴",
    "T18n0854_001:0165a01": "金剛薩埵真言曰：",
    "T18n0854_001:0165a02": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􈃄􇟄􄙖􇩶􈇪􄑈􄚲􇖴",
    "T18n0854_001:0165a03": "金剛鎧真言曰：",
    "T18n0854_001:0165a04": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􄙖􄦽􄑀􄙖􄑼􇩴􇖴",
    "T18n0854_001:0165a05": "如來眼真言曰：",
    "T18n0854_001:0165a06": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄕗􄕤􄑘􄕘􄑼􄚸􅧩􄙖􄙒",
    "T18n0854_001:0165a07": "􄑀􄗱􇩉􄚩􇖴",
    "T18n0854_001:0165a08": "塗香真言曰：",
    "T18n0854_001:0165a09": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􄙘􄙦􇧹􄑘􆪤􇪮􄙖􇩉􄚩􇖴",
    "T18n0854_001:0165a10": "華供養真言曰：",
    "T18n0854_001:0165a11": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄗥􄚩􄗬􈇢􄡼􆲪􄕝􇩉",
    "T18n0854_001:0165a12": "􄚩􇖴",
    "T18n0854_001:0165a13": "燒香真言曰：",
    "T18n0854_001:0165a14": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄕻􇩷􄕻􄽙􄖭􄑘􄕝􇩉",
    "T18n0854_001:0165a15": "􄚩􇟄",
    "T18n0854_001:0165a16": "飲食真言曰：",
    "T18n0854_001:0165a17": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􄗽􄗽􄑀􄗽􄗽􄙖􄙌􅛈",
    "T18n0854_001:0165a18": "􄕵􄗥􄚩􄙖􈁖􇩉􄚩􇖴",
    "T18n0854_001:0165a19": "燈明真言曰：",
    "T18n0854_001:0165a20": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄕗􄕤􄑘􄕘􅗗􇟄􆷱􄗽􄕋",
    "T18n0854_001:0165a21": "􄙖􄗚􄙺􄖩􇟄􄑘􄑘􄖲􄕰􅝩􇩉􄚩􇖴",
    "T18n0854_001:0165a22": "閼伽真言曰：",
    "T18n0854_001:0165a23": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄑘􄑘􄖩􄙺􄗦􄙺􄗥􇩉",
    "T18n0854_001:0165a24": "􄚩􇖴",
    "T18n0854_001:0165a25": "如來頂相真言曰：",
    "T18n0854_001:0165a26": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄑘􄑘􄖪􄖩􆩖􆷱􄗽􄕋􇟄",
    "T18n0854_001:0165a27": "􄙘􄙦􇧹􄕻􇩷􄖫􈅉􄕝􇩉􄚩􇖴",
    "T18n0854_001:0165a28": "如來甲真言曰：",
    "T18n0854_001:0165a29": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􇩄􄑼􆧽􄙖􄦽􈁊􄙊􇟄",
    "T18n0854_001:0165a30": "􄙘􆷵􄗽􇩴􇖴",
    "T18n0854_001:0165a31": "如來舌真言：",
    "T18n0854_001:0165b01": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄗥􄚩􄗥􄚩􄕗􄕤􄑘􄕘􄒸",
    "T18n0854_001:0165b02": "􅁰􇟄􄙺􄟗􄕻􅝝􇩄􄕙􆵍􄕗􇩉􄚩􇖴",
    "T18n0854_001:0165b03": "如來圓光真言曰：",
    "T18n0854_001:0165b04": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􈁊􄙋􄗦􄙌􄖫􇟄􄕗􄕤􄑘",
    "T18n0854_001:0165b05": "􄕘􅗗􇩉􄚩􇖴",
    "T18n0854_001:0165b06": "無礙力真言曰（押紙云：已下第二卷，普通",
    "T18n0854_001:0165b07": "真言藏品）：",
    "T18n0854_001:0165b08": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙺􄗥􄕘􄖭􄑘􄕗􄙖􄗽􄒶",
    "T18n0854_001:0165b09": "􄕻􅝝􄖫􅗭􄕗􇟄􄗥􄚩􄗥􄚨􇩉􄚩􇖴",
    "T18n0854_001:0165b10": "彌勒菩薩真言曰：",
    "T18n0854_001:0165b11": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􄒸􄕡􄒶􄗷􇟄􄙺􇩆",
    "T18n0854_001:0165b12": "􄙺􄽚􄙢􄗲􄖭􄑘􄕗􇟄􇩉􄚩􇖴",
    "T18n0854_001:0165b13": "虛空藏真言曰：",
    "T18n0854_001:0165b14": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􄑁􄙢􄙺􄗥􄕘􄖭􄑘",
    "T18n0854_001:0165b15": "􄕘􇟄􄙘􄑾􈂷􄗍􄗽􄕻􄗽􇟄􇩉􄚩􇖴",
    "T18n0854_001:0165b16": "除蓋障真言曰：",
    "T18n0854_001:0165b17": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􈅺􄙺􄽙􄚪􄕘􄡼􆲪􄕗",
    "T18n0854_001:0165b18": "􇟄􄩨􄩨􄙈􄙈􇩉􄚩􇖴",
    "T18n0854_001:0165b19": "觀自在真言曰：",
    "T18n0854_001:0165b20": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙺􇩆􄕗􄕤􄑘􄕘􄙖􄙒􄑂",
    "T18n0854_001:0165b21": "􄕗􇟄􄑀􄙃􄕋􄗥􄗱􇟄􄗽􄗽􄗽􇩴􄓁􇩉􄚩􇖴",
    "T18n0854_001:0165b22": "得大勢至真言曰：",
    "T18n0854_001:0165b23": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄒶􄒶􄚧􇩉􄚩􇖴",
    "T18n0854_001:0165b24": "多羅尊真言曰：",
    "T18n0854_001:0165b25": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄑀􄙃􄕑􇪮􄙜􇟄􄕘􄙄􄕘",
    "T18n0854_001:0165b26": "􄙀􄕍􇟄􇩉􄚩􇖴",
    "T18n0854_001:0165b27": "毘俱胝真言曰：",
    "T18n0854_001:0165b28": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙺􇩆􄗙􄗱􄩟􈍇􈍇􇩴",
    "T18n0854_001:0165b29": "􆷱􈁆􄗱􇩉􄚩􇖴",
    "T18n0854_001:0165b30": "白處尊真言曰：",
    "T18n0854_001:0165b31": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄕗􄕤􄑘􄕗􄙘􄙮􄗱􇟄􄚦",
    "T18n0854_001:0165c01": "􄗙􄙜􇟄􄖵􇪯􄗦􄙌􄖫􇩉􄚩􇖴",
    "T18n0854_001:0165c02": "何耶揭嘌嚩真言曰：",
    "T18n0854_001:0165c03": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􇩴􄑍􄕯􄕉􄓀􇟄􆷱􈁆",
    "T18n0854_001:0165c04": "􄗱􇟄􇩉􄚩􇖴",
    "T18n0854_001:0165c05": "地藏菩薩真言曰：",
    "T18n0854_001:0165c06": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄚨􄚨􄚨􇟄􈂼􄕗􄖭􇟄􇩉",
    "T18n0854_001:0165c07": "􄚩􇖴",
    "T18n0854_001:0165c08": "文殊師利真言曰：",
    "T18n0854_001:0165c09": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄚮􄚮􄑄􄗦􄗽􄑀􇟄􄙘􈁃",
    "T18n0854_001:0165c10": "􈁍􄖵􄕣􅁦􄕗􇟄􅋌􄗽􇥗􇩄􄕙􈇍􇩉􄚩􇖴",
    "T18n0854_001:0165c11": "金剛手真言曰：",
    "T18n0854_001:0165c12": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􄑼􆧽􄗥􄚩􄙆􄙮􄕋",
    "T18n0854_001:0165c13": "􇩴􇖴",
    "T18n0854_001:0165c14": "忙莾計真言曰：",
    "T18n0854_001:0165c15": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􄩠􄓚􄩠􄓚􇟄􄒶􄗻􄕙􇟄􇩉",
    "T18n0854_001:0165c16": "􄚩􇖴",
    "T18n0854_001:0165c17": "金剛鎖真言曰：",
    "T18n0854_001:0165c18": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􇩴􄗍􈏎􄗍􈏎􄗱􇟄􄗭",
    "T18n0854_001:0165c19": "􄓚􄗭􄓚􄗱􇟄􄙖􄧃􇪮􄙜􇟄􄙺􇩆􈅆􇩄􄕙􄚨􄕝􇟄",
    "T18n0854_001:0165c20": "􇩉􄚩􇖴",
    "T18n0854_001:0165c21": "金剛月靨真言曰：",
    "T18n0854_001:0165c22": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􈁐􇩴􄗁􄓚􇟄􇩉􄚩􇖴",
    "T18n0854_001:0165c23": "金剛針真言曰：",
    "T18n0854_001:0165c24": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􄙺􅞣􄕻􈆺􄖫􄙜􄕻􄖫",
    "T18n0854_001:0165c25": "􄙖􄦽􈂼􄑾􄙖􄗽􄕵􇟄􇩉􄚩􇖴",
    "T18n0854_001:0165c26": "一切持金剛真言曰：",
    "T18n0854_001:0165c27": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􇩴􇩴􇩴􄗁􈁆􄗁􈁆",
    "T18n0854_001:0165c28": "􄓀􄓀􇩉􄚩􇖴",
    "T18n0854_001:0165c29": "一切奉教真言曰：",
    "T18n0854_001:0165c30": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􄚮􄚮􈃛􄑾􄗾􄗱􄙼􇟄",
    "T18n0854_001:0165c31": "􈃈􈁒􈃈􈁒􇟄􄑍􄕯􄑍􄕯􄖵􄙀􈁂􄗽􄗱􇟄􄙺􅞣􈆡",
    "T18n0854_001:0166a01": "􄑀􄗾􄕕􅁥􇩄􄕙􈂤􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166a02": "釋迦牟尼真言曰：",
    "T18n0854_001:0166a03": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙺􇩆􄯁􄙢􄖫􄙺􈍇􄖩􇟄",
    "T18n0854_001:0166a04": "􄙺􇩆􄕻􈆺􄙖􄙤􄗾􄪼􆻤􇟄􄑘􄑘􄖩􄙺􄗦􄙺",
    "T18n0854_001:0166a05": "􄗥􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166a06": "毫相真言曰：",
    "T18n0854_001:0166a07": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙖􄗾􄕵􄙖􄗽􄪽􆻪",
    "T18n0854_001:0166a08": "􇩴􇖴",
    "T18n0854_001:0166a09": "一切諸佛頂真言曰：",
    "T18n0854_001:0166a10": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙠􄙠􄙠􇩴􇩴􄗁",
    "T18n0854_001:0166a11": "􈁆􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166a12": "無能勝真言曰：",
    "T18n0854_001:0166a13": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􈁔􈁔􈋈􈋈􈋀􈋀􇩉􄚩􇖴",
    "T18n0854_001:0166a14": "無能勝妃真言曰：",
    "T18n0854_001:0166a15": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􄖶􄗾􄒸􄕝􇟄􄒶􄗻",
    "T18n0854_001:0166a16": "􄕙􄕗􄓴􄕝􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166a17": "地神真言曰：",
    "T18n0854_001:0166a18": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆯷􄕥􄣅􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166a19": "毘紐天真言曰：",
    "T18n0854_001:0166a20": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙘􈇡􄙜􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166a21": "伊舍那天真言曰：",
    "T18n0854_001:0166a22": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙃􇪰􄗲􇩉􄚩􇖴",
    "T18n0854_001:0166a23": "風神真言曰：",
    "T18n0854_001:0166a24": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙗􄗱􄙜􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166a25": "六美音天：",
    "T18n0854_001:0166a26": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􈂼􄗽􇩉􄟞􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166a27": "羅剎主真言曰：",
    "T18n0854_001:0166a28": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄗾􄚴􄙻􄕽􄖵􄕗􄗷􇟄",
    "T18n0854_001:0166a29": "􇩉􄚩􇖴",
    "T18n0854_001:0166a30": "四閻魔真言曰：",
    "T18n0854_001:0166a31": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙝􄙖􅁤􄕘􄗱􇟄􇩉",
    "T18n0854_001:0166b01": "􄚩􇖴",
    "T18n0854_001:0166b02": "三死王真言曰：",
    "T18n0854_001:0166b03": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􇩸􄟗􄙜􇩉􄚩􇖴",
    "T18n0854_001:0166b04": "黑夜神真言曰：",
    "T18n0854_001:0166b05": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄑁􄙊􄗾􈋁􄗷􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166b06": "七母等真言曰：",
    "T18n0854_001:0166b07": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄗥􆯣􇧸􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166b08": "釋提桓因真言曰：",
    "T18n0854_001:0166b09": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙢􄥇􄗱􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166b10": "嚩嚕拏龍真言曰：",
    "T18n0854_001:0166b11": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􈃽􄖵􄕗􄗷􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166b12": "五梵天真言曰：",
    "T18n0854_001:0166b13": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􇩄􄒶􄖵􄕗􄗷􇟄􄚩􇖴",
    "T18n0854_001:0166b14": "日天真言曰：",
    "T18n0854_001:0166b15": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􄕱􄟘􄗱􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166b16": "月天真言：",
    "T18n0854_001:0166b17": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄑼􈆦􄗱􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166b18": "十諸龍真言：",
    "T18n0854_001:0166b19": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄗫􄑤􄙢􄖬􄗷􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166b20": "難陀跋難陀真言曰：",
    "T18n0854_001:0166b21": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄖩􆩴􄖵􄖩􆩮􄗱􇟄􇩉",
    "T18n0854_001:0166b22": "􄚩􇖴",
    "T18n0854_001:0166b23": "虛空眼明妃真言曰：",
    "T18n0854_001:0166b24": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄑘􄑘􄖩􄙖􄗽􄙊􄚴􄕑􇟄",
    "T18n0854_001:0166b25": "􄑘􄑘􄖩􄙺􄗥􄗱􇟄􄙺􇩆􄕟􆲪􄕘􇟄􄗛􄙻􄗽􄚦",
    "T18n0854_001:0166b26": "􄗙􄙜􇟄􈁊􄙊􄖪􄗭􄑥􈁀􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166b27": "不動主真言曰：",
    "T18n0854_001:0166b28": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􄑼􄓺􄗥􄚩􄙆􄙮􄕋􇟄",
    "T18n0854_001:0166b29": "􆷱􈁆􄗱􇩴􈇳􄑀􈁅􈁝􇖴",
    "T18n0854_001:0166b30": "降三世真言曰：",
    "T18n0854_001:0166b31": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􄚨􄚨􄚨􇟄􄙘􅋌􄗷􇟄􄙺",
    "T18n0854_001:0166c01": "􅞣􄕗􄕤􄑘􄕘􇟄􄙘􄙮􄗱􄚦􄗙􄙖􇟄􈉩􄙒􄛀􄙘",
    "T18n0854_001:0166c02": "􄒶􄗱􇩴􄓁􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166c03": "聲聞真言曰：",
    "T18n0854_001:0166c04": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄑆􄕛􇩄􄟗􄗱􇟄􄙘􄑘",
    "T18n0854_001:0166c05": "􄕗􄑀􇩷􄖫􅗭􄕗􇟄􇩴􇖴",
    "T18n0854_001:0166c06": "緣覺真言曰：",
    "T18n0854_001:0166c07": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙡􇖴",
    "T18n0854_001:0166c08": "普一切諸佛菩薩心真言曰：",
    "T18n0854_001:0166c09": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙺􅞣􈎭􇧺􄗕􄕽􄙺􄽙􇟄",
    "T18n0854_001:0166c10": "􆱨􄕯􄗻􄡉􄙜􄙢􈋃􇟄􄖩􄗰􄙺􅞣􄙘􄕵􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166c11": "普世明妃真言曰：",
    "T18n0854_001:0166c12": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙒􄑁􄙒􄑁􄑀􄗾􄗱􇟄",
    "T18n0854_001:0166c13": "􄙺􇩆􄕵􄙖􄖪􄑘􄗱􄚴􄑘􈏎􅞤􇟄􆿅􈂼􄗽􄑘􄙃",
    "T18n0854_001:0166c14": "􄓲􈆡􄕯􄗽􇟄􄗥􄚩􄗽􄑙􄕱􇟄􆱨􄕯􄗲􇟄􄡉􄑀",
    "T18n0854_001:0166c15": "􅞻􄗱􇟄􄙘􄑾􇩌􄑘􄕙􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166c16": "一切諸佛真言曰：",
    "T18n0854_001:0166c17": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙺􇩆􄕤􇟄􄙘􄗥􄕙􄙘􄑂",
    "T18n0854_001:0166c18": "􄗽􈇱􇟄􄕻􇩷􄕼􄕛􄖫􅗭􄕗􄚦􄚦􄚨􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166c19": "不可越守護門真言曰：",
    "T18n0854_001:0166c20": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄓲􅛔􅞻􄗥􄚩􄙆􄙮",
    "T18n0854_001:0166c21": "􄕋􇟄􄑍􄕯􄗱􄙺􈊷􄕗􄕤􄑘􄕘􈆷􄑄􄙃􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166c22": "相向守護門真言曰：",
    "T18n0854_001:0166c23": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􄗛􈁃􄑌􄚮􄗥􄚩􄪼􄑼",
    "T18n0854_001:0166c24": "􄓺􇟄􆿅􄗛􈁃􄑍􇟄􈃈􈁒􄑌􄕯􄗱􇟄􄑂􄑾􄗽􄗱",
    "T18n0854_001:0166c25": "􄙼􇟄􄙺􄗥􄗱􄗥􄖭􅋌􄗽􇟄􇩉􄚩􇖴",
    "T18n0854_001:0166c26": "大結界真言曰：",
    "T18n0854_001:0166c27": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􄙺􇩆􈇳􄖭􄑘􄕝􇟄􄙖􄾫",
    "T18n0854_001:0166c28": "􄗱􄙽􄗯􇟄􄗥􄚩􄙺􄗥􄗱􄖫􅗭􄕝􇟄􅋌􄗽􄕋􆿅􇩄",
    "T18n0854_001:0166c29": "􄕙􄚨􄕵􇟄􄕻􄑀􄕻􄑀􇟄􄑼􄗽􄑼􄗽􇟄􄙖􄾫􇟄",
    "T18n0854_001:0166c30": "􄕯􄙢􈋅􄙬􇟄􄙺􇩆􄕗􄕤􄑘􄕘􄓲􈂤􄕝􇟄􇩄􄙖",
    "T18n0854_001:0166c31": "􄗽􄕻􅝝􄙊􇧹􄗏􄒶􄗷􇟄􄗙􄑘􄙖􄕙􇟄􄗏􄑄􄙃􄗏",
    "T18n0854_001:0167a01": "􄑄􄙐􇟄􄙐􄙌􄖹􄙀􇟄􇩉􄚩􇖴",
    "T18n0854_001:0167a02": "菩提心真言：",
    "T18n0854_001:0167a03": "􄗕􄕽􆿅􇖴",
    "T18n0854_001:0167a04": "菩提行真言：",
    "T18n0854_001:0167a05": "􄑼􇩈􆿆􇖴",
    "T18n0854_001:0167a06": "成菩提真言曰：",
    "T18n0854_001:0167a07": "􄚦􄗕􄕽􆿓􇖴",
    "T18n0854_001:0167a08": "涅槃真言曰：",
    "T18n0854_001:0167a09": "􄖫􅞤􄕋􇟄􆿔􇖴",
    "T18n0854_001:0167a10": "降三世真言曰：",
    "T18n0854_001:0167a11": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􄩞􄙒􄛀􄗏􄒶􄗱􈉦􇖴",
    "T18n0854_001:0167a12": "不動尊真言曰：",
    "T18n0854_001:0167a13": "􄖩􄗰􄙺􄗥􆩖􄙖􄦽􄕕􇟄􈍇􇖴",
    "T18n0854_001:0167a14": "無動尊真言曰：",
    "T18n0854_001:0167a15": "􆿅􄑼􄙊􄖩􈉸􇖴",
    "T18n0854_001:0167a16": "除蓋障真言曰：",
    "T18n0854_001:0167a17": "􄙺􅞣􄖬􄙖􄗽􄕋􄗏􈋆􄗜􇖴",
    "T18n0854_001:0167a18": "除蓋障真言曰：",
    "T18n0854_001:0167a19": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿔􇖴",
    "T18n0854_001:0167a20": "觀自在真言曰：",
    "T18n0854_001:0167a21": "􆿅􄙖􄙒􄑂􄕝􈂫􄗽􇟄􄚧􇖴",
    "T18n0854_001:0167a22": "金剛手真言曰：",
    "T18n0854_001:0167a23": "􄙖􄦽􄖶􄕍􇟄􄙖􄦽􄕕􄙡􇖴",
    "T18n0854_001:0167a24": "文殊師利真言曰：",
    "T18n0854_001:0167a25": "􈆿􄒺􈊸􇟄􈎭􇧹􈁀􄗯􇖴",
    "T18n0854_001:0167a26": "虛空眼真言曰：",
    "T18n0854_001:0167a27": "􄑘􄑘􄖩􄙒􄑼􄖪􇟄􄑢􇖴",
    "T18n0854_001:0167a28": "法界真言曰：",
    "T18n0854_001:0167a29": "􄕻􇩷􄕼􄩩􄙈􇖴",
    "T18n0854_001:0167a30": "大勤勇真言：",
    "T18n0854_001:0167b01": "􄗥􄚩􄙙􄙉􇟄􄑖􇖴",
    "T18n0854_001:0167b02": "水自在真言曰：",
    "T18n0854_001:0167b03": "􄒶􄙑􈂫􄗾􇟄􄓀􇖴",
    "T18n0854_001:0167b04": "多羅尊真言曰：",
    "T18n0854_001:0167b05": "􄕘􄗾􄕵􄙙􇟄􄕡􇖴",
    "T18n0854_001:0167b06": "毘俱胝真言曰：",
    "T18n0854_001:0167b07": "􇧸􇟄􆱄􄑄􄓝􇖴",
    "T18n0854_001:0167b08": "得大勢至真言曰：",
    "T18n0854_001:0167b09": "􄚦􇟄􄗥􄚩􇩉􄗥􄪽􆻯􇖴",
    "T18n0854_001:0167b10": "白處尊真言曰：",
    "T18n0854_001:0167b11": "􄖿􇟄􄖵􄓸􄗽􄙗􄙼􄖬􇖴",
    "T18n0854_001:0167b12": "何耶揭嘌嚩真言曰：",
    "T18n0854_001:0167b13": "􄚲􇟄􄚨􄗱􄥢􄙡􇖴",
    "T18n0854_001:0167b14": "耶輸陀羅真言曰：",
    "T18n0854_001:0167b15": "􄗻􇟄􄗱􄙪􄕻􄗾􇖴",
    "T18n0854_001:0167b16": "寶手真言：",
    "T18n0854_001:0167b17": "􄚦􇟄􄗽􅑈􄖶􄕍􇖴",
    "T18n0854_001:0167b18": "光網真言曰：",
    "T18n0854_001:0167b19": "􄓀􇟄􄒶􄙌􄖬􇩄􄗙􇖴",
    "T18n0854_001:0167b20": "釋迦牟尼真言曰：",
    "T18n0854_001:0167b21": "􄗤􇟄􄙢􄛀􈁃􄖫􇖴",
    "T18n0854_001:0167b22": "□佛頂真言曰：",
    "T18n0854_001:0167b23": "􇩴􇩴􄚦􈆭􇟄􇩴􈋱􇟄􆿉􈃇􄙮􇩌􄗻􇖴",
    "T18n0854_001:0167b24": "白傘蓋佛頂真言曰：",
    "T18n0854_001:0167b25": "􄙔􇟄􄙼􄕘􄕗􄖵􇩌􇖴",
    "T18n0854_001:0167b26": "勝佛頂真言曰：",
    "T18n0854_001:0167b27": "􄙬􇟄􄒶􄗹􈃇􄙮􇖴",
    "T18n0854_001:0167b28": "最勝佛頂真言曰：",
    "T18n0854_001:0167b29": "􄙥􄙽􇟄􄙘􄒶􄗹􈃇􄙮􇖴",
    "T18n0854_001:0167c01": "光聚佛頂真言：",
    "T18n0854_001:0167c02": "􈁓􇟄􄕝􄒼􄗾􄙤􇖴",
    "T18n0854_001:0167c03": "除障佛頂真言：",
    "T18n0854_001:0167c04": "􄮭􇟄􄙘􄑂􄗽􄕋􄖿􄒦􈃇􄙮􇖴",
    "T18n0854_001:0167c05": "世明妃真言曰：",
    "T18n0854_001:0167c06": "􄕡􄚲􄖿􄚲􄗻􇟄􄗏􄟰􄗾􈃇􄙒􄑆􇖴",
    "T18n0854_001:0167c07": "無能勝真言曰：",
    "T18n0854_001:0167c08": "􈆭􇟄􆿅􄖵􄗾􄒸􄗾􇖴",
    "T18n0854_001:0167c09": "地神真言曰：",
    "T18n0854_001:0167c10": "􄗏􇟄􆯷􄕥􄙙􇖴",
    "T18n0854_001:0167c11": "計設尼真言曰：",
    "T18n0854_001:0167c12": "􄑂􄙌􇟄􄑆􄙤􄖬􇖴",
    "T18n0854_001:0167c13": "烏婆計設尼真言曰：",
    "T18n0854_001:0167c14": "􄕱􄙌􇟄􆿉􄖵􄑆􄙤􄖬􇖴",
    "T18n0854_001:0167c15": "質多羅童子真言曰：",
    "T18n0854_001:0167c16": "􄗧􄙌􇟄􄑾􄩟􇖴",
    "T18n0854_001:0167c17": "財惠童子真言曰：",
    "T18n0854_001:0167c18": "􄚪􄙌􇟄􄙖􈂼􄗥􄕙􇖴",
    "T18n0854_001:0167c19": "除疑怪真言曰：",
    "T18n0854_001:0167c20": "􄚨􄙺􈁀􇟄􄚱􄑄􄚨􄙌􄖴􇖴",
    "T18n0854_001:0167c21": "施一切眾生無畏真言曰：",
    "T18n0854_001:0167c22": "􄗽􄙺􈁀􇟄􄙺􇩆􄙺􄽚􄗙􄗻􄕯􄕵􇖴",
    "T18n0854_001:0167c23": "除一切惡趣真言曰：",
    "T18n0854_001:0167c24": "􈍇􄙺􄖳􇟄􄙺􇩆􄖶􄗲􄒶􄚳􇖴",
    "T18n0854_001:0167c25": "哀愍惠真言曰：",
    "T18n0854_001:0167c26": "􈍇􄙺􄖳􇟄􈍇",
    "T18n0854_001:0167c27": "大慈生真言：",
    "T18n0854_001:0167c28": "􄓰􇟄􄗥􄚩􄗬􈇢􈋇􆲪􄕗􇖴",
    "T18n0854_001:0167c29": "大□纏真言曰：",
    "T18n0854_001:0167c30": "􄗻􇟄􄗥􄚩􄑀􄙃􈇱􇩄􄕙􄕗􇖴",
    "T18n0854_001:0168a01": "除一切熱惱真言曰：",
    "T18n0854_001:0168a02": "􈉁􈍇􅞣􄕰􄚨􇩄􄙢􄗧􄖩􇖴",
    "T18n0854_001:0168a03": "不可思議真言曰：",
    "T18n0854_001:0168a04": "􇇴􇟄􆿅􄑾􈃭􄗥􄕙􄕯􆽕􇖴",
    "T18n0854_001:0168a05": "地藏旗真言曰：",
    "T18n0854_001:0168a06": "􄚨􄚨􄚨􇟄􄗏􄙺􇩆􄙣􄖵􄙀􈁂􄗾􄑀􇩉􄚩􇖴",
    "T18n0854_001:0168a07": "寶處真言曰：",
    "T18n0854_001:0168a08": "􄕹􄓀􇟄􄗽􇩋􄑀􄗽􇖴",
    "T18n0854_001:0168a09": "寶手真言曰：",
    "T18n0854_001:0168a10": "􄙮􇟄􄗽􅑈􄖶􄕍􇖴",
    "T18n0854_001:0168a11": "持地真言曰：",
    "T18n0854_001:0168a12": "􄑺􇟄􄕻􄗽􄕍􄾫􄗽􄓘􇖴",
    "T18n0854_001:0168a13": "寶印手真言曰：",
    "T18n0854_001:0168a14": "􄗋􇟄􄗽􅑈􈁃􄩷􄚨􆶩􇖴",
    "T18n0854_001:0168a15": "堅固意真言曰：",
    "T18n0854_001:0168a16": "􈃄􇟄􆯫􄓾􄟼􄙢􄗱􇖴",
    "T18n0854_001:0168a17": "虛空無垢真言曰：",
    "T18n0854_001:0168a18": "􄚲􇟄􄑘􄑘􄖪􄗥􄙊􇖴",
    "T18n0854_001:0168a19": "虛空惠真言曰：",
    "T18n0854_001:0168a20": "􈋈􇟄􄑘􄑘􄖩􄗥􄕝􇖴",
    "T18n0854_001:0168a21": "清淨惠真言曰：",
    "T18n0854_001:0168a22": "􄑘􄕡􇟄􄗏􄙦􇧹􄗥􄕝􇖴",
    "T18n0854_001:0168a23": "行□真言曰：",
    "T18n0854_001:0168a24": "􄕽􄙈􇟄􄙀􇩌􄗥􄕝􇖴",
    "T18n0854_001:0168a25": "□惠真言曰：",
    "T18n0854_001:0168a26": "􈍇􄙼􄗽􈎭􇧼􇖴",
    "T18n0854_001:0168a27": "□：",
    "T18n0854_001:0168a28": "􈍇􈊸􄚨􄭛􇟄􄑂􄗾􈇱􇖴",
    "T18n0854_001:0168a29": "諸菩薩所說真言曰：",
    "T18n0854_001:0168b01": "􄚿􄓲􄕗􄗽􄗻􄑊􇟄􄗱􄕫􈅂􄗕􄕽􄙺􄽚􇖴",
    "T18n0854_001:0168b02": "淨居真言曰：",
    "T18n0854_001:0168b03": "􄖩􄗭􄗽􄗥􄕻􅝝􄚦􄗙􄙖􄗏􄗙􄙖􇟄􄑀􄕣􄖩􄚦",
    "T18n0854_001:0168b04": "􄚦􄙺􄕝􇩉􄚩􇖴",
    "T18n0854_001:0168b05": "淨居天真言曰：",
    "T18n0854_001:0168b06": "􄙦􈒻􄙖􄚩􇖴",
    "T18n0854_001:0168b07": "羅剎娑真言曰：",
    "T18n0854_001:0168b08": "􄥑􄑆􄙀􇟄􄗾􄚴􄙺􇖴",
    "T18n0854_001:0168b09": "諸荼吉尼真言曰：",
    "T18n0854_001:0168b10": "􇩵􄚳􇟄􄓲􄑂􄖬􈁀􇖴",
    "T18n0854_001:0168b11": "藥叉女真言曰：",
    "T18n0854_001:0168b12": "􄗱􄚴􄗏􄟰􄕻􄙀􇟄􄗱􄚶􄕎􈁀􇖴",
    "T18n0854_001:0168b13": "諸毘舍遮真言曰：",
    "T18n0854_001:0168b14": "􄖷􄑾􄖷􄑾􇟄􄖷􄙣􄑾􄖬􈁀􇖴",
    "T18n0854_001:0168b15": "諸部多羅真言曰：",
    "T18n0854_001:0168b16": "􈉎􈉁􄑜􆿇􄗯􄚦􄕝􇟄􄗞􄕘􈁀􇟄",
    "T18n0854_001:0168b17": "諸阿修羅真言曰：",
    "T18n0854_001:0168b18": "􄗽􄓤􄗽􄓤􈆮􄕡􄫬􇟄􆿅􆿅􇩄􇖴",
    "T18n0854_001:0168b19": "諸摩睺囉伽真言曰：",
    "T18n0854_001:0168b20": "􄗾􇟄􄑘􄗽􄙔􈃐􄗽􈃗􇖴",
    "T18n0854_001:0168b21": "摩睺羅伽真言曰：",
    "T18n0854_001:0168b22": "􄗥􄚰􄗽􄑘􇖴",
    "T18n0854_001:0168b23": "諸緊那羅真言曰：",
    "T18n0854_001:0168b24": "􄚨􄑌􄙺􄖳􄗏􄚨􄙺􄖳􇟄􄑂􆩖􄗽􈃄􇖴",
    "T18n0854_001:0168b25": "諸人真言曰：",
    "T18n0854_001:0168b26": "􆿇􇩺􄖵􄙈􄗥􈃂􄗥􄗷􄗫􅁤􄚩􄗥􄖭􄣖􄕕􇖴",
    "T18n0854_001:0168b27": "􄓦􇖴",
    "T18n0854_001:0168b28": "無所不至真言曰　已下第三卷（押紙云：",
    "T18n0854_001:0168b29": "已下第三卷悉地出現品）：",
    "T18n0854_001:0168b30": "􄖩􄗰􄙺􅞣􄕗􄕤􄑘􄙄􄢢􇟄􄗏􈂫􈁃􄑒􇧸􄙺􇩆",
    "T18n0854_001:0168c01": "􄕤􇟄􆿅􆿆􆿓􆿔􇖴",
    "T18n0854_001:0168c02": "□空藏明妃真言曰：",
    "T18n0854_001:0168c03": "􄖩􄗰􄙺􇩆􄕗􄕤􄑘􄕝􄢢􇟄􄙘􈂫􈁃􄑒􇧸􇟄􄙺􇩆",
    "T18n0854_001:0168c04": "􄕤􄑖􇟄􇇳􆲪􄕝􆷱􄗽􄚫􄗯􇟄􄑘􄑘􄖩􄑊􇟄􇩉",
    "T18n0854_001:0168c05": "􄚩􇖴",
    "T18n0854_001:0168c06": "滿足一切金剛字句真言曰：",
    "T18n0854_001:0168c07": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􈅺􄗏􄗽􇩴􄑖􇖴",
    "T18n0854_001:0168c08": "無礙力明妃真言曰（押紙云：已下第三卷",
    "T18n0854_001:0168c09": "轉字輪マタラ行品）：",
    "T18n0854_001:0168c10": "􄕗􇩊􄕤􄑘􄑘􄖩􄙺􄗫􇟄􆿅􇩄􄕙􄙺􄗫􇟄􄙺􇩆",
    "T18n0854_001:0168c11": "􄕗􄕤􄑘􄕘􄙺􆩖􆯣􄑘􇟄􄑘􄑘􄖩􄙺􄗥􇟄　􄙖",
    "T18n0854_001:0168c12": "􄗽􄙊􄚴􄕑􇟄􇩉􄚩􇖴",
    "T18n0854_001:0168c13": "救世者真言曰：",
    "T18n0854_001:0168c14": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􇟄􆿅􇖴",
    "T18n0854_001:0168c15": "無能害力明妃真言曰：",
    "T18n0854_001:0168c16": "􄖩􄗰􄙺􅞣􄕗􄕤􄑘􄕝􇧸􇟄􄙺􅞣􈁃􄑒􇧸􇟄􆿅􄙺",
    "T18n0854_001:0168c17": "􄗫􄖵􄗽􄗫􇟄􆿅􄑼􄙐􇟄􄑘􄑘􄖯􇟄􅋌􄗽􄕑􇟄",
    "T18n0854_001:0168c18": "􄙺􅞣􄩟􄖭􄑘􄕝􇟄􇩉􄚩􇖴",
    "T18n0854_001:0168c19": "置字句：",
    "T18n0854_001:0168c20": "􄖩􄗰􄙺􄗥􆩖􈎭􇧺􈁀􄗯􇖴已下第四卷。"
}